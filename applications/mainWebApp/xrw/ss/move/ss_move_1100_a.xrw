					model.setValue("/root/reqData/reqform/move_save_form/ipju_aptcd_cnfm",model.getValue("/root/resData/gridData/detailGrid/ipju_aptcd"));
					model.setValue("/root/reqData/reqform/move_save_form/post_dlvyn_cnfm",model.getValue("/root/resData/gridData/detailGrid/post_dlvyn"));
