				function gridClick() 
				{
					if( PID1100datagrid1.textMatrix(PID1100datagrid1.row, PID1100datagrid1.colRef("regdt")) == "")
						return;
					
					model.setValue("/root/reqData/reqform/move_list_form/regdt",PID1100datagrid1.textMatrix(PID1100datagrid1.row, PID1100datagrid1.colRef("regdt")));
					model.setValue("/root/reqData/reqform/move_list_form/regno", PID1100datagrid1.textMatrix(PID1100datagrid1.row, PID1100datagrid1.colRef("regno")));						
					
					remakeNode("/root/tempData/dataTemp");
					model.send("PID1100gridSearch", false);
					 
					if(!submit_completed())
						return;
	
					childValueCopy("/root/reqData/reqform/move_list_form", "root/tempData/dataTemp/detailInfo"); // 조회된 노드들을 대상에 한번에 전부 카피한다.
					childValueCopy("/root/reqData/reqform/move_detail_form", "root/tempData/dataTemp/detailInfo");
					
					childValueCopy("/root/resData/gridData/detailGrid", "root/tempData/dataTemp/detailInfo"); // 조회된 노드들을 대상에 한번에 전부 카피한다.
					
					model.copyNode("/root/initData/comboSet/ipju_aptCombo", "/root/tempData/dataTemp/ipju_aptCombo");
					
					model.setValue("/root/resData/gridData/detailGrid/busu", model.getValue("/root/tempData/dataTemp/busu")); //매체/부수
					model.setValue("/root/resData/gridData/detailGrid/rdrtel", model.getValue("/root/tempData/dataTemp/rdrtel")); //독자정보 전화번호
					model.setValue("/root/resData/gridData/detailGrid/rdrptph", model.getValue("/root/tempData/dataTemp/rdrptph")); //독자정보 휴대폰번호
					model.setValue("/root/resData/gridData/detailGrid/zip", model.getValue("/root/tempData/dataTemp/zip")); //독자정보 현주소
					model.setValue("/root/resData/gridData/detailGrid/movmtel", model.getValue("/root/tempData/dataTemp/movmtel")); //이사지정보 전화번호
					model.setValue("/root/resData/gridData/detailGrid/Movmzip", model.getValue("/root/tempData/dataTemp/Movmzip")); //이사지정보 배달주소
					model.setValue("/root/resData/gridData/detailGrid/trsfbocnfmdt_tm", model.getValue("/root/tempData/dataTemp/trsfbocnfmdt_tm")); //인계지국 일시			
					model.setValue("/root/resData/gridData/detailGrid/acptbocnfmdt_tm", model.getValue("/root/tempData/dataTemp/acptbocnfmdt_tm")); //인수지국 일시			
					model.setValue("/root/resData/gridData/detailGrid/cns_empcnfmdt", model.getValue("/root/tempData/dataTemp/cns_empcnfmdt")); //상담원 일시			
					model.setValue("/root/resData/gridData/detailGrid/bocd", model.getValue("/root/tempData/dataTemp/bocd")); //bocd 값 가져온다.
					
					
					
					model.removenode("/root/tempData/dataTemp/detailInfo");	
					
					PID1100group4.refresh();
				}
				
				function onClickBranchPopup()
				{
					openBranch("/root/reqData/reqform/move_search_form", "/root/tempData/tempBranch");
					
	               		if(model.getValue("/root/tempData/tempBranch/bocd") == "")
						return;

					iptCommonmBranchQ.value = model.getValue("/root/tempData/tempBranch/bonm");
					modelCopyValue("/root/reqData/reqform/move_search_form/bocd", "/root/tempData/tempBranch/bocd");
					
					if(model.getValue("/root/initData/gs_request") == "L")
					{
						model.setValue("/root/initData/gs_request", "N");
						search();
					}
				}
				
				function onKeyDown()
				{
					if(event.keyCode == 13)
					{
						model.setFocus("btnSearchBranchQ");
		
						if(iptCommonmBranchQ.value == '')
							onClickBranchPopup();
						else
						{
							modelCopyValue("/root/reqData/reqform/move_search_form/common_deptcd", "/root/reqData/reqform/move_search_form/deptcd");
							//검색 공통 모듈 호출. param : "입력param path", "출력 path", "callback함수", flag 값을 가지고 있는 path"
							//처리순서 : 지국조회 -> callback함수 호출 -> flag값에 따른 처리.
							//callbak함수는 옵션. search() 및 searchWithBranch() 함수가 존재해야 함.
							searchBranchByName("/root/reqData/reqform/move_search_form", "/root/tempData/dataTemp/branchGrid", "searchBranchCallBack()", "/root/initData/gs_request");
						}
					}
				}
				
				//기존의 bo_list_search
