				model.setValue("/root/reqData/reqform/rdr_detail_form/seq", PID4300datagrid1.textMatrix(PID4300datagrid1.row, PID4300datagrid1.colRef("seq")));
				model.setValue("/root/reqData/reqform/rdr_detail_form/closmm", PID4300datagrid1.textMatrix(PID4300datagrid1.row, PID4300datagrid1.colRef("closmm")));
				
				model.send("PID4300svyBsComp", false);
