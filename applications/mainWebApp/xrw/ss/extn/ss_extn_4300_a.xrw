					model.setFocus("btnSearchBranchQ");
					if(iptCommonmBranchQ.value == '')
						onClickBranchPopup();
					else
					{
						//화면별 선처리
						modelCopyValue("/root/reqData/reqform/rdr_search_form/deptcd", "/root/reqData/reqform/rdr_search_form/deptcd");
