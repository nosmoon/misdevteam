			//센터전달
			function bo_send(){

				if(  PID4300datagrid1.textMatrix(PID4300datagrid1.row, PID4300datagrid1.colRef("rdr_no")) == "" ){
					alert("목록에서 센터에 전달할 독자를 선택해주세요.");
					return;
				}
				
				if (model.getValue("/root/resData/gridData/detailData/bo_sendyn")=="Y"){
					alert("이미 센터에 전달을 완료하였습니다.");
					return;
				}	
				
				if(PID4300datagrid1.textMatrix(PID4300datagrid1.row, PID4300datagrid1.colRef("issup")) == "Y"){
					alert("전체마감이 되어 더이상 센터에 전달할 수 없습니다.");
					return;
				}	
				
				model.setValue("/root/reqData/reqform/rdr_detail_form/workflag", '1');					
				model.setValue("/root/reqData/reqform/rdr_detail_form/extndt", PID4300datagrid1.textMatrix(PID4300datagrid1.row, PID4300datagrid1.colRef("rdr_extndt")));
