			//기존의 bo_list_search
			function searchWithBranch()
			{
				model.setValue("/root/initData/gs_request", "L");

				if(iptCommonmBranchQ.value == "" && iptCommonmBranchQ1.value == "")
				{
					model.setValue("/root/reqData/reqform/extn_search_form/corrbocd", "");
					model.setValue("/root/reqData/reqform/extn_search_form/rdr_extnbocd", "");
					model.setValue("/root/initData/gs_request", "N");
					search();
				}else if(iptCommonmBranchQ.value != "" && iptCommonmBranchQ1.value == "")
				{
					modelCopyValue("/root/reqData/reqform/extn_search_form/common_deptcd", "/root/reqData/reqform/extn_search_form/deptcd");
					searchBranchByName("/root/reqData/reqform/extn_search_form", "/root/tempData/resTemp/branchGrid", "searchBranchCallBack()", "/root/initData/gs_request");
				}else if(iptCommonmBranchQ.value == "" && iptCommonmBranchQ1.value != "")
				{
					searchBranchByName("/root/reqData/reqform/bo_enter_search_form", "/root/tempData/resTemp/branchGrid", "searchBranchCallBack1()", "/root/initData/gs_request");
				}else
				{
					searchBranchByName("/root/reqData/reqform/extn_search_form", "/root/tempData/resTemp/branchGrid", "searchBranchCallBack()", "/root/initData/gs_request");
					searchBranchByName("/root/reqData/reqform/bo_enter_search_form", "/root/tempData/resTemp/branchGrid", "searchBranchCallBack1()", "/root/initData/gs_request");				
				}
			}
										
