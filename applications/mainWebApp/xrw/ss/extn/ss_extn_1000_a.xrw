			function search()
			{
				if(!commValidate(PID1000group_scr))
					return;        
				
				remakeNode("/root/tempData/dataTemp");
				
				var extnType = model.getValue("/root/reqData/reqform/extn_search_form/rdr_extntypecd");

				if(extnType == '') {		// 전체
				        model.send("PID1020search", false);
//			       }else if(extnType == '10') {	// 사원  사원, 사이버센터일 때 소속 나오도록 20160405 장선희
			       }else if(extnType == '10' || extnType == '70' || extnType == '90') {	// 사원, 사이버센터, 기업확장
				        model.send("PID1040search", false);
			       }else if(extnType == '20') {	// 품앗이
				        model.send("PID1050search", false);
			       }else if(extnType == '30') {	// 지국자체
				        model.send("PID1060search", false);
//			       }else if(extnType == '40' || extnType == '50' || extnType == '60' || extnType == '70' || extnType == '80') { 	// 자진신청, 사이버확장, 이사연결, 사이버센터, 캠페인확장 20160405 장선희
			       }else if(extnType == '40' || extnType == '50' || extnType == '60' || extnType == '80') { 	// 자진신청, 사이버확장, 이사연결, 캠페인확장
				        model.send("PID1030search", false);
			       }else {
			       		return;
			       }  																
				
