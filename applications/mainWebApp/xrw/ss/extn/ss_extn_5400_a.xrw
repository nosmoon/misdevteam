					model.setValue("/root/reqData/reqform/search_form/extn_gbcd", model.getValue("/root/tempData/gridTemp/cicodeval"));
				
					model.send("PID5400search", false);
					
					if(!submit_completed())
						return;
					//데이터셋팅
					//-조회목록
					model.copyNode("/root/resData/gridData/listGrid", "/root/tempData/dataTemp/listGrid");					
											
					//model.refresh();
