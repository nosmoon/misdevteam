			function execute()
			{
				if(model.getValue("/root/reqData/reqform/execute_form/sql_nm") == "")
				{
					alert("SQL name은 필수 입력 항목입니다.");
					return;
				}
				
				if(model.getValue("/root/reqData/reqform/execute_form/sql_text") == "")
				{
					alert("SQL을 입력하십시오.");
					return;
				}
/*				
				if(model.getValue("/root/reqData/reqform/execute_form/sql_text").indexOf("*") != -1){
					alert("Asterisk(*,전체컬럼선택) 문자는 사용할 수 없습니다.");
					return;
				}
*/				
				if(model.getValue("/root/reqData/reqform/execute_form/sql_text").indexOf(";") != -1){
					alert("Semicolon(;) 문자는 사용할 수 없습니다.");
					return;
				}
/*				
				if(model.getValue("/root/reqData/reqform/execute_form/sql_text").toUpperCase().substring(0,6) != PID1800combo1.label){
					alert("SQL문과 DML 옵션 선택값이 맞지 않습니다. SQL 수정 또는 DML 옵션을 변경해주세요.");
					return;
				}
*/				
				remakeNode("/root/tempData/resTemp");
				
				var replace_text = model.getValue("/root/reqData/reqform/execute_form/sql_text").toUpperCase();
				
				while(-1){
					if(replace_text.indexOf("SELECT") == -1 
						&& replace_text.indexOf("FROM") == -1  
						&& replace_text.indexOf("WHERE") == -1 
						&& replace_text.indexOf("UPDATE") == -1 
						&& replace_text.indexOf("SET") == -1 
						&& replace_text.indexOf("INSERT") == -1
						&& replace_text.indexOf("INTO") == -1
						&& replace_text.indexOf("VALUES") == -1
						&& replace_text.indexOf("DELETE") == -1
						&& replace_text.indexOf("ORDER") == -1
						&& replace_text.indexOf("GROUP") == -1
						&& replace_text.indexOf("HAVING") == -1
						&& replace_text.indexOf("/*") == -1
					){
						break;
					}
					replace_text = replace_text.replace("SELECT","S#ELECT");
					replace_text = replace_text.replace("FROM","F#ROM");
					replace_text = replace_text.replace("WHERE","W#HERE");
					replace_text = replace_text.replace("UPDATE","U#PDATE");
					replace_text = replace_text.replace("SET","S#ET");
					replace_text = replace_text.replace("INSERT","I#NSERT");
					replace_text = replace_text.replace("INTO","I#NTO");
					replace_text = replace_text.replace("VALUES","V#ALUES");
					replace_text = replace_text.replace("DELETE","D#ELETE");
					replace_text = replace_text.replace("ORDER","O#RDER");
					replace_text = replace_text.replace("GROUP","G#ROUP");
					replace_text = replace_text.replace("HAVING","H#AVING");
					replace_text = replace_text.replace("/*","/#*");
				}
				
				model.setValue("/root/reqData/reqform/execute_form/sql_text",replace_text);
				/*
				if(model.getValue("/root/reqData/reqform/execute_form/fname") == "")
				{
					alert("전송할 파일을 먼저 업로드 해주세요.");
					return;
				}
				*/	
				if(PID1800checkbox1.value=='Y'){
					model.setValue("/root/reqData/reqform/execute_form/dml_flag","S");
				}else{
					model.setValue("/root/reqData/reqform/execute_form/dml_flag","A");
				}				
				model.send("PID1800execute", false);
					
				if(!submit_completed())
					return;
				init();
				search();
			}
/*			
			function upload()
			{
				var files = window.fileDialog("open", "|", false);
			
				var index = 0;
				// 파일이 선택되면 구분자로 구분해내서 List 에 추가 시킨다.
				if( files != "" )
				{
					files += "|";

					for(var i=0; i<files.length; i++)
					{
						if( files.charAt(i) == "|")
						{
							filename = files.substring(index, i);
							index = i+1;
							PID1800input2.value = filename;
						}
					}
				}
			}
*/			
			function search(){
				remakeNode("/root/tempData/resTemp");
				
				model.send("PID1800search", false);
				
				if(!submit_completed())
					return;
				
				model.copyNode("/root/resData/gridData/listGrid", "/root/tempData/resTemp/listGrid");
						 	
			 	if(instance1.selectNodes("/root/resData/gridData/listGrid/record/sql_nm").length == 0)
					model.alert("검색결과가 없습니다.");
													
				model.removenode("/root/tempData/resTemp");
				
				PID1800datagrid1.rebuild();
			}
			
			function dwload(){
				makeNodesetToAppendForm("/root/resData/gridData/listGrid/record", "/root/tempData/gridTemp", "check", "Y", "##", true);
		
				model.setValue("/root/reqData/reqform/excel_form/execute_dt", model.getValue("/root/tempData/gridTemp/execute_dt"));
				model.setValue("/root/reqData/reqform/excel_form/sql_seq", model.getValue("/root/tempData/gridTemp/sql_seq"));
				
				if(model.getValue("/root/reqData/reqform/excel_form/execute_dt") == ""){
					alert("리스트에서 선택 후 출력하십시오.");
					return;
				}
				
				if(model.getValue("/root/tempData/gridTemp/data_cnt") == "0"){
					alert("저장할 데이터가 없습니다.");
					return;
				}
				
				if(model.getValue("/root/tempData/gridTemp/data_cnt") > 65535 ){		
					alert("65535건이 초과되는 데이터는 csv로 저장됩니다.");
					model.send("PID1800csv", false);
				}else{
					model.send("PID1800excel", false);
				}
					
				if(!submit_completed())
					return;
				
				init();
			}
			
			function sampleList(){
				makeNodesetToAppendForm("/root/resData/gridData/listGrid/record", "/root/tempData/gridTemp", "check", "Y", "##", true);
				
				if(model.getValue("/root/tempData/gridTemp/erryn") == "실행중"){
					alert("실행중입니다. 조회 후 다시 확인해주세요.");
					return;
				}
				
				if(model.getValue("/root/tempData/gridTemp/data_cnt") == "0"){
					alert("데이터가 없습니다.");
					model.copyNode("/root/resData/gridData/sampleGrid","");
					PID1800datagrid2.caption = "";		
					PID1800datagrid2.rebuild();
					return;
				}
		
				model.setValue("/root/reqData/reqform/excel_form/execute_dt", model.getValue("/root/tempData/gridTemp/execute_dt"));
				model.setValue("/root/reqData/reqform/excel_form/sql_seq", model.getValue("/root/tempData/gridTemp/sql_seq"));
				
				model.send("PID1800sample", false);
					
				if(!submit_completed())
					return;

				model.copyNode("/root/resData/gridData/sampleGrid", "/root/tempData/resTemp/sampleGrid");
		/*		
				var t_caption = model.getValue("/root/tempData/resTemp/t_caption");

				datagrid2.caption = t_caption;

				if(instance1.selectNodes("/root/resData/gridData/sampleGrid/record/a1").length == 0)
					model.alert("샘플 데이터가 없습니다.");
*/				
				var t_caption = model.getValue("/root/tempData/resTemp/t_caption");
				PID1800datagrid2.caption = t_caption;	
				
				model.removenode("/root/tempData/resTemp");
				
				PID1800datagrid2.rebuild();
			}
		
			function query_delete(){
				makeNodesetToAppendForm("/root/resData/gridData/queryGrid/record", "/root/tempData/gridTemp", "check", "Y", "##", true);
		
				model.setValue("/root/reqData/reqform/querylist_form/seq", model.getValue("/root/tempData/gridTemp/seq"));
				model.setValue("/root/reqData/reqform/querylist_form/flag", "D");
				
				model.send("PID1800queryD", false);
					
				if(!submit_completed())
					return;
				
				query_search();	
			}
			
			function query_search(){
				remakeNode("/root/tempData/resTemp");
				
				model.send("PID1800queryS", false);
				
				if(!submit_completed())
					return;
				
				model.copyNode("/root/resData/gridData/queryGrid", "/root/tempData/resTemp/queryGrid");
																
				model.removenode("/root/tempData/resTemp");
				
				PID1800datagrid3.rebuild();
