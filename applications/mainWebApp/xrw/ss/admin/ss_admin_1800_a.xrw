			function query_detail(){
				makeNodesetToAppendForm("/root/resData/gridData/queryGrid/record", "/root/tempData/gridTemp", "check", "Y", "##", true);
		
				model.setValue("/root/reqData/reqform/querylist_form/seq", model.getValue("/root/tempData/gridTemp/seq"));
				
				model.send("PID1800queryDe", false);
					
				if(!submit_completed())
					return;

				model.copyNode("/root/resData/gridData/queryDetail_form", "/root/tempData/resTemp/queryDetail_form");

				model.setValue("/root/reqData/reqform/execute_form/sql_nm",model.getValue("/root/resData/gridData/queryDetail_form/sql_nm"));
				model.setValue("/root/reqData/reqform/execute_form/sql_text",model.getValue("/root/resData/gridData/queryDetail_form/sql_text"));
				model.setValue("/root/reqData/reqform/execute_form/db_flag",model.getValue("/root/resData/gridData/queryDetail_form/db_cd"));
				
				model.removenode("/root/tempData/resTemp");
				model.refresh();
			}
			
			function openTablelist()
