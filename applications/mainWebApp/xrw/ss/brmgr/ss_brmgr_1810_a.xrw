			//폼구성 후 initDetailView.
			function callDetailView()
			{
				if(model.getValue("/root/reqData/reqform/boSearchForm/bolist") == "")
					return;
					
				PID1810input1.value = "";

				modelCopyValue("/root/reqData/reqform/rdrsrchListForm/bocd", "/root/reqData/reqform/boSearchForm/bolist");
				model.setValue("/root/reqData/reqform/rdrsrchListForm/rdr_no", "");
				model.setValue("/root/reqData/reqform/rdrsrchListForm/find_type", "Y");
				
				initDetailView();
			}
			
			//데이터 화면단 initializing.
			function initDetailView()
			{
				remakeNode("/root/tempData/resTemp");
				model.send("PID1810subViewInit", false);
				
				//reset
				clearChildNodes("/root/reqData/reqform/thrwHeadForm");
				//clearChildNodes("/root/reqData/reqform/mediThrwRateForm");
				//clearChildNodes("/root/reqData/reqform/mediThrwRateOutForm");
				clearChildNodes("/root/reqData/reqform/thrwSearchForm");
				clearChildNodes("/root/reqData/reqform/tabForm");
				clearChildNodes("/root/reqData/reqform/zipForm");
				clearChildNodes("/root/reqData/reqform/aptThrwTempForm");
				clearChildNodes("/root/reqData/reqform/calcThrwRateForm");
				initHousThrw(false);
				initThrwResearch(false);
				
				//data copy
				childValueCopy("/root/initData/defaultData", "/root/tempData/resTemp/defaultData");
				childValueCopy("/root/initData/naviInfo", "/root/tempData/resTemp/naviInfo");
				model.copyNode("/root/initData/comboSet/useData", "/root/tempData/resTemp/useData");
				
				//thrwHeadForm setting
				modelCopyValue("/root/reqData/reqform/thrwHeadForm/bocd", "/root/initData/defaultData/bocd");
				modelCopyValue("/root/reqData/reqform/thrwHeadForm/applySession", "/root/initData/defaultData/applySession");
				modelCopyValue("/root/reqData/reqform/thrwHeadForm/year", "/root/initData/defaultData/year");
				modelCopyValue("/root/reqData/reqform/thrwHeadForm/month", "/root/initData/defaultData/month");
				modelCopyValue("/root/reqData/reqform/thrwHeadForm/day", "/root/initData/defaultData/day");
				model.setValue("/root/reqData/reqform/thrwHeadForm/hour", "00");
				model.setValue("/root/reqData/reqform/thrwHeadForm/minute", "00");
				modelCopyValue("/root/reqData/reqform/thrwHeadForm/sel_regtype1", "/root/initData/defaultData/sel_regtype1");
				modelCopyValue("/root/reqData/reqform/thrwHeadForm/invsgpers", "/root/initData/defaultData/invsgpers");
				
				//thrwSearchForm setting
				modelCopyValue("/root/reqData/reqform/thrwSearchForm/bocd", "/root/initData/defaultData/bocd");
				model.setValue("/root/reqData/reqform/thrwSearchForm/curr_page_no", "1");
				modelCopyValue("/root/reqData/reqform/thrwSearchForm/records_per_page", "/root/initData/naviInfo/records_per_page");
				modelCopyValue("/root/reqData/reqform/thrwSearchForm/jobcdset", "/root/initData/defaultData/jobcdset");
				modelCopyValue("/root/reqData/reqform/thrwSearchForm/invsgdt1", "/root/initData/defaultData/invsgdt1");
				modelCopyValue("/root/reqData/reqform/thrwSearchForm/invsgdt2", "/root/initData/defaultData/invsgdt2");
				
				//zipForm
				modelCopyValue("/root/reqData/reqform/zipForm/bocd", "/root/initData/defaultData/bocd");
				
				//aptThrwTempForm
				modelCopyValue("/root/reqData/reqform/aptThrwTempForm/bocd", "/root/initData/defaultData/bocd");
				
				model.refresh();
				
				model.removenode("/root/tempData/resTemp");
			}
			
			//format이 가능하게 되면 사용할 필요 없음. 일단 만들어 놓음. -> format 99999-999-###으로 해결됨. 사용안함.
			function setSearchView(type)
			{
				var qsearchVal = model.getValue("/root/reqData/reqform/boSearchForm/qsearch");
				var newVal = "";
				
				qsearchVal = qsearchVal.replace(/-/g,"");

				if( type == 1 ) {
					newVal = qsearchVal.substring(0,5) + "-" + qsearchVal.substring(5,8) + "-" + qsearchVal.substring(8);
				} else {
					newVal = qsearchVal.replace(/-/g,"");
				}
				
				model.setValue("/root/reqData/reqform/boSearchForm/qsearch", newVal);
			}
			
			function quickSearch(zip, addr)
			{
				model.setValue("/root/reqData/reqform/thrwHeadForm/qsearch", PID1810input1.value);
				PID1810input2.value = zip;
				PID1810input3.value = addr;
				//model.setValue("/root/reqData/reqform/thrwHeadForm/zip", zip);
				//model.setValue("/root/reqData/reqform/thrwHeadForm/addr", addr);
				
				btnInit("0");
				initInForms();
				
				remakeNode("/root/tempData/resTemp");
				model.send("PID1810subQuickSearch", false);
				if(!submit_completed())
					return;
					
				//아파트정보 부분.
				childValueCopy("/root/reqData/reqform/thrwHeadForm", "/root/tempData/resTemp/thrwHeadForm");
				
				//팀지국부분.
				childValueCopy("/root/reqData/reqform/boSearchForm", "/root/tempData/resTemp/boSearchForm");
				model.copyNode("/root/initData/comboSet/codeData/arealist", "/root/tempData/resTemp/arealist");
				model.copyNode("/root/initData/comboSet/codeData/codelist", "/root/tempData/resTemp/codelist");
				
				//조회form
				childValueCopy("/root/reqData/reqform/rdrsrchListForm", "/root/tempData/resTemp/rdrsrchListForm");
				
				if(model.getValue("/root/tempData/resTemp/nSessionCheck") == "1")
				{
					//호별투입현황 부분.
					childValueCopy("/root/reqData/reqform/housThrwForm", "/root/tempData/resTemp/housThrwForm");
					//호별 투입매체 현황 데이터 및 그리드 처리.
					if(instance1.selectNodes("/root/tempData/resTemp/housThrwGrid/record/floor").length > 0)
					{
						clearGridCols(PID1810datagrid1, false);
						var rec = instance1.selectSingleNode("/root/tempData/resTemp/housThrwGrid/record");
						buildGridCols(PID1810datagrid1, "층", "호", 1, false, false, "/root/tempData/resTemp/housThrwGrid/record", "input", Math.round(926/rec.childNodes.length), "left");
						for(var i=1; i<PID1810datagrid1.cols; i++)
							PID1810datagrid1.colAttribute(i, "imemode") = "disabled";
						remakeNode("/root/resData/gridData/housThrwGrid");
						model.copyNode("/root/resData/gridData/housThrwGrid", "/root/tempData/resTemp/housThrwGrid");
					}
				
					childValueCopy("/root/reqData/reqform/thrwHeadForm", "/root/tempData/resTemp/thrwHeadFormEx");
					model.copyNode("/root/initData/comboSet/useData/addrcdCombo", "/root/tempData/resTemp/addrcdCombo");
					model.copyNode("/root/initData/comboSet/useData/dongnoCombo", "/root/tempData/resTemp/dongnoCombo");
					
					//조사리스트
					model.copyNode("/root/initData/comboSet/useData/thrwAddrcdCombo", "/root/tempData/resTemp/thrwAddrcdCombo");
					model.setValue("/root/reqData/reqform/thrwSearchForm/thrw_addrcd", "");
					remakeNode("/root/resData/gridData/thrwResearchGrid");
					
					//bocd
					modelCopyValue("/root/reqData/reqform/thrwSearchForm/bocd", "/root/tempData/resTemp/rdrsrchListForm/bocd");
					modelCopyValue("/root/reqData/reqform/zipForm/bocd", "/root/tempData/resTemp/rdrsrchListForm/bocd");
					modelCopyValue("/root/reqData/reqform/aptThrwTempForm/bocd", "/root/tempData/resTemp/rdrsrchListForm/bocd");
					
					model.refresh();
					
					if(model.getValue("/root/tempData/resTemp/housThrwForm/gridReadOnly") == "readonly")
					{
						PID1810datagrid1.isReadOnly(0, 0, PID1810datagrid1.rows-1, PID1810datagrid1.cols-1) = true;
					}
					else
					{
						//층수는 output으로 처리.
						PID1810datagrid1.colType(PID1810datagrid1.colRef("floor")) = "output";
						//존재하지 않는 호수는 output으로 처리.
						for(var i=PID1810datagrid1.fixedRows; i<PID1810datagrid1.rows; i++)
							for(var j=PID1810datagrid1.fixedCols; j<PID1810datagrid1.cols; j++)
							{
								var ref = PID1810datagrid1.colAttribute(j, "ref");
								if(model.getValue("/root/resData/gridData/housThrwGrid/record[" + (i+1) +"]/" + ref + "/@exist") == "false")
									//PID1810datagrid1.cellAttribute("type", i, j) = "output";
									PID1810datagrid1.cellType( i, j) = "output";
							}
					}
				}
				else
				{
					model.alert("사용권한이 없습니다");
					remakeNode("/root/resData/gridData/housThrwGrid");
					remakeNode("/root/resData/gridData/thrwResearchGrid");
					
					clearSelectCombo(PID1810combo4, "전체", "", false);
					clearSelectCombo(PID1810combo1, "전체", "", false);
					clearSelectCombo(PID1810combo8, "전체", "", false);
					
					model.setValue("/root/reqData/reqform/thrwSearchForm/bocd", "");
					model.setValue("/root/reqData/reqform/boSearchForm/common_deptcd", "");
					model.setValue("/root/reqData/reqform/boSearchForm/common_areacd", "");
					model.setValue("/root/reqData/reqform/rdrsrchListForm/bocd", "");
					model.setValue("/root/reqData/reqform/thrwHeadForm/bocd", "");
					model.setValue("/root/reqData/reqform/thrwSearchForm/bocd", "");
					model.setValue("/root/reqData/reqform/zipForm/bocd", "");
					model.setValue("/root/reqData/reqform/aptThrwTempForm/bocd", "");
					
					model.refresh();
					
					cmbCommonTeamN.select(0);
					PID1810combo4.select(0);
					PID1810combo1.select(0);
					PID1810combo8.select(0);
				}

				model.removenode("/root/tempData/resTemp");
			}
			
			function save()
			{
				var regtype1 = PID1810combo5.value;
				var regtype2 = PID1810combo6.value;
				var incmgtype = model.getValue("/root/initData/defaultData/incmgtype");
				
				if(PID1810combo4.value == '' || PID1810combo1.value == '') {
					model.alert("대상아파트와 동을 선택하세요");
					return;
