			<![CDATA[
			function init()
			{
				commEventHandler(PID1200group_scr);
				
				makeTeamSearch("getTeamInit", "/ss/cns/1200", "/root/reqData/reqform/rdr_Form", "/root/initData/comboSet/codeData", "cmbCommonTeamQ", "cmbCommonPartQ", "cmbCommonAreaQ");
				//아래는 임시용. makeTeamEvent
				//makeTeamEvent("getAreaList", "/ss/common/1100", "/root/reqData/reqform/rdr_dsct_search_form", "/root/initData/comboSet/codeData", "cmbCommonTeamQ", "cmbCommonAreaQ", "preActionTeam();");
				//reset을 위해 데이터를 복사해 놓는다.
				model.copyNode("/root/initData/defaultData", "/root/initData/comboSet/codeData");
				childValueCopy("/root/reqData/reqform/rdr_dsct_detail_form", "/root/reqData/reqform/rdr_Form"); // 조회된 노드들을 대상에 한번에 전부 카피한다.				
				//기타 데이터 셋팅
				modelCopyValue("/root/reqData/reqform/rdr_dsct_search_form/applySession", 	"/root/initData/comboSet/codeData/applySession");
				modelCopyValue("/root/reqData/reqform/rdr_dsct_search_form/pagesize", 		"/root/initData/comboSet/codeData/pagesize");
				modelCopyValue("/root/initData/naviInfo/pagesize", 					"/root/initData/comboSet/codeData/pagesize");
				
				modelCopyValue("/root/reqData/reqform/rdr_dsct_search_form/frdt", "/root/initData/comboSet/codeData/fromDate");
				modelCopyValue("/root/reqData/reqform/rdr_dsct_search_form/todt", "/root/initData/comboSet/codeData/toDate");
				
				model.copyNode("/root/initData/comboSet/rmsCombo", "/root/initData/comboSet/codeData/rmsCombo");
				model.copyNode("/root/initData/comboSet/typeCombo", "/root/initData/comboSet/codeData/typeCombo");
				
				model.setValue("/root/reqData/reqform/rdr_dsct_detail_form/accflag", 	"I");
				model.setValue("/root/reqData/reqform/rdr_dsct_detail_form/jobflag", 	"D");
				
				model.setValue("/root/reqData/reqform/rdr_dsct_detail_form/path_id", "TM");
				model.setValue("/root/reqData/reqform/rdr_dsct_detail_form/cns_empnm_id", model.getValue("/root/initData/comboSet/codeData/cns_empnm_id"));
				
				PID1200group_scr.refresh();
			}
			
			function preActionTeam()
			{
				modelCopyValue("/root/reqData/reqform/rdr_dsct_search_form/common_deptcd", "/root/reqData/reqform/rdr_dsct_search_form/deptcd");
			}
			
			function onKeyDown()
			{
				if(event.keyCode == 13)
				{
					model.setFocus("btnSearchBranchQ");
					
					if(iptCommonmBranchQ.value == '')
						onClickBranchPopup();
					else
					{
						//화면별 선처리
						modelCopyValue("/root/reqData/reqform/rdr_dsct_search_form/common_deptcd", "/root/reqData/reqform/rdr_dsct_search_form/deptcd");
						//검색 공통 모듈 호출. param : "입력param path", "출력 path", "callback함수", flag 값을 가지고 있는 path"
						//처리순서 : 지국조회 -> callback함수 호출 -> flag값에 따른 처리.
						//callbak함수는 옵션. search() 및 searchWithBranch() 함수가 존재해야 함.
						searchBranchByName("/root/reqData/reqform/rdr_dsct_search_form", "/root/tempData/resTemp/branchGrid", "searchBranchCallBack()", "/root/initData/gs_request");
					}
				}
			}
			
			function onClickBranchPopup()
			{
				//openBranch("parameter. deptcd,partcd,areacd,bonm", "result. deptcd, deptnm, areacd, areanm, bocd, bonm") - part도 추가될 것으로 보임.
				openBranch("/root/reqData/reqform/rdr_dsct_search_form", "/root/tempData/tempBranch");

               		if(model.getValue("/root/tempData/tempBranch/bocd") == "")
					return;
				
				iptCommonmBranchQ.value = model.getValue("/root/tempData/tempBranch/bonm");
				modelCopyValue("/root/reqData/reqform/rdr_dsct_search_form/bocd", "/root/tempData/tempBranch/bocd");

				if(model.getValue("/root/initData/gs_request") == "L")
				{
					model.setValue("/root/initData/gs_request", "N");
					search();
				}
			}
			
			function searchBranchCallBack()
			{
				//검색 후처리.
				if((instance1.selectNodes("/root/tempData/resTemp/branchGrid/record/bocd")).length == 1)
				{
					iptCommonmBranchQ.value = model.getValue("/root/tempData/resTemp/branchGrid/record/bonm");
					modelCopyValue("/root/reqData/reqform/rdr_dsct_search_form/bocd", "/root/tempData/resTemp/branchGrid/record/bocd");
				}
				else
					onClickBranchPopup();
			}
			
			function search()
			{
				if(!commValidate(PID1200group_scr))
					return;
					
				model.setValue("/root/reqData/reqform/rdr_dsct_search_form/pageno", "1");
				
				//조회
				remakeNode("/root/tempData/resTemp");
				model.send("PID1200subSearch", false);
				
				//데이터셋팅
				//-조회목록
				model.copyNode("/root/resData/gridData/listGrid", "/root/tempData/resTemp/listGrid");

				//-기타
				modelCopyValue("/root/initData/naviInfo/pageno", "/root/tempData/resTemp/pageno");
				modelCopyValue("/root/initData/naviInfo/totalcnt", "/root/tempData/resTemp/totalcnt");
				modelCopyValue("/root/initData/naviInfo/js_fn_nm", "/root/tempData/resTemp/js_fn_nm");
				modelCopyValue("/root/initData/naviInfo/pagesize", "/root/tempData/resTemp/pagesize");
				
				PID1200datagrid1.refresh();
		
				if(instance1.selectNodes("/root/resData/gridData/listGrid/record/regno").length == 0)
					model.alert("검색결과가 없습니다.");
					
				model.removenode("/root/tempData/resTemp");
			}
			
			function searchWithBranch()
			{
				model.setValue("/root/initData/gs_request", "L");
				
				if(iptCommonmBranchQ.value == "")
				{
					model.setValue("/root/reqData/reqform/rdr_dsct_search_form/bocd", "");
					model.setValue("/root/initData/gs_request", "N");
					search();
				}
				else
				{
					modelCopyValue("/root/reqData/reqform/rdr_dsct_search_form/common_deptcd", "/root/reqData/reqform/rdr_dsct_search_form/deptcd");
					searchBranchByName("/root/reqData/reqform/rdr_dsct_search_form", "/root/tempData/resTemp/branchGrid", "searchBranchCallBack()", "/root/initData/gs_request");
				}
			}
			
			function onZipKeyDown(pageNo)
			{
				if(event.keyCode == 13)
				{
					model.setFocus("PID1200button5");
					
					//우편번호 패턴이 아니라면 zip_search 함수를 call 한다.
					if(!validate_zip(PID1200input26.value))
						onClickZipPopup();
					else
					{
						//model.setValue("/root/reqData/reqform/zip_search_form/zip", model.getValue("/root/reqData/reqform/rdr_dsct_detail_form/pageno"));
						model.setValue("/root/reqData/reqform/zip_search_form/zip", PID1200input26.value);
						model.setValue("/root/reqData/reqform/zip_search_form/pageno", pageNo);
						model.setValue("/root/reqData/reqform/zip_search_form/pagesize", "10000");
						
						searchZip("/root/reqData/reqform/zip_search_form", "/root/tempData/resTemp/zipGrid");
						
						if((instance1.selectNodes("/root/tempData/resTemp/zipGrid/record/zip")).length == 1)
						{
							
							PID1200input26.value = model.getValue("/root/tempData/resTemp/zipGrid/record/zip");
							PID1200input27.value = model.getValue("/root/tempData/resTemp/zipGrid/record/addr");
							PID1200input17.value = model.getValue("/root/tempData/resTemp/zipGrid/record/bonm");
							PID1200input38.value = model.getValue("/root/tempData/resTemp/zipGrid/record/bocd");
							PID1200input8.value = model.getValue("/root/tempData/resTemp/zipGrid/record/telno3");
							PID1200input9.value = model.getValue("/root/tempData/resTemp/zipGrid/record/telno2");
							PID1200input10.value = model.getValue("/root/tempData/resTemp/zipGrid/record/telno1");
						}
						else
							onClickZipPopup();
					}
				}
			}
			
			function onClickZipPopup()
			{
				var ls_medicd = model.getValue("/root/reqData/reqform/rdr_dsct_detail_form/medicd");
				
				if(ls_medicd == '210' || ls_medicd == '220' || ls_medicd =='230' || ls_medicd == '240' || ls_medicd == '270' || ls_medicd == '420' || ls_medicd == ' 421' || ls_medicd == '422'){
					pb_rdr_enter_search('2');
					return
				}
				//model.setValue("/root/reqData/reqform/zipForm/job_flag", "A");
				model.setValue("/root/reqData/reqform/zip_search_form/zip", PID1200input26.value);
				
				//openBranch("parameter. deptcd,partcd,areacd,bonm", "result. deptcd, deptnm, areacd, areanm, bocd, bonm") - part도 추가될 것으로 보임.
				openZip("/root/reqData/reqform/zip_search_form", "/root/tempData/tempZip");
				
				if(model.getValue("/root/tempData/tempZip/zip") == "")
					return;
				
				PID1200input26.value = model.getValue("/root/tempData/tempZip/zip");
				PID1200input27.value = model.getValue("/root/tempData/tempZip/addr");
				PID1200input17.value = model.getValue("/root/tempData/tempZip/bonm");
				PID1200input38.value = model.getValue("/root/tempData/tempZip/bocd");
				PID1200input8.value = model.getValue("/root/tempData/tempZip/telno3");
				PID1200input9.value = model.getValue("/root/tempData/tempZip/telno2");
				PID1200input10.value = model.getValue("/root/tempData/tempZip/telno1");
				model.setValue("/root/reqData/reqform/rdr_dsct_detail_form/bldmngnn",model.getValue("/root/tempData/tempZip/bldmngnn"));
			}
			
			function gridClick()
			{
				if( PID1200datagrid1.textMatrix(PID1200datagrid1.row, PID1200datagrid1.colRef("regdt")) == "")
						return;
					
					remakeNode("/root/tempData/resTemp");
					
					model.setValue("/root/reqData/reqform/rdr_dsct_list_form/regdt",PID1200datagrid1.textMatrix(PID1200datagrid1.row, PID1200datagrid1.colRef("regdt")));
					model.setValue("/root/reqData/reqform/rdr_dsct_list_form/regno", PID1200datagrid1.textMatrix(PID1200datagrid1.row, PID1200datagrid1.colRef("regno")));						
										
					model.send("PID1200gridSearch", false);
					 
					if(!submit_completed())
						return;
						
					childValueCopy("/root/reqData/reqform/rdr_dsct_detail_form", "/root/tempData/resTemp/detailData");
					
					model.setValue("/root/reqData/reqform/rdr_dsct_detail_form/dt", model.getValue("/root/tempData/resTemp/detailData/regdt"));
					model.setValue("/root/reqData/reqform/rdr_dsct_detail_form/no", model.getValue("/root/tempData/resTemp/detailData/regno"));
					model.setValue("/root/reqData/reqform/rdr_dsct_detail_form/path_id", model.getValue("/root/tempData/resTemp/path_id")); //신청경로
					model.setValue("/root/reqData/reqform/rdr_dsct_detail_form/acpndt_id", model.getValue("/root/tempData/resTemp/acpndt_id")); //접수일시
					model.setValue("/root/reqData/reqform/rdr_dsct_detail_form/rdremail", model.getValue("/root/tempData/resTemp/rdremail")); //이메일
					model.setValue("/root/reqData/reqform/rdr_dsct_detail_form/telno1", model.getValue("/root/tempData/resTemp/telno1")); //지국전화번호1
					model.setValue("/root/reqData/reqform/rdr_dsct_detail_form/telno2", model.getValue("/root/tempData/resTemp/telno2")); //지국전화번호2
					model.setValue("/root/reqData/reqform/rdr_dsct_detail_form/telno3", model.getValue("/root/tempData/resTemp/telno3")); //지국전화번호3
					model.setValue("/root/reqData/reqform/rdr_dsct_detail_form/boprocyn_id", model.getValue("/root/tempData/resTemp/boprocyn_id")); //불편내용 처리여부
					model.setValue("/root/reqData/reqform/rdr_dsct_detail_form/boprocdt_id", model.getValue("/root/tempData/resTemp/boprocdt_id")); //불편내용 처리 일시
					model.setValue("/root/reqData/reqform/rdr_dsct_detail_form/cnscnfmtm_id", model.getValue("/root/tempData/resTemp/cnscnfmtm_id")); //상담원 일시
					model.setValue("/root/reqData/reqform/rdr_dsct_detail_form/remk", model.getValue("/root/tempData/resTemp/remk")); //상담원 비고
					
					var cns_empcnfmyn = model.getValue("/root/tempData/resTemp/cns_empcnfmyn"); //상담원 확인여부
					
					if(cns_empcnfmyn == ("Y")){
						model.setValue("/root/reqData/reqform/rdr_dsct_detail_form/cnscnfmyn", model.getValue("/root/tempData/resTemp/cns_empcnfmyn"));
					}else{
						model.setValue("/root/reqData/reqform/rdr_dsct_detail_form/cnscnfmyn", "");
					}
					model.setValue("/root/reqData/reqform/rdr_dsct_detail_form/bldmngnn", model.getValue("/root/tempData/resTemp/detailData/bldmngnn")); 
					
					model.removenode("/root/tempData/resTemp");
					model.setValue("/root/reqData/reqform/rdr_dsct_detail_form/accflag", "U");
					PID1200group4.refresh();					
			}
			
			function clear()
			{
				model.setValue("/root/reqData/reqform/rdr_dsct_detail_form/path_id", "TM");
				model.setValue("/root/reqData/reqform/rdr_dsct_detail_form/cns_empnm_id", model.getValue("/root/initData/comboSet/codeData/cns_empnm_id"));
				
				model.setValue("/root/reqData/reqform/rdr_dsct_detail_form/acpndt_id", "");
				model.setValue("/root/reqData/reqform/rdr_dsct_detail_form/boprocyn_id", "");
				model.setValue("/root/reqData/reqform/rdr_dsct_detail_form/boprocdt_id", "");
				model.setValue("/root/reqData/reqform/rdr_dsct_detail_form/cnscnfmtm_id", "");
				
				resetChildControl("PID1200group4", true,false);
				
				model.setValue("/root/reqData/reqform/rdr_dsct_detail_form/medicd", "110"); //조선일보 기본
				model.setValue("/root/reqData/reqform/rdr_dsct_detail_form/jobflag", "D"); //작업구분을 불편으로 설정
				model.setValue("/root/reqData/reqform/rdr_dsct_detail_form/accflag", "I"); //작업구분을 입력으로 설정
			}
			
			function save()
			{
				model.setValue("/root/reqData/reqform/rdr_dsct_detail_form/jobflag", "D");
				
				if(model.getValue("/root/reqData/reqform/rdr_dsct_detail_form/accflag") == "D"){
					model.setValue("/root/reqData/reqform/rdr_dsct_detail_form/accflag", "U");
				}
				
				if(!commValidate("PID1200group4"))
					return;
					
				var dscttypecd	= model.getValue("/root/reqData/reqform/rdr_dsct_detail_form/dscttypecd");
				if(model.getValue("/root/reqData/reqform/rdr_dsct_detail_form/dlvzip") == "" || model.getValue("/root/reqData/reqform/rdr_dsct_detail_form/dlvaddr")== "" || model.getValue("/root/reqData/reqform/rdr_dsct_detail_form/dlvdtlsaddr") =="" )
				{
					if(dscttypecd == "50" || dscttypecd == "60" || dscttypecd == "70" || dscttypecd == "80" || dscttypecd == "90" || dscttypecd == "95")
					{
						;
					}else{
						alert("주소를 입력하세요");
						return;
