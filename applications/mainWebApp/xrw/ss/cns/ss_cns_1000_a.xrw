					if(iptCommonmBranchQ1.value == '')
						onClickBranchPopup1();
					else
					{
						//화면별 선처리
						//modelCopyValue("/root/reqData/reqform/extn_search_form/common_deptcd1", "/root/reqData/reqform/extn_search_form/extn_deptcd");
						//검색 공통 모듈 호출. param : "입력param path", "출력 path", "callback함수", flag 값을 가지고 있는 path"
