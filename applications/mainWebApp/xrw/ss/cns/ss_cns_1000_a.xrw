			function getSelectList()
			{
				var extnType = model.getValue("/root/reqData/reqform/extn_search_form/rdr_extntypecd");

				if(extnType == '10') {		// 사원확장
			       		PID1000caption14.value =  "사원명";
			       }else if(extnType == '20') {	// 품앗이
					PID1000caption14.value =  "확장자";
				}else if(extnType == '30') {	// 지국자체확장
					PID1000caption14.value =  "확장자";
			       }else if(extnType == '40') { 	// 자진신청
					PID1000caption14.value =  "신청자";
			       }else if(extnType == '50' || extnType == '70'){ // 사이버, 사이버센터
			       		PID1000caption14.value =  "확장자";
			       }
			       
				remakeNode("/root/tempData/initTemp");
			
				model.send("PID1000search", false);
				model.copyNode("/root/initData/comboSet/extnCombo", "/root/tempData/initTemp/extnCombo");
				model.removenode("/root/tempData/initTemp");
				PID1000group_scr.refresh();
				
				PID1000combo8.select(0);
			}
			
			function extnblngcdSelect()
			{
				var extnType = model.getValue("/root/reqData/reqform/extn_search_form/rdr_extntypecd");
				var lv_select1  = model.getValue("/root/reqData/reqform/extn_search_form/extnblngcd");

				if(extnType == '30') {	// 지국자체확장
					if(lv_select1 =='10' || lv_select1 == '20'){ //조선IS요원, 사설요원 확장
						PID1000caption14.value =  "요원명";
					}else if(lv_select1 == '30'){ //지국종사원 확장
						PID1000caption14.value =  "종사원";
					}else{
						PID1000caption14.value =  "확장자";
					}
			       }
			}
			function search()
			{
				if(!commValidate(PID1000group_scr))
					return;    
					
				tmp_fromdt = new Date(model.getValue("/root/reqData/reqform/extn_search_form/fromdt").substring(0,4) + "/" + model.getValue("/root/reqData/reqform/extn_search_form/fromdt").substring(4,6) + "/" + model.getValue("/root/reqData/reqform/extn_search_form/fromdt").substring(6,8));
				tmp_todt = new Date(model.getValue("/root/reqData/reqform/extn_search_form/todt").substring(0,4) + "/" + model.getValue("/root/reqData/reqform/extn_search_form/todt").substring(4,6) + "/" + model.getValue("/root/reqData/reqform/extn_search_form/todt").substring(6,8));

				tmp_resultdt = ((tmp_todt.getTime() - tmp_fromdt.getTime()) / (1000 * 60 * 60 * 24));
				tmp_resultdt = Math.floor(tmp_resultdt);
				
				if(tmp_resultdt > 31){
					alert("조회시작일자와 조회종료일자의 간격일은 \n 시스템의 과부하로 인해 31일을 넘을 수 없습니다.");
					return;
				}
				remakeNode("/root/tempData/dataTemp");

				var extnType = model.getValue("/root/reqData/reqform/extn_search_form/rdr_extntypecd");

				if(extnType == '') {		// 전체
					model.send("PID1000searchTwo", false);
					PID1000datagrid1.colHidden(4) = false;
					PID1000datagrid1.textMatrix(0, 0) = "확장자";
					PID1000datagrid1.textMatrix(0, 4) = "지역";
					PID1000datagrid1.textMatrix(0, 5) = "지국";
			       }else if(extnType == '10') {	// 사원
			       		model.send("PID1000searchFour", false);
			       		PID1000caption14.value =  "사원명";
					PID1000datagrid1.colHidden(4) = false;
					PID1000datagrid1.textMatrix(0, 0) = "사원[회사]";
					PID1000datagrid1.textMatrix(0, 4) = "지역";
					PID1000datagrid1.textMatrix(0, 5) = "지국";
			       }else if(extnType == '20') {	// 품앗이
					model.send("PID1000searchFive", false);
       					PID1000datagrid1.colHidden(4) = true;
					PID1000datagrid1.textMatrix(0, 0) = "확장자";
					PID1000datagrid1.textMatrix(0, 4) = "권유지국[지역] --> 인수지국[지역]";
					PID1000datagrid1.textMatrix(0, 5) = "권유지국[지역] --> 인수지국[지역]";
				}else if(extnType == '30') {	// 지국자체
					model.send("PID1000searchSix", false);
					PID1000datagrid1.colHidden(4) = false;
					PID1000datagrid1.textMatrix(0, 0) = "확장자";
					PID1000datagrid1.textMatrix(0, 4) = "지역";
					PID1000datagrid1.textMatrix(0, 5) = "지국";
			       }else if(extnType == '40' || extnType == '50' || extnType == '60' || extnType == '70' || extnType == '80') { 	// 자진신청, 사이버확장, 이사연결, 사이버센터, 캠페인확장
					model.send("PID1000searchThree", false);
					PID1000datagrid1.colHidden(4) = false;
					PID1000datagrid1.textMatrix(0, 0) = "신청자";
					PID1000datagrid1.textMatrix(0, 4) = "지역";
					PID1000datagrid1.textMatrix(0, 5) = "지국";
			       }else {
			       		return;
			       }  																
				if(!submit_completed())
					return;
				
			       model.copyNode("/root/resData/gridData/listGrid", "/root/tempData/dataTemp/listGrid");

				//PID1000caption4.value= "총건수 " + model.getValue("/root/tempData/dataTemp/totalCount") + " 건";
				
				modelCopyValue("/root/initData/naviInfo/pageno", "/root/tempData/dataTemp/pageno");
				modelCopyValue("/root/initData/naviInfo/totalcnt", "/root/tempData/dataTemp/totalcnt");
				modelCopyValue("/root/initData/naviInfo/js_fn_nm", "/root/tempData/dataTemp/js_fn_nm");
				modelCopyValue("/root/initData/naviInfo/pagesize", "/root/tempData/dataTemp/pagesize");
							 	
			 	if(instance1.selectNodes("/root/resData/gridData/listGrid/record/extnflnm").length == 0)
					model.alert("검색결과가 없습니다.");

				model.removenode("/root/tempData/dataTemp");
				
				PID1000datagrid1.refresh();

				if(model.getValue("/root/reqData/reqform/extn_detail_form/accflag") =="D" ){
					return;
				}

				clear();
				model.setValue("/root/reqData/reqform/extn_detail_form/qty", "1");
				PID1000input4.refresh();			
			}
			
			function onKeyDown()
			{
