				window.load("/xrw/ss/brsup/ss_brsup_2504_p.xrw", "modal", "ZIPPOP", style="width:600px; height:400px; align:center; caption:hidden;", "/root/reqData/reqform/detail_search_form", "/root/reqData/reqform/outSumForm");
