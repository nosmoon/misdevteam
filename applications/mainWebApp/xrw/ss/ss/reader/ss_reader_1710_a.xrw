		<script type="javascript">
			<![CDATA[
			function init()
			{				
				model.send("PID1710init", false);
				
				toggleGrid("1");
				model.setValue("/root/reqData/reqform/from", model.getValue("/root/tempData/codeData/defaultDate"));
				model.setValue("/root/reqData/reqform/to", model.getValue("/root/tempData/codeData/defaultDate"));
				
				model.copyNode("/root/initData/comboSet/custCombo","/root/tempData/codeData/custCombo");
				
				PID1710group_scr.refresh();
			}			
			
			function toggleGrid(flag)
			{				
				if(flag == "1") {					
					PID1710button3.selected = true;					
					model.toggle("PID1710case1");
					
				} else if(flag == "2") {									
 					PID1710button6.selected = true;					
					model.toggle("PID1710case2");
					
				} else if(flag == "3") {					
 					PID1710button2.selected = true;					
					model.toggle("PID1710case3");
				}	
			}
			
			function search()
			{
				toggleGrid("1");
				
				clearChildNodes("/root/resData/gridData/detailGrid/record", true);

				var clsf = model.getValue("/root/reqData/reqform/clsf");
				
				remakeNode("/root/tempData/resTemp");
																					
				if(clsf == "") {
					alert("구분 조건은 반드시 1개이상 선택되어야 하며 모플(일반회원)은 단일선택만 가능합니다.");
				} else {
					
					if(!commValidate("PID1710group_scr"))
					return;
																												
					var array_data = clsf.split("##");  // split 함수사용..
					
					if( array_data.length >1 ) {
						if(array_data[0] == "A" || array_data[1] == "A" || array_data[2] == "A") {
							alert("모플(일반회원)항목은 단일선택만 가능합니다.");
							return;
							
						} else {
							model.send("PID1710select", false);
						}
					} else if (array_data[0] == "A") {
						model.send("PID1710select1", false);
					} else {
						model.send("PID1710select", false);
					}
					
					model.copyNode("/root/resData/gridData/listGrid","/root/tempData/resTemp/listGrid");
				
					if(instance1.selectNodes("/root/resData/gridData/listGrid/record/custclas").length == 0)
						model.alert("검색결과가 없습니다.");
													
					PID1710datagrid1.refresh()
					
					model.removenode("/root/tempData/resTemp");
				}								
			}
			
			function gridClick()
			{						       			       
				if(PID1710datagrid1.textMatrix(PID1710datagrid1.row, PID1710datagrid1.colRef("cmpycd")) == "")
						return;
				
				toggleGrid("2");
					
				model.setValue("/root/reqData/detailform/cmpycd",  PID1710datagrid1.textMatrix(PID1710datagrid1.row, PID1710datagrid1.colRef("cmpycd")));
				model.setValue("/root/reqData/detailform/custclas", PID1710datagrid1.textMatrix(PID1710datagrid1.row, PID1710datagrid1.colRef("custclas")));
				model.setValue("/root/reqData/detailform/custseq",  PID1710datagrid1.textMatrix(PID1710datagrid1.row, PID1710datagrid1.colRef("custseq")));
				
				remakeNode("/root/tempData/resTemp");
				
				model.send("PID1710detail", false);
				
				model.copyNode("/root/resData/gridData/detailGrid","/root/tempData/resTemp/detailGrid");
				
				if(instance1.selectNodes("/root/resData/gridData/detailGrid/record/custclas").length == 0)
					model.alert("검색결과가 없습니다.");
												
				PID1710group2.refresh();
				
				model.removenode("/root/tempData/resTemp");
			}
			
			function save()
			{
				var gubun = model.getValue("/root/reqData/upload_form/gubun");
				
				if(gubun != "upload") {
					
					childValueCopy("/root/reqData/detailform","/root/resData/gridData/detailGrid/record");
					
					if(model.getValue("/root/reqData/detailform/cmpycd") == "" || model.getValue("/root/reqData/detailform/custclas") == "" || model.getValue("/root/reqData/detailform/custseq") == "" ) {
						alert("목록에서 상세조회하여 수정할 항목을 선택하세요.");
						return;
					}
					
					if(model.alert("수정하시겠습니까?", "", 4) == "6") {				
					    model.send("PID1710update", false);
					    
					    if(!submit_completed())
						return;					
						
					    search();
					}
					
				} else {
				
					makeNodesetToAppendForm("/root/resData/gridData/uploadGrid/record", "/root/tempData/gridTemp", "check", "", "##", true);
					
					model.setValue("/root/reqData/upload_form/clsf", model.getValue("/root/tempData/gridTemp/clsf"));
					model.setValue("/root/reqData/upload_form/regdt", model.getValue("/root/tempData/gridTemp/regdt"));
					model.setValue("/root/reqData/upload_form/custclas", model.getValue("/root/tempData/gridTemp/custclas"));
					model.setValue("/root/reqData/upload_form/custnm", model.getValue("/root/tempData/gridTemp/custnm"));
					model.setValue("/root/reqData/upload_form/sex", model.getValue("/root/tempData/gridTemp/sex"));
					model.setValue("/root/reqData/upload_form/age", model.getValue("/root/tempData/gridTemp/age"));
					model.setValue("/root/reqData/upload_form/home_zip", model.getValue("/root/tempData/gridTemp/home_zip"));
					model.setValue("/root/reqData/upload_form/home_addr", model.getValue("/root/tempData/gridTemp/home_addr"));
					model.setValue("/root/reqData/upload_form/rdrptph_no1", model.getValue("/root/tempData/gridTemp/rdrptph_no1"));
					model.setValue("/root/reqData/upload_form/email", model.getValue("/root/tempData/gridTemp/email"));
					model.setValue("/root/reqData/upload_form/medinm", model.getValue("/root/tempData/gridTemp/medinm"));
					model.setValue("/root/reqData/upload_form/subs_prd", model.getValue("/root/tempData/gridTemp/subs_prd"));				
					model.setValue("/root/reqData/upload_form/chrn_pre", model.getValue("/root/tempData/gridTemp/chrn_pre"));
					model.setValue("/root/reqData/upload_form/chrn_emy", model.getValue("/root/tempData/gridTemp/chrn_emy"));
					model.setValue("/root/reqData/upload_form/chrn_mid", model.getValue("/root/tempData/gridTemp/chrn_mid"));
					model.setValue("/root/reqData/upload_form/chrn_hig", model.getValue("/root/tempData/gridTemp/chrn_hig"));
					model.setValue("/root/reqData/upload_form/evntdate_from", model.getValue("/root/tempData/gridTemp/evntdate_from"));
					model.setValue("/root/reqData/upload_form/evntdate_to", model.getValue("/root/tempData/gridTemp/evntdate_to"));		
								
					if(model.getValue("/root/reqData/upload_form/clsf") == "")
					{
						alert("엑셀 파일을 먼저 업로드해 주세요.");
						return;
					}
					
					model.send("PID1710upload", false);
					
					if(!submit_completed())
						return;
					
					model.setValue("/root/reqData/upload_form/gubun", "");	
				 	clearChildNodes("/root/resData/gridData/uploadGrid", true);
				}
							
			}
			
			function delete1()
			{
				if(model.getValue("/root/reqData/detailform/cmpycd") == "" || model.getValue("/root/reqData/detailform/custclas") == "" || model.getValue("/root/reqData/detailform/custseq") == "" ) {
					alert("목록에서 상세조회하여 삭제할 항목을 선택하세요.");
					return;
				}
				
				if(model.alert("삭제하시겠습니까?", "", 4) == "6") {				
				    model.send("PID1710delete", false);
				    
				    if(!submit_completed())
					return;					
					
				    search();
				}					
			}
			
			function Upload()
			{								
				toggleGrid("3");
				
				var filename = window.fileDialog("open", ",", false, "", "xls", "Excel File(*.xls)|*.xls");

				if(filename!="")				
				{
					PID1710datagrid2.loadExcel(filename);
					model.setValue("/root/reqData/upload_form/gubun", "upload");
					//PID1630datagrid1.mergeCells = "bycol";
				}
				PID1710datagrid2.refresh();								
			}
			
			function Excel()
			{
				var filePath = window.fileDialog("save", "", false, "", "xls", "Excel Files(*.xls)|*.xls");
				
				if(filePath != "") {
					PID1710datagrid1.saveExcel(filePath);
					window.exec(filePath);
				}
			}
			
			function onClickZipPopup()
			{
				model.setValue("/root/reqData/zipForm/job_flag", "A");
				model.setValue("/root/reqData/zipForm/zip", PID1710input13.value);	//"/root/reqData/reqform/detailForm/vaca_areazip"
				
				//openBranch("parameter. deptcd,partcd,areacd,bonm", "result. deptcd, deptnm, areacd, areanm, bocd, bonm") - part도 추가될 것으로 보임.
				openZip("/root/reqData/zipForm", "/root/tempData/tempZip");
				
				if(model.getValue("/root/tempData/tempZip/zip") == "")
					return;
				
				PID1710input13.value = model.getValue("/root/tempData/tempZip/zip");
				PID1710input12.value = model.getValue("/root/tempData/tempZip/addr");
			}
			
			function onZipKeyDown(pageNo)
			{
				if(event.keyCode == 13)
				{
					//model.setFocus("PID1500button6");
					
					//우편번호 패턴이 아니라면 zip_search 함수를 call 한다.
					if(!validate_zip(PID1710input13.value))
						onClickZipPopup();
					else
					{
						model.setValue("/root/reqData/zipForm/zip", PID1710input13.value);
						model.setValue("/root/reqData/zipForm/pageno", pageNo);
						model.setValue("/root/reqData/zipForm/pagesize", "10000");
						
						searchZip("/root/reqData/zipForm", "/root/tempData/resTemp/zipGrid");
						
						if((instance1.selectNodes("/root/tempData/resTemp/zipGrid/record/zip")).length == 1)
						{
							PID1710input13.value = model.getValue("/root/tempData/resTemp/zipGrid/record/zip");
							PID1710input12.value = model.getValue("/root/tempData/resTemp/zipGrid/record/addr");
						}
						else
							onClickZipPopup();
					}
				}
			}
			]]>
		</script>
