			// 독자 정보 저장 accflg = "I" (독자 정보 등록 ) accflg = "U" (독자 정보 수정)
			function readerSave()
			{
				//************** 중요. ******************//
				//저장시(update시) makeNodesetWithoutDisabled 를사용할 경우, 매체명에 연결된 medicd를 반드시 다시 포함시킬것.
				//상세조회시 combo는 readonly가 동작하지 않으므로 disable 시켰으므로..
				
				// 폼에 대한 공통적인 유효성 검증
				if(!commValidate(PID1700group2))
					return;
				
				//우편번호 유효성검증
				if(!validate_zip(PID1700input6.value)){
				    model.alert("유효하지 않은 우편번호 입니다.");
				    model.setFocus("PID1700input6");
				    return;
				}
				
				//상세주소 XOR 번지 XOR (동 AND 호) 입력하도록 체크
				if(!(trim(PID1700input8.value)!="" || trim(PID1700input9.value)!="") ^ (trim(PID1700input12.value)!="" & trim(PID1700input11.value)!="")){
				    model.alert("상세주소, 번지 또는 (동과 호) 중 하나를 선택하여 입력하셔야 합니다.\n단, 동과 호 입력의 경우 둘다 입력하셔야 합니다.");
				    model.setFocus("PID1700input8");
				    return;
				}
				
				//전화번호, 휴대폰 둘중 하나는 입력해야 함.
				var ls_tel_no = trim(PID1700input15.value) + "-" + trim(PID1700input14.value) + "-" + trim(PID1700input13.value);
				var ls_ptph_no = trim(PID1700input18.value) + "-" + trim(PID1700input17.value) + "-" + trim(PID1700input16.value);
				if( ls_tel_no == "--" && ls_ptph_no == "--" ){
				    model.alert("전화번호나 휴대폰번호 중 적어도 하나는 입력하셔야 합니다.");
				    model.setFocus("PID1700input15");
				    return;
				}else if(ls_tel_no != "--" && !isValidPhone(ls_tel_no)){
				    model.alert("전화번호가 유효하지 않습니다.\n예)02-1234-1234");
				    model.setFocus("PID1700input15");
				    return;
				}else if(ls_ptph_no != "--" && !isValidPcs(ls_ptph_no)){
				    model.alert("휴대폰번호가 유효하지 않습니다.\n예)011-1234-1234");
				    model.setFocus("PID1700input15");
				    return;
				}
				
				if(model.getValue("/root/reqData/reqform/rdrDetailForm/accflg") == "I"){ //독자추가시 필수 체크사항
				    //유료부수와 무료부수 둘 중 적어도 하나는 입력값이 존재해야 한다.
				    if(trim(PID1700input24.value)=="" && trim(PID1700input34.value)==""){
				        model.alert("유료부수와 무료부수 중 적어도 하나는 입력하셔야 합니다.");
				        model.setFocus("PID1700input24");
				        return;
				    }
				
				    //유료부수와 무료부수는 1보다는 커야한다.
				    if(trim(PID1700input24.value)!="" && eval(PID1700input24.value) < 1){
				        model.alert("유료부수는 1보다는 커야 합니다.");
				        model.setFocus("PID1700input24");
				        return;
				    }
				
				    //유료부수와 무료부수는 1보다는 커야한다.
				    if(trim(PID1700input34.value)!="" && eval(PID1700input34.value) < 1){
				        model.alert("무료부수는 1보다는 커야 합니다.");
				        model.setFocus("PID1700input34");
				        return;
				    }
				
				    //할인금액, 우송료는 1000원 이상이어야 함
				    if(trim(PID1700input25.value)!="" && eval(PID1700input25.value) < 1000){
				        model.alert("할인금액은 1000원 보다는 커야 합니다.");
				        model.setFocus("PID1700input25");
				        return false;
				    }
				
				    //할인금액, 우송료는 1000원 이상이어야 함
				    if(trim(PID1700input30.value)!="" && eval(PID1700input30.value) < 1000){
				        model.alert("우송료은 1000원 보다는 커야 합니다.");
				        model.setFocus("PID1700input30");
				        return false;
				    }
				
				    //할인금액 <= 구독금액
				    if(eval(trim(PID1700input25.value)) > eval(trim(PID1700input28.value))){
				        model.alert("할인금액은 구독금액 보다 클 수 없습니다.");
				        model.setFocus("PID1700input25");
				        return;
				    }
				
				    //유료부수는 "" 또는 1보다 작고 무료부수만 입력한 경우(유료부수는 입력하지 않은 것으로 간주함)
				    if((trim(PID1700input24.value)=="" || eval(PID1700input24.value) < 1) && trim(PID1700input34.value)!="" ){
				        //할인금액은 "" 이어야 한다.
				        if(trim(PID1700input25.value)!=""){
				            model.alert("무료 부수만을 입력하신 경우 할인금액은 입력하실 수 없습니다.");
				            PID1700input24.value = "";
				            PID1700input25.value = "";
				            return;
				        }
				    }
				
				    //무료부수를 입력한 경우 무료구분은 필수이고 입력하지 않은 경우는 구분은 "" 값으로 셋팅
				    if(trim(PID1700input34.value)!=""){
				        if(trim(PID1700combo7.value)==""){
				            model.alert("무료부수 입력시 무료구분은 필수입력항목 입니다.");
				            model.setFocus("PID1700combo7");
				            return;
				        }
				    }else{
				        PID1700combo7.select(0);
				    }
				
				    //확장유형은 지국자체확장이거나 자진신청이어야 한다.
				    if(PID1700combo6.value != "30" && PID1700combo6.value != "40"){
				        model.alert("확장유형은 지국자체확장이나 자진신청만 선택가능합니다.");
				        //왜 여기서 return을 안하지?
				    }
				    //확장자 입력이 검색에 의해 이뤄졌는지 검증
				    if(PID1700combo6.value == "30" && trim(model.getValue("/root/reqData/reqform/rdrDetailForm/extnflnm"))==""){
				        model.alert("확장자 검색이 되어 있지 않습니다.");
				        model.setFocus("PID1700input37");
				        return;
				    }else if(PID1700input37.value != model.getValue("/root/reqData/reqform/rdrDetailForm/extnflnm")){
				        model.alert("검색된 확장자명("+model.getValue("/root/reqData/reqform/rdrDetailForm/extnflnm")+")과 입력하신 확장자명이 일치하지 않습니다.");
				        PID1700input37.value = model.getValue("/root/reqData/reqform/rdrDetailForm/extnflnm");
				        model.setFocus("PID1700input37");
				        return;
				    }
				}
				
				/********
				* 실제 저장을 하지 않음. (권한없음 에러 발생시킴). 따라서 disable 개체 처리 하지 않음.
				********/
	    			remakeNode("/root/tempData/resTemp");
				if(model.getValue("/root/reqData/reqform/rdrDetailForm/accflg") != "U"){  	//등록이면
				    model.setValue("/root/reqData/reqform/rdrDetailForm/accflg", "I")
				    model.send("PID1700subInsert", false);
				}else{	                            //수정이면
				    model.send("PID1700subUpdate", false);
				}
				
				model.removenode("/root/tempData/resTemp");
			}
			
