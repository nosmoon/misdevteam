			function setRdrSearchPttn()
			{
				var sword = PID1700input92.value;
				var pttn = "";
				//패턴결정
			        //통합검색시 검색어의 패턴('1':구역, '2':구역+배달, '3':독자번호, '4':독자명, '5':주소, '6':전화번호, '7':전독자번호)
			        if(valid_rdr_no(sword)){ //독자번호
			            pttn = "3";
			        }else if(valid_prev_rdr_no(sword)){ //전 독자번호
			            pttn = "7";
			        }else if(valid_dstc_dlvno(sword)){ //구역+배달
			            pttn = "2";
			        }else if(valid_dstc_no(sword)){ //구역
			            pttn = "1";
			        }else if(valid_tel_no(sword)){ //전화번호
			            pttn = "6";
			        }else if(valid_addr(sword)){ //주소
			            pttn = "5";
			        }else{ //그 밖에는 독자명이라 간주
			            pttn = "4";
			        }
			        
			        model.setValue("/root/reqData/reqform/rdrSearchForm/search_pttn", pttn);
			}
			
			function validateReaderSearchWord()
			{
				var scond = PID1700combo28.value;
				var sword = PID1700input92.value;
				
				if(scond == "DSTC"){  		// 구역일 경우
			            if(!isValidFormatX(sword, /^(\d{3})$/)){
			                model.alert("유효하지 않은 구역입니다.");
			                model.setFocus("PID1700input92");
			                return false;
			            }
			        }else if(scond == "DSNO") {	// 구역+배달번호
			            if(!isValidFormatX(sword, /^(\d{6})$/)){
			                model.alert("유효하지 않은 구역+배달번호 입니다.\n 예) 101101");
			                model.setFocus("PID1700input92");
			                return false;
			            }
			        }else if(scond == "NO") {		// 독자번호
			            if(!isValidFormatX(sword, /^(\d{9})$/)){
			                model.alert("유효하지 않은 독자번호 입니다.\n 예)031234567");
			                model.setFocus("PID1700input92");
			                return false;
			            }
			        }else if(scond == "TEL") {	// 전화번호
			            if(!isValidFormatX(sword, /^(\d{4,5}|\d{5,8}|\d{10,}|(\d{2,4})-{0,1}(\d{3,4})-{0,1}(\d{4}))$/)){
			                model.alert("유효하지 않은 전화번호 입니다.\n 예)1234 또는 1234234 또는 0212341234 또는 1234-1234 또는 02-1234-1234");
			                model.setFocus("PID1700input92");
			                return false;
			            }
			        }
			        return true;
			}
			
