			//구역코드 입력시 구역명 및 시작배달번호 및 종료배달번호 셋팅
			//숨겨진 구역 combo를 이용하여 입력값을 validation함.
			//combo는 form parameter를 맞추기 위해 그대로 놔둔 것이고, 실제 처리는 combo에 매핑된 nodeset을 이용하기로 한다.
			function setDstcInfo()
			{
				var lb_searched = false; //사용자가 입력한 구역코드에 해당하는 값이 지국에 존재하는가?
				
				if(trim(PID1700input2.value)==""){
					PID1700input5.value = ""; //구역명 초기화
					model.setValue("/root/reqData/reqform/rdrDetailForm/dlvteamcd", ""); //배달조코드 초기화
					return;
				}

				var node = instance1.selectSingleNode("/root/initData/comboSet/useData/detailArea/dlvdstccdHiddenCombo/item[value='" + PID1700input2.value + "']");
				if(node == null)
				{
					model.alert("존재하지 않는 구역입니다.");
					model.setFocus("PID1700input2");
