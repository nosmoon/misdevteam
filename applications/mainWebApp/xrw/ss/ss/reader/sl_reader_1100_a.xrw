		<script type="javascript">
			<![CDATA[
			function init()
			{
				removeGsRdrNo();
				
				initSearch();
				
				if(model.getValue("/root/initData/postData/find_type") != "Y")
					readerDetail(model.getValue("/root/initData/postData/rdr_no"), "", "");
			}
			
			function setPostParameter(arr)
			{
				if(arr==null ||typeof(arr.length)=="undefined" || arr.length < 3)
					return;
				
				model.setValue("/root/initData/postData/bocd", arr[0]);
				model.setValue("/root/initData/postData/rdr_no", arr[1]);
				model.setValue("/root/initData/postData/find_type", arr[2]);
			}
			
			function finish()
			{
				removeGsRdrNo();
			}
			
			function initSearch()
			{
				if(model.getValue("/root/initData/postData/bocd") == "")
					return;
					
				model.send("PID1700subInit", false);
				
				modelCopyValue("/root/reqData/reqform/rdrSearchForm/search_word", "/root/initData/comboSet/useData/firstDlvdstccd");
				modelCopyValue("/root/reqData/reqform/rdrDetailForm/rdrtel_no1", "/root/initData/comboSet/useData/dsData/bo_tel1");
				modelCopyValue("/root/reqData/reqform/rdrDetailForm/subsuprc", "/root/initData/comboSet/useData/firstMediUprc");
				modelCopyValue("/root/reqData/reqform/rdrDetailForm/subsamt", "/root/initData/comboSet/useData/firstMediUprc");
				modelCopyValue("/root/reqData/reqform/rdrDetailForm/rptvsubsdt", "/root/initData/comboSet/useData/today");
				modelCopyValue("/root/reqData/reqform/rdrDetailForm/rptvrdr_extndt", "/root/initData/comboSet/useData/today");
				modelCopyValue("/root/reqData/reqform/rdrDetailForm/rptvvalmm", "/root/initData/comboSet/useData/rptvvalmm");
		
				setSelectDefault("PID1700combo1", "--", 0);
				setSelectDefault("PID1700radio1", "10", -1);
				setSelectDefault("PID1700combo2", "--", 0);
				setSelectDefault("PID1700combo3", "--", 0);
				setSelectDefault("PID1700radio2", "10", -1);
				setSelectDefault("PID1700combo4", "--", 0);
				setSelectDefault("PID1700combo5", "--", 0);
				setSelectDefault("PID1700combo7", "--", 0);
				setSelectDefault("PID1700combo8", "--", 0);
				setSelectDefault("PID1700combo6", "30", 0);
				setSelectDefault("PID1700combo9", "--", 0);
		               setSelectDefault("PID1700combo27", null, 0);
		               setSelectDefault("PID1700combo28", "ALL", 0);
				setSelectDefault("PID1700combo29", "--", 0);
				
				controlReaderMedia("I");
				controlRcpmInput("I");

				model.refresh();
			}
			
			//독자상세 reset
			//독자리스트를 통해 독자 상세를 호출할 때 호출되어져야 함.
			function resetDetail(refresh)
			{
				//데이터 reset.
				clearChildNodes("/root/reqData/reqform/rdrDetailForm");
				//clearChildNodes("/root/reqData/reqform/rdrSearchForm");
				//clearChildNodes("/root/reqData/reqform/zipForm");
				//clearChildNodes("/root/reqData/reqform/rdrDetailViewForm");
				//clearChildNodes("/root/reqData/reqform/rdrExtnSearchForm");
				//clearChildNodes("/root/reqData/reqform/bnsItemcdSearchForm");
				//clearChildNodes("/root/reqData/reqform/rcpmListSearchForm");
				//clearChildNodes("/root/reqData/reqform/rcpmDetailSearchForm");
				//clearChildNodes("/root/reqData/reqform/dupCheckForm");
				//clearChildNodes("/root/reqData/reqform/rdrCardPrintForm");
				//clearChildNodes("/root/reqData/reqform/rdrServiceForm");
				
				remakeNode("/root/resData/resform/mediForm");
				remakeNode("/root/resData/resform/summary");
				
				model.setValue("/root/reqData/reqform/rdrDetailForm/accflg", "I");
				model.setValue("/root/reqData/reqform/rdrDetailForm/aplcpathcd", "20");
				model.setValue("/root/reqData/reqform/rdrDetailForm/valqty", "1");
				//model.setValue("/root/reqData/reqform/dupCheckForm/accflag", "10");
				//model.setValue("/root/reqData/reqform/dupCheckForm/duplicated", "F");
				
				//tab button reset 및 버튼 hidden.
				removeAllGroupButton(PID1700group_btnT, "tab");
				PID1700group4.visible = false;

				if(refresh)
				{
					PID1700group2.refresh();
					PID1700group8.refresh();
				}

				controlReaderMedia("I");
				resetRcpm(refresh);
			}
			
			//입금내역 영역 reset
			function resetRcpm(refresh)
			{
				clearChildNodes("/root/reqData/reqform/rcpmDetailForm");
				
				remakeNode("/root/resData/resform/basiyymm");
				remakeNode("/root/resData/resform/medicd");
				
				clearChildNodes("/root/resData/resform/rcpmForm");
				model.setValue("/root/resData/resform/rcpmForm/milgcurTitle", "마일리지(on/off)");
				model.setValue("/root/resData/resform/rcpmForm/prevclamtynmsTitle_f", "입금내역");
				model.setValue("/root/resData/resform/rcpmForm/currclamtynmsTitle_f", "입금내역");
				model.setValue("/root/resData/resform/rcpmForm/rcpmyymmTitle", "입금내역");
				
				remakeNode("/root/resData/gridData/rcpmListGrid");

				//clearSelectCombo(PID1700combo10, "", "", false);	//수금방법 controlRcpmInput에 포함.
				//clearSelectCombo(PID1700combo11, "", "", false);	//입금구분
				controlRcpmInput("I");
				
				if(refresh)
					PID1700group3.refresh();
			}
			
			function controlReaderMedia(accflg)
			{
				if(accflg == "U")
				{
					PID1700combo5.disabled = true;					//매체명 		--> 저장시(update시) makeNodesetWithoutDisabled 를사용할 경우, 연결된 medicd를 반드시 다시 포함시킬것.
					PID1700input24.attribute("editable") = "false";		//유료부수 
					PID1700input34.attribute("editable") = "false";		//무료부수 
					PID1700input25.attribute("editable") = "false";		//할인금액 
