			function checkValue()
			{
				var checkName = "";
				var checkCount = 0;				
				
				for(var i=1; i<PID2100datagrid3.rows; i++) {
					if(PID2100datagrid3.valueMatrix(i,0) == "Y") {
						checkName = PID2100datagrid3.valueMatrix(i,1);
						break;
					}
				}
				
				for(var i=1; i<PID2100datagrid3.rows; i++) {
					if(PID2100datagrid3.valueMatrix(i,0) == "Y") {
						checkCount ++;				
					}
				}
				
				checkCount = checkCount -1;
				
				if(checkName == "") {
					PID2100input7.value = "선택해주세요.";
				} else {
					PID2100input7.value = checkName + (checkCount > 0 ? "외" + checkCount + "건" : "");
				}
			}
						
