			function save()
			{								
				if( model.getValue( "/root/resData/gridData/detailData/rdr_extndt" ) == "" && model.getValue( "/root/resData/gridData/detailData/rdr_no" ) == "" ) {
					model.alert("저장할 내용을 선택해주세요");
					return;
				}
				
				if( PID2230radio1.label == "" ) {
					model.alert("담당의견을 선택해주세요");
					return;
				}
				
				model.setValue( "/root/reqData/reqform/extn_detail_form/extndt", model.getValue("/root/resData/gridData/detailData/rdr_extndt") );
