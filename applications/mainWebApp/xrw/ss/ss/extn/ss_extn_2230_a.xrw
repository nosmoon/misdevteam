				model.setValue( "/root/reqData/reqform/extn_detail_form/cont", model.getValue("/root/reqData/reqform/extn_detail_form/ddremk") );
				model.setValue( "/root/reqData/reqform/extn_detail_form/seq", model.getValue("/root/resData/gridData/detailData/seq") );
				model.refresh();

				model.send("PID2230save", false);
