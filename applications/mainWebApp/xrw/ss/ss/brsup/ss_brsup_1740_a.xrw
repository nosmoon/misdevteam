		<script type="javascript">
			<![CDATA[
			function init()
			{
				model.send("PID1740subInit", false);
				
				childValueCopy("/root/reqData/reqform/boSearchForm", "/root/initData/comboSet/codeData/boSearchForm");
				setSelectDefault(PID1740combo8, "10", -1, false);
				
				PID1740group4.refresh();
				PID1740group2.refresh();
			}
			
			function sendShftaplc()
			{
				remakeNode("/root/tempData/resTemp");
				model.send("PID1740subSendShftaplc", false);
			}
			
			function receiveShftaplc()
			{
				remakeNode("/root/tempData/resTemp");
				model.send("PID1740subReceiveShftaplc", false);
			}
			
			function search()
			{
				model.setValue("/root/reqData/reqform/boSearchForm/pageno", "1");
				if(PID1740input4.value == "")
					model.setValue("/root/reqData/reqform/boSearchForm/bocd", "");
					
				if(!commValidate(PID1740group4))
					return;
				
				model.setValue("/root/reqData/reqform/aplcSearchForm/bocd", "");
				model.setValue("/root/reqData/reqform/aplcSearchForm/aplcdt", "");
				model.setValue("/root/reqData/reqform/aplcSearchForm/bonm", "");
				model.setValue("/root/reqData/reqform/aplcSearchForm/botel", "");
				PID1740caption86.value = model.getValue("/root/initData/gs_bonm");
				
				remakeNode("/root/tempData/resTemp");
				model.send("PID1740subSearch", false);
				if(!submit_completed())
					return;
				
				childValueCopy("/root/initData/naviInfo", "/root/tempData/resTemp/naviInfo");
				model.copyNode("/root/resData/gridData/aplcSumGrid", "/root/tempData/resTemp/aplcSumGrid");
				PID1740datagrid3.refresh();
				
				if(instance1.selectNodes("/root/resData/gridData/aplcSumGrid/record/bocd").length == 0)
					model.alert("검색결과가 없습니다.");
					
				model.removenode("/root/tempData/resTemp");
			}
			
			function searchWithBranch()
			{
				model.setValue("/root/initData/gs_request", "L");
				
				if(PID1740input4.value == "")
				{
					model.setValue("/root/reqData/reqform/boSearchForm/bocd", "");
					search();
				}
				else
				{
					//modelCopyValue("/root/reqData/reqform/shft_clam_form/common_deptcd", "/root/reqData/reqform/shft_clam_form/deptcd");
					searchBranchByName("/root/reqData/reqform/boSearchForm", "/root/tempData/dataTemp/branchGrid", "searchBranchCallBack()", "/root/initData/gs_request");
				}
			}
			
			function onClickBranchPopup()
			{
				//openBranch("parameter. deptcd,partcd,areacd,bonm", "result. deptcd, deptnm, areacd, areanm, bocd, bonm") - part도 추가될 것으로 보임.
				openBranch("/root/reqData/reqform/boSearchForm", "/root/tempData/tempBranch");

               		if(model.getValue("/root/tempData/tempBranch/bocd") == "")
					return;
				
				PID1740input4.value = model.getValue("/root/tempData/tempBranch/bonm");
				modelCopyValue("/root/reqData/reqform/boSearchForm/bocd", "/root/tempData/tempBranch/bocd");

				if(model.getValue("/root/initData/gs_request") == "L")
				{
					model.setValue("/root/initData/gs_request", "N");
					search();
				}
				
			}
			
			function onKeyDown()
			{
				if(event.keyCode == 13)
				{
					model.setFocus("PID1740button5");
					
					if(PID1740input4.value == '')
						onClickBranchPopup();
					else
					{
						//화면별 선처리
						//modelCopyValue("/root/reqData/reqform/shft_clam_form/common_deptcd", "/root/reqData/reqform/shft_clam_form/deptcd");
						//검색 공통 모듈 호출. param : "입력param path", "출력 path", "callback함수", flag 값을 가지고 있는 path"
						//처리순서 : 지국조회 -> callback함수 호출 -> flag값에 따른 처리.
						//callbak함수는 옵션. search() 및 searchWithBranch() 함수가 존재해야 함.
						searchBranchByName("/root/reqData/reqform/boSearchForm", "/root/tempData/dataTemp/branchGrid", "searchBranchCallBack()", "/root/initData/gs_request");
					}
				}
			}
			
			//지국 enter key 검색 시 callback 함수. onKeyDown
			function searchBranchCallBack()
			{
				//검색 후처리.
				if((instance1.selectNodes("/root/tempData/dataTemp/branchGrid/record/bocd")).length == 1)
				{
					PID1740input4.value = model.getValue("/root/tempData/dataTemp/branchGrid/record/bonm");
					modelCopyValue("/root/reqData/reqform/boSearchForm/bocd", "/root/tempData/dataTemp/branchGrid/record/bocd");
				}
				else
					onClickBranchPopup();
			}
			
			function setBoinfo()
			{
				//정보 셋팅.
				var botel = ""
				if(curRowRefColValue(PID1740datagrid3, "tel_no11") == "")
					botel = curRowRefColValue(PID1740datagrid3, "tel_no12") + "-" + curRowRefColValue(PID1740datagrid3, "tel_no13")
				else
					botel = curRowRefColValue(PID1740datagrid3, "tel_no11") + "-" + curRowRefColValue(PID1740datagrid3, "tel_no12") + "-" + curRowRefColValue(PID1740datagrid3, "tel_no13")
				
				//aplcSearchForm
				gridRowValueCopy("/root/reqData/reqform/aplcSearchForm", PID1740datagrid3, PID1740datagrid3.row);	//bocd, bonm, migryn
				model.setValue("/root/reqData/reqform/aplcSearchForm/botel", botel);
				modelCopyValue("/root/reqData/reqform/aplcSearchForm/aplcdt", "/root/reqData/reqform/boSearchForm/aplcdt");
				
				//readerSearchForm
				gridRowValueCopy("/root/reqData/reqform/readerSearchForm", PID1740datagrid3, PID1740datagrid3.row);	//bocd, bonm, migryn
				model.setValue("/root/reqData/reqform/readerSearchForm/botel", botel);
				
				//title
				PID1740caption86.value = "[" + curRowRefColValue(PID1740datagrid3, "bonm") + " 지국] " + botel;
					
				search2();
			}
			
			//신청목록.
			function search2()
			{
				model.toggle("case1");
				
				if(model.getValue("/root/reqData/reqform/aplcSearchForm/bocd") == "")
				{
					model.alert("지국을 선택하신 후 조회해주세요.");
					return;
				}
				
				model.setValue("/root/reqData/reqform/aplcSearchForm/pageno", "1");
				
				if(!commValidate(PID1740group2))
					return;
				
				remakeNode("/root/tempData/resTemp");
				model.send("PID1740subSearch2", false);
				if(!submit_completed())
					return;
				
				childValueCopy("/root/reqData/reqform/aplcListForm", "/root/tempData/resTemp/aplcListForm");
				childValueCopy("/root/initData/naviInfo2", "/root/tempData/resTemp/naviInfo2");
				model.copyNode("/root/resData/gridData/aplcGrid", "/root/tempData/resTemp/aplcGrid");
				PID1740datagrid4.refresh();
				
				//title
				PID1740caption86.value = "[" + model.getValue("/root/tempData/resTemp/summary/bonm") + " 지국] " + model.getValue("/root/tempData/resTemp/summary/botel");
				//summary 총건수는 사용하지 않는다.
				PID1740caption87.value = "*미확인 " + model.getValue("/root/tempData/resTemp/summary/uncnfmcnt") + "건 *확인 " + model.getValue("/root/tempData/resTemp/summary/cnfmcnt") + "건 *취소 " + model.getValue("/root/tempData/resTemp/summary/canccnt") + "건 *승인오류 " + model.getValue("/root/tempData/resTemp/summary/aprverrcnt") + "건";
				
				if(instance1.selectNodes("/root/resData/gridData/aplcGrid/record/aplcno").length == 0)
					model.alert("검색결과가 없습니다.");
					
				model.removenode("/root/tempData/resTemp");
			}
			
			function detail()
			{
				model.toggle("case2");
				
				gridRowValueCopy("/root/reqData/reqform/aplcListForm2", PID1740datagrid4, PID1740datagrid4.row);
				modelCopyValue("/root/reqData/reqform/aplcListForm2/bonm", "/root/reqData/reqform/aplcSearchForm/bonm");
				modelCopyValue("/root/reqData/reqform/aplcListForm2/botel", "/root/reqData/reqform/aplcSearchForm/botel");
				modelCopyValue("/root/reqData/reqform/aplcListForm2/bocd", "/root/reqData/reqform/aplcSearchForm/bocd");
				
				remakeNode("/root/tempData/resTemp");
				model.send("PID1740subDetail", false);
				if(!submit_completed())
					return;
					
				clearChildNodes("/root/reqData/reqform/aplcDetailForm");
				clearChildNodes("/root/reqData/reqform/aplcDetailForm2");
				
				childValueCopy("/root/reqData/reqform/aplcDetailForm", "/root/tempData/resTemp/detailData/record");
				childValueCopy("/root/reqData/reqform/aplcDetailForm2", "/root/tempData/resTemp/detailData/record");
				model.copyNode("/root/initData/comboSet/useData/aplyyymmCombo", "/root/tempData/resTemp/aplyyymmCombo");
				model.copyNode("/root/initData/comboSet/useData/medicdCombo", "/root/tempData/resTemp/medicdCombo");
				PID1740group1.refresh();
				
				PID1740caption2.value = "[" + model.getValue("/root/tempData/resTemp/bonm") + " 지국] " + model.getValue("/root/tempData/resTemp/botel");
				var closyn = model.getValue("/root/reqData/reqform/aplcDetailForm/closyn");
				var migryn = model.getValue("/root/reqData/reqform/aplcDetailForm2/migryn");
				if(closyn == "Y")
				{
					PID1740combo1.visible = false;		//aplyyymm
					PID1740caption83.visible = true;
					PID1740input1.visible = false;		//rdr_no
					PID1740button13.visible = false;
					PID1740caption84.visible = true;
					PID1740combo3.visible = false;		//medicd
					PID1740caption85.visible = true;
				}
				else
				{
					PID1740combo1.visible = true;
					PID1740caption83.visible = false;
					PID1740input1.visible = true;
					PID1740button13.visible = true;
					PID1740caption84.visible = false;
					PID1740combo3.visible = true;	
