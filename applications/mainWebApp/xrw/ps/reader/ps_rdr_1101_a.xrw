				function search()
				{
					if(!commValidate(PID1101group_scr))
						return;
						
					if((model.getValue("/root/reqData/searchData/reqform/search_form/sh_rdrprn1") != "") || (model.getValue("/root/reqData/searchData/reqform/search_form/sh_rdrprn2") != "")) {
						if(!isValidJumin(model.getValue("/root/reqData/searchData/reqform/search_form/sh_rdrprn1") + model.getValue("/root/reqData/searchData/reqform/search_form/sh_rdrprn2"))) {
					                alert('유효하지 않은 주민등록번호 입니다.');
							return;
					  	}
					}
					
					if(model.getValue("/root/reqData/searchData/reqform/search_form/sh_phone1") != "" || model.getValue("/root/reqData/searchData/reqform/search_form/sh_phone2") != "" || model.getValue("/root/reqData/searchData/reqform/search_form/sh_phone3") != "" ) {
						if(!isValidPcs(model.getValue("/root/reqData/searchData/reqform/search_form/sh_phone1") + "-" + model.getValue("/root/reqData/searchData/reqform/search_form/sh_phone2") +"-"+ model.getValue("/root/reqData/searchData/reqform/search_form/sh_phone3"))) {
					                alert('유효한 휴대폰 번호가 아닙니다.');
							return;
					  	}
					}
											
					remakeNode("/root/tempData/dataTemp");
					model.send("PID1131search", false);
					
				       model.copyNode("/root/resData/gridData/userGrid", "/root/tempData/dataTemp/userGrid");
				       
					model.removenode("/root/tempData/dataTemp");
										
					if(instance1.selectNodes("/root/resData/gridData/userGrid/record/rdr_extnno").length == 0)
						model.alert("검색결과가 없습니다.");
					
					PID1101datagrid2.rebuild();
					clear1();
					model.setValue("/root/reqData/searchData/reqform/rdr_extn_form/qty", "1");
					PID1101input16.refresh();
				}

				function gridClick() 
				{											
					if(PID1101datagrid2.textMatrix(PID1101datagrid2.row, PID1101datagrid2.colRef("rdr_extndt")) == "")
