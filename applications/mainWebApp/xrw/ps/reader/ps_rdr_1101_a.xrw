					model.setValue("/root/reqData/searchData/reqform/rdr_extn_form/dlvzip1", model.getValue("root/tempData/dataTemp/detailGrid/record/dlvzip").substring(0,3));
					model.setValue("/root/reqData/searchData/reqform/rdr_extn_form/dlvzip2", model.getValue("root/tempData/dataTemp/detailGrid/record/dlvzip").substring(3));
					model.setValue("/root/reqData/searchData/reqform/rdr_extn_form/pymtzip1", model.getValue("root/tempData/dataTemp/detailGrid/record/pymtzip").substring(0,3));
					model.setValue("/root/reqData/searchData/reqform/rdr_extn_form/pymtzip2", model.getValue("root/tempData/dataTemp/detailGrid/record/pymtzip").substring(3));
	
					model.refresh();
