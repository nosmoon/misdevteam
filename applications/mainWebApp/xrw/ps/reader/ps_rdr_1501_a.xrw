		<script type="javascript">
			<![CDATA[
			function init()
			{
				remakeNode("/root/tempData/initTemp");
					
				model.send("PID1501subInit", false);
					
				//model.setValue("/root/reqData/searchData/listSearch/pagesize", model.getValue("/root/tempData/initTemp/showRecCnt"));
				model.setValue("/root/reqData/searchData/listSearch/pagesize", "5000");
				model.setValue("/root/initData/naviInfo/records_per_page", model.getValue("/root/tempData/initTemp/showRecCnt"));
				model.setValue("/root/reqData/searchData/listSearch/sh_extndt_fr", model.getValue("/root/tempData/initTemp/defaultFrom"));
				model.setValue("/root/reqData/searchData/listSearch/sh_extndt_to", model.getValue("/root/tempData/initTemp/defaultTo"));
				
				model.copyNode("/root/initData/comboSet/mcCombo", "/root/tempData/initTemp/mcCombo");
				model.copyNode("/root/initData/comboSet/branchCombo", "/root/tempData/initTemp/branchCombo");
				
				model.removenode("/root/tempData/initTemp");
				
				model.refresh();
				PID1501combo1.select(0);
				PID1501combo3.select(0);
				PID1501combo2.select(0);
			}
			
			function search()
			{
				remakeNode("/root/tempData/resTemp");
				
				model.send("PID1501subSearch", false);
				
				model.setValue("/root/initData/naviInfo/curr_page_no", model.getValue("/root/tempData/resTemp/pageno"));
				model.setValue("/root/initData/naviInfo/total_records", model.getValue("/root/tempData/resTemp/total_records"));
				model.copyNode("/root/resData/resform/branchCombo", "/root/tempData/resTemp/branchCombo");
				model.copyNode("/root/resData/gridData/assignGrid", "/root/tempData/resTemp/assignGrid");
								
				model.removenode("/root/tempData/resTemp");
				//model.refresh;
				PID1501multilinegrid1.refresh();
				
				for(var i=0; i<PID1501multilinegrid1.rows; i++){
					if(gridRefColValue(PID1501multilinegrid1, i, "isReadOnly") == "true")
						PID1501multilinegrid1.isReadOnly(i, PID1501multilinegrid1.colRef("asin_check"), i, PID1501multilinegrid1.colRef("asinbocd")) = true;
				}
				
				remakeNode("/root/resData/gridData/compareGrid");
				PID1501datagrid1.rebuild();	
			}
			
			function save()
			{
				// nodeset 전체를 넘기고자 할 때는 makeNodesetToForm(_srcNodesetPath, _tarPath) 를 사용하면 된다.
				// 아래는 필드가 너무 많아 비효율적이므로 특정 필드를 지정하여 넘기는 방법이다.
				// 필드명을 변경하고 싶을때는 fields[fields.length] = new Array("asin_check", "asin") 과 같이 array로 넘기면 된다.
				
				if(!checkData())
					return;
				
				remakeNode("/root/tempData/formTemp");
				remakeNode("/root/tempData/resTemp");
				
				var fields = new Array();
				fields[fields.length] = "asin_check";
				fields[fields.length] = "rdr_extndt";
				fields[fields.length] = "rdr_extnno";
				fields[fields.length] = new Array("asinbocd", "resitypecd");
				
				makeNodesetToFormNamed("/root/resData/gridData/assignGrid/record", "/root/tempData/formTemp", fields);

				if(model.getValue("/root/tempData/formTemp/rdr_extndt0") == ""){
					return;
				}
				
				// 그외 필요한 필드 추가.
				makeCopyNode("/root/tempData/formTemp/listcnt", "/root/reqData/searchData/listSearch/pagesize");
				
				model.send("PID1501subSave", false);
				if(!submit_completed())
				return;
				
				model.removenode("/root/tempData/formTemp");
				model.removenode("/root/tempData/resTemp");
				
				search();
			}
			
			function checkData()
			{
				// 조회결과가 있는지, 선택된 항목이 있는지 확인.
				// 작업요망.
				
				return true;
			}
			
			function onClickAssignGrid()
			{
				if(curRowRefColValue(PID1501multilinegrid1, "rdr_extnno") == "")
				{
					return;
				}
				model.setValue("/root/reqData/searchData/userSearch/rdr_extnno", curRowRefColValue(PID1501multilinegrid1, "rdr_extnno"));
				model.setValue("/root/reqData/searchData/userSearch/rdr_extndt", curRowRefColValue(PID1501multilinegrid1, "rdr_extndt"));
				
				model.send("PID1501subCmpr", false);
				
				if(!submit_completed())
					return;

				if(instance1.selectNodes("/root/resData/gridData/compareGrid/record/bonm").length == 0)
				{
					model.alert("검색결과가 없습니다.");
					return;
				}

				PID1501datagrid1.rebuild();		
			}
			
			function gridClick()
			{
				if( PID1501multilinegrid1.textMatrix(PID1501multilinegrid1.row, PID1501multilinegrid1.colRef("rdr_extnno")) =="")
				{
					return;
				}
				
				model.setValue("/root/reqData/searchData/userSearch/rdr_extnno", PID1501multilinegrid1.textMatrix(PID1501multilinegrid1.row, PID1501multilinegrid1.colRef("rdr_extnno")));
				model.setValue("/root/reqData/searchData/userSearch/rdr_extndt", PID1501multilinegrid1.textMatrix(PID1501multilinegrid1.row, PID1501multilinegrid1.colRef("rdr_extndt")));
				openUserPub("/root/reqData/searchData/userSearch", "/root/tempData/userForm");				
			}
			]]>
		</script>
