		<datagrid id="PID1301datagrid1" nodeset="/root/resData/gridData/listGrid/record" caption="번호^제목^작성자^작성일자^조회수^첨부파일^다운로드수^추천" colsep="^" colwidth="0, 295, 100, 153, 60, 187, 74, 55" ellipsis="true" explorerbar="sortshowmove" mergecellsfixedrows="bycolrec" rowheader="reverseseq" rowsep="|" selectionmode="free" tooltip="true" style="left:10px; top:120px; width:926px; height:595px; ">
			<col ref="seq"/>
			<col ref="titl" style="color:#0000ff; text-align:left; cursor:hand; ">
				<script type="javascript" ev:event="ondblclick">
					<![CDATA[
					if(event.button == 1)
					{
						if(PID1301datagrid1.mouseCol>PID1301datagrid1.fixedCols-1 && PID1301datagrid1.mouseRow>PID1301datagrid1.fixedRows-1)
					       {
							gridClick();
