<?xml version="1.0" encoding="EUC-KR"?>
<?xml-stylesheet href="../../../css/style_sheet.css" type="text/css" ?>
<xhtml:html xmlns:ex="http://www.comsquare.co.kr/xforms/excel" xmlns:xlink="http://www.w3.org/1999/xlink" xmlns:xhtml="http://www.w3.org/1999/xhtml" xmlns:my="http://www.comsquare.co.kr/example" xmlns:ms="urn:schemas-microsoft-com:xslt" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns="http://www.w3.org/2002/01/xforms" xmlns:ev="http://www.w3.org/2001/xml-events" xmlns:cs="http://www.comsquare.co.kr/xforms">
	<xhtml:head>
		<xhtml:title>고정자산조회</xhtml:title>
		<model id="model1">
			<instance id="instance1">
				<root xmlns="">
					<initData>
						<!-- 화면 로딩시의 초기 데이터를 담당 -->
						<session/>
						<!-- 서버의 세션 정보를 담당한다 -->
						<comboSet>
							<!-- 선택계열의 정보를 담당 -->
							<radio/>
							<checkbox1/>
							<checkbox2/>
						</comboSet>
						<!-- 콤보리스트와 같은 선택게열의 데이터는 <item>을 반복함으로써 list를 구성한다. -->
						<pageInfo>
							<!-- 해당 화면의 필요한 기타 정보들을 추가운영 할 수 있다 -->
						</pageInfo>
						<!-- 해당 화면의 필요한 기타 정보들을 추가운영 할 수 있다 -->
					</initData>
					<reqData>
						<!-- 전달하고자 하는 데이터를 담당 ( submit 함수에서 처리) -->
						<searchData>
							<aset_no/>
							<aset_nm/>
							<search_tag/>
							<cmpy_cd/>
							<leas_bldg_cd/>
						</searchData>
						<!-- 검색조건 처리 -->
						<reqForm/>
						<!-- formData 팝업,화면전환시 data이동시 -->
						<multiUpDate/>
						<!-- MultiUpDate유형 사용시 처리 -->
						<fwdData>
							<func_nm/>
							<aset_no/>
							<leas_bldg_cd/>
							<search_tag/>
						</fwdData>
					</reqData>
					<resData>
						<!-- 전달 받은 데이터를 담당 ( submit의 결과가 담김) -->
						<resForm/>
						<!-- 전달 받은 formData를 담당 ( submit의 결과가 담김) -->
						<!-- 전달 받은 데이터를 가공할 임시저장소 ( submit의 결과가 담김) -->
						<!-- 전달 받은 errorCode를 담당 ( submit의 결과가 담김) -->
						<msg/>
						<!-- 전달 받은 errorMsg를 담당 ( submit의 결과가 담김) -->
						<gridData>
							<!-- 전달 받은 grid데이터를 담당 ( submit의 결과가 담김) -->
							<record>
								<!-- datagrid1 -->
								<aset_no/>
								<aset_nm/>
							</record>
							<!-- datagrid2 -->
							<!-- datagrid3 -->
						</gridData>
					</resData>
					<tempData/>
				</root>
			</instance>
			<script type="javascript" ev:event="xforms-ready">
				<![CDATA[
					if ( model1.getValue("/root/reqData/fwdData/leas_bldg_cd") != "" ) {
						model1.setValue("/root/reqData/searchData/leas_bldg_cd" , model1.getValue("/root/reqData/fwdData/leas_bldg_cd") );
						model1.refresh();
					}				
					if ( model1.getValue("/root/reqData/fwdData/aset_no") != "" ) {
						model1.setValue("/root/reqData/searchData/aset_no" , model1.getValue("/root/reqData/fwdData/aset_no") );
						model1.refresh();
					}
					if ( model1.getValue("/root/reqData/fwdData/search_tag") != "" ) {
						model1.setValue("/root/reqData/searchData/search_tag" , model1.getValue("/root/reqData/fwdData/search_tag") );
						model1.refresh();
					}
				]]>
			</script>
			<submission id="as_asrd_1600" action="/as/asrd/1600" mediatype="application/x-www-form-urlencoded" method="post" ref="/root/reqData/searchData" resultref="/root/tempData/dataSet"/>
		</model>
		<script type="javascript" src="/js/CIISComm.js"/>
		<script type="javascript" src="/js/TFCommon.js"/>
	</xhtml:head>
	<xhtml:body class="pop400" pagewidth="640" pageheight="480" style="font-family:굴림; ">
		<import id="iv_Title" src="../../../common/xrw/iv_TitleBar_pop400.xrw" style="left:10px; top:5px; width:380px; height:29px; "/>
		<group id="group_line" class="gro_line" style="left:10px; top:125px; width:380px; height:5px; "/>
		<group id="group1" class="gro_sel" style="left:10px; top:71px; width:380px; height:53px; ">
			<caption id="caption25" class="list_cap01" style="left:0px; top:0px; width:105px; height:26px; text-align:center; ">자산번호</caption>
			<caption id="caption50" class="list_cap02" style="left:104px; top:0px; width:276px; height:26px; text-align:center; "/>
			<input id="input30" ref="/root/reqData/searchData/aset_no" imemode="disabled" style="left:108px; top:4px; width:100px; height:18px; background-color:#ffffff; ">
				<script type="javascript" ev:event="onkeypress">
					<![CDATA[
					if (event.keyCode == 13) {						
						model1.setValue("/root/reqData/searchData/aset_no", input30.currentText);
						btnA4.dispatch("onclick");
					}
					]]>
				</script>
				<script type="javascript" ev:event="DOMFocusOut">
					<![CDATA[
					model1.setValue("/root/reqData/searchData/aset_no", input30.currentText.toUpperCase());
					input30.refresh();
					]]>
				</script>
			</input>
			<caption id="caption1" class="list_cap01" style="left:0px; top:25px; width:105px; height:26px; text-align:center; ">자산명칭</caption>
			<caption id="caption4" class="list_cap02" style="left:104px; top:25px; width:276px; height:26px; text-align:center; "/>
			<input id="input1" ref="/root/reqData/searchData/aset_nm" imemode="hangul" style="left:108px; top:29px; width:200px; height:18px; background-color:#ffffff; ">
				<script type="javascript" ev:event="onkeypress">
					<![CDATA[
					if (event.keyCode == 13) {						
						model1.setValue("/root/reqData/searchData/aset_nm", input1.currentText);
						btnA4.dispatch("onclick");
					}
					]]>
				</script>
			</input>
		</group>
		<datagrid id="datagrid2" nodeset="/root/resData/gridData/record" caption="자산번호^자산명칭" colsep="^" colwidth="121, 253" extendlastcol="scroll" mergecellsfixedrows="bycolrec" rowheader="seq" rowsep="|" noeset="/root/gridData/main/a" style="left:10px; top:135px; width:381px; height:158px; ">
			<col ref="aset_no"/>
			<col ref="aset_nm" style="text-align:left; "/>
			<script type="javascript" ev:event="onclick">
				<![CDATA[
					if(datagrid2.row == 0) return;			
					
					var selectedIdx = datagrid2.selectedRow(0);
					var aset_no = model1.getValue("/root/resData/gridData/record["+ selectedIdx +"]/aset_no");
					var aset_nm = model1.getValue("/root/resData/gridData/record["+ selectedIdx +"]/aset_nm");
					//alert("dept_cd=" + dept_cd + "\ndept_nm=" + dept_nm);
					model1.makeNode("/root/reqData/fwdData/aset_no");
					model1.setValue("/root/reqData/fwdData/aset_no", aset_no);
					
					model1.makeNode("/root/reqData/fwdData/aset_nm");
					model1.setValue("/root/reqData/fwdData/aset_nm", aset_nm);
				]]>
			</script>
			<script type="javascript" ev:event="ondblclick">
				<![CDATA[
				if ((datagrid2.mouseCol > 0) && (datagrid2.mouseCol < datagrid2.cols) && (datagrid2.mouseRow >= datagrid2.fixedRows))  {
					var aset_no = datagrid2.valueMatrix(datagrid2.selectedRow(0), 1);
					var aset_nm = datagrid2.valueMatrix(datagrid2.selectedRow(0), 2);	
					
					var retFunc = model1.getValue("/root/reqData/fwdData/func_nm");
					var selectedIdx = datagrid2.selectedRow(0);
					var aset_no = model1.getValue("/root/resData/gridData/record["+ selectedIdx +"]/aset_no");
					var aset_nm = model1.getValue("/root/resData/gridData/record["+ selectedIdx +"]/aset_nm");
					eval("window.opener.javascript." + retFunc + "('" + aset_no + "', '" + aset_nm + "')");
					window.close();	
				}
				]]>
			</script>
		</datagrid>
		<group id="group2" class="gro_btn" style="left:9px; top:45px; width:381px; height:21px; ">
			<button id="btnA4" class="btn02" style="left:156px; top:0px; width:52px; height:21px; ">
				<caption>조회</caption>
				<script type="javascript" ev:event="onclick">
					<![CDATA[
//					var aset_no = model1.getValue("/root/reqData/fwdData/aset_no");
//					if (aset_no != "") {
//						model1.setValue("/root/reqData/searchData/aset_no", aset_no);
//					}

					var leas_bldg_cd = model1.getValue("/root/reqData/fwdData/leas_bldg_cd");
					if (leas_bldg_cd != '') {
						model1.setValue("/root/reqData/searchData/leas_bldg_cd", leas_bldg_cd);
					}
					
//					if( model1.getValue("/root/reqData/searchData/aset_no") == "" && model1.getValue("/root/reqData/searchData/aset_nm") == "" ) {
//						alert("자산번호 또는 자산명칭을 입력해야 조회가능합니다.");
//						return;
//					}
					model1.send("as_asrd_1600");
					model1.copyNode("/root/resData/gridData", "/root/tempData/dataSet/CURLIST");
					datagrid2.refresh();
					]]>
				</script>
			</button>
			<button id="btnReset" class="btn02" style="left:214px; top:0px; width:52px; height:21px; ">
				<caption>초기화</caption>
				<script type="javascript" ev:event="onclick">
					<![CDATA[
					model1.reset();		
					model1.resetInstanceNode("/root/reqData");
					model1.refresh();
					]]>
				</script>
			</button>
			<button id="button3" class="btn02" style="left:271px; top:0px; width:52px; height:21px; ">
				<caption>확인</caption>
				<script type="javascript" ev:event="onclick">
					<![CDATA[
						var aset_no = model1.getValue("/root/reqData/fwdData/aset_no");
						var aset_nm = model1.getValue("/root/reqData/fwdData/aset_nm");
						
						if (aset_no == ''){
							alert("고정자산을 선택해주십시오.");
							return;
						} else {
							var retFunc = model1.getValue("/root/reqData/fwdData/func_nm");
							var selectedIdx = datagrid2.selectedRow(0);
							var aset_no = model1.getValue("/root/resData/gridData/record["+ selectedIdx +"]/aset_no");
							var aset_nm = model1.getValue("/root/resData/gridData/record["+ selectedIdx +"]/aset_nm");
							eval("window.opener.javascript." + retFunc + "('" + aset_no + "', '" + aset_nm + "')");
							window.close();
						}
					]]>
				</script>
			</button>
			<button id="btnClose" class="btn02" style="left:329px; top:0px; width:52px; height:21px; ">
				<caption>종료</caption>
				<script type="javascript" ev:event="onclick">
					<![CDATA[
					//viewer("mainFrame").window.javaScript.closeMDIWindow(model.getValue("/root/initData/pageInfo/pageID"));
					window.close();
					]]>
				</script>
			</button>
		</group>
		<caption id="caption2" class="tit01" style="left:10px; top:5px; width:130px; height:27px; ">고정자산조회</caption>
	</xhtml:body>
</xhtml:html>
