			<script type="javascript" ev:event="xforms-ready">
				<![CDATA[
					model.send("co_cocd_1250_m");
					if(submit_completed()){
						model.setValue("/root/initData/title", model.getValue("/root/tempData/title"));
						model.refresh();
						
						var cd 	= model.getValue("/root/reqData/searchData/cd");
						var cdnm = model.getValue("/root/reqData/searchData/cdnm");
						
						if(cd != "" || cdnm != "" || model.getValue("/root/tempData/title") == "관리구분"){
							model.send("co_cocd_1210_l");
						}
					}
					
					if(cd == ""){	
						model.setFocus("input1",true);
					}else{
						model.setFocus("datagrid1",true);
					}
				]]>
			</script>
			<submission id="co_cocd_1210_l" action="/co/cocd/1210" mediatype="application/x-www-form-urlencoded" method="post" ref="/root/reqData/searchData" resultref="/root/resData/gridData"/>
			<submission id="co_cocd_1250_m" action="/co/cocd/1250" mediatype="application/x-www-form-urlencoded" method="post" ref="/root/reqData/searchData" resultref="/root/tempData"/>
