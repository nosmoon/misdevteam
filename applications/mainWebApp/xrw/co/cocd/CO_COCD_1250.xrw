			<caption>종료</caption>
			<script type="javascript" ev:event="onclick">
				<![CDATA[
					window.close();				
				]]>
			</script>
		</button>
		<caption id="caption1" class="list_cap01" style="left:10px; top:35px; width:80px; height:26px; ">코드</caption>
		<caption id="caption9" class="list_cap01" style="left:10px; top:60px; width:80px; height:26px; ">코드명</caption>
		<input id="input1" ref="/root/reqData/searchData/cd" style="left:95px; top:39px; width:340px; height:18px; ">
			<script type="javascript" ev:event="onkeypress">
				<![CDATA[

				if(event.keyCode == "13"){
					var cd = input1.currentText;
					
					if(cd == ""){
						alert("코드를 입력하세요.");
						return;
					}
					model.setValue("/root/reqData/searchData/cd",cd);
					model.send("co_cocd_1210_l");
					model.setFocus("datagrid1",true);
				}				
				]]>
			</script>
		</input>
		<input id="input2" ref="/root/reqData/searchData/cdnm" imemode="hangul" style="left:95px; top:64px; width:340px; height:18px; ">
			<script type="javascript" ev:event="onkeypress">
				<![CDATA[
				if(event.keyCode == "13"){
					var cdnm = input2.currentText;
					if(cdnm == ""){
						alert("코드명을 입력하세요.");
						return;
					}
					model.setValue("/root/reqData/searchData/cdnm", cdnm);
					model.send("co_cocd_1210_l");
					model.setFocus("datagrid1",true);
				}
				]]>
			</script>
		</input>
		<script type="javascript" ev:event="onkeypress">
			<![CDATA[
			if(event.keyCode == "27"){
					window.close();	
				}
			]]>
