			<script type="javascript">
				<![CDATA[
					function setAppmt_cd(cd, cdnm){
						model1.setValue("/root/reqData/reqForm/appmt_cd", cd);
						model1.setValue("/root/reqData/reqForm/appmt_nm", cdnm);
						model1.refresh();
					}
					function loadData(emp, nm, gubun, sort){
						model1.setValue("/root/resData/resForm/emp_no", emp);
						model1.setValue("/root/reqData/searchData/emp_no", emp);
						model1.setValue("/root/reqData/reqForm/emp_no", emp);
						model1.setValue("/root/resData/resForm/nm_korn", nm);
						model1.setValue("/root/reqData/searchData/flnm", nm);
						model1.setValue("/root/reqData/searchData/nm_korn", nm);
						model1.setValue("/root/resData/resForm/sort", sort);
						model1.setValue("/root/reqData/searchData/sort", sort);
						
						if(gubun == "N"){
							model1.setValue("/root/reqData/reqForm/emp_no", emp);
							model1.setValue("/root/reqData/reqForm/flnm", nm);
						}
						
						model1.send("hd_appmt_1201");
						model1.copyNode("/root/resData/gridData", "/root/tempData");
						model1.removenode("/root/tempData");
						datagrid2.clearStatus();
						model1.refresh();
						
						model1.send("hd_info_1002");
						img1.refresh();
						gridSearch();
							
						var totcnt = datagrid2.rows;
						model1.setValue("/root/resData/totcnt", "총 " + Number(totcnt - 1) + " 건");
						model1.refresh();
					}
					function setEmp_no_i(emp_no, nm_korn){
						model1.setValue("/root/reqData/reqForm/emp_no", emp_no);
						model1.setValue("/root/reqData/reqForm/flnm", nm_korn);
						model1.refresh();
					}
					function setDept_cd_i(iv_dept_cd, iv_dept_nm){
						model1.setValue("/root/reqData/reqForm/dept_cd", iv_dept_cd);
						var dept_temp = iv_dept_cd + " " + iv_dept_nm;
						model1.setValue("/root/reqData/reqForm/dept_nm", dept_temp);
						model1.refresh();
					}
					function setEmp_no(emp_no, nm_korn){
						var flnm = nm_korn;
						//alert(emp_no);
						//alert(flnm);
						model1.reset();
						model1.setValue("/root/resData/resForm/emp_no", emp_no);
						model1.setValue("/root/reqData/searchData/emp_no", emp_no);
						model1.setValue("/root/resData/resForm/nm_korn", flnm);
						model1.setValue("/root/reqData/searchData/flnm", flnm);
						model1.setValue("/root/reqData/searchData/nm_korn", flnm);
						model1.setValue("/root/reqData/reqForm/emp_no", emp_no);
						model1.setValue("/root/reqData/reqForm/flnm", flnm);
						var sort = model1.getValue("/root/resData/resForm/sort");
						model1.setValue("/root/reqData/searchData/sort", sort);
						
						if(!get_appmt_1204()){
							return;
						}
						get_appmt_1007();
						get_appmt_1205();
						get_appmt_1201();
						get_info_1002();
						gridSearch();
						rowCnt.value = "총 "+ (parseInt(datagrid2.rows)- datagrid2.fixedRows) + " 건";
						model1.refresh();
					}
					function setDept_cd(iv_dept_cd, iv_dept_nm){
						//model1.reset();
						model1.setValue("/root/resData/resForm/dept_cd", iv_dept_cd);
						model1.setValue("/root/resData/resForm/dept_nm", iv_dept_nm);
						model1.setValue("/root/reqData/searchData/dept_cd", iv_dept_cd);
						var sort = model1.getValue("/root/resData/resForm/sort");
						model1.setValue("/root/reqData/searchData/sort", sort);
						model1.refresh();
//						model1.send("hd_appmt_1201");
//						model1.copyNode("/root/resData/gridData", "/root/tempData");
//						model1.removenode("/root/tempData");
//						model1.refresh();
//							
//						var totcnt = datagrid2.rows;
//						model1.setValue("/root/resData/totcnt", "총 " + Number(totcnt - 1) + " 건");
//						model1.refresh();
					}
					function gridSearch(){
						model1.setValue("/root/reqData/reqForm/emp_no", model1.getValue("/root/resData/gridData/record[1]/emp_no")); // 사번
						model1.setValue("/root/reqData/reqForm/flnm", model1.getValue("/root/resData/gridData/record[1]/flnm")); // 이름
						model1.setValue("/root/reqData/reqForm/dept_cd", model1.getValue("/root/resData/gridData/record[1]/dept_cd")); // 부서코드
						model1.setValue("/root/reqData/reqForm/dept_nm", model1.getValue("/root/resData/gridData/record[1]/dept_nm")); // 부서이름
						model1.setValue("/root/reqData/reqForm/emp_clsf_cd", model1.getValue("/root/resData/gridData/record/emp_clsf_cd")); // 사원구분코드
						model1.setValue("/root/reqData/reqForm/emp_clsf_nm", model1.getValue("/root/resData/gridData/record/emp_clsf_nm")); // 사원구분이름
						model1.setValue("/root/reqData/reqForm/base_saly", model1.getValue("/root/resData/gridData/record[1]/base_saly")); // 기본급
						model1.setValue("/root/reqData/reqForm/posk_saly", model1.getValue("/root/resData/gridData/record[1]/posk_saly")); // 직무급
						model1.setValue("/root/reqData/reqForm/saly_stot", model1.getValue("/root/resData/gridData/record[1]/saly_stot")); // 급여합계
						model1.setValue("/root/reqData/reqForm/dspch_enpr_main_clsf", model1.getValue("/root/resData/gridData/record[1]/dspch_enpr_main_clsf")); // 소속회사
						model1.setValue("/root/reqData/reqForm/posk_clas_cd", model1.getValue("/root/resData/gridData/record[1]/posk_clas_cd")); // 직무코드
						model1.setValue("/root/reqData/reqForm/posk_clas_nm", model1.getValue("/root/resData/gridData/record[1]/posk_clas_nm")); // 직무명
						
						//초기화..
						caption32.value = "";
						var rows = datagrid2.rows;
						var fixrows = datagrid2.fixedRows;
						for(i=fixrows;i<=rows;i++){
							datagrid2.rowStyle(i,"all","color")="";	
						}
						changeGrid(); //등록상태인것 색깔 바꾸기..
					}
					function changeGrid(){
						var col = datagrid2.colRef("clos_proc_yn_nm");
						var rows = datagrid2.rows;
						var fixrows = datagrid2.fixedRows;
						for(i=fixrows;i<=rows;i++){
							var appmt_stat_clsf = datagrid2.valueMatrix(i,col);
							if(appmt_stat_clsf == "등록"){
								datagrid2.rowStyle(i,"all","color")="#0000FF";	
							}
						}
					}
					function get_appmt_1007(){//파견사원 사번/이름/부서 가꼬오기....
						model1.send("hd_appmt_1007");
						model1.setValue("/root/resData/resForm/emp_no", model1.getValue("/root/dataSet/CURLIST/record/emp_no"));
						model1.setValue("/root/reqData/searchData/flnm", model1.getValue("/root/dataSet/CURLIST/record/nm_korn"));
						model1.setValue("/root/resData/resForm/nm_korn", model1.getValue("/root/dataSet/CURLIST/record/nm_korn"));
						//model1.setValue("/root/resData/resForm/dept_cd", model1.getValue("/root/dataSet/CURLIST/record/dept_cd"));
						//model1.setValue("/root/reqData/searchData/dept_cd", model1.getValue("/root/dataSet/CURLIST/record/dept_cd"));
						//model1.setValue("/root/resData/resForm/dept_nm", model1.getValue("/root/dataSet/CURLIST/record/dept_nm"));
						model1.refresh();
						model1.removenode("/root/dataSet");
					}
					function get_appmt_1204(){ //파견사원인지 check
						var flag = false;
						model1.send("hd_appmt_1204");
						if(model1.getValue("/root/dataSet/CURLIST/record/cnt") == "0"){
							model1.removenode("/root/dataSet");
							alert("사원이 없거나 고용상태가 틀립니다.");
							model1.reset();
							group2.disabled = true;
							return flag;
						}else{
							flag = true;
							return flag;
						}
					}
					function get_info_1002(){//사진가꼬오기..
						model1.send("hd_info_1002");
						img1.refresh();
					}
					function get_appmt_1201(){//그리드 가꼬오기..
						model1.send("hd_appmt_1201");
						model1.copyNode("/root/resData/gridData", "/root/tempData");
						model1.removenode("/root/tempData");
						model1.refresh();
						group2.disabled = false;
					}
					function get_appmt_1205(){//파견발령 데이터유무..
						model1.send("hd_appmt_1205");
						if(model1.getValue("/root/dataSet/CURLIST/record/cnt") == "0"){
							input18.visible = true;
							input16.visible = false;
							combo3.disabled = true;
							combo3.value = '';
							output3.value = '';
						}else if(model1.getValue("/root/dataSet/CURLIST/record/cnt") > "0"){
							input16.visible = true;
							input18.visible = false;
						}
						model1.removenode("/root/dataSet");
					}
				]]>
