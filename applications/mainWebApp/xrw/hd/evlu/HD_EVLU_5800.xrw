					}
					//평가구분값 파싱
					if(model1.getValue("/root/reqData/searchData/evlu_cd") != ""){
						model1.setValue("/root/reqData/searchData/evlu_yy", model1.getValue("/root/reqData/searchData/evlu_cd").substring(0,4));
						model1.setValue("/root/reqData/searchData/tms_clsf", model1.getValue("/root/reqData/searchData/evlu_cd").substring(4,6));
					}
				*/
				]]>
			</script>
		</select1>
		<!-- 평가대상 제외자 선정기준일 -->
		<caption id="caption1" class="list_cap02" style="left:10px; top:94px; width:926px; height:26px; "/>
		<!-- 평가대상 제외자 선정기준 -->
		<caption id="caption5" class="tit02" style="left:10px; top:132px; width:926px; height:20px; ">평가자 공지사항</caption>
		<button id="button4" class="btn_grid05" visibility="hidden" style="left:851px; top:142px; width:75px; height:17px; ">
			<caption>개인알림</caption>
			<script type="javascript" ev:event="DOMActivate">
				<![CDATA[
					if(!commGridReqCheck("datagrid1")) return;
					//체크박스가 체크유무
					var chk_flag = true;
					for(var i=datagrid1.fixedCols; i<=(datagrid1.rows-datagrid1.fixedCols); i++){
						if(datagrid1.valueMatrix(i, datagrid1.colRef("sel_chk"))=="true") chk_flag = false;
					}
					if(chk_flag){ alert("공지하실 평가자를 선택해 주세요."); return; }
					if(!commReqCheck("group1")) return;
					if(window.alert("개인알림을 등록하시겠습니까?", "확인", 1)!=1) return;
					model1.setValue("/root/reqData/multiUpDate", datagrid1.getUpdateData());
					model1.send("hd_evlu_5802_i");
					model1.removenode("/root/dataSet");
					model1.refresh();
					if(submit_completed()) searchList();
				]]>
			</script>
		</button>
		<datagrid id="datagrid1" nodeset="/root/resData/gridData/record" caption="선택^부서^사번^성명^직책^직급^DB_상태값^부서코드^직책코드^직급코드" colsep="#" colwidth="50, 100, 85, 85, 82, 70, 5, 5, 5, 5" mergecellsfixedrows="bycolrec" rowheader="update" rowsep="|" style="left:10px; top:152px; width:510px; height:298px; ">
			<col ref="sel_chk" type="checkbox"/>
			<col ref="dept_nm" style="text-align:left; "/>
			<col ref="emp_no"/>
			<col ref="nm_korn"/>
			<col ref="dty_nm"/>
			<col ref="posi_nm"/>
			<col ref="db_status" visibility="hidden"/>
			<col ref="dept_cd" visibility="hidden"/>
			<col ref="dty_cd" visibility="hidden"/>
			<col ref="posi_cd" visibility="hidden"/>
			<script type="javascript" ev:event="xforms-select">
				<![CDATA[
					datagrid1.rowStatus(datagrid1.row) = 1;
				]]>
			</script>
			<script type="javascript" ev:event="xforms-deselect">
				<![CDATA[
					datagrid1.rowStatus(datagrid1.row) = -1;
				]]>
			</script>
		</datagrid>
		<group id="group1" overflow="hidden" style="left:10px; top:482px; width:940px; height:228px; ">
			<!-- 검색조건 항목 -->
			<caption id="caption3" class="list_cap02" style="left:0px; top:0px; width:926px; height:26px; "/>
			<caption id="caption4" class="list_cap01" style="left:0px; top:0px; width:100px; height:26px; ">공지기간</caption>
			<input id="input1" ref="/root/reqData/reqForm/annc_frdt" class="req" editable="false" format="yyyy/mm/dd" caption="공지기간 시작일" style="left:104px; top:4px; width:100px; height:18px; "/>
			<caption id="caption12" style="left:215px; top:3px; width:25px; height:20px; ">~</caption>
			<input id="input2" ref="/root/reqData/reqForm/annc_todt" class="req" editable="false" format="yyyy/mm/dd" caption="공지기간 종료일" style="left:249px; top:4px; width:100px; height:18px; "/>
			<caption id="caption10" class="list_cap02" style="left:0px; top:25px; width:926px; height:200px; "/>
			<caption id="caption11" class="list_cap01" style="left:0px; top:25px; width:100px; height:200px; ">공지내용</caption>
			<textarea id="textarea1" ref="/root/reqData/reqForm/annc_cont" class="req" caption="공지내용" style="left:104px; top:30px; width:812px; height:188px; text-align:left; vertical-align:top; "/>
		</group>
		<select1 id="radio1" ref="/root/reqData/searchData/sel_pgm_id" appearance="full" cellspacing="20" cols="3" rows="1" overflow="visible" style="left:110px; top:99px; width:175px; height:20px; border-style:none; ">
			<choices>
				<item>
					<label>평가자1</label>
					<value>1tms</value>
				</item>
				<item>
					<label>평가자2</label>
					<value>2tms</value>
				</item>
			</choices>
		</select1>
		<caption id="rowCnt" style="left:390px; top:455px; width:125px; height:20px; font-weight:bold; text-align:right; "/>
