<?xml version="1.0" encoding="EUC-KR"?>
<?xml-stylesheet type="text/css" href="../../../css/style_sheet.css" ?>
<xhtml:html xmlns:ex="http://www.comsquare.co.kr/xforms/excel" xmlns:xlink="http://www.w3.org/1999/xlink" xmlns:xhtml="http://www.w3.org/1999/xhtml" xmlns:ms="urn:schemas-microsoft-com:xslt" xmlns:my="http://www.comsquare.co.kr/example" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns="http://www.w3.org/2002/01/xforms" xmlns:ev="http://www.w3.org/2001/xml-events" xmlns:cs="http://www.comsquare.co.kr/xforms">
	<xhtml:head>
		<xhtml:title>파견직(MIS) - 평가자 개인알림 화면</xhtml:title>
		<model id="model1">
			<instance id="instance1">
				<root xmlns="">
					<initData>
						<session/>
						<comboSet>
							<evlu_cd>
								<item/>
							</evlu_cd>
						</comboSet>
						<pageInfo>
						</pageInfo>
					</initData>
					<reqData>
						<searchData>
							<evlu_cd/>
							<evlu_yy/>
							<tms_clsf/>
							<sel_pgm_id>1tms</sel_pgm_id>
						</searchData>
						<reqForm>
							<pgm_id/>
							<pgm_nm/>
							<pgm_url/>
							<job_clsf/>
							<annc_frdt/>
							<annc_todt/>
							<annc_cont/>
							<annc_clsf/>
						</reqForm>
						<multiUpDate/>
						<fwdData/>
					</reqData>
					<resData>
						<msg/>
						<resForm>
							<valid_evlu_cd/>
						</resForm>
						<gridData>
							<record>
								<sel_chk/>
								<emp_no/>
								<nm_korn/>
								<dept_nm/>
								<dty_nm/>
								<posi_nm/>
								<db_status/>
								<dept_cd/>
								<dty_cd/>
								<posi_cd/>
							</record>
						</gridData>
					</resData>
					<tempData/>
				</root>
			</instance>
			<script type="javascript" ev:event="xforms-ready">
				<![CDATA[
					model1.send("hd_evlu_8000_l");
					//model1.setValue("/root/resData/resForm/valid_evlu_cd",model1.getValue("/root/dataSet/valid_evlu_cd"));
					model1.copyNode("/root/initData/comboSet/evlu_cd", "/root/dataSet/CURLIST");
					model1.removeNode("/root/dataSet");
					model1.refresh();
					combo1.select(0);
					//평가구분값 파싱
					if(model1.getValue("/root/reqData/searchData/evlu_cd") != ""){
						model1.setValue("/root/reqData/searchData/evlu_yy", model1.getValue("/root/reqData/searchData/evlu_cd").substring(0,4));
						model1.setValue("/root/reqData/searchData/tms_clsf", model1.getValue("/root/reqData/searchData/evlu_cd").substring(4,6));
					}
					model1.refresh();
					model1.send("hd_evlu_5800_m");
					model1.setValue("/root/reqData/reqForm/annc_frdt", model1.getValue("/root/dataSet/annc_frdt"));
					model1.setValue("/root/reqData/reqForm/annc_todt", model1.getValue("/root/dataSet/annc_todt"));
					model1.removeNode("/root/dataSet");
					model1.refresh();
					model1.removeNode("/root/resData/gridData/record");
					model1.refresh();
				]]>
			</script>
			<submission id="hd_evlu_8000_l" action="/servlet/hd/evlu/8000" mediatype="application/x-www-form-urlencoded" method="post" resultref="/root/tempData"/>
			<submission id="hd_evlu_5800_m" action="/servlet/hd/evlu/5800" mediatype="application/x-www-form-urlencoded" method="post" ref="/root/reqData" resultref="/root/tempData"/>
			<submission id="hd_evlu_5801_l" action="/servlet/hd/evlu/5801" mediatype="application/x-www-form-urlencoded" method="post" ref="/root/reqData" resultref="/root/tempData"/>
			<submission id="hd_evlu_5802_i" action="/servlet/hd/evlu/5802" mediatype="application/x-www-form-urlencoded" method="post" ref="/root/reqData" resultref="/root/tempData"/>
		</model>
		<script type="javascript" src="/js/CIISComm.js"/>
		<script type="javascript">
			<![CDATA[
				//평가대상 목록생성
				function searchList(){
					//상태값이 남아있기 때문에 기존의 노드를 모두 초기화
					model1.removeNode("/root/resData/gridData");
					model1.makeNode("/root/resData/gridData");
					model1.refresh();
					//데이터 생성
					model1.send("hd_evlu_5801_l");	
					model1.setValue("/root/reqData/reqForm/pgm_id",model1.getValue("/root/dataSet/pgm_id"));
					model1.setValue("/root/reqData/reqForm/pgm_nm",model1.getValue("/root/dataSet/pgm_nm"));
					model1.setValue("/root/reqData/reqForm/pgm_url",model1.getValue("/root/dataSet/pgm_url"));
					model1.setValue("/root/reqData/reqForm/job_clsf",model1.getValue("/root/dataSet/job_clsf"));
					model1.setValue("/root/reqData/reqForm/annc_clsf",model1.getValue("/root/dataSet/annc_clsf"));
					model1.copyNode("/root/resData/gridData","/root/dataSet/CURLIST");
					model1.removenode("/root/dataSet");
					model1.refresh();
					rowCnt.value = "총 "+ (parseInt(datagrid1.rows)- 1) + " 건";
				}
			]]>
		</script>
	</xhtml:head>
	<xhtml:body class="main" pagewidth="946" pageheight="765" guideline="1,10;1,935;2,60;2,69;2,94;2,120;2,127;2,132;2,142;2,746;2,473;2,477;" style="font-family:굴림; ">
		<!-- 표준 항목. 타이틀, 기본버튼, 표시라인 -->
		<import id="import1" src="../../../common/xrw/iv_TitleBar.xrw" style="left:10px; top:5px; width:926px; height:29px; "/>
		<group id="group_ActionBtn" style="left:10px; top:39px; width:926px; height:21px; ">
			<button id="button1" class="btn03" style="left:863px; top:0px; width:63px; height:21px; background-image:../../../images/btn3.gif; ">
				<caption>종료</caption>
				<script type="javascript" ev:event="DOMActivate">
					<![CDATA[
						if(model.getValue("/root/initData/pageInfo/pageID") != ""){
						 	viewer("mainFrame").window.javaScript.closeMDIWindow(model.getValue("/root/initData/pageInfo/pageID"));
						}else{
							window.close();
						}		
					]]>
				</script>
			</button>
			<button id="button2" class="btn03" style="left:731px; top:0px; width:63px; height:21px; background-image:../../../images/btn3.gif; ">
				<caption>조회</caption>
				<script type="javascript" ev:event="DOMActivate">
					<![CDATA[
						searchList();
					]]>
				</script>
			</button>
			<button id="button3" class="btn03" style="left:797px; top:0px; width:63px; height:21px; background-image:../../../images/btn3.gif; ">
				<caption>개인알림</caption>
				<script type="javascript" ev:event="DOMActivate">
					<![CDATA[
						if(!commGridReqCheck("datagrid1")) return;
						//체크박스가 체크유무
						var chk_flag = true;
						for(var i=datagrid1.fixedCols; i<=(datagrid1.rows-datagrid1.fixedCols); i++){
							if(datagrid1.valueMatrix(i, datagrid1.colRef("sel_chk"))=="true") chk_flag = false;
						}
						if(chk_flag){ alert("공지하실 평가자를 선택해 주세요."); return; }
						if(!commReqCheck("group1")) return;
						if(window.alert("개인알림을 등록하시겠습니까?", "확인", 1)!=1) return;
						model1.setValue("/root/reqData/multiUpDate", datagrid1.getUpdateData());
						model1.send("hd_evlu_5802_i");
						model1.removenode("/root/dataSet");
						model1.refresh();
						if(submit_completed()) searchList();
					]]>
				</script>
			</button>
		</group>
		<group id="group_line" class="gro_line" style="left:10px; top:127px; width:926px; height:5px; "/>
		<!-- 검색조건 항목 -->
		<caption id="captoin1" class="list_cap02" style="left:10px; top:69px; width:926px; height:26px; "/>
		<caption id="caption2" class="list_cap01" style="left:10px; top:69px; width:100px; height:26px; ">평가구분</caption>
		<select1 id="combo1" ref="/root/reqData/searchData/evlu_cd" appearance="minimal" style="left:113px; top:73px; width:150px; height:18px; ">
			<choices>
				<itemset nodeset="/root/initData/comboSet/evlu_cd/item">
					<label ref="evlu_cdnm"/>
					<value ref="evlu_cd"/>
				</itemset>
			</choices>
			<script type="javascript" ev:event="xforms-select">
				<![CDATA[
				/*
					//버튼토글
					if(model1.getValue("/root/reqData/searchData/evlu_cd")==model1.getValue("/root/resData/resForm/valid_evlu_cd")){
