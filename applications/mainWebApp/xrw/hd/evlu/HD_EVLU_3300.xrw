				//평가그룹 목록생성
				function searchList(){
					

					//평가구분값 파싱
					if(model1.getValue("/root/reqData/searchData/evlu_cd") != ""){
						model1.setValue("/root/reqData/reqForm/evlu_yy", model1.getValue("/root/reqData/searchData/evlu_cd").substring(0,4));
						model1.setValue("/root/reqData/reqForm/tms_clsf", model1.getValue("/root/reqData/searchData/evlu_cd").substring(4,6));
					}
					//상태값 초기화
					model1.removeNode("/root/resData/gridData1");
					model1.makeNode("/root/resData/gridData1");
					model1.refresh();
					//데이터 생성
					model1.send("hd_evlu_3301_l");	
					model1.copyNode("/root/resData/gridData1","/root/dataSet/CURLIST");
					model1.removeNode("/root/dataSet");
					model1.refresh();
					
					for(var i=datagrid1.fixedRows; i<=datagrid1.rows; i++){
						if(datagrid1.valueMatrix(i,datagrid1.colRef("evlu_proc_stat_2tms")) == "2차평가완료")
						{
							datagrid1.cellstyle("background-image", i, 3) =     "/images/hd/btn_text_002.gif";						
						}
						else 
						{
							datagrid1.cellstyle("background-image", i, 3) =     "/images/hd/btn_text_001.gif";
						}
					}
				}
				//그룹별 피평가자 목록생성
				function searchList2(stat_2tms){ //2차완료여부 받아옴
		
					//datagrid1.selectedRow(0)
					//선택한 그룹코드
					model1.setValue("/root/reqData/reqForm/evlu_grp_cd",datagrid1.valueMatrix(datagrid1.selectedRow(0) ,datagrid1.colRef("evlu_grp_cd")));
					
				   var  rStat_2tms  =  datagrid1.valueMatrix(datagrid1.selectedRow(0) ,datagrid1.colRef("evlu_proc_stat_2tms")); // 2차평가 완료 여부 
                    
					if(rStat_2tms == "2차평가완료") // 2차 평가 완료 상태로 .. 버튼을 사용 못하게 함 
					{
						button7.disabled = true ; 
						stat_2tms = "Y";
					}
					else 
					{
						button7.disabled = false ; 
					}
					
						
					for(var i =datagrid2.fixedRows; i<datagrid2.rows; i++){ //조정등급초기화
						datagrid2.colDisabled(datagrid2.colRef("evlu_pers_deci_grad_2tms")) = false;
					}
					
					//상태값 초기화
					model1.removeNode("/root/resData/gridData2");
					model1.makeNode("/root/resData/gridData2");
					//model1.refresh();
					//데이터 생성
					model1.send("hd_evlu_3302_l");	
					model1.setValue("/root/resData/resForm/s_nops_2tms",model1.getValue("/root/dataSet/s_nops_2tms"));
					model1.setValue("/root/resData/resForm/a_pls_nops_2tms",model1.getValue("/root/dataSet/a_pls_nops_2tms"));
					//model1.setValue("/root/resData/resForm/s_nops_2tms_str","'S' 등급 가능 인원은 "+model1.getValue("/root/resData/resForm/s_nops_2tms")+" 명 이내 입니다.");
				    caption3.value =  model1.getValue("/root/resData/resForm/s_nops_2tms");
					//caption3.refresh();
					model1.copyNode("/root/resData/gridData2","/root/dataSet/CURLIST");
					model1.removeNode("/root/dataSet");
					model1.refresh();
					//선택한 사번이 있다면
					if(model1.getValue("/root/reqData/reqForm/emp_no") != ""){
						for(var i=datagrid2.fixedRows; i<=datagrid2.rows; i++){
							if(datagrid2.valueMatrix(i,datagrid2.colRef("emp_no")) == model1.getValue("/root/reqData/reqForm/emp_no")){
								datagrid2.isSelected(i) = true;
								datagrid2.valueMatrix(i,datagrid2.colRef("sel_chk")) = 'true';
								break;
							}
						}
					}
					for(var k =datagrid2.fixedRows; k<=datagrid2.rows; k++){  //성과부문,능력부문 이미지보여주기		
						var grad = 10;
						if(datagrid2.valueMatrix(k,datagrid2.colRef("busn_rslt_scor")) > "0" )  //영업 점수가 있을 경우 이미지 대신 업무성과에 영업점수를 반영함  
						{
							datagrid2.valueMatrix(k,datagrid2.colRef("evlu_grad_nm_01")) = datagrid2.valueMatrix(k,datagrid2.colRef("busn_rslt_scor"));
							for(grad = 10; grad <= 50 ; grad)
							{
								var evlu_grad_cd = "evlu_grad_cd_"+grad ; 
								for(var l = 8; l <= 14 ; l )
								{								
									if(datagrid2.valueMatrix(k,l) == grad )
									{
										 var m = l - 1 ;
										 var  img_num =  grad.toString().substring(0,1);   // grad.substring(0,1);
										datagrid2.cellstyle("background-image", k, m) =   "/images/hd/icon_score_0"+img_num+".gif";      				     
									}
									
									l = l + 2;
								}
								grad = grad +10 ;
							}
						}
						else 
						{
