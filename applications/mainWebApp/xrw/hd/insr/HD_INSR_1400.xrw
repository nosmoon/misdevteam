								<sum_insr_fee/>
								<sum_care_insr_fee/>
								<impt_stot/>
								<bet/>
							</record>
						</gridData>
					</resData>
					<tempData/>
				</root>
			</instance>
			<submission id="hd_insr_1400" action="/hd/insr/1400" mediatype="application/x-www-form-urlencoded" method="post" ref="/root/reqData/reqForm/saly_yy" resultref="/root/tempData"/>
		</model>
