								var guest_room_cnt = model1.getValue("/root/resData/gridData/record["+i+"]/guest_room_cnt");	
								var grid_emp_no = model1.getValue("/root/resData/gridData/record["+i+"]/emp_no");	
								if(cmpy_sply_use_yn_nm == "사용" && datagrid2.rowStatus(i) == 1 && emp_no == grid_emp_no){
									sum_dds = Number(sum_dds) + (Number(grid_use_dds) * Number(guest_room_cnt)); 
								}
								if(cmpy_sply_use_yn_nm == "사용" && datagrid2.rowStatus(i) == 2 && emp_no == grid_emp_no){
									sum_dds_update = Number(sum_dds) + (Number(grid_use_dds) * Number(guest_room_cnt)); 
								}
								
								
								if(cmpy_sply_use_yn_nm == "사용" && datagrid2.rowStatus(i) != 4 && emp_no == grid_emp_no){
									sum_dds_all = Number(sum_dds_all) + (Number(grid_use_dds) * Number(guest_room_cnt)); 
								}
								
							}
							//alert(sum_dds+"//"+sum_dds_all);
							
							if(count_day < sum_dds){
								flag = true;
							}
							if(count_day < sum_dds_all){
								flag = true;
							}
							if(count_day < sum_dds_update){
								flag = true;
							}
							
							return flag;
