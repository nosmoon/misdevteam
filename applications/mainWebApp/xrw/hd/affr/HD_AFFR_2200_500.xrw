			</case>
			<case id="case2" scroll="none">
				<iviewer id="iv2" src="/xrw/hd/affr/HD_AFFR_2220_500.xrw" overflow="hidden" scroll="none" style="left:0px; top:0px; width:923px; height:595px; "/>
