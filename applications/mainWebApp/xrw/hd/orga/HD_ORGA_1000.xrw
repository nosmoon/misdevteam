				function setDept_cd(iv_dept_cd, iv_dept_nm){
					var row = model1.getValue("/root/reqData/row_temp");
					model1.setValue("/root/resData/gridData/main/record[" + (row) + "]/pre_dept_cd", iv_dept_cd);
					model1.setValue("/root/resData/gridData/main/record[" + (row) + "]/pre_dept_nm", iv_dept_nm);
					model1.setValue("/root/resData/gridData/main/record[" + (row) + "]/pre_dept_flnm", iv_dept_cd + " " + iv_dept_nm);
