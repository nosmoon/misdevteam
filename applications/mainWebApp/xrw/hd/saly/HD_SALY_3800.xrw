							model1.setValue("/root/reqData/reqForm/flnm",nm_korn );
							//alert(emp_no);
							model1.send("hd_affr_2402");
							model1.copyNode("/root/resData/resForm","/root/tempData");
							var totalcnt = model1.getValue("/root/tempData/totalcnt");
