						model1.setValue("/root/resData/gridData/record[" + (row) + "]/posi_nm", posi_nm);
		    
						model1.refresh();
					}
					
					function chkempno(){
							var rowIdx =datagrid1.row;			
							var emp_no = datagrid1.valueMatrix(rowIdx,5);
							var nm_korn = datagrid1.valueMatrix(rowIdx,6);
