						retu_ch.disabled = false;
					}else if(emp_no == ""){
						datagrid1.isReadOnly(1, 1, 1, 13) = true;
						send_ch.disabled = true;
						retu_ch.disabled = true;
					}
					model1.refresh();
					var totcnt = datagrid1.rows;
