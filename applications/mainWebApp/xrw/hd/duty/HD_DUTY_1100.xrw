						var tmp1 = model1.getValue("/root/reqData/searchData/duty_dt");
						model1.setValue("/root/reqData/searchData2/fr_dt",    tmp1.substr(0,6)+"01" );
						model1.setValue("/root/reqData/searchData2/to_dt",  tmp1.substr(0,6)+commLastDay(tmp1));
						group3.refresh();
						button2.dispatch("onclick");
					}
					
					function 	emp_no_ready_duty()
					{	
						var obj = htmldocument.SessionInfo;
						
						
						if(obj.cmpycd.value == "100")  // 조선일보일 경우 통합결재라인을 탈수 있도록 수정2018.07.22수정 이가연 
						{
							model1.send("hd_com_5004");
						}
						else 
						{
							model1.send("hd_duty_1114");
						}
						
						model1.copyNode("/root/reqData/appData", "/root/dataSet/CURLIST1/record");
						model1.makeNode("/root/reqData/appData/app_emp_no");
						model1.makeNode("/root/reqData/appData/app_emp_nm");
						model1.makeNode("/root/reqData/appData/app_posi_dty");
						model1.makeNode("/root/reqData/appData/posi_nm");
						model1.makeNode("/root/reqData/appData/dty_nm");
						model1.makeNode("/root/reqData/appData/app_dept_cd");
						
						var appCnt = model.getValue("/root/dataSet/CURLIST1/totalcnt");
						if(appCnt == "0"){
							model.setValue("/root/reqData/appData/app_emp_no","");
							model.setValue("/root/reqData/appData/app_emp_nm","");
							model.setValue("/root/reqData/appData/app_posi_dty","");
							model.removenode("/root/dataSet");
							model.refresh();
						}else if(appCnt == "1") {
							model.setValue( "/root/reqData/appData/app_emp_no", model.getValue("/root/reqData/appData/emp_no") );
							model.setValue( "/root/reqData/appData/app_emp_nm", model.getValue("/root/reqData/appData/nm_korn") );
							var posi_nm = model.getValue("/root/reqData/appData/posi_nm");
							var dty_nm = model.getValue("/root/reqData/appData/dty_nm");
							model.setValue("/root/reqData/appData/app_posi_dty",posi_nm+"/"+dty_nm);
							model.setValue("/root/reqData/appData/app_dept_cd",model.getValue("/root/dataSet/CURLIST2/record/dept_cd"));
							model.removenode("/root/dataSet");
							model.refresh();
						//	model1.setValue("/root/reqData/appData/app_emp_nm",model1.getValue("/root/reqData/appData/emp_no"));
						}
						else 
						{
							alert("결재자 중복입니다.");
							model.setValue("/root/reqData/appData/app_emp_no","");
							model.setValue("/root/reqData/appData/app_emp_nm","");
							model.setValue("/root/reqData/appData/app_posi_dty","");
							model.removenode("/root/dataSet");
							model.refresh();	
						}
						model1.removenode("/root/dataSet");
						
					}
					
					
