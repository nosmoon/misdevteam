					var non_tax_frnc_03 = model1.getValue("/root/reqData/reqForm/non_tax_frnc_03");
					var non_tax_jrnst_03 = model1.getValue("/root/reqData/reqForm/non_tax_jrnst_03");
					var non_tax_frnc_04 = model1.getValue("/root/reqData/reqForm/non_tax_frnc_04");
					var non_tax_jrnst_04 = model1.getValue("/root/reqData/reqForm/non_tax_jrnst_04");
					var non_tax_frnc_05 = model1.getValue("/root/reqData/reqForm/non_tax_frnc_05");
					var non_tax_jrnst_05 = model1.getValue("/root/reqData/reqForm/non_tax_jrnst_05");
					var non_tax_frnc_06 = model1.getValue("/root/reqData/reqForm/non_tax_frnc_06");
					var non_tax_jrnst_06 = model1.getValue("/root/reqData/reqForm/non_tax_jrnst_06");
					var non_tax_frnc_07 = model1.getValue("/root/reqData/reqForm/non_tax_frnc_07");
					var non_tax_jrnst_07 = model1.getValue("/root/reqData/reqForm/non_tax_jrnst_07");
					var non_tax_frnc_08 = model1.getValue("/root/reqData/reqForm/non_tax_frnc_08");
					var non_tax_jrnst_08 = model1.getValue("/root/reqData/reqForm/non_tax_jrnst_08");
					var non_tax_frnc_09 = model1.getValue("/root/reqData/reqForm/non_tax_frnc_09");
					var non_tax_jrnst_09 = model1.getValue("/root/reqData/reqForm/non_tax_jrnst_09");
					var non_tax_frnc_10 = model1.getValue("/root/reqData/reqForm/non_tax_frnc_10");
					var non_tax_jrnst_10 = model1.getValue("/root/reqData/reqForm/non_tax_jrnst_10");
					var non_tax_frnc_11 = model1.getValue("/root/reqData/reqForm/non_tax_frnc_11");
					var non_tax_jrnst_11 = model1.getValue("/root/reqData/reqForm/non_tax_jrnst_11");
					var non_tax_frnc_12 = model1.getValue("/root/reqData/reqForm/non_tax_frnc_12");
					var non_tax_jrnst_12 = model1.getValue("/root/reqData/reqForm/non_tax_jrnst_12");
					var non_tax_stot = Number(non_tax_frnc_01) + Number(non_tax_jrnst_01)
									+ Number(non_tax_frnc_02) + Number(non_tax_jrnst_02)
									+ Number(non_tax_frnc_03) + Number(non_tax_jrnst_03)
									+ Number(non_tax_frnc_04) + Number(non_tax_jrnst_04)
									+ Number(non_tax_frnc_05) + Number(non_tax_jrnst_05)
									+ Number(non_tax_frnc_06) + Number(non_tax_jrnst_06)
									+ Number(non_tax_frnc_07) + Number(non_tax_jrnst_07)
									+ Number(non_tax_frnc_08) + Number(non_tax_jrnst_08)
									+ Number(non_tax_frnc_09) + Number(non_tax_jrnst_09)
									+ Number(non_tax_frnc_10) + Number(non_tax_jrnst_10)
									+ Number(non_tax_frnc_11) + Number(non_tax_jrnst_11)
									+ Number(non_tax_frnc_12) + Number(non_tax_jrnst_12)
					model1.setValue("/root/reqData/reqForm/non_tax_stot", non_tax_stot);
					output9.refresh();
					
					var tot_saly = model1.getValue("/root/reqData/reqForm/tot_saly"); // 수입합계
					var labr_incm_amt_tm = Number(tot_saly) - Number(non_tax_stot)
					
					// 총액 - 기타금액 -> 근로소득공제
					/*********** 근로소득공제 ***********************/
					/* 500만원 까지 - 정액 공제 *********************/
					/* 500~1500만원 - 500 + 500만원 초과분의 50% ****/
					/* 1500~3000만원 - 1000 + 1500만원 초과분의 15% */
					/* 3000~4500만원 - 1225 + 3000만원 초과분의 10% */
					/* 4500만원 초과 - 1375 + 4500만원 초과분의 5% **/
					
					var labr_incm_amt = "";
					if(labr_incm_amt_tm <= 5000000){
						labr_incm_amt = labr_incm_amt_tm;
					}else if((5000000 < labr_incm_amt_tm) && (labr_incm_amt_tm <= 15000000)){
						labr_incm_amt = 5000000 + ((labr_incm_amt_tm - 5000000) * 0.5);
					}else if((15000000 < labr_incm_amt_tm) && (labr_incm_amt_tm <= 30000000)){
						labr_incm_amt = 10000000 + ((labr_incm_amt_tm - 15000000) * 0.15);
					}else if((30000000 < labr_incm_amt_tm) && (labr_incm_amt_tm <= 45000000)){
						labr_incm_amt = 12250000 + ((labr_incm_amt_tm - 30000000) * 0.10);
					}else if(45000000 < labr_incm_amt_tm){
						labr_incm_amt = 13750000 + ((labr_incm_amt_tm - 45000000) * 0.05);
					}
					model1.setValue("/root/reqData/reqForm/labr_incm_amt", labr_incm_amt);
					input20.refresh();
				}
				function spc_dduc(){ // 특별공제 계 구하기
