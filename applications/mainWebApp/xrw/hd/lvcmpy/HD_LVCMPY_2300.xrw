					var spc_dduc_wedd_etc = model1.getValue("/root/reqData/reqForm/spc_dduc_wedd_etc");
					var spc_dduc_stot = Number(spc_dduc_insr_fee) + Number(spc_dduc_hosp_fee) + Number(spc_dduc_hous_fund) + Number(spc_dduc_dona_amt) + Number(spc_dduc_wedd_etc);
					if(1000000 < spc_dduc_stot){
						model1.setValue("/root/reqData/reqForm/spc_dduc_stot", spc_dduc_stot); // 계
						model1.setValue("/root/reqData/reqForm/std_dduc_amt", "0"); // 표준공제
					}else if(spc_dduc_stot <= 1000000){
						model1.setValue("/root/reqData/reqForm/spc_dduc_stot", "0"); // 계
						model1.setValue("/root/reqData/reqForm/std_dduc_amt", "1000000"); // 표준공제
					}
					input57.refresh();
					input58.refresh();
				}
				]]>
