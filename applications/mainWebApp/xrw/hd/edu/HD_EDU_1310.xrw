					function loadData(emp, nm, gubun){
						model1.setValue("/root/resData/resForm/emp_no", emp);
						model1.setValue("/root/reqData/searchData/emp_no", emp);
						model1.setValue("/root/resData/resForm/nm_korn", nm);
						model1.setValue("/root/reqData/searchData/nm_korn", nm);
						model1.setValue("/root/reqData/searchData/flnm", nm);
						
						if(gubun == "N"){
							model1.setValue("/root/reqData/reqForm/emp_no", emp);
							model1.setValue("/root/reqData/reqForm/flnm", nm);
						}
						model1.copyNode("/root/dummyGridData/gridData", "/root/tempData");
						model1.removenode("/root/tempData");
						model1.refresh();
						var totcnt = datagrid2.rows;
						model1.setValue("/root/resData/totcnt", "총 " + Number(totcnt - 1) + " 건");
						model1.refresh();
					}
					function setEmp_no_i(emp_no, nm_korn){
						model1.setValue("/root/reqData/reqForm/emp_no", emp_no);
						model1.setValue("/root/reqData/reqForm/flnm", nm_korn);
						model1.refresh();
					}
					function setDept_cd_i(iv_dept_cd, iv_dept_nm){
						model1.setValue("/root/reqData/reqForm/dept_cd", iv_dept_cd);
						var dept_temp = iv_dept_cd + " " + iv_dept_nm;
						model1.setValue("/root/reqData/reqForm/dept_nm", dept_temp);
						model1.refresh();
					}
					function setEmp_no(emp_no, nm_korn){
						model1.setValue("/root/reqData/reqForm/emp_no", emp_no);
						model1.setValue("/root/reqData/reqForm/flnm", nm_korn);
						model1.send("hd_edu_1301");
						
						model1.copyNode("/root/initData/infoData", "/root/tempEmpNo/CURLIST/infoData");
						model1.setValue("/root/reqData/reqForm/emp_no"	, model.getValue("/root/tempEmpNo/infoData/emp_no"));
						model1.setValue("/root/reqData/reqForm/flnm"		, model.getValue("/root/tempEmpNo/infoData/nm_korn"));
						model1.setValue("/root/reqData/reqForm/dept_cd"	, model.getValue("/root/tempEmpNo/infoData/dept_cd"));
						model1.setValue("/root/reqData/reqForm/dept_nm"	, model.getValue("/root/tempEmpNo/infoData/dept_nm"));
						model1.setValue("/root/reqData/reqForm/dty_cd"	, model.getValue("/root/tempEmpNo/infoData/dty_cd"));
						model1.setValue("/root/reqData/reqForm/dty_nm"	, model.getValue("/root/tempEmpNo/infoData/dty_nm"));
						model1.setValue("/root/reqData/reqForm/posi_cd"	, model.getValue("/root/tempEmpNo/infoData/posi_cd"));
						model1.setValue("/root/reqData/reqForm/posi_nm"	, model.getValue("/root/tempEmpNo/infoData/posi_nm"));
						model1.setValue("/root/reqData/reqForm/offi_nm"	, model.getValue("/root/tempEmpNo/infoData/offi_nm"));
						model1.removenode("/root/tempEmpNo");
						model1.refresh();

					}
					
					function setDept_cd(iv_dept_cd, iv_dept_nm){
						model1.setValue("/root/resData/resForm/dept_cd", iv_dept_cd);
						model1.setValue("/root/resData/resForm/dept_nm", iv_dept_nm);
						model1.setValue("/root/reqData/searchData/dept_cd", iv_dept_cd);
						var lvcmpy_dt = model1.getValue("/root/resData/resForm/lvcmpy_dt");
						model1.setValue("/root/reqData/searchData/lvcmpy_dt", lvcmpy_dt);
						model1.copyNode("/root/dummyGridData/gridData", "/root/tempData");
						model1.removenode("/root/tempData");
						model1.refresh();
					}
					
					function sendfuc_1(edu_frdt,edu_todt,dept_cd,dept_nm,complt_yn,emp_no,flnm){
						model1.setValue("/root/reqData/reqForm/edu_frdt",edu_frdt);
						model1.setValue("/root/reqData/reqForm/edu_todt",edu_todt);
						model1.setValue("/root/reqData/reqForm/dept_cd",dept_cd);
						model1.setValue("/root/reqData/reqForm/dept_nm",dept_nm);
						model1.setValue("/root/reqData/reqForm/complt_yn",complt_yn);
						model1.setValue("/root/reqData/reqForm/emp_no",emp_no);
						model1.setValue("/root/reqData/reqForm/flnm",flnm);
						//alert(emp_no + " " + flnm);
						model1.send("hd_edu_1303");		
						model1.copyNode("/root/dummyGridData/gridData", "/root/tempData");
						model1.removenode("/root/tempData");
						datagrid2.autoResize = true;
						model1.refresh();	

						var totcnt = datagrid2.rows;
						model1.setValue("/root/resData/totcnt", "총 " + Number(totcnt - 1) + " 건");
						model1.refresh();				
					}
					
					function sendfuc_2(){
						if(!rowInSubmit(datagrid2))
							return;
						
						if (window.alert("저장하시겠습니까?", "확인", 1) != 1) {
							return;
						}
						//model1.setValue("/root/dummyGridData/gridData/record/edu_clsf", "10");
						model1.setValue("/root/dummyGridData/gridData/record/edu_clsf", model1.getValue("/root/initData/hd_edu_1310_m/dataSet/xx_edu_clsf_010"));
						
						model1.setValue("/root/reqData/multiUpData", datagrid2.getUpdateData());
						model1.send("hd_edu_1304");
						
						if(submit_completed()) {							
							sendfuc_3();
							parent.javascript.getEduList();
						}
						/*
						var emp_no = model1.getValue("/root/dummyGridData/gridData/record/emp_no");
