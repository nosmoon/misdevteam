					}
					
					function sendfuc_4(){
						//excel 파일로 저장한다
						var filePath = window.fileDialog("save", "", false, "", "xls", "Excel Files(*.xls)|*.xls");
						if(filePath != "") {
							datagrid2.saveExcel(filePath);
							window.exec(filePath);
						}
					}
					
					function sendfuc_5(){
						if(!rowDelSubmit(datagrid2))
							return;
						
						if (window.alert("삭제하시겠습니까?", "확인", 1) != 1) {
							return;
						}
						//model1.setValue("/root/dummyGridData/gridData/record/edu_clsf", "10");
						model1.setValue("/root/dummyGridData/gridData/record/edu_clsf", model1.getValue("/root/initData/hd_edu_1310_m/dataSet/xx_edu_clsf_010"));
						
						model1.setValue("/root/reqData/multiUpData", datagrid2.getUpdateData());
						model1.send("hd_edu_1304");
						
						if(submit_completed()) {							
							sendfuc_3();
							parent.javascript.getEduList();
						}
					}
					function sendfuc_6(){ // 엑셀업로드 
						var i;
						var headName;
						var filename = window.fileDialog("open", ",", false, "", "xls*", "Excel File(*.xls*)|*.xls*");
						if( filename!="" )
						{		
							model1.reset();
							datagrid3.loadExcel(filename);
							datagrid3.mergeCells = "never";
							datagrid3.refresh();
							
							for(var i=1; i< datagrid3.rows; i++)
							{
								var status = datagrid3.rowstatus(i);
							
								if(status == "0") {
									datagrid3.addStatus(i,"insert");
								}
							}
							
						datagrid3.deleteRow(1);
						
						model1.setValue("/root/dummyGridData/gridData3/record/edu_clsf", model1.getValue("/root/initData/hd_edu_1310_m/dataSet/xx_edu_clsf_010"));
						model1.setValue("/root/reqData/multiUpData", datagrid3.getUpdateData());
						
						model1.send("hd_edu_1304");
						
						if(submit_completed()) {							
							sendfuc_3();
							parent.javascript.getEduList();
						}
						
						}
					}
