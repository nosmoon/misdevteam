							}
							model1.setValue("/root/dummyGridData/gridData/record/edu_clsf", "10");
							
							model1.setValue("/root/reqData/multiUpData", datagrid2.getUpdateData());
							model1.send("hd_edu_1304");
							
							if(submit_completed()) {							
								sendfuc_3();
							}
						}                        			
						*/
					}
					
					function sendfuc_3(){
						model1.reset();
						datagrid2.clearStatus();
						model1.send("hd_edu_1302");
						model1.copyNode("/root/initData/comboSet/complt_yn", "/root/tempData/complt_yn");
