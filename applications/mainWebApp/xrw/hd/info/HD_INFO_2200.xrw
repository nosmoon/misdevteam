						model1.setValue("/root/reqData/reqForm/dty_cd", "");
						model1.setValue("/root/reqData/reqForm/dty_nm", "");
						model1.setValue("/root/reqData/reqForm/posi_cd", "");
						model1.setValue("/root/reqData/reqForm/posi_nm", "");
						model1.setValue("/root/reqData/reqForm/flnm_engl", "");
