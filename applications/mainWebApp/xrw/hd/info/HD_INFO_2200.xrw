						var issu_resn              	= model1.getValue("/root/reqData/reqForm/issu_resn");
						var real_fee_paymt_yn      = model1.getValue("/root/reqData/reqForm/real_fee_paymt_yn");
						var id_card_typ                	= model1.getValue("/root/reqData/reqForm/id_card_typ");
						
						//var issu_resn_nm = combo1.label;
						var issu_resn_tm = combo1.label;
						var issu_resn_nm = issu_resn_tm.substring(3, issu_resn_tm.length);
						var real_fee_paymt_yn_nm = combo2.label;
						var id_card_typ_nm = combo3.label;
						var issu_yn_nm = combo4.label;
						model1.setValue("/root/resData/gridData/record["+temp+"]/emp_no", emp_no );
						model1.setValue("/root/resData/gridData/record["+temp+"]/nm_korn", nm_korn	);
						model1.setValue("/root/resData/gridData/record["+temp+"]/dept_nm", dept_nm );
						model1.setValue("/root/resData/gridData/record["+temp+"]/dty_nm", dty_nm );
						model1.setValue("/root/resData/gridData/record["+temp+"]/posi_nm", posi_nm );
						model1.setValue("/root/resData/gridData/record["+temp+"]/flnm_engl", flnm_engl );
						model1.setValue("/root/resData/gridData/record["+temp+"]/flnm_chin", flnm_chin );
						model1.setValue("/root/resData/gridData/record["+temp+"]/bi_yymm", bi_yymm );
						model1.setValue("/root/resData/gridData/record["+temp+"]/issu_resn", issu_resn );
						model1.setValue("/root/resData/gridData/record["+temp+"]/real_fee_paymt_yn" , real_fee_paymt_yn );
						model1.setValue("/root/resData/gridData/record["+temp+"]/id_card_typ", id_card_typ );
						model1.setValue("/root/resData/gridData/record["+temp+"]/issu_dt", issu_dt );
						model1.setValue("/root/resData/gridData/record["+temp+"]/issu_yn", issu_yn );
						model1.setValue("/root/resData/gridData/record["+temp+"]/issu_yn_nm", issu_yn_nm );
						model1.setValue("/root/resData/gridData/record["+temp+"]/remk", remk );
						model1.setValue("/root/resData/gridData/record["+temp+"]/dept_cd", dept_cd );
						model1.setValue("/root/resData/gridData/record["+temp+"]/dty_cd", dty_cd );
						model1.setValue("/root/resData/gridData/record["+temp+"]/posi_cd", posi_cd );

						model1.setValue("/root/resData/gridData/record["+temp+"]/issu_resn_nm", issu_resn_nm );
						model1.setValue("/root/resData/gridData/record["+temp+"]/real_fee_paymt_yn_nm", real_fee_paymt_yn_nm );
						model1.setValue("/root/resData/gridData/record["+temp+"]/id_card_typ_nm", id_card_typ_nm );
						
					}
					
					function init(){
						
						model1.setValue("/root/reqData/reqForm/emp_no",	"");
						model1.setValue("/root/reqData/reqForm/flnm", "");
						model1.setValue("/root/reqData/reqForm/dept_cd", "");
