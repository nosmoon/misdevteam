						var dty_nm            		= model1.getValue("/root/reqData/reqForm/dty_nm");
						var posi_cd            		= model1.getValue("/root/reqData/reqForm/posi_cd");
						var posi_nm          	 	= model1.getValue("/root/reqData/reqForm/posi_nm");
						var flnm_engl            	= model1.getValue("/root/reqData/reqForm/flnm_engl");
						var flnm_chin    		= model1.getValue("/root/reqData/reqForm/flnm_chin");
						var bi_yymm			= model1.getValue("/root/reqData/reqForm/bi_yymm");                       
						var issu_dt			= model1.getValue("/root/reqData/reqForm/issu_dt");                     
						var issu_yn			= model1.getValue("/root/reqData/reqForm/issu_yn");               
