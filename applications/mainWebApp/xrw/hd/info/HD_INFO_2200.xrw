			<script type="javascript">
				<![CDATA[
					function load(){
						
						var now_date = new Date();
						var year = now_date.getYear();
						var month = now_date.getMonth() + 1;
						var day = now_date.getDate();    
						var month_tm = '0' + month;
						var day_tm = '0' + day;  
						if(month_tm.length == 3){
							month_tm = month_tm.substring(1, 3);
						}
						if(day_tm.length == 3){
							day_tm = day_tm.substring(1, 3);
						}
						var now_dt_sub = year + month_tm + "01";
						
						//model1.setValue("/root/reqData/reqForm/issu_resn", "40"); // 발급사유
						//model1.setValue("/root/reqData/reqForm/real_fee_paymt_yn", "20"); // 실비변상여부
						
						model1.setValue("/root/reqData/reqForm/issu_resn", model1.getValue("/root/initData/hd_info_2204_m/dataSet/xx_cd_010")); // 발급사유
						model1.setValue("/root/reqData/reqForm/real_fee_paymt_yn", "20"); // 실비변상여부
						
						//btnG1.disabled = true;
						datagrid1.deleteRow(1);
						datagrid1.clearStatus();
						
						model1.setValue("/root/reqData/searchData/occr_dt_fr", now_dt_sub);
						model1.setValue("/root/reqData/searchData/occr_dt_to", getCurrentDate());
						model1.setValue("/root/reqData/reqForm/issu_dt", getCurrentDate());

						model1.send("hd_info_2100");
						model1.copyNode("/root/initData/comboSet/issu_resn", "/root/dataSet/CURLIST1");
						model1.removenode("/root/dataSet");
						
						model1.send("hd_info_2200");
						model1.copyNode("/root/initData/comboSet/issu_yn2", "/root/tempData/issu_yn2");
						model1.copyNode("/root/initData/comboSet/issu_yn", "/root/tempData/issu_yn");
						model1.copyNode("/root/initData/comboSet/real_fee_paymt_yn", "/root/tempData/real_fee_paymt_yn");
						model1.copyNode("/root/initData/comboSet/id_card_typ", "/root/tempData/id_card_typ");		
						model1.removenode("/root/tempData");			
						model1.refresh();
					}
					
					function setDept_cd(iv_dept_cd, iv_dept_nm){
						model1.setValue("/root/reqData/reqForm/dept_cd", iv_dept_cd);
						model1.setValue("/root/reqData/reqForm/dept_nm", iv_dept_nm);
						model1.refresh();
					}
					function setEmp_no(emp_no, nm_korn){
						
						model1.setValue("/root/reqData/reqForm/emp_no", emp_no);
						model1.setValue("/root/reqData/reqForm/flnm", nm_korn);
						
						model1.send("hd_info_2201");
						model1.setValue("/root/reqData/reqForm/emp_no", model1.getValue("/root/tempData/CURLIST/record/emp_no"));
						model1.setValue("/root/reqData/reqForm/flnm", model1.getValue("/root/tempData/CURLIST/record/nm_korn"));
						model1.setValue("/root/reqData/reqForm/dept_cd", model1.getValue("/root/tempData/CURLIST/record/dept_cd"));
						model1.setValue("/root/reqData/reqForm/dept_nm", model1.getValue("/root/tempData/CURLIST/record/dept_nm"));
						model1.setValue("/root/reqData/reqForm/dty_cd", model1.getValue("/root/tempData/CURLIST/record/dty_cd"));
						model1.setValue("/root/reqData/reqForm/dty_nm", model1.getValue("/root/tempData/CURLIST/record/dty_nm"));
						model1.setValue("/root/reqData/reqForm/posi_cd", model1.getValue("/root/tempData/CURLIST/record/posi_cd"));
						model1.setValue("/root/reqData/reqForm/posi_nm", model1.getValue("/root/tempData/CURLIST/record/posi_nm"));
						model1.setValue("/root/reqData/reqForm/flnm_engl", model1.getValue("/root/tempData/CURLIST/record/nm_engl"));
						model1.setValue("/root/reqData/reqForm/flnm_chin", model1.getValue("/root/tempData/CURLIST/record/nm_chin"));
						model1.setValue("/root/reqData/reqForm/bi_yymm", model1.getValue("/root/tempData/CURLIST/record/prn"));
						model1.refresh();
					}
					
					function funSearch(){
						
						var issu_yn = model1.getValue("/root/reqData/searchData/issu_yn");
						
						if(issu_yn == "1" || issu_yn == "2"){
							
							var occr_dt_fr = model1.getValue("/root/reqData/searchData/occr_dt_fr");
							var occr_dt_to = model1.getValue("/root/reqData/searchData/occr_dt_to");
							
							if(occr_dt_fr == "" && occr_dt_to == ""){
								
								alert("조회기간을 모두 입력해주세요.");
								return;
							}
						}
						
						init();
						model1.send("hd_info_2202");
						model1.copyNode("/root/resData/gridData","/root/tempData/CURLIST1");
						model1.removenode("/root/tempData");
						
						datagrid1.refresh();
						var totcnt = datagrid1.rows;
						model1.setValue("/root/resData/totcnt", "총 " + Number(totcnt - 1) + " 건");
						model1.refresh();
					}
					
