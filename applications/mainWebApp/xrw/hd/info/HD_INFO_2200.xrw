						model1.setValue("/root/reqData/reqForm/bi_yymm", "");
						//model1.setValue("/root/reqData/reqForm/issu_resn", "");
						//model1.setValue("/root/reqData/reqForm/real_fee_paymt_yn", "");
						model1.setValue("/root/reqData/reqForm/id_card_typ", "");
						//model1.setValue("/root/reqData/reqForm/issu_dt", "");
						model1.setValue("/root/reqData/reqForm/issu_yn", "");
						model1.setValue("/root/reqData/reqForm/remk", "");
						model1.setValue("/root/reqData/reqForm/temp", "");
						
