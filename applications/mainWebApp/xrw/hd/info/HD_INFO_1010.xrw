							input3.visible = true;
							input4.visible = true;
							input1.visible = false;
						}else{ // 정사원
							input7.visible = true;
							input3.visible = false;
							input4.visible = false;
							input1.visible = true;
						}
						
						model1.setValue("/root/reqData/searchData/emp_no", fID);
						
						model1.send("hd_info_1011");
						model1.makeNode("/root/resData/resForm/emp_no");
