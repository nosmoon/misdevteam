					function loadData(fID, cnt_yn, emp_clsf){
						
						if(cnt_yn == "N"){
							group1.disabled = true;
							btnA2.disabled = true;
						}else if(cnt_yn == "Y"){
							group1.disabled = false;
							btnA2.disabled = false;
						}
						//if(emp_clsf == "70"){	// 파견사원
