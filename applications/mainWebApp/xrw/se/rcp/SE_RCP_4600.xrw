			<col ref="area_nm"/>
			<col ref="bo_cd"/>
			<col ref="bo_nm"/>
			<col ref="bo_seq"/>
			<col ref="bo_head_nm"/>
			<col ref="net_sale_amt" format="#,###" style="text-align:right; "/>
			<col ref="amt1" format="(-)#,###" style="text-align:right; "/>
			<col ref="amt2" type="input" format="(-)#,###" style="text-align:right; "/>
			<col ref="sub_tot_amt" format="(-)#,###" style="text-align:right; "/>
			<col ref="amt3" type="input" format="(-)#,###" style="text-align:right; "/>
			<col ref="tot_amt" format="(-)#,###" style="text-align:right; "/>
			<col ref="amt_rate" format="(-)##.99%" style="text-align:right; "/>
			<col ref="amt4" type="input" format="(-)#,###" style="text-align:right; "/>
			<script type="javascript" ev:event="xforms-value-changed">
				<![CDATA[
				var iCol = datagrid3.col;
				var iRow = datagrid3.row;
				var Rows = datagrid3.rows;
				var iTot = 0;

				if(iCol == 8){		//스포츠입금액					
					calc_subamt(iRow, iCol);
					calc_totamt(iRow, iCol);
					
					for(var i=2; i<Rows-1; i++){
						iTot = iTot + se_eval(datagrid3.valueMatrix(i,8));
					}
					datagrid3.valueMatrix(Rows-1, 8) = iTot;

				}else if(iCol == 10){	//공제금액
					calc_totamt(iRow, iCol);

					for(var i=2; i<Rows-1; i++){
						iTot = iTot + se_eval(datagrid3.valueMatrix(i,10));
					}
					datagrid3.valueMatrix(Rows-1, 10) = iTot;

				}else if(iCol == 13){	//결손금액

					for(var i=2; i<Rows-1; i++){
						iTot = iTot + se_eval(datagrid3.valueMatrix(i,13));
					}
					datagrid3.valueMatrix(Rows-1, 13) = iTot;

				}
				
		
				//입금소계
				iTot = 0;				
				for(var i=2; i<Rows-1; i++){
					iTot = iTot + se_eval(datagrid3.valueMatrix(i,9));
				}
				datagrid3.valueMatrix(Rows-1, 9) = iTot;
				//입금계
				iTot = 0;				
				for(var i=2; i<Rows-1; i++){
					iTot = iTot + se_eval(datagrid3.valueMatrix(i,11));
				}
				datagrid3.valueMatrix(Rows-1, 11) = iTot;
				//입금율계
				iTot = 0;								
				if( datagrid3.valueMatrix(Rows-1, 6) == "0" ) {
					datagrid3.valueMatrix(Rows-1, 12) = "0";
				} else {
					datagrid3.valueMatrix(Rows-1, 12) = ( ( se_eval(datagrid3.valueMatrix(Rows-1, 11)) / se_eval(datagrid3.valueMatrix(Rows-1, 6)) ) * 100);
				}

				//model1.setValue("/root/resData/gridData/grid2/tot_amt",iTot);
				//datagrid2.refresh();
				//input14.refresh();

				]]>
			</script>
		</datagrid>
