		<script type="javascript">
			<![CDATA[
				//처리내역조회(상단)
				function go_topsearch(){
						
					if(!commReqCheck(group_scr)) return;
					if(!se_chkOverDate(model.getValue("/root/reqData/searchData/searchTop/rcpm_dt1"),model.getValue("/root/reqData/searchData/searchTop/rcpm_dt2"))) return;
					model.send("se_rcp_4610_l");
					se_chkGridCnt("datagrid1");
					
					model.resetInstanceNode("/root/resData/gridData/grid3/record");
					model.resetInstanceNode("/root/resData/gridData/grid3");
					se_setGridStatReset("datagrid3");
					datagrid3.refresh();

					//model.resetInstanceNode("/root/resData/gridData/grid2/rcpmlist/record");	
					//model.resetInstanceNode("/root/resData/gridData/grid2");	
					//se_setGridStatReset("datagrid2");					
					//매출액지대반영 disable
					//button2.disabled = true;					
					//datagrid2.refresh();
									
				}
				
				//처리내역조회(하단)
				function go_dtlsearch(){
					var rCnt = datagrid1.row;
					model.setValue("/root/reqData/searchData/searchDtl/area_cd", datagrid1.valueMatrix(rCnt,8));
					model.setValue("/root/reqData/searchData/searchDtl/occr_dt", datagrid1.valueMatrix(rCnt,1));
					model.setValue("/root/reqData/searchData/searchDtl/slip_clsf", datagrid1.valueMatrix(rCnt,2));
					model.setValue("/root/reqData/searchData/searchDtl/seq", datagrid1.valueMatrix(rCnt,3));
					model.setValue("/root/reqData/searchData/searchDtl/rcpm_dt", datagrid1.valueMatrix(rCnt,4));
									
					model.setValue("/root/reqData/reqform/area_cd", datagrid1.valueMatrix(rCnt,8));
					model.setValue("/root/reqData/reqform/occr_dt", datagrid1.valueMatrix(rCnt,1));
					model.setValue("/root/reqData/reqform/slip_clsf", datagrid1.valueMatrix(rCnt,2));
					model.setValue("/root/reqData/reqform/seq", datagrid1.valueMatrix(rCnt,3));
					model.setValue("/root/reqData/reqform/rcpm_dt", datagrid1.valueMatrix(rCnt,4));
					
					model.resetInstanceNode("/root/resData/gridData/grid3");
					model.send("se_rcp_4620_l");
					model.setValue("/root/reqData/reqform/accflag", "U");	//수정/취소를 위한 모드
					
					
					//상세내용합계
					var rowCnt = datagrid3.rows;
					datagrid3.isReadOnly(1,6,rowCnt-1,13) = true;
					
					datagrid3.attribute("frozenbottomrows") = 1;
					datagrid3.subtotalPosition = "below";
					datagrid3.subtotal("sum", -1, 6, "(-)#,###", se_subtotstyle(), 2, "계"); 
					datagrid3.subtotal("sum", -1, 7, "(-)#,###", se_subtotstyle(), 2, "계"); 
					datagrid3.subtotal("sum", -1, 8, "(-)#,###", se_subtotstyle(), 2, "계"); 
					datagrid3.subtotal("sum", -1, 9, "(-)#,###", se_subtotstyle(), 2, "계"); 
					datagrid3.subtotal("sum", -1, 10, "(-)#,###", se_subtotstyle(), 2, "계"); 
					datagrid3.subtotal("sum", -1, 11, "(-)#,###", se_subtotstyle(), 2, "계"); 
					datagrid3.subtotal("sum", -1, 12, "(-)##.99%", se_subtotstyle(), 2, "계"); 
					datagrid3.subtotal("sum", -1, 13, "(-)#,###", se_subtotstyle(), 2, "계"); 
					
					//매출액지대반영 disable
					button2.disabled = false; //true;
					
					datagrid3.refresh();
									
				}
				
				//처리대상조회
				function go_trgtsearch(){
					
					if(!commReqCheck(group_scr)) return;

					//model.removenode("/root/resData/gridData/grid1"); //grid초기화
					//datagrid1.addRow();
					
					model.send("se_rcp_4630_l");
					se_chkGridCnt("datagrid3");

					if(!submit_completed()) return;
					
					model.setValue("/root/reqData/reqform/accflag", "I");	//저장을 위한 모드
					model.setValue("/root/reqData/reqform/area_cd", model.getValue("/root/reqData/searchData/searchTop/area_cd"));
									
					//상세내용합계					
					var rowCnt = datagrid3.rows;
					datagrid3.isReadOnly(1,6,rowCnt-1,13) = false;
					
					datagrid3.attribute("frozenbottomrows") = 1;
					datagrid3.subtotalPosition = "below";
					datagrid3.subtotal("sum", -1, 6, "(-)#,###", se_subtotstyle(), 2, "계"); 
					datagrid3.subtotal("sum", -1, 7, "(-)#,###", se_subtotstyle(), 2, "계"); 
					datagrid3.subtotal("sum", -1, 8, "(-)#,###", se_subtotstyle(), 2, "계"); 
					datagrid3.subtotal("sum", -1, 9, "(-)#,###", se_subtotstyle(), 2, "계"); 
					datagrid3.subtotal("sum", -1, 10, "(-)#,###", se_subtotstyle(), 2, "계"); 
					datagrid3.subtotal("sum", -1, 11, "(-)#,###", se_subtotstyle(), 2, "계"); 
					datagrid3.subtotal("sum", -1, 12, "(-)##.99%", se_subtotstyle(), 2, "계"); 
					datagrid3.subtotal("sum", -1, 13, "(-)#,###", se_subtotstyle(), 2, "계"); 
					
					//매출액지대반영 disable
					button2.disabled = false;
					
					datagrid3.refresh();
				}

				//입금액 소계
				function calc_subamt(row, col) {
					datagrid3.valueMatrix(row, 9) = se_eval(datagrid3.valueMatrix(row, 7)) + se_eval(datagrid3.valueMatrix(row, 8));
					calc_amtrate(row, col);
				}

				//공제금액포함 입금액 합계
				function calc_totamt(row, col) {
					datagrid3.valueMatrix(row, 11) = (se_eval(datagrid3.valueMatrix(row, 7)) + se_eval(datagrid3.valueMatrix(row, 8))) - se_eval(datagrid3.valueMatrix(row, 10));
					calc_amtrate(row, col);
				}
				
				//입금률
				function calc_amtrate(row, col) {
					if( datagrid3.valueMatrix(row, 6) == "0" ) {
						datagrid3.valueMatrix(row, 12) = "0";
					} else {
						//datagrid3.valueMatrix(row, 12) = (( (se_eval(datagrid3.valueMatrix(row, 11)) - se_eval(datagrid3.valueMatrix(row, 13))) / se_eval(datagrid3.valueMatrix(row, 6)) ) * 100);
						datagrid3.valueMatrix(row, 12) = (( se_eval(datagrid3.valueMatrix(row, 11)) / se_eval(datagrid3.valueMatrix(row, 6)) ) * 100);
					}
				}
			]]>
		</script>
