			//텝선택시 저장,삭제 버튼 활성화 또는 비활성화
			function buttonControl(){
				var rowcnt = datagrid5.rows - datagrid5.fixedRows;
				var rownum = datagrid5.row;

				if(rownum > 0){
					var bo_reg_clsf = model.getValue("/root/resData/gridData/bolistData/record[" + (rownum) + "]/bo_reg_clsf");
	
					var selCase = switch1.selectedIndex;
					var sCrud = model.getValue("/root/initData/pageInfo/strCrud");
					var inx = sCrud.indexOf("A");
	
					//저장,삭제 활성화 또는 비활성화
					if((selCase == 0 || selCase == 4) && bo_reg_clsf == "2" && inx == -1){
						button9.disabled = true;
						button10.disabled = true;
					}else{
						button9.disabled = false;
						button10.disabled = false;
					}
				}else{
					button9.disabled = false;
					button10.disabled = false;
				}
			}
			
			
			function update_search(){
				
			var sAccflag = model.getValue("/root/reqData/reqform/accflag");				
			var bo_cd;
			var bo_seq;	
			
			combo15.disabled = true;

				if(sAccflag =="I"	){
					bo_cd = model.getValue("/root/resData/msgData/bo_cd_key");
					bo_seq = model.getValue("/root/resData/msgData/bo_seq_key");

					button9.disabled = false;
					button10.disabled = false;

				}else if(sAccflag =="U"){					
					var rowcnt = datagrid5.rows - datagrid5.fixedRows;
					var rownum = datagrid5.row;
					if(rownum < 1) return;
					var bo_cdseq = model.getValue("/root/resData/gridData/bolistData/record[" + (rownum) + "]/bo_cd");
					var bo_head_prn = model.getValue("/root/resData/gridData/bolistData/record[" + (rownum) + "]/bo_head_prn");
					var bo_reg_clsf = model.getValue("/root/resData/gridData/bolistData/record[" + (rownum) + "]/bo_reg_clsf");

					var selCase = switch1.selectedIndex;
					var sCrud = model.getValue("/root/initData/pageInfo/strCrud");
					var inx = sCrud.indexOf("A");

					//저장,삭제 활성화 또는 비활성화
					if((selCase == 0 || selCase == 4) && bo_reg_clsf == "2" && inx == -1){
						button9.disabled = true;
						button10.disabled = true;
					}else{
						button9.disabled = false;
						button10.disabled = false;
					}

					if(bo_cdseq.length == 8){
						bo_cd = bo_cdseq.substring(0,5);
						bo_seq = bo_cdseq.substring(5,8);
					}
					
				}				
				if(sAccflag =="I"	 ||  sAccflag =="U"){				
					model.setValue("/root/reqData/searchData/searchDtl/bo_cd",bo_cd);
					model.setValue("/root/reqData/searchData/searchDtl/bo_seq",bo_seq);
					model.setValue("/root/reqData/searchData/searchDtl/bo_head_prn",bo_head_prn);
					
					var iSel = switch1.selectedIndex;
					if(iSel == 0){
						model.send("se_boi_1020_s");
						var bo_reg_dt = model.getValue("/root/resData/resform/tab1form/bo_reg_dt" );			
						
						radio3.disabled = true;
						input10.disabled = true;
						input42.disabled = true;
						input28.disabled = true;
					
						if(bo_reg_dt !=""){
							input11.disabled = true;
							input12.disabled = true;
						}else{
							input11.disabled = false;
							input12.disabled = false;
						}
						model.refresh();
						cbo_bo_spcf_clsf.disabled = false;
						cbo_area_cd.disabled = true;
						input4.disabled = true;
						
																
						jsInit();
				
					}else if(iSel == 1){
						model.send("se_boi_1030_s");	
						model.send("se_boi_1092_s"); //20170427 장선희 추가
					}else if(iSel == 2){
						model.send("se_boi_1040_s");
					}else if(iSel == 3){
						model.send("se_boi_1050_l");
						model.send("se_boi_1055_l");
					}else if(iSel == 4){
						model.send("se_boi_1056_l");
					}else if(iSel == 5){
						model.send("se_boi_1057_l");
						setBoinfoCaption();
					}else if(iSel == 6){
						model.send("se_boi_1059_l");
					}				
					
					if(sAccflag =="I"	){
						go_search();
					}
					model.setValue("/root/reqData/reqform/accflag","U");	
				
				}
				if(sAccflag =="D"){
					model.reset();
				
					model.toggle("case1");
					button11.selected = true;
					
					model.setValue("/root/reqData/reqform/accflag","I");	
					
					model.setValue("/root/reqData/searchData/searchDtl/yymm", getCurrentMonth());
					
					radio3.disabled = false;
					input10.disabled = false;
					input42.disabled = false;
					input11.disabled = false;
					input12.disabled = false;
					cbo_bo_spcf_clsf.disabled = true;
					cbo_area_cd.disabled = false;
					input4.disabled = false;
					jsInit();
					
					model.refresh();
					go_search();
					
				}
				
			}
			
		function addrow_focus(grid){
			var rows = eval(grid+".rows") ;			
		//	eval(grid+".row="+(rows-1));
			eval(grid+".topRow="+(rows-1));
		}
