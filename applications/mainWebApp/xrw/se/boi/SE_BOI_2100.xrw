		<script type="javascript">
			<![CDATA[
				//처리내역조회(상단)
				function go_topsearch(){
						
					if(!commReqCheck(group_scr)) return;
					if(!se_chkOverDate(model.getValue("/root/reqData/searchData/searchTop/rcpm_dt1"),model.getValue("/root/reqData/searchData/searchTop/rcpm_dt2"))) return;
					model.send("se_boi_2110_l");
					se_chkGridCnt("datagrid1");
							
					if(submit_completed()){
						clearNode("/root/resData/resform");	
						model.resetInstanceNode("/root/resData/resform/grid2/record");	
						datagrid2.refresh();
						jsSetDisbl("1");
					}											
				}
				
				//처리내역조회(하단)
				function go_dtlsearch(){
					var rCnt = datagrid1.row;
					model.setValue("/root/reqData/searchData/searchDtl/area_cd", datagrid1.valueMatrix(rCnt,7));
					model.setValue("/root/reqData/searchData/searchDtl/occr_dt", datagrid1.valueMatrix(rCnt,1));
					model.setValue("/root/reqData/searchData/searchDtl/slip_clsf", datagrid1.valueMatrix(rCnt,2));
					model.setValue("/root/reqData/searchData/searchDtl/seq", datagrid1.valueMatrix(rCnt,3));
					
					model.send("se_boi_2120_l");
					if(submit_completed()){
						model.setValue("/root/reqData/reqform/accflag", "");
						jsSetDisbl("1");
					}
					
				}
				
				//처리대상조회
				function go_trgtsearch(bo_cdseq,gb){
					model.setValue("/root/tempData/tmpsend/bo_cd", bo_cdseq.substring(0,5));
					model.setValue("/root/tempData/tmpsend/bo_seq", bo_cdseq.substring(5,8));
					model.setValue("/root/tempData/tmpsend/bogb", gb);
					
					model.send("se_boi_2130_l");
					if(submit_completed()){
						var bo_cd = model.getValue("/root/tempData/boinfo/bo_cdseq");
						if(bo_cd == ""){
							commAlert("대상 내역이 없습니다.");
							return;
						}else{
							if(gb=="1"){
								model.setValue("/root/resData/resform/bo_head_nm", model.getValue("/root/tempData/boinfo/bo_head_nm"));
								model.setValue("/root/resData/resform/area_cd", model.getValue("/root/tempData/boinfo/area_cd"));
								model.setValue("/root/resData/resform/area_nm", model.getValue("/root/tempData/boinfo/area_nm"));
								model.setValue("/root/resData/resform/chrg_pers", model.getValue("/root/tempData/boinfo/chrg_pers"));
								model.setValue("/root/resData/resform/amt1", model.getValue("/root/tempData/boinfo/amt1"));
								model.setValue("/root/resData/resform/amt2", model.getValue("/root/tempData/boinfo/amt2"));
								model.setValue("/root/resData/resform/misudt", model.getValue("/root/tempData/boinfo/misudt"));
								model.refresh();
							}else{
								var rCnt = datagrid2.row;
								datagrid2.valueMatrix(rCnt,3) = model.getValue("/root/tempData/boinfo/area_cd");
								datagrid2.valueMatrix(rCnt,4) = model.getValue("/root/tempData/boinfo/area_nm");
								datagrid2.valueMatrix(rCnt,5) = model.getValue("/root/tempData/boinfo/chrg_pers");
								datagrid2.valueMatrix(rCnt,6) = model.getValue("/root/tempData/boinfo/bo_head_nm");
								datagrid2.valueMatrix(rCnt,7) = model.getValue("/root/tempData/boinfo/amt1");
								datagrid2.valueMatrix(rCnt,8) = 0;
								datagrid2.refresh();
							}
						}	
					}							
				}
				
				//지국선택
				function jsGetBocdSeq(bo_cd,bo_seq,bo_nm,bogb){
					var gb = model.getValue("/root/resData/resform/bogb");
					
					if(gb=="1"){		//인계						
						model.setValue("/root/resData/resform/bo_cdseq", bo_cd+bo_seq);
						model.setValue("/root/resData/resform/bo_nm", bo_nm);		
																
						go_trgtsearch(bo_cd+bo_seq,"1");
					}else{			//인수
						var rCnt = datagrid2.row;
						datagrid2.valueMatrix(rCnt, 1) = bo_cd+bo_seq;
						datagrid2.valueMatrix(rCnt, 2) = bo_nm;	
												
						go_trgtsearch(bo_cd+bo_seq,"2");	
					}
				}
				
				function go_clear(){
					clearNode("/root/resData/resform");	
					model.resetInstanceNode("/root/resData/resform/grid2/record");	
					datagrid2.rowStatus(1) = "1";
					datagrid2.refresh();
					model.setValue("/root/reqData/reqform/accflag", "I");
					jsSetDisbl("2");
				
				}
				
				function jsSetDisbl(gb){		
					if(gb == "1"){
						ipt_bo_cdseq.disabled = true;
						datagrid2.disabled = true;
						button28.disabled = true;
						button29.disabled = true;
					}else{
						ipt_bo_cdseq.disabled = false;
						datagrid2.disabled = false;
						button28.disabled = false;
						button29.disabled = false;
					}					
				}
			]]>
		</script>
	</xhtml:head>
