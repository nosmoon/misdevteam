		<button id="button8" class="btn03" crud="C" style="left:737px; top:44px; width:63px; height:21px; ">
			<caption>저장</caption>
			<script type="javascript" ev:event="onclick">
				<![CDATA[
					
				if(!commConfirmBox("인수인계 처리 하시겠습니까?")) return;				
				
				var accflag = model.getValue("/root/reqData/reqform/accflag");
				if(accflag != "I"){
					commAlert("신규 입력후 처리하십시오.");
					return;
				}
				
				if(!commReqCheck(group_dtl)) return;
				if(!commGridReqCheck("datagrid2")) return;	
				
				//지국 중복선택했는지 검사
				var bo_cd1 = model.getValue("/root/resData/resform/bo_cdseq");
				bo_cd1 = bo_cd1.substring(0,5);
				var rTot = datagrid2.rows;
				var bo_cd2 = "";
				for(var i=1; i<rTot; i++){
					bo_cd2 = datagrid2.valueMatrix(i,1);
					bo_cd2= bo_cd2.substring(0,5);
					if(bo_cd1 == bo_cd2){
						commAlert("동일한 센터를 선택할수 없습니다.");
						return;
					}					
				}
				
				for(var i=1; i<rTot; i++){
					bo_cd1 = datagrid2.valueMatrix(i,1);
					bo_cd1= bo_cd1.substring(0,5);
					for(var j=i+1; j<rTot; j++){
						bo_cd2 = datagrid2.valueMatrix(j,1);
						bo_cd2 = bo_cd2.substring(0,5);
						if(bo_cd1 == bo_cd2){
							commAlert("동일한 센터를 선택할수 없습니다.");
							return;
						}	
					}				
				}
				
				//금액확인(sp에서 실행)
				/*
				var amt1 = se_eval(model.getValue("/root/resData/resform/amt1")) - se_eval(model.getValue("/root/resData/resform/amt2"));	//인계대상(잔액)
				var amt2 = 0;
				for(var i=1; i<datagrid2.rows; i++){
					if(datagrid2.rowStatus(i) != 4) amt2 = amt2 + se_eval(datagrid2.valueMatrix(i,8));
				}
				if(amt1 != amt2){
					commAlert("승계받을 금액과 승계할 금액 합계가 일치하지않습니다.");
					return;
				}
				*/
				
				model.setValue("/root/reqData/reqform/occr_dt", model.getValue("/root/reqData/searchData/searchDtl/occr_dt"));
				model.setValue("/root/reqData/reqform/slip_clsf", model.getValue("/root/reqData/searchData/searchDtl/slip_clsf"));
				model.setValue("/root/reqData/reqform/seq", model.getValue("/root/reqData/searchData/searchDtl/seq"));
				model.setValue("/root/reqData/reqform/area_cd", model.getValue("/root/resData/resform/area_cd"));
				model.setValue("/root/reqData/reqform/chrg_pers", model.getValue("/root/resData/resform/chrg_pers"));
				model.setValue("/root/reqData/reqform/bo_cdseq", model.getValue("/root/resData/resform/bo_cdseq"));
				model.setValue("/root/reqData/reqform/amt", model.getValue("/root/resData/resform/amt1"));
				model.setValue("/root/reqData/reqform/leas_sply_amt_unrcp_occr_dt", model.getValue("/root/resData/resform/misudt"));
				model.setValue("/root/reqData/reqform/multiUpDate", datagrid2.getUpdateData());
				
				model.send("se_boi_2140_a");
				
				if(submit_completed()){
					model.setValue("/root/reqData/reqform/accflag", "");
					//저장한 지역으로 조회태움
					model.setValue("/root/reqData/searchData/searchTop/area_cd", model.getValue("/root/resData/resform/area_cd"));
					go_topsearch();
				}
				]]>
			</script>
