			<caption>전송</caption>
			<script type="javascript" ev:event="onclick">
				<![CDATA[		
					if(!commConfirmBox("전자계산서 전송 하시겠습니까?")) return;
					
					var slip_occr_dt, slip_clsf_cd, slip_seq;
					slip_occr_dt = model.getValue("/root/resData/resform/slip_occr_dt");	
					slip_clsf_cd = model.getValue("/root/resData/resform/slip_clsf_cd");	
					slip_seq = model.getValue("/root/resData/resform/slip_seq");					
					if ( slip_clsf_cd == "" ){
						commAlert("전송할 내용이 없습니다.");
						return;
					}					
 					model.setValue("/root/reqData/reqform/slip_occr_dt"	, slip_occr_dt	);		
 					model.setValue("/root/reqData/reqform/slip_clsf_cd"	, slip_clsf_cd	);		
 					model.setValue("/root/reqData/reqform/slip_seq"	, slip_seq		);	
					
					var pwd = model.getValue("/root/reqData/reqform/pwd");
					var semuro_id	= model.getValue("/root/reqData/reqform/semuro_id");
					
					if ( semuro_id == "" ){
						commAlert("세무로ID가 없습니다.");
						return;
					}			
					if ( pwd == "" ){
						commAlert("비밀번호가 없습니다.");
						model.setFocus("ipt_pwd");
						return;
					}					
					
					var tax_stmt_clsf 		= "3";		//증빙구분 1 : 과세, 2 : 영세, 3 : 면세
					var tmsg_type 			= "1"; 	//1 : 순발행, 2 : 역발행
					var rmsg_pers_clsf 		= "2"; 	//1 : 가입자, 2 : 비가입자, 3 : 수기등록
					model.setValue("/root/reqData/reqform/tmsg_type", tmsg_type);
					model.setValue("/root/reqData/reqform/stmt_clsf", tax_stmt_clsf);
					model.setValue("/root/reqData/reqform/pers_clsf", rmsg_pers_clsf);
							
					model.send("se_sal_2560_a");
					if(!submit_completed()) return;
					
					var rtnString;
					rtnString = datagrid5.valueMatrix(1, 1);
					//alert(rtnString);
					
					var ret = object1.misSndDocRtnMsg(tmsg_type, tax_stmt_clsf, rmsg_pers_clsf, semuro_id, pwd, semuro_id, rtnString, "","");
					 				
 					if ( ret == "" ){
 						return;		
					} else if ( ret.length != 24 ){
						commAlert(ret);
 						return;
					}
					slip_occr_dt	= datagrid5.valueMatrix(1, 2);
					slip_clsf_cd	= datagrid5.valueMatrix(1, 3);
					slip_seq		= datagrid5.valueMatrix(1, 4);
						
 					model.setValue("/root/reqData/reqform/slip_occr_dt"	, slip_occr_dt	);		
 					model.setValue("/root/reqData/reqform/slip_clsf_cd"	, slip_clsf_cd	);		
 					model.setValue("/root/reqData/reqform/slip_seq"	, slip_seq		);			
 					model.setValue("/root/reqData/reqform/semuro_no" , ret			);
					
					model.send("se_sal_2570_u");	
				]]>
			</script>
