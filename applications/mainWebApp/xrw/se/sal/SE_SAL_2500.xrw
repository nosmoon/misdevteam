		<script type="javascript">
			<![CDATA[
				//처리내역조회(상단)
				function go_topsearch(){
						
					var callgb = model.getValue("/root/reqData/searchData/searchTop/callgb");
					if(callgb == "1" || callgb == "2"){		//전표발행							
						if(!commReqCheck(group_scr)) return;							
						model.send("se_sal_2510_l");
						model.resetInstanceNode("/root/resData/gridData/grid2/record");	
						datagrid2.refresh();
						
					}
					if(callgb == "1" || callgb == "3"){		//계산서발행							
						var tax_no = model.getValue("/root/reqData/searchData/searchTax/tax_no");		
						if(tax_no == ""){
							commAlert("계산서번호를 입력하십시오.");
							model.setFocus("ipt_tax_no");
							return;
						}
						model.send("se_sal_2535_l");						
					}
					
					if(callgb == "2") se_chkGridCnt("datagrid1");
															
				}
				
				//처리내역조회(하단)
				function go_dtlsearch(){
					var rCnt = datagrid1.row;
					if(rCnt < 1) return;
					model.setValue("/root/reqData/searchData/searchDtl/area_cd", datagrid1.valueMatrix(rCnt,6));
					model.setValue("/root/reqData/searchData/searchDtl/occr_dt", datagrid1.valueMatrix(rCnt,1));
					model.setValue("/root/reqData/searchData/searchDtl/slip_clsf", datagrid1.valueMatrix(rCnt,2));
					model.setValue("/root/reqData/searchData/searchDtl/seq", datagrid1.valueMatrix(rCnt,3));
					model.setValue("/root/reqData/searchData/searchDtl/rcpm_dt", datagrid1.valueMatrix(rCnt,4));
									
					model.setValue("/root/reqData/reqform/area_cd", datagrid1.valueMatrix(rCnt,6));
					model.setValue("/root/reqData/reqform/occr_dt", datagrid1.valueMatrix(rCnt,1));
					model.setValue("/root/reqData/reqform/slip_clsf", datagrid1.valueMatrix(rCnt,2));
					model.setValue("/root/reqData/reqform/seq", datagrid1.valueMatrix(rCnt,3));
					model.setValue("/root/reqData/reqform/rcpm_dt", datagrid1.valueMatrix(rCnt,4));
					
					cbo_bank_cd.value = datagrid1.valueMatrix(rCnt,12);
					
					model.send("se_sal_2520_l");
					
					if(submit_completed()){
						model.setValue("/root/reqData/reqform/accflag", "U");	//삭제(취소)를 위한 모드
						var rowCnt = datagrid2.rows;
						if(rowCnt > 1){
							model.setValue("/root/reqData/reqform/rmtt_dt", datagrid2.valueMatrix(1,16));
							model.setValue("/root/reqData/reqform/rmtt_plac", datagrid2.valueMatrix(1,17));
							model.setValue("/root/reqData/reqform/rcpm_yymm", datagrid2.valueMatrix(1,23));
							
							//조회만가능
							datagrid2.isReadOnly(1,7,rowCnt-1,15) = true;
							model.refresh();
						}
					}
				}
				
				//처리대상조회
				function go_trgtsearch(){
					
					//if(!commReqCheck(group_scr)) return;
					var area_cd = model.getValue("/root/reqData/searchData/searchTop/area_cd");
					if(area_cd == ""){
						commAlert("지역은 필수입니다.");
						model.setFocus("area_combo");
						return;
					}
						
					model.send("se_sal_2530_l");
								
					model.setValue("/root/reqData/reqform/accflag", "I");	//저장을 위한 모드
					model.setValue("/root/reqData/reqform/area_cd", area_cd);
					
					//입력가능
					var rowCnt = datagrid2.rows;
					if(rowCnt < 2) return;
					model.setValue("/root/reqData/reqform/rmtt_dt", datagrid2.valueMatrix(1,16));
					model.setValue("/root/reqData/reqform/rmtt_plac", datagrid2.valueMatrix(1,17));
					model.refresh();
					datagrid2.isReadOnly(1,7,rowCnt-1,15) = false;								

					model.setValue("/root/tempData/keyData/acct_mang_cd","");	//은행코드 키값초기화
				}
				
				//공통거래처 선택후 호출되는 함수
				function setDlcoNo(dlco_no,dlco_nm,ern,persi_nm){					    
					model.setValue("/root/reqData/searchData/searchTax/dlco_no", dlco_no);											
					model.send("se_sal_2537_s");
										
					if(submit_completed()){
						model.setValue("/root/resData/resform/dlco_no", model.getValue("/root/tempData/dlcoinfo/dlco_no"));	
						model.setValue("/root/resData/resform/ern", model.getValue("/root/tempData/dlcoinfo/ern"));	
						model.setValue("/root/resData/resform/dlco_nm", model.getValue("/root/tempData/dlcoinfo/dlco_nm"));	
						model.setValue("/root/resData/resform/presi_nm", model.getValue("/root/tempData/dlcoinfo/presi_nm"));	
						//model.setValue("/root/resData/resform/zip_1", model.getValue("/root/tempData/dlcoinfo/zip_1"));	
						//model.setValue("/root/resData/resform/zip_2", model.getValue("/root/tempData/dlcoinfo/zip_2"));	
						model.setValue("/root/resData/resform/zip", model.getValue("/root/tempData/dlcoinfo/zip_1")+model.getValue("/root/tempData/dlcoinfo/zip_2"));	
						model.setValue("/root/resData/resform/addr", model.getValue("/root/tempData/dlcoinfo/addr"));	
						model.setValue("/root/resData/resform/dtls_addr", model.getValue("/root/tempData/dlcoinfo/dtls_addr"));	
						model.setValue("/root/resData/resform/bizcond", model.getValue("/root/tempData/dlcoinfo/bizcond"));	
						model.setValue("/root/resData/resform/item", model.getValue("/root/tempData/dlcoinfo/item"));	
						model.setValue("/root/resData/resform/corp_clsf", model.getValue("/root/tempData/dlcoinfo/corp_clsf"));		
						model.refresh();			
					}				
				}
				
				//계산서번호 팝업선택후
				function jsTaxnoPop(tax_no){
					model.setValue("/root/reqData/searchData/searchTax/tax_no",tax_no);
					ipt_tax_no.refresh();
				}
				
				function goReport(gbn){
					var report;
					if(gbn =="01"){
						report = "SP_SE_SAL_2580_P_01";
					}else{
						report = "SP_SE_SAL_2580_P_02";
					}
					
					// 오즈관련 노드 초기화..					
					resetOznode();					
										
					// ozr경로 및 odi 파일명으로 사용될 노드 및 value셋팅 ( OZR파일경로 .ozr 제거)					
					makeNodeValue("/root/oz_data/ozrName", "se/" + report);
					
					var slip_occr_dt = model.getValue("/root/resData/resform/slip_occr_dt");	
					var slip_clsf_cd = model.getValue("/root/resData/resform/slip_clsf_cd");	
					var slip_seq = model.getValue("/root/resData/resform/slip_seq");					
					if ( slip_clsf_cd == "" ){
						commAlert("인쇄할 내용이 없습니다.");
						return;
					}	
					
				
					makeNodeValue("/root/oz_data/odiParam/iv_cmpy_cd"		,model.property("ss_cmpycd"));	
					makeNodeValue("/root/oz_data/odiParam/iv_incmg_pers"	,model.property("ss_emp_no"));	
					makeNodeValue("/root/oz_data/odiParam/iv_slip_clsf_cd"	,slip_clsf_cd	);	
					makeNodeValue("/root/oz_data/odiParam/iv_slip_occr_dt"	,slip_occr_dt	);	
					makeNodeValue("/root/oz_data/odiParam/iv_slip_seq"		,slip_seq		);	
				
					// 레포트뷰어 호출.					
					openOzwin("/root/oz_data");	
				}
			]]>
		</script>
	</xhtml:head>
