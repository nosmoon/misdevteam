		<script type="javascript">
			<![CDATA[
			//저장시 사용할 필드.
			var saveField = new Array();
			saveField[saveField.length] = new Array("aplcdt", "aplcdt");
			saveField[saveField.length] = new Array("issudt", "issudt");
			saveField[saveField.length] = new Array("bocd", "bocd");
			saveField[saveField.length] = new Array("medicd", "medicd");
			saveField[saveField.length] = new Array("chrgupdicdcqty_u", "chrgupdicdcqty_u");
			saveField[saveField.length] = new Array("chrgupdicdcqty", "chrgupdicdcqty");
			saveField[saveField.length] = new Array("chrgcnfmyn", "chrgcnfmyn");
			
			function init()
			{
				makeTeamSearch("getTeamInit", "/ss/brmgr/1230", "", "/root/initData/comboSet/codeData", "cmbCommonTeamS", "cmbCommonPartS", "cmbCommonAreaS");
				//아래는 임시용. makeTeamEvent
				//makeTeamEvent("getAreaList", "/ss/common/1100", "/root/reqData/reqform/searchForm", "/root/initData/comboSet/codeData", "cmbCommonTeamS", "cmbCommonAreaS", "preActionTeam();");
				
				//기타 데이터 셋팅
				modelCopyValue("/root/reqData/reqform/searchForm/applySession", 	"/root/initData/comboSet/codeData/applySession");
				modelCopyValue("/root/reqData/reqform/searchForm/dt_clsf", 			"/root/initData/comboSet/codeData/dt_clsf");
				
				PID1230group_scr.refresh();
				PID1230combo1.select(0);
				model.setFocus("PID1230input1");
			}
			
			function preActionTeam()
			{
				modelCopyValue("/root/reqData/reqform/searchForm/common_deptcd", "/root/reqData/reqform/searchForm/deptcd");
			}
			
			//기존의 list_search
			function search()
			{
				if(!commValidate(PID1230group_scr))
					return;
				
				//조회
				remakeNode("/root/tempData/resTemp");
				model.send("PID1230subSearch", false);
				
				//데이터 셋팅.
				model.copyNode("/root/resData/gridData/listGrid", "/root/tempData/resTemp/listGrid");
				PID1230datagrid1.refresh();
				
				//총건수
				//PID1230caption4.value = "총건수 " + model.getValue("/root/tempData/resTemp/totalcnt_f") + "건";
				
				if(instance1.selectNodes("/root/resData/gridData/listGrid/record/bocd").length == 0)
					model.alert("검색결과가 없습니다.");
				else
				{
					//input box disabled 처리.
					for(var i=0; i<PID1230datagrid1.rows; i++)
						if(gridRefColValue(PID1230datagrid1, i, "chrgupdicdcqty_u") == "0")
							gridReadOnly(PID1230datagrid1, i, "chrgupdicdcqty");
						else
							gridReadOnly(PID1230datagrid1, i, "chrgupdicdcqty", i, "chrgupdicdcqty", false);
							
					//마감된 건은 일단 변경을 못하게 처리.
					for(var i=0; i<PID1230datagrid1.rows; i++)
						if(gridRefColValue(PID1230datagrid1, i, "closyn") == "Y")
						{
							gridReadOnly(PID1230datagrid1, i, "chrgupdicdcqty");
							gridReadOnly(PID1230datagrid1, i, "chrgcnfmyn");
						}
						else
						{
							gridReadOnly(PID1230datagrid1, i, "chrgupdicdcqty", i, "chrgupdicdcqty", false);
							gridReadOnly(PID1230datagrid1, i, "chrgcnfmyn", i, "chrgcnfmyn", false);
						}
				}
								
				model.removenode("/root/tempData/resTemp");
			}
			
			function save()
			{
				if(!commValidate(PID1230group_scr))
					return;
				
				//일단 마감이 안된 건만 저장되도록 수정함.
				//if(instance1.selectNodes("/root/resData/gridData/listGrid/record[chrgcnfmyn='Y']").length == 0)
				if(instance1.selectNodes("/root/resData/gridData/listGrid/record[closyn='N']").length == 0)
				{
					model.alert('저장할 자료가 없습니다.');
					return;
				}
				
				//일단 마감이 안된 건만 저장되도록 수정함.
				makeNodesetToArrayFormNamed("/root/resData/gridData/listGrid/record", "/root/reqData/reqform/listForm", "closyn", "N", saveField);
				makeValueNode("/root/reqData/reqform/listForm/checkedary", concatNodesetValues("/root/resData/gridData/listGrid/record[closyn=\"N\"]/chrgcnfmyn", "##", true));
				
				remakeNode("/root/resData/MsgData");
				model.send("PID1230subSave", false);
				
				if(model.getValue("/root/resData/MsgData") != "")
					model.alert(model.getValue("/root/resData/MsgData"));
					
				search();
			}
			
			function selectAll()
			{
				setNodesetValue("/root/resData/gridData/listGrid/record[closyn=\"N\"]/chrgcnfmyn", "Y");
				PID1230datagrid1.refresh();
			}
			
			function deselectAll()
			{
				setNodesetValue("/root/resData/gridData/listGrid/record[closyn=\"N\"]/chrgcnfmyn", "N");
				PID1230datagrid1.refresh();
			}
			]]>
		</script>
