		<script type="javascript">
			<![CDATA[
			function init()
			{
				commEventHandler(PID2000group1);
				
				makeTeamSearch("getTeamInit", "/ss/brmgr/2000", "", "/root/initData/comboSet/codeData", "cmbCommonTeamQ", "cmbCommonPartQ", "cmbCommonAreaQ");
				//아래는 임시용. makeTeamEvent
				//makeTeamEvent("getAreaList", "/ss/common/1100", "/root/reqData/reqform/boqtypcond_search_form", "/root/initData/comboSet/codeData", "cmbCommonTeamQ", "cmbCommonAreaQ", "preActionTeam();");

				//reset을 위해 데이터를 복사해 놓는다.
				model.copyNode("/root/initData/defaultData", "/root/initData/comboSet/codeData");
				
				model.setValue("/root/reqData/reqform/rdr_extn_list_search_form/workclsf", "2"); //캠페인 업무구분
				model.setValue("/root/reqData/reqform/rdr_extn_list_search_form/sech_campmclscd", "C"); 
				model.setValue("/root/reqData/reqform/rdr_extn_list_search_form/sech_camptypecd", "C"); 
				//기타 데이터 셋팅
				modelCopyValue("/root/reqData/reqform/boqtypcond_search_form/applySession", 	"/root/initData/comboSet/codeData/applySession");
				modelCopyValue("/root/reqData/reqform/boqtypcond_search_form/pagesize", 		"/root/initData/comboSet/codeData/pagesize");
				modelCopyValue("/root/initData/naviInfo/pagesize", 					"/root/initData/comboSet/codeData/pagesize");

				modelCopyValue("/root/reqData/reqform/rdr_extn_list_search_form/sech_campdate_s", 			"/root/initData/comboSet/codeData/sech_campdate_s");
				modelCopyValue("/root/reqData/reqform/rdr_extn_list_search_form/sech_campdate_e", 			"/root/initData/comboSet/codeData/sech_campdate_e");

				PID2000group1.refresh();
			}
			
			function preActionTeam()
			{
				modelCopyValue("/root/reqData/reqform/boqtypcond_search_form/common_deptcd", "/root/reqData/reqform/boqtypcond_search_form/deptcd");
			}
			
			function listSearch()
			{
				if(!commValidate(PID2000group1))
					return;
					
				model.setValue("/root/reqData/reqform/rdr_extn_list_search_form/pageno", "1");
				
				//조회
				remakeNode("/root/tempData/resTemp");
				model.send("PID2000listSearch", false);
				
				if(!submit_completed())
					return;
				
				//PID2000caption7.value = "총건수" + addCommaStr(model.getValue("root/tempData/resTemp/totalCount")) + "건" ;//총건수
				//데이터셋팅
				//-조회목록
				model.copyNode("/root/resData/gridData/listGrid", "/root/tempData/resTemp/listGrid");
				
				modelCopyValue("/root/initData/naviInfo/pageno", "/root/tempData/resTemp/pageno");
				modelCopyValue("/root/initData/naviInfo/totalcnt", "/root/tempData/resTemp/totalcnt");
				modelCopyValue("/root/initData/naviInfo/js_fn_nm", "/root/tempData/resTemp/js_fn_nm");
				modelCopyValue("/root/initData/naviInfo/pagesize", "/root/tempData/resTemp/pagesize");
				
				if(instance1.selectNodes("/root/resData/gridData/listGrid/record/campainSystem").length == 0)
					model.alert("검색결과가 없습니다.");
					
				model.removenode("/root/tempData/resTemp");
				PID2000datagrid2.rebuild();
			}
			
			function onKeyDown()
			{
				if(event.keyCode == 13)
				{
					model.setFocus("btnSearchBranchQ");
					
					if(iptCommonmBranchQ.value == '')
						onClickBranchPopup();
					else
					{
						//화면별 선처리
						modelCopyValue("/root/reqData/reqform/boqtypcond_search_form/common_deptcd", "/root/reqData/reqform/boqtypcond_search_form/deptcd");
						//검색 공통 모듈 호출. param : "입력param path", "출력 path", "callback함수", flag 값을 가지고 있는 path"
						//처리순서 : 지국조회 -> callback함수 호출 -> flag값에 따른 처리.
						//callbak함수는 옵션. search() 및 searchWithBranch() 함수가 존재해야 함.
						searchBranchByName("/root/reqData/reqform/boqtypcond_search_form", "/root/tempData/resTemp/branchGrid", "searchBranchCallBack()", "/root/initData/gs_request");
					}
				}
			}
			
			function onClickBranchPopup()
			{
				//openBranch("parameter. deptcd,partcd,areacd,bonm", "result. deptcd, deptnm, areacd, areanm, bocd, bonm") - part도 추가될 것으로 보임.
				openBranch("/root/reqData/reqform/boqtypcond_search_form", "/root/tempData/tempBranch");

               		if(model.getValue("/root/tempData/tempBranch/bocd") == "")
					return;
				
				iptCommonmBranchQ.value = model.getValue("/root/tempData/tempBranch/bonm");
				modelCopyValue("/root/reqData/reqform/boqtypcond_search_form/bocd", "/root/tempData/tempBranch/bocd");

				if(model.getValue("/root/initData/gs_request") == "L")
				{
					model.setValue("/root/initData/gs_request", "N");
					search();
				}
			}
			
			function searchBranchCallBack()
			{
				//검색 후처리.
				if((instance1.selectNodes("/root/tempData/resTemp/branchGrid/record/bocd")).length == 1)
				{
					iptCommonmBranchQ.value = model.getValue("/root/tempData/resTemp/branchGrid/record/bonm");
					modelCopyValue("/root/reqData/reqform/boqtypcond_search_form/bocd", "/root/tempData/resTemp/branchGrid/record/bocd");
				}
				else
					onClickBranchPopup();
			}
			
			function set_default(flag)
			{
				if(flag == '1'){			// 부서(팀),지역,지국 초기화
					// 부서(팀) 초기화
					model.copyNode("/root/initData/comboSet/codeData/teamlist", "/root/initData/defaultData/teamlist");
					cmbCommonTeamQ.select(0);		//event 등록으로 인해 team을 첫번째로 선택하면 area 역시 reset.
					
					// 지역 초기화
					//model.copyNode("/root/initData/comboSet/codeData/arealist", "/root/initData/defaultData/arealist");
					
					// 지국코드,지국명 초기화, 포커스이동
					model.setValue("/root/reqData/reqform/boqtypcond_search_form/bocd", "");
					iptCommonmBranchQ.value = "";
					model.setFocus("iptCommonmBranchQ");
					
				}else if(flag == '2'){		// 기간 초기화
				}else if(flag == '3'){		// 기타 초기화
				}
			}
			
			//기존의 bo_list_search
			function searchWithBranch()
			{
				model.setValue("/root/initData/gs_request", "L");
				
				if(iptCommonmBranchQ.value == "")
				{
					model.setValue("/root/reqData/reqform/boqtypcond_search_form/bocd", "");
					model.setValue("/root/initData/gs_request", "N");
					search();
				}
				else
				{
					modelCopyValue("/root/reqData/reqform/boqtypcond_search_form/common_deptcd", "/root/reqData/reqform/boqtypcond_search_form/deptcd");
					searchBranchByName("/root/reqData/reqform/boqtypcond_search_form", "/root/tempData/resTemp/branchGrid", "searchBranchCallBack()", "/root/initData/gs_request");
				}
			}
			
			//기존의 list_search
			function search()
			{
				model.setValue("/root/reqData/reqform/boqtypcond_search_form/campid",PID2000datagrid2.textMatrix(PID2000datagrid2.row, PID2000datagrid2.colRef("campid")));
								
				if(model.getValue("/root/reqData/reqform/boqtypcond_search_form/campid") == "")
				{
					alert("캠페인을 선택하세요");
					return;
				}
					
				if(!commValidate(PID2000group_scr))
					return;
					
				model.setValue("/root/reqData/reqform/boqtypcond_search_form/pageno", "1");
				
				//조회
				remakeNode("/root/tempData/resTemp");
				
				model.send("PID2000detailSearch", false);
			
				PID2000caption10.value = "확인:" + model.getValue("root/tempData/resTemp/yCheck");//확인
				PID2000caption9.value = "미확인" + model.getValue("root/tempData/resTemp/nCheck"); //미확인
				PID2000caption8.value = "총부수" + model.getValue("root/tempData/resTemp/totalBusu"); //총부수
				PID2000caption4.value = "총건수" + model.getValue("root/tempData/resTemp/total")+"건"; //총건수
				
				//데이터셋팅
				//-조회목록
				model.copyNode("/root/resData/gridData/detailGrid", "/root/tempData/resTemp/detailGrid");
				
				//-기타
				modelCopyValue("/root/initData/naviInfo/pageno", "/root/tempData/resTemp/pageno");
				modelCopyValue("/root/initData/naviInfo/totalcnt", "/root/tempData/resTemp/totalcnt");
				modelCopyValue("/root/initData/naviInfo/js_fn_nm", "/root/tempData/resTemp/js_fn_nm");
				modelCopyValue("/root/initData/naviInfo/pagesize", "/root/tempData/resTemp/pagesize");
				
				PID2000datagrid1.refresh();
				
				if(instance1.selectNodes("/root/resData/gridData/detailGrid/record/bonm").length == 0)
					model.alert("검색결과가 없습니다.");

				model.removenode("/root/tempData/resTemp");
			}
			]]>
		</script>
