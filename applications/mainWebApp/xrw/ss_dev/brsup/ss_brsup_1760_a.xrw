			<![CDATA[
			function init()
			{
				model.send("PID1760subInit", false);
				
				//콤보 데이터 셋팅
				model.copyNode("/root/initData/comboSet/bankCombo", "/root/tempData/resTemp/bankCombo");
				model.copyNode("/root/initData/comboSet/cardCombo", "/root/tempData/resTemp/cardCombo");
				
				PID1760combo1.select(0);
				PID1760combo2.refresh();
				PID1760combo3.refresh();
				
				remakeNode("/root/tempData/resTemp");
				model.setFocus("PID1760input1");
				model.removenode("/root/tempData/resTemp");
			}
			
			//기존의 list_search
			function search()
			{
				if(!commValidate(PID1760group2))
					return;
				
				//조회
				model.send("PID1760subPymt", false);
				PID1760datagrid3.refresh();
				
				if(instance1.selectNodes("/root/resData/gridData/pymtGrid/record/pymtseq").length == 0)
					model.alert("검색결과가 없습니다.");
				
				clearDetail();
				clearSub();
			}
			
			function detailView()
			{
				gridRowValueCopy("/root/reqData/reqform/detailSearchForm", PID1760datagrid3, PID1760datagrid3.row);
				
				remakeNode("/root/tempData/resTemp");
				model.send("PID1760subDetail", false);
				
				model.copyNode("/root/initData/comboSet/aplyyymmCombo", "/root/tempData/resTemp/aplyyymmCombo");
				childValueCopy("/root/reqData/reqform/detailForm", "/root/tempData/resTemp/detailForm");
				childValueCopy("/root/reqData/reqform/detailForm", "/root/tempData/resTemp/detailAdd");
				model.copyNode("/root/resData/gridData/pymtcntrGrid", "/root/tempData/resTemp/pymtcntrGrid");
				//model.removenode("/root/tempData/resTemp");
				
				PID1760group5.refresh();
				PID1760datagrid2.refresh();
				
				var shftclsf = model.getValue("/root/reqData/reqform/detailForm/shftclsf");
				if(shftclsf == "110")
				{
					PID1760combo2.disabled = false;
					PID1760input17.disabled = false;
					PID1760combo3.disabled = true;
					PID1760input18.disabled = true;
					PID1760combo4.disabled = true;
					PID1760combo5.disabled = true;
				}
				else if(shftclsf == "130")
				{
					PID1760combo2.disabled = true;
					PID1760input17.disabled = true;
					PID1760combo3.disabled = false;
					PID1760input18.disabled = false;
					PID1760combo4.disabled = false;
					PID1760combo5.disabled = false;
				}
				else
				{
					PID1760combo2.disabled = true;
					PID1760input17.disabled = true;
					PID1760combo3.disabled = true;
					PID1760input18.disabled = true;
					PID1760combo4.disabled = true;
					PID1760combo5.disabled = true;
				}
				
				var cnfm = model.getValue("/root/reqData/reqform/detailForm/cnfm");
				if(cnfm == "1")
				{
					PID1760input8.disabled = true;
					PID1760input16.disabled = true;
					PID1760input21.disabled = true;
					PID1760input15.disabled = true;
					PID1760combo6.disabled = true;
					PID1760input9.disabled = true;
					PID1760input10.disabled = true;
					PID1760input11.disabled = true;
				}
				else
				{
					PID1760input8.disabled = false;
					PID1760input16.disabled = false;
					PID1760input21.disabled = false;
					PID1760input15.disabled = false;
					PID1760combo6.disabled = false;
					PID1760input9.disabled = false;
					PID1760input10.disabled = false;
					PID1760input11.disabled = false;
				}
				
				clearSub();
			}
			
			function setSearchCondition()
			{
				gridRowValueCopy("/root/reqData/reqform/tempForm", PID1760datagrid2, PID1760datagrid2.row);
				clamtList(1);
			}
			
			function clamtList(pageno)
			{
				if(!checkCtntSelected())
					return;
				if(pageno == null)
					pageno=1;
					
				childValueCopy("/root/reqData/reqform/clamtForm", "/root/reqData/reqform/tempForm");
				model.setValue("/root/reqData/reqform/clamtForm/pageno", pageno);
				
				remakeNode("/root/tempData/resTemp");
				model.send("PID1760subClamt", false);
				
				clearSubData();

				//data 셋팅.
				model.copyNode("/root/resData/gridData/clamtGrid", "/root/tempData/resTemp/clamtGrid");
				//page navi 셋팅.
				modelCopyValue("/root/initData/naviInfo/pageno", "/root/tempData/resTemp/pageno");
				modelCopyValue("/root/initData/naviInfo/totalcnt", "/root/tempData/resTemp/totalcnt");
				modelCopyValue("/root/initData/naviInfo/js_fn_nm", "/root/tempData/resTemp/js_fn_nm");
				modelCopyValue("/root/initData/naviInfo/pagesize", "/root/tempData/resTemp/pagesize");
				model.removenode("/root/tempData/resTemp");
				
				selectSubList("1");
				
				if(instance1.selectNodes("/root/resData/gridData/clamtGrid/record/rdr_no").length == 0)
