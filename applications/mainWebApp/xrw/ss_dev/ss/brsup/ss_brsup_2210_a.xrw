			function selectSubList(flag)
			{
				var caseid = "";
				var gridid = "";
				var caption = "";
				switch(flag)
				{
					case '1': caseid = "PID2210case1"; gridid="PID2210datagrid1"; caption="수금내역 목록"; break;
					case '2': caseid = "PID2210case2"; gridid="PID2210datagrid4"; caption="청구내역 목록"; break;
					case '3': caseid = "PID2210case3"; gridid="PID2210datagrid7"; caption="신청내역"; break;
					case '4': caseid = "PID2210case4"; gridid="PID2210datagrid5"; caption="LOG내역-납부자"; break;
					case '5': caseid = "PID2210case5"; gridid="PID2210datagrid6"; caption="LOG내역-납부계약"; break;
					default: caseid = "PID2210case1"; gridid="PID2210datagrid1"; caption="수금내역 목록"; break;
