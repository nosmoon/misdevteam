			function init()
			{
				model.send("PID2210subInit", false);
				
				//콤보 데이터 셋팅
				model.copyNode("/root/initData/comboSet/bankCombo", "/root/tempData/resTemp/bankCombo");
				model.copyNode("/root/initData/comboSet/cardCombo", "/root/tempData/resTemp/cardCombo");
				
				PID2210combo1.select(0);
				PID2210combo2.refresh();
				PID2210combo3.refresh();
				
				remakeNode("/root/tempData/resTemp");
				model.setFocus("PID2210input1");
				model.removenode("/root/tempData/resTemp");
			}
			
			//기존의 list_search
			function search()
			{
				if(!commValidate(PID2210group2))
					return;
				
				//조회
				model.send("PID2210subPymt", false);
				PID2210datagrid2.refresh();
				
				if(instance1.selectNodes("/root/resData/gridData/pymtGrid/record/pymtseq").length == 0)
					model.alert("검색결과가 없습니다.");
				
				clearDetail();
				clearSub();
			}
			
			function detailView()
			{
				gridRowValueCopy("/root/reqData/reqform/detailSearchForm", PID2210datagrid2, PID2210datagrid2.row);
				
				remakeNode("/root/tempData/resTemp");
				model.send("PID2210subDetail", false);
				
				model.copyNode("/root/initData/comboSet/aplyyymmCombo", "/root/tempData/resTemp/aplyyymmCombo");
				childValueCopy("/root/reqData/reqform/detailForm", "/root/tempData/resTemp/detailForm");
				childValueCopy("/root/reqData/reqform/detailForm", "/root/tempData/resTemp/detailAdd");
				model.copyNode("/root/resData/gridData/pymtcntrGrid", "/root/tempData/resTemp/pymtcntrGrid");
				//model.removenode("/root/tempData/resTemp");
				
				PID2210group5.refresh();
				PID2210datagrid3.refresh();
				
				var shftclsf = model.getValue("/root/reqData/reqform/detailForm/shftclsf");
				if(shftclsf == "110")
				{
					PID2210combo2.disabled = false;
					PID2210input17.disabled = false;
					PID2210combo3.disabled = true;
					PID2210input18.disabled = true;
					PID2210combo4.disabled = true;
					PID2210combo5.disabled = true;
				}
				else if(shftclsf == "130")
				{
					PID2210combo2.disabled = true;
					PID2210input17.disabled = true;
					PID2210combo3.disabled = false;
					PID2210input18.disabled = false;
					PID2210combo4.disabled = false;
					PID2210combo5.disabled = false;
				}
				else
				{
					PID2210combo2.disabled = true;
					PID2210input17.disabled = true;
					PID2210combo3.disabled = true;
					PID2210input18.disabled = true;
					PID2210combo4.disabled = true;
					PID2210combo5.disabled = true;
				}
				
				var cnfm = model.getValue("/root/reqData/reqform/detailForm/cnfm");
				if(cnfm == "1")
				{
					PID2210input8.disabled = true;
					PID2210input16.disabled = true;
					PID2210input21.disabled = true;
					PID2210input15.disabled = true;
					PID2210combo6.disabled = true;
					PID2210input9.disabled = true;
					PID2210input10.disabled = true;
					PID2210input11.disabled = true;
				}
				else
				{
					PID2210input8.disabled = false;
					PID2210input16.disabled = false;
					PID2210input21.disabled = false;
					PID2210input15.disabled = false;
					PID2210combo6.disabled = false;
					PID2210input9.disabled = false;
					PID2210input10.disabled = false;
					PID2210input11.disabled = false;
				}
				
				clearSub();
			}
			
			function setSearchCondition(rdr_no, medicd, bocd, pymtseq, shftstatclsfcd)
