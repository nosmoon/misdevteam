			function savePymtInfo()
			{
				if(model.getValue("/root/reqData/reqform/detailForm/cnfm") == "1" && model.getValue("/root/reqData/reqform/detailForm/pymtseq") == "")
				{
					model.alert("납부자 조회 후 수정하실 수 있습니다.");
					return;
				}
				
				if(!commValidate(PID2210group5))
					return;
					
				if(!confirm("저장하시겠습니까?"))
					return;
					
				model.send("PID2210subSave", false);
			}
			
			function pymtShftAplc(accflag)
			{
				if(model.getValue("/root/reqData/reqform/detailForm/shftclsf")=="120")
				{
					model.alert("D.O카드는 신청하실 수 없습니다.");
					return;
				}
				
				if(model.getValue("/root/reqData/reqform/tempForm/shftstatclsfcd")=="20" && accflag != "E")
				{
					model.alert("이미 신청중인 상태입니다");
					return;
				}
				if(model.getValue("/root/reqData/reqform/tempForm/shftstatclsfcd")!="20" && accflag == "E")
				{
					model.alert("이미 신청중인 상태입니다");
					return;
				}
				
				if(!confirm("신청하시겠습니까"))
					return;
					
				childValueCopy("/root/reqData/reqform/pymtshftaplcForm", "/root/reqData/reqform/tempForm");
				model.setValue("/root/reqData/reqform/pymtshftaplcForm/accflag", accflag);
				model.send("PID2210subShftAplc", false);
			}
			
			function onClickZipPopup()
			{
				model.setValue("/root/reqData/reqform/zipForm/job_flag", "A");
				model.setValue("/root/reqData/reqform/zipForm/zip", PID2210input19.value);	//"/root/reqData/reqform/detailForm/vaca_areazip"
				
				//openBranch("parameter. deptcd,partcd,areacd,bonm", "result. deptcd, deptnm, areacd, areanm, bocd, bonm") - part도 추가될 것으로 보임.
				openZip("/root/reqData/reqform/zipForm", "/root/tempData/tempZip");
				
				if(instance1.selectNodes("/root/tempData/tempZip/zip").length == 0)
					return;
				
				PID2210input19.value = model.getValue("/root/tempData/tempZip/zip");
				PID2210input20.value = model.getValue("/root/tempData/tempZip/addr");
				PID2210input15.value = model.getValue("/root/tempData/tempZip/bonm");
				PID2210input21.value = model.getValue("/root/tempData/tempZip/bocd");
			}
			
			function onZipKeyDown(pageNo)
			{
				if(event.keyCode == 13)
				{
					model.setFocus("PID2210button5");
					
					//우편번호 패턴이 아니라면 zip_search 함수를 call 한다.
					if(!validate_zip(PID2210input19.value))
						onClickZipPopup();
					else
					{
						model.setValue("/root/reqData/reqform/zipForm/zip", PID2210input19.value);
						model.setValue("/root/reqData/reqform/zipForm/pageno", pageNo);
						model.setValue("/root/reqData/reqform/zipForm/pagesize", "10000");
						
						searchZip("/root/reqData/reqform/zipForm", "/root/tempData/resTemp/zipGrid");
						
						if((instance1.selectNodes("/root/tempData/resTemp/zipGrid/record/zip")).length == 1)
						{
							PID2210input19.value = model.getValue("/root/tempData/resTemp/zipGrid/record/zip");
							PID2210input20.value = model.getValue("/root/tempData/resTemp/zipGrid/record/addr");
						}
						else
							onClickZipPopup();
					}
				}
			}
			
