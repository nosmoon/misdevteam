				//gridRowValueCopy("/root/reqData/reqform/tempForm", PID2210datagrid2, PID2210datagrid2.row);
				model.setValue("/root/reqData/reqform/tempForm/pymtseq", pymtseq);
				model.setValue("/root/reqData/reqform/tempForm/rdr_no", rdr_no);
				model.setValue("/root/reqData/reqform/tempForm/medicd", medicd);
				model.setValue("/root/reqData/reqform/tempForm/shftstatclsfcd", shftstatclsfcd);
				if(bocd != "")
					model.setValue("/root/reqData/reqform/tempForm/bocd", bocd);
					
				clamtList(1);
			}
			
