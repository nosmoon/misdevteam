				modelCopyValue("/root/reqData/reqform/vs_search_form/pagesize", 		"/root/initData/comboSet/codeData/pagesize");
				modelCopyValue("/root/initData/naviInfo/pagesize", 					"/root/initData/comboSet/codeData/pagesize");

				modelCopyValue("/root/reqData/reqform/vs_search_form/frdt", "/root/initData/comboSet/codeData/fromDate");
				modelCopyValue("/root/reqData/reqform/vs_search_form/todt", "/root/initData/comboSet/codeData/toDate");

				model.copyNode("/root/initData/comboSet/rmsCombo", "/root/initData/comboSet/codeData/rmsCombo");
				model.copyNode("/root/initData/comboSet/serviceCombo", "/root/initData/comboSet/codeData/serviceCombo");
				model.copyNode("/root/initData/comboSet/curvacaCombo", "/root/initData/comboSet/codeData/curvacaCombo");
				model.copyNode("/root/initData/comboSet/vacaCombo", "/root/tempData/vacaCombo");
								
				model.setValue("/root/reqData/reqform/vs_detail_form/accflag", 	"I");
				model.setValue("/root/reqData/reqform/vs_detail_form/jobflag", 	"V");				
				model.setValue("/root/reqData/reqform/vs_detail_form/medicd", 	"110");
				
				model.setValue("/root/reqData/reqform/vs_detail_form/path_id", "TM");
				model.setValue("/root/reqData/reqform/vs_detail_form/qty", "1");
				model.setValue("/root/reqData/reqform/vs_detail_form/cns_empnm_id", model.getValue("/root/initData/comboSet/codeData/cns_empnm_id"));
				model.setValue("/root/reqData/reqform/vs_detail_form/medicdnm", "조선일보");

				setSelectDefault(PID1300combo8, null, 0, false);
				setSelectDefault(PID1300combo5, null, 0, false);
				setSelectDefault(PID1300combo3, null, 0, false);
								
				PID1300group_scr.refresh();
			}
			
			function preActionTeam()
			{
				modelCopyValue("/root/reqData/reqform/vs_search_form/common_deptcd", "/root/reqData/reqform/vs_search_form/deptcd");
			}
			
			function onKeyDown()
			{
				if(event.keyCode == 13)
				{
					model.setFocus("btnSearchBranchQ");
					
					if(iptCommonmBranchQ.value == '')
						onClickBranchPopup();
					else
					{
						//화면별 선처리
						modelCopyValue("/root/reqData/reqform/vs_search_form/common_deptcd", "/root/reqData/reqform/vs_search_form/deptcd");
						//검색 공통 모듈 호출. param : "입력param path", "출력 path", "callback함수", flag 값을 가지고 있는 path"
						//처리순서 : 지국조회 -> callback함수 호출 -> flag값에 따른 처리.
						//callbak함수는 옵션. search() 및 searchWithBranch() 함수가 존재해야 함.
						searchBranchByName("/root/reqData/reqform/vs_search_form", "/root/tempData/resTemp/branchGrid", "searchBranchCallBack()", "/root/initData/gs_request");
					}
				}
			}
			
			function searchBranchCallBack()
			{
				//검색 후처리.
				if((instance1.selectNodes("/root/tempData/resTemp/branchGrid/record/bocd")).length == 1)
				{
					iptCommonmBranchQ.value = model.getValue("/root/tempData/resTemp/branchGrid/record/bonm");
					modelCopyValue("/root/reqData/reqform/vs_search_form/bocd", "/root/tempData/resTemp/branchGrid/record/bocd");
				}
				else
					onClickBranchPopup();
			}
			
			function onClickBranchPopup()
			{
				//openBranch("parameter. deptcd,partcd,areacd,bonm", "result. deptcd, deptnm, areacd, areanm, bocd, bonm") - part도 추가될 것으로 보임.
				openBranch("/root/reqData/reqform/vs_search_form", "/root/tempData/tempBranch");

               		if(model.getValue("/root/tempData/tempBranch/bocd") == "")
					return;
				
				iptCommonmBranchQ.value = model.getValue("/root/tempData/tempBranch/bonm");
				modelCopyValue("/root/reqData/reqform/vs_search_form/bocd", "/root/tempData/tempBranch/bocd");

				if(model.getValue("/root/initData/gs_request") == "L")
				{
					model.setValue("/root/initData/gs_request", "N");
					search();
				}
			}
			
			function set_default(flag)
			{
				if(flag == '1'){			// 부서(팀),지역,지국 초기화
					// 부서(팀) 초기화
					model.copyNode("/root/initData/comboSet/codeData/teamlist", "/root/initData/defaultData/teamlist");
					cmbCommonTeamQ.select(0);		//event 등록으로 인해 team을 첫번째로 선택하면 area 역시 reset.
					
					// 지역 초기화
					//model.copyNode("/root/initData/comboSet/codeData/arealist", "/root/initData/defaultData/arealist");
					
					// 지국코드,지국명 초기화, 포커스이동
					model.setValue("/root/reqData/reqform/vs_search_form/bocd", "");
					iptCommonmBranchQ.value = "";
					model.setFocus("iptCommonmBranchQ");
					
				}else if(flag == '2'){		// 기간 초기화
				}else if(flag == '3'){		// 기타 초기화
				}
			}
			
			function searchWithBranch()
			{
				model.setValue("/root/initData/gs_request", "L");
				
				if(iptCommonmBranchQ.value == "")
				{
					model.setValue("/root/reqData/reqform/vs_search_form/bocd", "");
					model.setValue("/root/initData/gs_request", "N");
					search();
				}
				else
				{
					modelCopyValue("/root/reqData/reqform/vs_search_form/common_deptcd", "/root/reqData/reqform/vs_search_form/deptcd");
					searchBranchByName("/root/reqData/reqform/vs_search_form", "/root/tempData/resTemp/branchGrid", "searchBranchCallBack()", "/root/initData/gs_request");
				}
			}
			
			function search()
			{
				if(!commValidate(PID1300group_scr))
					return;
					
				if(model.getValue("/root/reqData/reqform/vs_search_form/fromtogb") == ''){
					if(model.getValue("/root/reqData/reqform/vs_search_form/frdt") > model.getValue("/root/reqData/reqform/vs_search_form/todt")){
						alert("종료일자가 시작일자보다 작습니다.");
						return;
					}
				}
				
				model.setValue("/root/reqData/reqform/vs_search_form/pageno", "1");
				
				//조회
				remakeNode("/root/tempData/resTemp");
				model.send("PID1300search", false);
				
				//데이터셋팅
				//-조회목록
				model.copyNode("/root/resData/gridData/listGrid", "/root/tempData/resTemp/listGrid");
				
				//PID1300caption4.value = "총건수" + model.getValue("root/tempData/resTemp/totalCount") + "건" ;//총건수
				
				modelCopyValue("/root/initData/naviInfo/pageno", "/root/tempData/resTemp/pageno");
				modelCopyValue("/root/initData/naviInfo/totalcnt", "/root/tempData/resTemp/totalcnt");
				modelCopyValue("/root/initData/naviInfo/js_fn_nm", "/root/tempData/resTemp/js_fn_nm");
				modelCopyValue("/root/initData/naviInfo/pagesize", "/root/tempData/resTemp/pagesize");
				
				if(instance1.selectNodes("/root/resData/gridData/listGrid/record/regno").length == 0)
					model.alert("검색결과가 없습니다.");
					
				model.removenode("/root/tempData/resTemp");		
				
				PID1300datagrid1.rebuild();			
			}
			
			function gridClick()
			{
				if( PID1300datagrid1.textMatrix(PID1300datagrid1.row, PID1300datagrid1.colRef("regdt")) == "")
						return;
					
					remakeNode("/root/tempData/resTemp");
					
					model.setValue("/root/reqData/reqform/vs_list_form/regdt",PID1300datagrid1.textMatrix(PID1300datagrid1.row, PID1300datagrid1.colRef("regdt")));
					model.setValue("/root/reqData/reqform/vs_list_form/regno", PID1300datagrid1.textMatrix(PID1300datagrid1.row, PID1300datagrid1.colRef("regno")));
					
					model.send("PID1300gridSearch", false);
					 
					if(!submit_completed())
						return;
						
					childValueCopy("/root/reqData/reqform/vs_detail_form", "/root/tempData/resTemp/detailData");
					
					PID1300input33.disabled = true;
					
					model.setValue("/root/reqData/reqform/vs_detail_form/dt", model.getValue("/root/tempData/resTemp/detailData/regdt"));
					model.setValue("/root/reqData/reqform/vs_detail_form/no", model.getValue("/root/tempData/resTemp/detailData/regno"));
					model.setValue("/root/reqData/reqform/vs_detail_form/dlvhopedt", model.getValue("/root/tempData/resTemp/detailData/spcfdlvdt")); //특정배달일
					model.setValue("/root/reqData/reqform/vs_detail_form/bocd2", model.getValue("/root/tempData/resTemp/detailData/vaca_areabocd")); //휴가지지국
					model.setValue("/root/reqData/reqform/vs_detail_form/bonm2", model.getValue("/root/tempData/resTemp/detailData/vaca_areabonm")); //휴가지지국코드
					model.setValue("/root/reqData/reqform/vs_detail_form/telno1", model.getValue("/root/tempData/resTemp/telno1")); //지국전화번호1
					model.setValue("/root/reqData/reqform/vs_detail_form/telno2", model.getValue("/root/tempData/resTemp/telno2")); //지국전화번호2
					model.setValue("/root/reqData/reqform/vs_detail_form/telno3", model.getValue("/root/tempData/resTemp/telno3")); //지국전화번호3
					model.setValue("/root/reqData/reqform/vs_detail_form/telno12", model.getValue("/root/tempData/resTemp/telno12")); //지국전화번호1
					model.setValue("/root/reqData/reqform/vs_detail_form/telno22", model.getValue("/root/tempData/resTemp/telno22")); //지국전화번호2
					model.setValue("/root/reqData/reqform/vs_detail_form/telno32", model.getValue("/root/tempData/resTemp/telno32")); //지국전화번호3
					model.setValue("/root/reqData/reqform/vs_detail_form/path_id", model.getValue("/root/tempData/resTemp/path_id")); //신청경로
					model.setValue("/root/reqData/reqform/vs_detail_form/bocnfm_id", model.getValue("/root/tempData/resTemp/bocnfm_id")); //소속지국확인
					model.setValue("/root/reqData/reqform/vs_detail_form/bo2cnfm_id", model.getValue("/root/tempData/resTemp/bo2cnfm_id")); //휴가지지국확인
					model.setValue("/root/reqData/reqform/vs_detail_form/cns_empnm_id", model.getValue("/root/tempData/resTemp/cns_empnm_id")); //상담원이름
					model.setValue("/root/reqData/reqform/vs_detail_form/cnscnfmtm_id", model.getValue("/root/tempData/resTemp/cnscnfmtm_id")); //상담원일시
					model.setValue("/root/reqData/reqform/vs_detail_form/remk", model.getValue("/root/tempData/resTemp/remk")); //상담원비고
					model.setValue("/root/reqData/reqform/vs_detail_form/vaca_areadlvoffiremk", model.getValue("/root/tempData/resTemp/vaca_areadlvoffiremk")); //휴가지지국비고
					model.setValue("/root/reqData/reqform/vs_detail_form/boremk", model.getValue("/root/tempData/resTemp/boremk")); //지국비고
					
					if(model.getValue("/root/tempData/resTemp/detailData/clsfcd") == "")
					{
						model.setValue("/root/reqData/reqform/vs_detail_form/clsfcd", "10");
					}
					
					var cns_empcnfmyn = model.getValue("/root/tempData/resTemp/cns_empcnfmyn"); //상담원 확인여부
					
					if(cns_empcnfmyn == ("Y")){
						model.setValue("/root/reqData/reqform/vs_detail_form/cnscnfmyn", model.getValue("/root/tempData/resTemp/cns_empcnfmyn"));
					}else{
						model.setValue("/root/reqData/reqform/vs_detail_form/cnscnfmyn", "");
					}				

					if(model.getValue("/root/tempData/resTemp/detailData/vaca_areacd") !=("")){
						model.copyNode("/root/initData/comboSet/vacaCombo", "/root/tempData/resTemp/vacaCombo");//휴가지
					}else{
						model.copyNode("/root/initData/comboSet/vacaCombo", "/root/tempData/vacaCombo"); //휴가지
					}

					/*if(PID1300datagrid1.textMatrix(PID1300datagrid1.row, PID1300datagrid1.colRef("sendplace"))  == "특정일"){
						PID1300input33.disabled = false;
					}else{
						PID1300input33.disabled = true;
					}*/
					model.removenode("/root/tempData/resTemp");
					model.setValue("/root/reqData/reqform/vs_detail_form/accflag", "U");
					PID1300group4.refresh();
			}
			
