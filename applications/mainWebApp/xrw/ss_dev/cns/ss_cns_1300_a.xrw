				model.copyNode("/root/initData/comboSet/vacaCombo", "/root/tempData/vacaCombo");
				model.setValue("/root/reqData/reqform/vs_detail_form/cnscnfmtm_id", "");
				model.setValue("/root/reqData/reqform/vs_detail_form/clsfcd", "10");
