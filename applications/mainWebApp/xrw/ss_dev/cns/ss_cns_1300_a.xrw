				}else{
					model.setValue("/root/reqData/reqform/vs_detail_form/cnscnfmyn", "N");
				}
				
				if(model.getValue("/root/reqData/reqform/vs_detail_form/accflag") == "I"){

					model.send("PID1300save", false);
					search();
				}else if(model.getValue("/root/reqData/reqform/vs_detail_form/accflag") =="U"){

					model.setValue("/root/reqData/reqform/vs_detail_form/pageno", model.getValue("/root/reqData/reqform/vs_search_form/pageno"));
