			function save()
			{
				model.setValue("/root/reqData/reqform/vs_detail_form/jobflag", "V");
				
				if(model.getValue("/root/reqData/reqform/vs_detail_form/accflag") == "D"){
					model.setValue("/root/reqData/reqform/vs_detail_form/accflag", "U");
				}
				
				if(!commValidate("PID1300group4"))
					return;
				
				if(model.getValue("/root/reqData/reqform/vs_detail_form/clsfcd") != "20")
				{
					if(model.getValue("/root/reqData/reqform/vs_detail_form/vaca_arearegncd") != "" || model.getValue("/root/reqData/reqform/vs_detail_form/vaca_areacd") != "")
					{
