					if(PID1620datagrid1.col == 1 && PID1620datagrid1.row == 0) {
						for(var i=1; i<PID1620datagrid1.rows; i++) {
							PID1620datagrid1.valueMatrix(i,1) = "Y";
						}
					}					
				]]>
			</script>
			<script type="javascript" ev:event="xforms-deselect">
