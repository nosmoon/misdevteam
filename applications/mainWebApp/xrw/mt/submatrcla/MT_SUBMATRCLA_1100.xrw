					//조회 기간 설정 요청으로 인하여 function추가 (2012.07.19 김성미) - 자재팀 황우주 과장님 요청
				    function date(gubun) {
						var d = new Date();
						var rec = "";
						var brec = "";
						var mm= "";
						var dd= "";
								
						rec =  "" + d.getYear();
						brec =  "" + (new Date(d.getYear(),0 ,0)).getYear();
						
						if(gubun.substring(0,2) == 'BY')
						{
							mm = (new Date(d.getYear(),0 ,0)).getMonth() + 1;							
						}else{
							if(gubun.substring(0,1) == 'B')
							{
								mm = d.getMonth() ;
								if(mm =='00'){
									mm = "12" ;
									rec =  "" + (new Date(d.getYear(),0 ,0)).getYear();
								}
							}
							else{
								mm = d.getMonth() + 1;
							}
						}
						
						var ldd = (new Date(d.getYear(),mm ,0)).getDate();
												
						
						
						if( d.getDate() < 10 ) dd = "0" + d.getDate();
						else dd =   d.getDate();
					
						if(mm< 10 )  mm = "0" + (mm) ;
						else if (mm>= 10 )mm = (mm) ;
						
						if( gubun == 'FROM'){
								rec   = rec + mm + "01";
						}else if ( gubun == 'TO' ) {
								rec   = rec + mm +   dd;
						}else if ( gubun == 'BFROM') {
								rec   = rec + mm +  "01";
						}else if ( gubun == 'BLAST') {
								rec   = rec + mm +  ldd;
						}else if ( gubun == 'BYFROM') {
								rec   = brec + "0101";
						}else if ( gubun == 'BYTO') {
								rec   = brec + "1231";
						}
											
						return rec;
					 }
