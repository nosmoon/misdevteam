		<switch id="switch1" style="left:10px; top:35px; width:870px; height:530px; ">
			<case id="case1" selected="true" style="left:0px; top:0px; width:870px; height:530px; ">
				<iviewer id="iviewer1" src="MT_PAPINOUT_2110.xrw" style="left:0px; top:0px; width:865px; height:520px; "/>
			</case>
