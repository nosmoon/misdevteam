					}
					
					model1.send("mt_papinout_4200_l");
					model1.copyNode("/root/resData/gridData","/root/tempData/gridData");
					//datagrid1.isReadOnly(1, 1, 1, 10)  = true;
					
					datagrid1.refresh();
				}
			]]>
		</script>
