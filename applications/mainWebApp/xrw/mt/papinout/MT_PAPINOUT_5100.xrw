			<![CDATA[
				function initBody() {
					model.setValue("/root/reqData/insertData/rcpm_dt",getDate("yyyymmdd"));
					model.setValue("/root/reqData/insertData/rcpm_dt2",getDate("yyyymmdd"));
					input7.refresh();
					input3.refresh();
					
					datagrid2.colhidden(0) = true;
					funPjKind();
					funBtnDisable(false,false,true,true,true,true,false,false,true);
				}
				
				/****************************************
				** funBtnDisable : 버튼 활성화/비활성화 *
				** Parameter Value : false / true       *
				*****************************************/
				function funBtnDisable(b1,b2,b3,b4,b5,b6,b7,b8,b9) {
					button1.disabled = b1;	//종료
					button2.disabled = b2;	//초기화
					button3.disabled = b3;	//파지수입결의
					button9.disabled = b9;	//파지수입결의취소
					button4.disabled = b4;	//삭제
					button5.disabled = b5;	//수정
					button6.disabled = b6;	//입력
					button7.disabled = b7;	//조회
					button8.disabled = b8;	//파지조회
				}
				
				//파지종류 조회
				function funPjKind() {
					//model1.send("mt_papinout_5100_l");
					//model1.copyNode("/root/resData/gridData2","/root/tempData/gridData2");
					combo1.refresh();
					datagrid2.refresh();
				}
				
				//파지종류별 금액 조회
				function funPjSearch(gubun) {
					if(gubun == "pj_search") {	//파지조회 버튼 클릭시
						if(!commReqCheck("group2")) return;
						
						model1.send("mt_papinout_5101_l");
						model1.copyNode("/root/resData/gridData2","/root/tempData/gridData2");
						
						datagrid2.refresh();
						
						//조회된 데이터의 해당 레코드마다 플래그를 insert로 변경한다.
						for(var i=1; i < (datagrid2.rows - 1); i++) {
							datagrid2.addStatus(i, "insert");
						}
						
						// 소모시작일자, 소모종료일자 셋팅 **
						model1.setValue("/root/reqData/insertData/cnsm_frdt", model1.getValue("/root/reqData/searchData/issu_dt_fr"));
						model1.setValue("/root/reqData/insertData/cnsm_todt", model1.getValue("/root/reqData/searchData/issu_dt_to"));
						
						funBtnDisable(false,false,true,true,true,false,false,false,true);
						
						var s_amt = datagrid2.valueMatrix(datagrid2.rows-1, datagrid2.cols-1);
						var v_amt = Math.round(eval(datagrid2.valueMatrix(datagrid2.rows-1, datagrid2.cols-1)) * 0.1);
						var tot_amt = Math.round(eval(datagrid2.valueMatrix(datagrid2.rows-1, datagrid2.cols-1)) * 0.1) + eval(datagrid2.valueMatrix(datagrid2.rows-1, datagrid2.cols-1));
						
						
						model.setValue("/root/reqData/reqForm/s_amt", s_amt);
						model.setValue("/root/reqData/reqForm/v_amt", v_amt);
						model.setValue("/root/reqData/reqForm/tot_amt", tot_amt);
						
						model.refresh();
						
						
					} else if(gubun == "search") {	//파지매각 그리드 클릭시
						model1.setValue("/root/reqData/searchData2/occr_dt", model1.getValue("/root/reqData/insertData/occr_dt"));
						model1.setValue("/root/reqData/searchData2/seq", model1.getValue("/root/reqData/insertData/seq"));
						
						model1.send("mt_papinout_5104_l");
						model1.copyNode("/root/resData/gridData2","/root/tempData/gridData2");
						datagrid2.refresh();
						
						var s_amt = datagrid2.valueMatrix(datagrid2.rows-1, datagrid2.cols-1);
						var v_amt = Math.round(eval(datagrid2.valueMatrix(datagrid2.rows-1, datagrid2.cols-1)) * 0.1);
						var tot_amt = Math.round(eval(datagrid2.valueMatrix(datagrid2.rows-1, datagrid2.cols-1)) * 0.1) + eval(datagrid2.valueMatrix(datagrid2.rows-1, datagrid2.cols-1));
						
						
						model.setValue("/root/reqData/reqForm/s_amt", s_amt);
						model.setValue("/root/reqData/reqForm/v_amt", v_amt);
						model.setValue("/root/reqData/reqForm/tot_amt", tot_amt);
						
						model.refresh();
					}
					
					model1.refresh();
				}
				
				//조회
				function funSearch() {
					model1.send("mt_papinout_5103_l");
					
					model1.copyNode("/root/resData/gridData1","/root/tempData/gridData1");
					datagrid1.refresh();
					
					funBtnDisable(false,false,true,true,true,true,false,false,true);
				}
				
				
				//거래처 팝업
				function setDlco_no(dlco_no, dlco_nm, ern, presi_nm) {
					model1.setValue("/root/reqData/insertData/dlco_no", dlco_no);
					model1.setValue("/root/reqData/insertData/dlco_nm", dlco_nm);
					model1.setValue("/root/reqData/insertData/ern", ern);
					
					model1.refresh();
				}
				
				
				// 입력
				function funInsert() {
					var rcpm_dt = model.getValue("/root/reqData/insertData/rcpm_dt");
					
					if(!commReqCheck("group3")) return;
					
					if (window.alert("저장하시겠습니까?", "확인", 1) != 1) {
						return;
					}
					
					model1.setValue("/root/reqData/insertData/multiUpData",datagrid2.getUpdateData());
					model1.send("mt_papinout_5102_i");
					
					if(submit_completed()) {
						alert("저장 완료되었습니다.");
						button2.dispatch("onclick");
						model.setValue("/root/reqData/insertData/rcpm_dt", rcpm_dt);
						button7.dispatch("onclick");
					}
				}
				
				//수정
				function funUpdate() {
					var rcpm_dt = model.getValue("/root/reqData/insertData/rcpm_dt");
					if (window.alert("수정하시겠습니까?", "확인", 1) != 1) {
						return;
					}
					
					model1.send("mt_papinout_5105_u");
					
					if(submit_completed()) {
						alert("수정 완료되었습니다.");
						button2.dispatch("onclick");
						model.setValue("/root/reqData/insertData/rcpm_dt", rcpm_dt);
						button7.dispatch("onclick");
						
					}
				}
				
				//삭제
				function funDelete() {
					var rcpm_dt = model.getValue("/root/reqData/insertData/rcpm_dt");
					if (window.alert("삭제하시겠습니까?", "확인", 1) != 1) {
						return;
					}
					
					model1.send("mt_papinout_5106_d");
					
					if(submit_completed()) {
						alert("삭제 완료되었습니다.");
						funReset();
					}
				}
				
				
				//datagrid1의 데이터를 해당 node에 set
				function funSelectData(iSelectedRow) {
					var occr_dt   = datagrid1.valueMatrix(iSelectedRow, 1);	//매각일자
					var dlco_no   = datagrid1.valueMatrix(iSelectedRow, 2);	//거래처코드
					var ern       = datagrid1.valueMatrix(iSelectedRow, 3);	//사업자등록번호
					var dlco_nm   = datagrid1.valueMatrix(iSelectedRow, 4);	//거래처명
					var rcpm_dt   = datagrid1.valueMatrix(iSelectedRow, 5);	//입금일자
					var slip_seq1 = datagrid1.valueMatrix(iSelectedRow, 6);	//전표번호(화면표시)
					var slip_seq  = datagrid1.valueMatrix(iSelectedRow,13);	//전표번호(실데이터)
					var tax_seq1   = datagrid1.valueMatrix(iSelectedRow,7);	//계산서번호(화면표시)
					var tax_seq   = datagrid1.valueMatrix(iSelectedRow,14);	//계산서번호(실데이터)
					var cnsm_frdt = datagrid1.valueMatrix(iSelectedRow, 8);	//소모시작일자
					var cnsm_frto = datagrid1.valueMatrix(iSelectedRow, 9);	//소모종료일자
					var seq       = datagrid1.valueMatrix(iSelectedRow,10);	//일련번호
					var bank_cd       = datagrid1.valueMatrix(iSelectedRow,15);	//은행코드
					var rcpm_dt2       = datagrid1.valueMatrix(iSelectedRow,16);	//계좌번호
					var rcpm_nm       = datagrid1.valueMatrix(iSelectedRow,17);	//계좌번호
					
					model1.setValue("/root/reqData/insertData/rcpm_dt"      , rcpm_dt  );
					model1.setValue("/root/reqData/insertData/dlco_no"      , dlco_no  );
					model1.setValue("/root/reqData/insertData/ern"          , ern      );
					model1.setValue("/root/reqData/insertData/dlco_nm"      , dlco_nm  );
					model1.setValue("/root/reqData/insertData/tax_stmt_seq1", tax_seq1 );
					model1.setValue("/root/reqData/insertData/tax_stmt_seq" , tax_seq  );
					model1.setValue("/root/reqData/insertData/slip_proc_seq", slip_seq );
					model1.setValue("/root/reqData/insertData/slip_proc_seq1", slip_seq1);
					model1.setValue("/root/reqData/insertData/cnsm_frdt"    , cnsm_frdt);
					model1.setValue("/root/reqData/insertData/cnsm_todt"    , cnsm_frto);
					model1.setValue("/root/reqData/insertData/seq"          , seq      );
					model1.setValue("/root/reqData/insertData/occr_dt"      , occr_dt  );
					model1.setValue("/root/reqData/insertData/bank_cd"	 , bank_cd  );
					model1.setValue("/root/reqData/insertData/rcpm_dt2"	 , rcpm_dt2  );
					model1.setValue("/root/reqData/insertData/rcpm_nm"	 , rcpm_nm  );
					
					model1.setValue("/root/reqData/searchData/issu_dt_fr", cnsm_frdt);
					model1.setValue("/root/reqData/searchData/issu_dt_to", cnsm_frto);
					
					model1.refresh();
					
					
				}
				
				//초기화
				function funReset() {
					model1.reset();
					model1.copyNode("/root/resData/gridData2",	parent.root.tempData.gridData2);
					model1.copyNode("/root/initData/comboSet/bank_cd",	parent.root.tempData.bank_cd);
					initBody();
				}
				
				//파지조회 버튼 클릭시 datagrid1, group3 초기화
				function funPjClear() {
					model1.copyNode("/root/resData/gridData1","");
					model1.setValue("/root/reqData/insertData/rcpm_dt","");
					model1.setValue("/root/reqData/insertData/dlco_no","");
					model1.setValue("/root/reqData/insertData/ern","");
					model1.setValue("/root/reqData/insertData/dlco_nm","");
					model1.setValue("/root/reqData/insertData/tax_stmt_seq","");
					model1.setValue("/root/reqData/insertData/cnsm_frdt","");
					model1.setValue("/root/reqData/insertData/cnsm_todt","");
					model1.setValue("/root/reqData/insertData/multiUpData","");
					model1.setValue("/root/reqData/insertData/seq","");
					model1.setValue("/root/reqData/insertData/occr_dt","");
					
					model1.refresh();
				}
				
				//조회버튼 클릭시 파지종류별 금액 초기화
				function funPjKindClear() {
					model1.setValue("/root/reqData/searchData/issu_dt_fr","");
					model1.setValue("/root/reqData/searchData/issu_dt_to","");
					model1.copyNode("/root/resData/gridData2","");
					funPjKind();
					model1.refresh();
				}
				
				//파지수입결의
				function funEarInsert() {
					var sTax_stmt_seq = "";	//계산서번호
					var sTotalAmt = "";		//파지종류 총금액
					var iChaAmt   = 0;		//차변금액
					var iDeaAmt   = 0;		//대변금액
					var iVatAmt   = 0;		//부가세
					var sDlco     = "";		//거래처코드
					var sDlcoNm   = "";		//거래처명
					var sErn      = "";		//사업자등록번호
					var sRcpmDt   = "";		//입금일자
				 	var sSeq      = "";
				 	var sOccrDt   = "";
				 	var sIssu_dt_fr = "";		//발행기간
				 	var sIssu_dt_to = "";		//발행기간

					sSeq          = model1.getValue("/root/reqData/insertData/seq");
					sOccrDt       = model1.getValue("/root/reqData/insertData/occr_dt");
					sDlco         = model1.getValue("/root/reqData/insertData/dlco_no");
					sDlcoNm       = model1.getValue("/root/reqData/insertData/dlco_nm");
					sErn          = model1.getValue("/root/reqData/insertData/ern");
					sRcpmDt       = model1.getValue("/root/reqData/insertData/rcpm_dt");
					sTax_stmt_seq = model1.getValue("/root/reqData/insertData/tax_stmt_seq");
					sIssu_dt_fr   = model1.getValue("/root/reqData/searchData/issu_dt_fr");
					sIssu_dt_to   = model1.getValue("/root/reqData/searchData/issu_dt_to");
					sTotalAmt     = model1.getValue("/root/reqData/reqForm/tot_amt");
					
					iChaAmt = model.getValue("/root/reqData/reqForm/tot_amt");
					iVatAmt = model.getValue("/root/reqData/reqForm/v_amt");
					iDeaAmt = model.getValue("/root/reqData/reqForm/s_amt");
										
					model1.setValue("/root/reqData/earInsertData/dlco_no"     ,sDlco);
					model1.setValue("/root/reqData/earInsertData/tax_stmt_seq",sTax_stmt_seq);
					model1.setValue("/root/reqData/earInsertData/cha_amt"     ,iChaAmt);
					model1.setValue("/root/reqData/earInsertData/dea_amt"     ,iDeaAmt);
					model1.setValue("/root/reqData/earInsertData/vat_amt"     ,iVatAmt);
					model1.setValue("/root/reqData/earInsertData/dlco_nm"     ,sDlcoNm);
					model1.setValue("/root/reqData/earInsertData/ern"         ,sErn);
					model1.setValue("/root/reqData/earInsertData/rcpm_dt"     ,sRcpmDt);
					model1.setValue("/root/reqData/earInsertData/seq"         ,sSeq);
