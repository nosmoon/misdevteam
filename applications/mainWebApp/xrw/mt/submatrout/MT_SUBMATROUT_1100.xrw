					function date(){
						var d = new Date();
						var rec = "";
						var dd = "";
					     	    
						rec = "" + d.getYear();
						dd = (d.getDate() < 10 ? "0" + d.getDate() : d.getDate());
						
						if( d.getMonth() + 1 < 10 )  rec = rec + "0" + (d.getMonth() + 1 )  + dd;
						else if ( d.getMonth() > 10 )  rec = rec  + (d.getMonth() + 1)      + dd;
					     
					     rec = getCurrentDate();
						return (rec);
					}
					
