<?xml version="1.0" encoding="EUC-KR"?>
<?xml-stylesheet type="text/css" href="../../../css/style_sheet.css" ?>
<xhtml:html xmlns:ex="http://www.comsquare.co.kr/xforms/excel" xmlns:xlink="http://www.w3.org/1999/xlink" xmlns:xhtml="http://www.w3.org/1999/xhtml" xmlns:my="http://www.comsquare.co.kr/example" xmlns:ms="urn:schemas-microsoft-com:xslt" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns="http://www.w3.org/2002/01/xforms" xmlns:ev="http://www.w3.org/2001/xml-events" xmlns:cs="http://www.comsquare.co.kr/xforms">
	<xhtml:head>
		<xhtml:title>Untitle</xhtml:title>
		<model id="model1">
			<instance id="instance1">
				<root xmlns="">
					<initData>
						<!-- 화면 로딩시의 초기 데이터를 담당 -->
						<session/>
						<!-- 서버의 세션 정보를 담당한다 -->
						<comboSet>
							<!-- 선택계열의 정보를 담당 -->
							<clam_clsf>
								<item>
									<value/>
									<label/>
								</item>
							</clam_clsf>
						</comboSet>
						<!-- 콤보리스트와 같은 선택게열의 데이터는 <item>을 반복함으로써 list를 구성한다. -->
						<pageInfo>
							<!-- 해당 화면의 필요한 기타 정보들을 추가운영 할 수 있다 -->
						</pageInfo>
						<!-- 해당 화면의 필요한 기타 정보들을 추가운영 할 수 있다 -->
						<mt_report_1701_500_m>
							<dataSet>
								<oz_cnt/>
							</dataSet>
						</mt_report_1701_500_m>
					</initData>
					<reqData>
						<!-- 전달하고자 하는 데이터를 담당 ( submit 함수에서 처리) -->
						<searchData>
							<cmpy_cd/>
						</searchData>
						<!-- 검색조건 처리 -->
						<reqForm>
							<cmpy_cd/>
							<occr_dt/>
							<seq/>
							<clam_clsf/>
						</reqForm>
						<!-- formData 팝업,화면전환시 data이동시 -->
						<multiUpdate/>
						<!-- MultiUpDate유형 사용시 처리 -->
						<fwdData>
							<func_nm/>
						</fwdData>
					</reqData>
					<resData>
						<!-- 전달 받은 데이터를 담당 ( submit의 결과가 담김) -->
						<resForm>
							<mode/>
							<cmpy_cd/>
						</resForm>
						<!-- 전달 받은 formData를 담당 ( submit의 결과가 담김) -->
						<!-- 전달 받은 데이터를 가공할 임시저장소 ( submit의 결과가 담김) -->
						<!-- 전달 받은 errorCode를 담당 ( submit의 결과가 담김) -->
						<msg>
							<errMsg/>
							<errCode/>
						</msg>
						<!-- 전달 받은 errorMsg를 담당 ( submit의 결과가 담김) -->
						<gridData>
							<!-- 전달 받은 grid데이터를 담당 ( submit의 결과가 담김) -->
							<!-- datagrid2 -->
							<!-- datagrid3 -->
							<record>
								<cmpy_cd/>
							</record>
						</gridData>
						<gridData2>
							<record>
							</record>
						</gridData2>
					</resData>
					<tempData>
					</tempData>
				</root>
			</instance>
			<script type="javascript" ev:event="xforms-ready">
				<![CDATA[
				    model1.send("mt_submatrcla_1104");
				    var cur_dt = commCurDate("ymd");
					
    				model.setValue("/root/reqData/reqForm/occr_dt",cur_dt);
					model.setValue("/root/reqData/reqForm/seq","001");
					model.setValue("/root/reqData/reqForm/clam_clsf","01");
					
				    model1.copyNode("/root/initData/comboSet/clam_clsf" , "/root/tempData/clam_clsf");
				    model1.refresh();
				]]>
			</script>
			<script type="javascript">
				<![CDATA[
					
				]]>
			</script>
			<submission id="mt_submatrcla_1104" action="/mt/submatrcla/1104" mediatype="application/x-www-form-urlencoded" method="post" resultref="/root/tempData"/>
			<submission id="mt_report_1701_500_m" action="/mt/report/1701500" mediatype="application/x-www-form-urlencoded" method="post" ref="/root/reqData/reqForm" resultref="/root/initData/mt_report_1701_500_m/dataSet"/>
		</model>
		<script type="javascript" src="/js/CIISComm.js"/>
		<script type="javascript" src="/js/TFCommon.js"/>
		<script type="javascript" src="/js/mt_common.js"/>
	</xhtml:head>
	<xhtml:body class="pop640" pagewidth="640" pageheight="480" guideline="1,936;1,927;1,926;" style="font-family:굴림; ">
		<import id="import1" src="../../../common/xrw/iv_TitleBar_Pop640.xrw" style="left:10px; top:5px; width:620px; height:30px; "/>
		<group id="group_btnA" class="gro_btn" style="left:10px; top:45px; width:620px; height:21px; ">
			<button id="btnReset" class="btn02" navindex="3" style="left:514px; top:0px; width:52px; height:21px; ">
				<caption>초기화</caption>
				<script type="javascript" ev:event="onclick">
					<![CDATA[
						model1.reset();	
					]]>
				</script>
			</button>
			<button id="btnClose" class="btn02" navindex="4" style="left:568px; top:0px; width:52px; height:21px; ">
				<caption>종료</caption>
				<script type="javascript" ev:event="onclick">
					<![CDATA[
						if(window.type == "mdi"){
							viewer("mainFrame").window.javaScript.closeMDIWindow(model.getValue("/root/initData/pageInfo/pageID"));
						}else{
							window.close();
						}
					]]>
				</script>
			</button>
			<button id="button1" class="btn02" navindex="1" style="left:460px; top:0px; width:53px; height:21px; ">
				<caption>출력</caption>
				<script type="javascript" ev:event="onclick">
					<![CDATA[
						var occr_dt = model1.getValue("/root/reqData/reqForm/occr_dt");
						var seq = model1.getValue("/root/reqData/reqForm/seq");
						var clam_clsf = model1.getValue("/root/reqData/reqForm/clam_clsf");
						if(occr_dt == ""){
							alert("청구일자를 입력하세요");
							return;
						}
						if(seq == ""){
							alert("청구번호를 입력하세요");
							return;
						}
						if(clam_clsf == ""){
							alert("청구구분을 선택하세요");
							return;
						}
						
						model.send("mt_report_1701_500_m");
						model.refresh();
						var oz_cnt =  model.getValue("/root/initData/mt_report_1701_500_m/dataSet/oz_cnt");
						
						//오즈관련 노드초기화
						resetOznode();
						if(clam_clsf == "01"){
							
							//ozr경로 및 odi 파일명으로 사용될 노드 및 value셋팅
							makeNodeValue("/root/oz_data/ozrName" , "mt/SP_MT_REPORT_1700_500_P");
							makeNodeValue("/root/oz_data/preview","preview"); 			//preview : 미리보기, print : 인쇄
							makeNodeValue("/root/oz_data/printsize","B5 (JIS)"); 		//페이지용지설정(기본 A4)
							
							//odi파라미터 설정
							if(oz_cnt =="1"){
								
								makeNodeValue("/root/oz_data/odiParam/iv_cmpy_cd" , getProperty("ss_cmpycd") );
								makeNodeValue("/root/oz_data/odiParam/iv_occr_dt" , occr_dt );
								makeNodeValue("/root/oz_data/odiParam/iv_seq" , seq );
								makeNodeValue("/root/oz_data/odiParam/iv_oz_cnt" , "" );
							}						
							else if(oz_cnt =="2")
							{
								
								makeNodeValue("/root/oz_data/odiParam/iv_cmpy_cd" , getProperty("ss_cmpycd") );
								makeNodeValue("/root/oz_data/odiParam/iv_occr_dt" , occr_dt );
								makeNodeValue("/root/oz_data/odiParam/iv_seq" , seq );
								makeNodeValue("/root/oz_data/odiParam/iv_oz_cnt" , "1" );
							
								
								makeNodeValue("/root/oz_data/childCount", "1");
								
								makeNodeValue("/root/oz_data/child1/printsize","B5 (JIS)"); 		//페이지용지설정(기본 A4)
							
								makeNodeValue("/root/oz_data/child1/odiParam/iv_cmpy_cd" , getProperty("ss_cmpycd") );
								makeNodeValue("/root/oz_data/child1/ozrName" , "mt/SP_MT_REPORT_1700_500_P");
								makeNodeValue("/root/oz_data/child1/odiParam/iv_occr_dt" , occr_dt );
								makeNodeValue("/root/oz_data/child1/odiParam/iv_seq" , seq );
								makeNodeValue("/root/oz_data/child1/odiParam/iv_oz_cnt" , "2" );
							}
							
							
//							//ozr경로 및 odi 파일명으로 사용될 노드 및 value셋팅
//							makeNodeValue("/root/oz_data/ozrName" , "mt/SP_MT_REPORT_1700_500_P");
//							makeNodeValue("/root/oz_data/preview","preview"); 			//preview : 미리보기, print : 인쇄
//	               				makeNodeValue("/root/oz_data/printsize",""); 				       //페이지용지설정(기본 A4)
//							
//							//odi파라미터 설정
//							makeNodeValue("/root/oz_data/odiParam/iv_cmpy_cd" , getProperty("ss_cmpycd") );
//							makeNodeValue("/root/oz_data/odiParam/iv_occr_dt" , occr_dt );
//							makeNodeValue("/root/oz_data/odiParam/iv_seq" , seq );
						}else{
							//ozr경로 및 odi 파일명으로 사용될 노드 및 value셋팅
							makeNodeValue("/root/oz_data/ozrName" , "mt/SP_MT_REPORT_1701_P");
							makeNodeValue("/root/oz_data/preview","preview"); 			//preview : 미리보기, print : 인쇄
	               				makeNodeValue("/root/oz_data/printsize","B5(JIS)"); 				       //페이지용지설정(기본 A4)
							
							//odi파라미터 설정
							makeNodeValue("/root/oz_data/odiParam/iv_cmpy_cd" , getProperty("ss_cmpycd") );
							makeNodeValue("/root/oz_data/odiParam/iv_occr_dt" , occr_dt );
							makeNodeValue("/root/oz_data/odiParam/iv_seq" , seq );
						}
						
						//레포트뷰어 호출
						openOzwin("/root/oz_data");
						
					]]>
				</script>
			</button>
		</group>
		<group id="group3" class="gro_sel" style="left:10px; top:80px; width:620px; height:110px; ">
			<caption id="caption3" class="list_cap02" style="left:99px; top:0px; width:521px; height:26px; border-style:solid; "/>
			<caption id="caption1" class="list_cap01" style="left:0px; top:0px; width:100px; height:26px; border-style:solid; ">청구일자</caption>
			<input id="input1" ref="/root/reqData/reqForm/occr_dt" inputtype="date" format="yyyy-mm-dd" style="left:104px; top:4px; width:100px; height:18px; background-color:#ffffcc; "/>
			<caption id="caption2" class="list_cap01" style="left:0px; top:25px; width:100px; height:26px; border-style:solid; ">청구번호</caption>
			<caption id="caption4" class="list_cap02" style="left:99px; top:25px; width:521px; height:26px; border-style:solid; "/>
			<caption id="caption5" class="list_cap01" style="left:0px; top:50px; width:100px; height:26px; border-style:solid; ">청구부서</caption>
			<caption id="caption6" class="list_cap02" style="left:99px; top:50px; width:521px; height:26px; border-style:solid; "/>
			<input id="input2" ref="/root/reqData/reqForm/seq" style="left:104px; top:29px; width:100px; height:18px; background-color:#ffffcc; "/>
			<input id="input3" style="left:104px; top:54px; width:200px; height:18px; "/>
			<caption id="caption7" class="list_cap01" style="left:0px; top:75px; width:100px; height:26px; border-style:solid; ">청구구분</caption>
			<caption id="caption8" class="list_cap02" style="left:99px; top:75px; width:521px; height:26px; border-style:solid; "/>
			<select1 id="combo1" ref="/root/reqData/reqForm/clam_clsf" appearance="minimal" style="left:103px; top:79px; width:100px; height:18px; background-color:#ffffcc; ">
				<choices>
					<itemset nodeset="/root/initData/comboSet/clam_clsf/item">
						<label ref="label"/>
						<value ref="value"/>
					</itemset>
				</choices>
			</select1>
		</group>
	</xhtml:body>
</xhtml:html>
