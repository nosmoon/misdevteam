				//초기화 Set
				function setReady(){
					model.send("pl_bas_1200_m");						//Submission

					model.copyNode("/root/initData/comboSet/dlco_clsf","/root/resData/comboSet/dlco_clsf");				//거래처구분 temp노드에 copy
					model.copyNode("/root/initData/comboSet/dlco_clsf1","/root/resData/comboSet/dlco_clsf");				//거래처구분 temp노드에 copy
					model.copyNode("/root/initData/comboSet/area","/root/resData/comboSet/area");						//지역 copy
					model.copyNode("/root/initData/comboSet/area1","/root/resData/comboSet/area");						//지역 copy
					model.copyNode("/root/initData/comboSet/wh_cd","/root/resData/comboSet/wh_cd");					//창고코드 copy
					model.copyNode("/root/initData/comboSet/sendclsf","/root/resData/comboSet/sendclsf");				//발송구분 copy
					model.copyNode("/root/initData/comboSet/colt_clsf","/root/resData/comboSet/colt_clsf");				//담보구분 copy
					model.copyNode("/root/initData/comboSet/setup_stat","/root/resData/comboSet/setup_stat");				//설정상태 copy
					model.copyNode("/root/initData/comboSet/rshp","/root/resData/comboSet/rshp");						//관계 copy

					//콤보 초기구분값 세팅(전체(A), 선택(S), 공백("") -> value는 "")
					makeComboGubun("A", "/root/initData/comboSet/dlco_clsf1", "item", "cdnm", "cd");
					makeComboGubun("A", "/root/initData/comboSet/area1", "item", "cdnm", "cd");

					model.setValue("/root/tempData/btState","RE");			//버튼제어상태 (RE:초기화)

					model.setFocus("combo7");
					model.refresh();
				}

				//조회
				function srcRetrieve(){
					model.send("pl_bas_1210_l");						//Submission

					model.setValue("/root/tempData/btState","RT");			//버튼제어상태 (RT:조회)
					
					//초기화
					model.resetInstanceNode( "/root/resData/resform" );
					model.setFocus("combo7");
					model.refresh();
				}

				//상세검색 Set
				function serchDtl(){
					var dlco_cd = datagrid2.valueMatrix(datagrid2.row, 15);				//선택한 레코드의 거래처코드
					var dlco_seq = datagrid2.valueMatrix(datagrid2.row, 16);				//선택한 레코드의 거래처순번

					//REQUEST 노드에 키값 SET
					model.setValue("/root/reqData/searchData/searchDtl/dlco_cd",dlco_cd);			//거래처코드
					model.setValue("/root/reqData/searchData/searchDtl/dlco_seq",dlco_seq);		//거래처순번

					model.send("pl_bas_1220_l");						//Submission

					model.setFocus("combo3");
					model.setValue("/root/tempData/btState","DT");			//버튼제어상태 (DT:상세조회)
					//우편번호 숨기고 보여주기
					var zip_1 = model.getValue("/root/resData/resform/DLCODTLLIST/zip_1");
					zip_visible(zip_1,"1");	
					model.refresh();
				}

				//등록처리
				function insertProcess(){
					var btnctl = model.getValue("/root/tempData/btState");	//버튼제어상태 Get
					
					//Confirm체크
					if(!commConfirmBox("등록 하시겠습니까?", "등록")){
						return;
					}

					//필수체크
					if(!commReqCheck(group1)) {
						return;
					}
					
					model.setValue("/root/resData/resform/DLCODTLLIST/acc_flag","I");				//작업FLAG Set(I:등록)
					model.copyNode("/root/reqData/reqForm","/root/resData/resform/DLCODTLLIST");		//Request 노드카피
					model.setValue("/root/tempData/btState","IN");			//버튼제어상태 (IN:등록)
					
					model.send("pl_bas_1230_a");						//Submission

					//에러체크
					if(submit_completed()){
						//결과값 상세조회 노드로 Set
						model.copyNode("/root/reqData/searchData/searchDtl","/root/resData/resultData/searchDtl");
						model.refresh();
	
						model.send("pl_bas_1220_l");					//Submission(상세조회)
						model.refresh();
					}
					model.setFocus("combo3");
				}

				//수정처리
				function updateProcess(){
					var btnctl = model.getValue("/root/tempData/btState");	//버튼제어상태 Get
					
					//Confirm체크
					if(!commConfirmBox("수정 하시겠습니까?", "수정")){
						return;
					}

					//버튼체크
					if(btnctl != "IN" && btnctl != "DT" && btnctl != "UP" && btnctl != "SS" && btnctl != "SD"){
						commAlert("상세조회 후 수정할 수 있습니다.");
						return;
					}

					//필수체크
					if(!commReqCheck(group1)) {
						return;
					}
					
					model.setValue("/root/resData/resform/DLCODTLLIST/acc_flag","U");				//작업FLAG Set(U:수정)
					model.copyNode("/root/reqData/reqForm","/root/resData/resform/DLCODTLLIST");		//Request 노드카피
					
					model.send("pl_bas_1230_a");						//Submission

					//에러체크
					if(submit_completed()){
						//결과값 상세조회 노드로 Set
						model.copyNode("/root/reqData/searchData/searchDtl","/root/resData/resultData/searchDtl");
						model.refresh();
	
						model.send("pl_bas_1220_l");					//Submission(상세조회)
						model.refresh();

						model.setValue("/root/tempData/btState","UP");		//버튼제어상태 (UP:수정)
					}
					model.setFocus("combo3");
				}

				//삭제처리
				function deleteProcess(){
					var btnctl = model.getValue("/root/tempData/btState");	//버튼제어상태 Get
					
					//Confirm체크
					if(!commConfirmBox("삭제 하시겠습니까?", "삭제")){
						return;
					}

					//버튼체크
					if(btnctl != "IN" && btnctl != "DT" && btnctl != "UP" && btnctl != "SS" && btnctl != "SD"){
						commAlert("상세조회 후 삭제할 수 있습니다.");
						return;
					}

					//필수체크
					if(!commReqCheck(group1)) {
						return;
					}
					
					model.setValue("/root/resData/resform/DLCODTLLIST/acc_flag","D");				//작업FLAG Set(D:삭제)
					model.copyNode("/root/reqData/reqForm","/root/resData/resform/DLCODTLLIST");		//Request 노드카피
					
					model.send("pl_bas_1230_a");						//Submission

					//에러체크
					if(submit_completed()){
						//목록조회
						srcRetrieve();

						model.setValue("/root/tempData/btState","DE");		//버튼제어상태 (DE:삭제)
					}
					model.setFocus("combo7");
				}

				//서브저장처리
				function ssaveProcess(){
					var tabSel = switch1.selectedIndex;					//선택 탭인덱스 get
					var btnctl = model.getValue("/root/tempData/btState");	//버튼제어상태 get
					
					//Confirm체크
					if(!commConfirmBox("저장 하시겠습니까?", "저장")){
						return;
					}

					//버튼체크
					if(btnctl != "IN" && btnctl != "DT" && btnctl != "UP" && btnctl != "SS" && btnctl != "SD"){
						commAlert("상세조회 후 저장할 수 있습니다.");
						return;
					}

					//키등록상태 체크(상세조회인지 체크)
					var dlco_cd = model.getValue("/root/reqData/searchData/searchDtl/dlco_cd");		//거래처코드
					var dlco_seq = model.getValue("/root/reqData/searchData/searchDtl/dlco_seq");	//거래처순번
					
					if(dlco_cd == "" || dlco_seq == ""){
						commAlert("상세조회 후 저장할 수 있습니다.");
						return;
					}
					
					model.setValue("/root/reqData/multiUpdate/acc_flag","S");					//처리구분 Set(S:저장)

					//선택탭에 따라 처리
					if(tabSel == 0){
						//취급매체 처리 *************
						model.setValue("/root/reqData/multiUpdate/updateData",datagrid1.getUpdateData());
						model.send("pl_bas_1240_a");
					}else if(tabSel == 1){
						//발송처관리 처리 *************
						model.setValue("/root/reqData/multiUpdate/updateData",datagrid3.getUpdateData());
						model.send("pl_bas_1250_a");
					}else if(tabSel == 2){
						//담보내역 처리 *************
						model.setValue("/root/reqData/multiUpdate/updateData",datagrid4.getUpdateData());
						model.send("pl_bas_1260_a");
					}else if(tabSel == 3){
						//보증인 처리 *************
						model.setValue("/root/reqData/multiUpdate/updateData",datagrid5.getUpdateData());
						model.send("pl_bas_1270_a");
					}

					//에러체크
					if(submit_completed()){
						model.send("pl_bas_1220_l");					//Submission(상세조회)
						model.refresh();

						model.setValue("/root/tempData/btState","SS");		//버튼제어상태 (SS:서브저장)
					}
				}

				//서브삭제처리
				function sdeleteProcess(){
					var tabSel = switch1.selectedIndex;					//선택 탭인덱스 get
					var btnctl = model.getValue("/root/tempData/btState");	//버튼제어상태 get
					
					//Confirm체크
					if(!commConfirmBox("삭제 하시겠습니까?", "삭제")){
						return;
					}

					//버튼체크
					if(btnctl != "IN" && btnctl != "DT" && btnctl != "UP" && btnctl != "SS" && btnctl != "SD"){
						commAlert("상세조회 후 삭제할 수 있습니다.");
						return;
					}

					//키등록상태 체크(상세조회인지 체크)
					var dlco_cd = model.getValue("/root/reqData/searchData/searchDtl/dlco_cd");		//거래처코드
					var dlco_seq = model.getValue("/root/reqData/searchData/searchDtl/dlco_seq");	//거래처순번
					
					if(dlco_cd == "" || dlco_seq == ""){
						commAlert("상세조회 후 삭제할 수 있습니다.");
						return;
					}
					
					model.setValue("/root/reqData/multiUpdate/acc_flag","D");					//처리구분 Set(D:삭제)

					//선택탭에 따라 처리
					if(tabSel == 0){
						//취급매체 처리 *************
						pl_grid_delete("datagrid1");		//선택 row 삭제상태로 set
						model.setValue("/root/reqData/multiUpdate/updateData",datagrid1.getUpdateData());
						model.send("pl_bas_1240_a");
					}else if(tabSel == 1){
						//발송처관리 처리 *************
						pl_grid_delete("datagrid3");		//선택 row 삭제상태로 set
						model.setValue("/root/reqData/multiUpdate/updateData",datagrid3.getUpdateData());
						model.send("pl_bas_1250_a");
					}else if(tabSel == 2){
						//담보내역 처리 *************
						pl_grid_delete("datagrid4");		//선택 row 삭제상태로 set
						model.setValue("/root/reqData/multiUpdate/updateData",datagrid4.getUpdateData());
						model.send("pl_bas_1260_a");
					}else if(tabSel == 3){
						//보증인 처리 *************
