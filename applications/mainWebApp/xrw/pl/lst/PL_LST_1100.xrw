				//초기화 Set
				function setReady(){
					model.send("pl_lst_1000_m");						//Submission

					//콤보 초기구분값 세팅(전체(A), 선택(S), 공백("") -> value는 "")
					makeComboGubun("A", "/root/initData/comboSet/medi_clsf2", "item", "cdnm", "cd");
					makeComboGubun("A", "/root/initData/comboSet/medi_cd", "item", "cdnm", "cd");
					makeComboGubun("A", "/root/initData/comboSet/deal_clsf", "item", "cdnm", "cd");
					makeComboGubun("A", "/root/initData/comboSet/issu_cmpy", "item", "cdnm", "cd");
					makeComboGubun("A", "/root/initData/comboSet/wh_cd", "item", "cdnm", "cd");
					makeComboGubun("A", "/root/initData/comboSet/dlco_clsf", "item", "cdnm", "cd");
					makeComboGubun("A", "/root/initData/comboSet/area", "item", "cdnm", "cd");
					makeComboGubun("A", "/root/initData/comboSet/absence_clsf", "item", "cdnm", "cd");
					makeComboGubun("A", "/root/initData/comboSet/medi_cd_book", "item", "cdnm", "cd");
					makeComboGubun("A", "/root/initData/comboSet/ewh_clsf", "item", "cdnm", "cd");

					//조건 초기화
					model.setValue("/root/reqData/searchData/searchTop/sel_radio","2");

					//시작일자,종료일자
					var fr_dt = addDate("d",-7,commCurDate("ymd"),"");
					var to_dt = commCurDate("ymd");
					
					var fr_dt_180 = addDate("d",-180,commCurDate("ymd"),"");
					
					model.setValue("/root/reqData/searchData/searchCase1/fr_dt",fr_dt);
					model.setValue("/root/reqData/searchData/searchCase1/to_dt",to_dt);
					model.setValue("/root/reqData/searchData/searchCase2/fr_dt",fr_dt);
					model.setValue("/root/reqData/searchData/searchCase2/to_dt",to_dt);
					model.setValue("/root/reqData/searchData/searchCase3/fr_dt",fr_dt);
					model.setValue("/root/reqData/searchData/searchCase3/to_dt",to_dt);
					model.setValue("/root/reqData/searchData/searchCase5/fr_dt",fr_dt_180);
					model.setValue("/root/reqData/searchData/searchCase5/to_dt",to_dt);
					model.setValue("/root/reqData/searchData/searchCase6/fr_dt",fr_dt);
					model.setValue("/root/reqData/searchData/searchCase6/to_dt",to_dt);
					model.setValue("/root/reqData/searchData/searchCase14/fr_dt",to_dt);
					model.setValue("/root/reqData/searchData/searchCase14/to_dt",to_dt);
					model.setValue("/root/reqData/searchData/searchCase16/fr_dt",fr_dt);
					model.setValue("/root/reqData/searchData/searchCase16/to_dt",to_dt);
					model.setValue("/root/reqData/searchData/searchCase17/basi_fr_dt",fr_dt);
					model.setValue("/root/reqData/searchData/searchCase17/basi_to_dt",to_dt);
					//기준년월,비교년월1,비교년월2
					var basi_yymm = addDate("m",-1,commCurDate("ymd"),"");
					var ref1_yymm = addDate("m",-2,commCurDate("ymd"),"");
					var ref2_yymm = addDate("m",-13,commCurDate("ymd"),"");
					
					model.setValue("/root/reqData/searchData/searchCase4/basi_yymm",basi_yymm.substr(0,6));
					model.setValue("/root/reqData/searchData/searchCase5/basi_yymm",basi_yymm.substr(0,6));
					model.setValue("/root/reqData/searchData/searchCase7/basi_yymm",basi_yymm.substr(0,6));
					model.setValue("/root/reqData/searchData/searchCase10/basi_yymm",basi_yymm.substr(0,6));
					model.setValue("/root/reqData/searchData/searchCase11/basi_yymm",basi_yymm.substr(0,6));
					model.setValue("/root/reqData/searchData/searchCase11/ref1_yymm",ref1_yymm.substr(0,6));
					model.setValue("/root/reqData/searchData/searchCase11/ref2_yymm",ref2_yymm.substr(0,6));

					//기준일자
					var basi_dt = commCurDate("ymd");

					model.setValue("/root/reqData/searchData/searchCase8/basi_dt",basi_dt);
					model.setValue("/root/reqData/searchData/searchCase9/basi_dt",basi_dt);

					//매체구분
					model.setValue("/root/reqData/searchData/searchCase1/medi_clsf","10");
					model.setValue("/root/reqData/searchData/searchCase4/medi_clsf","10");
					model.setValue("/root/reqData/searchData/searchCase5/medi_clsf","10");
					model.setValue("/root/reqData/searchData/searchCase7/medi_clsf","10");
					model.setValue("/root/reqData/searchData/searchCase8/medi_clsf","10");
					model.setValue("/root/reqData/searchData/searchCase9/medi_clsf","20");
					model.setValue("/root/reqData/searchData/searchCase11/medi_clsf","10");
					model.setValue("/root/reqData/searchData/searchCase12/medi_clsf","10");
					model.setValue("/root/reqData/searchData/searchCase14/medi_clsf","20");
					model.setValue("/root/reqData/searchData/searchCase15/medi_clsf","10");

					model.refresh();
				}

				//출력
				function printProcess(){
					var sel_radio			= model.getValue("/root/reqData/searchData/searchTop/sel_radio");
					var sel_form			= "";

					//group
					var group_sel			= "group" + sel_radio;

					//root
					var root				= "/root/reqData/searchData/searchCase" + sel_radio + "/";
					var odi_root			= "/root/oz_data/odiParam/";

					//필수체크
					if(!commReqCheck(group_sel)) {
						return;
					}

					//오즈관련 노드 초기화.
					resetOznode();	

					//오즈데이타 설정 set
					makeNodeValue("/root/oz_data/preview","");
					makeNodeValue("/root/oz_data/printsize","");

					//session 정보 odi root set
					makeNodeValue(odi_root+"iv_cmpy_cd",model.property("ss_cmpycd"));
					makeNodeValue(odi_root+"iv_incmg_pers",model.property("ss_emp_no"));

					//출력선택에 따라 조건 처리
					if(sel_radio == "1"){
						//거래처원장
						// ozr경로 및 odi 파일명으로 사용될 노드 및 value셋팅 ( OZR파일경로 .ozr 제거)					
						makeNodeValue("/root/oz_data/ozrName", "pl/SP_PL_LST_1010_P");
	
						var iv_fr_dt				= model.getValue(root + "fr_dt");
						var iv_to_dt			= model.getValue(root + "to_dt");
						var iv_medi_clsf		= model.getValue(root + "medi_clsf");
						var iv_dlco_cd			= model.getValue(root + "dlco_cd");
						var iv_dlco_seq			= model.getValue(root + "dlco_seq");
														
						makeNodeValue(odi_root+"iv_fr_dt",iv_fr_dt);
						makeNodeValue(odi_root+"iv_to_dt",iv_to_dt);
						makeNodeValue(odi_root+"iv_medi_clsf",iv_medi_clsf);
						makeNodeValue(odi_root+"iv_dlco_cd",iv_dlco_cd);
						makeNodeValue(odi_root+"iv_dlco_seq",iv_dlco_seq);

					}else if(sel_radio == "2"){
						//거래현황
						//선택리스트
						sel_form	= model.getValue("/root/reqData/searchData/searchCase2/sel_kind");
						
						//선택 체크
						if(sel_form == "A"){
							// ozr경로 및 odi 파일명으로 사용될 노드 및 value셋팅 ( OZR파일경로 .ozr 제거)					
							makeNodeValue("/root/oz_data/ozrName", "pl/SP_PL_LST_1020_P");
						}else if(sel_form == "B"){
							makeNodeValue("/root/oz_data/ozrName", "pl/SP_PL_LST_1030_P");
						}else if(sel_form == "C"){
							makeNodeValue("/root/oz_data/ozrName", "pl/SP_PL_LST_1040_P");
						}else if(sel_form == "D"){
							makeNodeValue("/root/oz_data/ozrName", "pl/SP_PL_LST_1050_P");
						}else if(sel_form == "E"){
							makeNodeValue("/root/oz_data/ozrName", "pl/SP_PL_LST_1060_P");
						}else if(sel_form == "F"){
							makeNodeValue("/root/oz_data/ozrName", "pl/SP_PL_LST_1070_P");
						}

						var iv_fr_dt				= model.getValue(root + "fr_dt");
						var iv_to_dt			= model.getValue(root + "to_dt");
						var iv_medi_clsf		= model.getValue(root + "medi_clsf");
						var iv_basi_yymm		= model.getValue(root + "basi_yymm");
						var iv_deal_clsf			= model.getValue(root + "deal_clsf");
						var iv_issu_cmpy		= model.getValue(root + "issu_cmpy");
						var iv_wh_cd			= model.getValue(root + "wh_cd");
						var iv_medi_cd			= model.getValue(root + "medi_cd");
						var iv_medi_ser_no		= model.getValue(root + "medi_ser_no");
						var iv_dlco_cd			= model.getValue(root + "dlco_cd");
						var iv_dlco_seq			= model.getValue(root + "dlco_seq");
						var iv_sel_kind			= model.getValue(root + "sel_kind");
														
						makeNodeValue(odi_root+"iv_fr_dt",iv_fr_dt);
						makeNodeValue(odi_root+"iv_to_dt",iv_to_dt);
						makeNodeValue(odi_root+"iv_medi_clsf",iv_medi_clsf);
						makeNodeValue(odi_root+"iv_basi_yymm",iv_basi_yymm);
						makeNodeValue(odi_root+"iv_deal_clsf",iv_deal_clsf);
						makeNodeValue(odi_root+"iv_issu_cmpy",iv_issu_cmpy);
						makeNodeValue(odi_root+"iv_wh_cd",iv_wh_cd);
						makeNodeValue(odi_root+"iv_medi_cd",iv_medi_cd);
						makeNodeValue(odi_root+"iv_medi_ser_no",iv_medi_ser_no);
						makeNodeValue(odi_root+"iv_dlco_cd",iv_dlco_cd);
						makeNodeValue(odi_root+"iv_dlco_seq",iv_dlco_seq);
						makeNodeValue(odi_root+"iv_sel_kind",iv_sel_kind);

					}else if(sel_radio == "3"){
						//거래통계
						//선택리스트
						sel_form	= model.getValue("/root/reqData/searchData/searchCase3/sel_kind");
						
