				// 초기화 함수 //
				function init_controls(){
					set_default(
						datagrid1
					);
					set_format(
						"month_1",
						srch_work_cym
					);
					set_format(
						"left",
						datagrid1,"3,5,7"
					);
					set_format(
						"number4",
						datagrid1,"8"
					);
					set_format(
						"rate_002",
						datagrid1,"9,10"
					);
					
					// 그리드 초기화
					datagrid1.addStatus(1,"insert");
					
					for (i=0; i<200; i++){
						datagrid1.addRow(false);
						datagrid1.colDisabled(1) = true;
					}
					
					send_mc_cost_2000_m();
					
					incmg_pers.value = getProperty("ss_emp_no");
					incmg_pers_nm.value = getProperty("ss_emp_nm");
					incmg_pers_dept_nm.value = getProperty("ss_deptnm");
				}
				
				// 작업년월 불러오기 //
				function send_mc_cost_2000_m(){
					model.send("mc_cost_2000_m");
					set_refvalue("/root/reqData/searchData/work_cym",get_refvalue("/root/resData/resform/mc_cost_1030_m/dataSet/CURLIST/record/work_cym"));
					model1.setValue("/root/reqData/searchData/cost_clsf",model1.getValue("/root/resData/resform/mc_cost_2000_m/dataSet/xx_cost_clsf_010"));
					model1.setValue("/root/reqData/searchData/dstb_cdfr",model1.getValue("/root/resData/resform/mc_cost_2000_m/dataSet/xx_dstb_cdfr"));
					model1.setValue("/root/reqData/searchData/dstb_cdto",model1.getValue("/root/resData/resform/mc_cost_2000_m/dataSet/xx_dstb_cdto"));
					model1.setValue("/root/reqData/searchData/clos_clsf",model1.getValue("/root/resData/resform/mc_cost_2000_m/dataSet/xx_clos_clsf"));
					model1.setValue("/root/resData/resform/cost_clsf",model1.getValue("/root/resData/resform/mc_cost_2000_m/dataSet/xx_cost_clsf_020"));
					/*
					alert(model1.getValue("/root/reqData/searchData/cost_clsf"));
					alert(model1.getValue("/root/reqData/searchData/dstb_cdfr"));
					alert(model1.getValue("/root/reqData/searchData/dstb_cdto"));
					alert(model1.getValue("/root/reqData/searchData/clos_clsf"));
					alert(model1.getValue("/root/resData/resform/cost_clsf"));
