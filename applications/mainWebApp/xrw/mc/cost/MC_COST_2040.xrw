					alert(model1.getValue("/root/reqData/searchData/cost_dstb_cdfr"));
					alert(model1.getValue("/root/reqData/searchData/cost_dstb_cdto"));
					alert(model1.getValue("/root/reqData/searchData/clos_clsf"));
					alert(model1.getValue("/root/resData/resform/mc_cost_2040_m/dataSet/xx_cd"));
					alert(model1.getValue("/root/resData/resform/mc_cost_2040_m/dataSet/xx_cost_dstb_cd"));
					alert(model1.getValue("/root/resData/resform/mc_cost_2040_m/dataSet/xx_cost_dstb_cd_020"));
