				// 배부구분 콤보데이터 불러오기 //
				function send_mc_cost_2011_m(){
					model.send("mc_cost_2011_m");
					makeComboGubun("","/root/initData/comboSet/mc_cost_2011_m/dataSet/CURLIST","record","cdnm", "cd");
				}
				
				// 등록자팝업 데이터 설정 //
				function setEmp_no(emp_no, nm_korn, dept_nm){

					incmg_pers.value= emp_no;
					incmg_pers_nm.value = nm_korn;
					incmg_pers_dept_nm.value = dept_nm;

				}
				
				// 등록자 조회 //
				function open_hd_com_1000(){
					if (event != null && event.name == "xforms-value-changed") {
						if(incmg_pers.value.length == 6){
							model.removeNodeset("/root/reqData/fwdData");  
							model.makeNode("/root/reqData/fwdData/emp_no");
							model.setValue("/root/reqData/fwdData/emp_no", incmg_pers.value);
							model.send("hd_com_1002");
							
							if (model.getValue("/root/tempData/record/emp_no") != "") {
								incmg_pers_nm.value = model.getValue("/root/tempData/record/nm_korn");
								incmg_pers_dept_nm.value = model.getValue("/root/tempData/record/dept_nm");
							}else{
								incmg_pers.value = "";
								incmg_pers_nm.value = "";
								incmg_pers_dept_nm.value = "";
							}
						}else{
							incmg_pers.value = "";
							incmg_pers_nm.value = "";
							incmg_pers_dept_nm.value = "";
						}
					}else{
						model.removeNodeset("/root/reqData/fwdData");  
						model.makeNode("/root/reqData/fwdData/func_nm");
						model.setValue("/root/reqData/fwdData/func_nm", "setEmp_no");
						window.load("/xrw/hd/com/HD_COM_1000.xrw","mdichild", "hd_com_1000", "align:center; caption:hidden;", "/root/reqData/fwdData", "/root/reqData/fwdData");
					}
				}
				
				// 원가배부코드조회 //
				function open_mc_co_1030_input() {
					var popid = "mc_co_1030";
					var rcvs_flag = false;
					
					if (event != null && event.name == "xforms-value-changed") {
						//값이 변경되면 기존에 조회된 자료를 모두 제거한다.
						reset_popup_rcvs_data(popid);
					}
					
					set_popup_send_data(popid,"mode","sr");
					set_popup_send_data(popid,"title","");
					set_popup_send_data(popid,"readonly","0");
					set_popup_send_data(popid,"datafilter","");				
					set_popup_send_data(popid,"title","원가배부코드조회");
					set_popup_send_data(popid,"cd",srch_cost_dstb_cd.value);
					
					rcvs_flag = call_mc_co_1030(false);
					
					if (rcvs_flag) {
						//정상처리
					}
					else {
						//취소버튼을 누른 경우 조회 조회를 위한 값과 조회된 값이 틀린 경우 기존 값을 모두 제거한다.
					}
					
					//value-changed 와 buttonclick이 동시 발생했을 경우
					//이하 코드는 두번씩 실행되므로 무거운 코드는 넣지 않도록 하자
	
					//만약  value-changed 이벤트이면서 결과값이 없는 경우면 현재 값과 현재값에 종속되는 모든 값을 삭제한다.
					if (event != null && event.name == "xforms-value-changed" && !rcvs_flag) {
						//부서가 변경되면 고유행사코드는 삭제한다.
						//단 부서의 앞 2자리가 같은 경우 제외
						//입력값에 대한 oldValue와 currentText는 input에만 있으므로 조회하는 컨트롤이 input 컨트롤이라고 가정한다.
						//현재값이 공백이어도 변경 처리
						set_refvalue(srch_cost_dstb_cd,"");
						set_refvalue(dstb_nm,"");
					}
					
					//if(get_popup_rcvs_data(popid,"cd").substr(0,2) == "93"){
					if(get_popup_rcvs_data(popid,"cd").substr(0,2) == model1.getValue("/root/resData/resform/mc_cost_2040_m/dataSet/xx_cd")){
						show_message("매체배부비만 처리할 수 있습니다.");
						return;
					}
	
					set_refvalue(srch_cost_dstb_cd,get_popup_rcvs_data(popid,"cd"));
					set_refvalue(dstb_nm,get_popup_rcvs_data(popid,"cdnm"));
	
					return rcvs_flag;
				}
				
				// 원가배부코드조회 //
				function open_mc_co_1030_grid(){
					var popid = "mc_co_1030";
					var rcvs_flag = false;
					var selectedValue = datagrid1.valueMatrix(datagrid1.row,datagrid1.col);
					var selectedIdx = datagrid1.row;
					
					reset_popup_rcvs_data(popid);
					
					if (event != null && event.name == "xforms-value-changed") {
						//값이 변경되면 기존에 조회된 자료를 모두 제거한다.
						reset_popup_rcvs_data(popid);
					}
					
					set_popup_send_data(popid,"mode","sr");
					set_popup_send_data(popid,"title","");
					set_popup_send_data(popid,"readonly","0");
					set_popup_send_data(popid,"datafilter","");				
					set_popup_send_data(popid,"title","원가배부코드조회");
					set_popup_send_data(popid,"cd",selectedValue);
					
					rcvs_flag = call_mc_co_1030(false);
					if (rcvs_flag) {
						//정상처리
					}
					else {
						//취소버튼을 누른 경우 조회 조회를 위한 값과 조회된 값이 틀린 경우 기존 값을 모두 제거한다.
					}
					
					//value-changed 와 buttonclick이 동시 발생했을 경우
					//이하 코드는 두번씩 실행되므로 무거운 코드는 넣지 않도록 하자
	
					//만약  value-changed 이벤트이면서 결과값이 없는 경우면 현재 값과 현재값에 종속되는 모든 값을 삭제한다.
					if (event != null && event.name == "xforms-value-changed" && !rcvs_flag) {
						//부서가 변경되면 고유행사코드는 삭제한다.
						//단 부서의 앞 2자리가 같은 경우 제외
						//입력값에 대한 oldValue와 currentText는 input에만 있으므로 조회하는 컨트롤이 input 컨트롤이라고 가정한다.
						//현재값이 공백이어도 변경 처리
						
						datagrid1.valueMatrix(datagrid1.row,datagrid1.col) = "";
						datagrid1.valueMatrix(datagrid1.row,datagrid1.col+1) = "";
					}
					
					if(get_popup_rcvs_data(popid,"cd") != ""){
						//if(get_popup_rcvs_data(popid,"cd").substr(0,2) == "93"){
						if(get_popup_rcvs_data(popid,"cd").substr(0,2) == model1.getValue("/root/resData/resform/mc_cost_2040_m/dataSet/xx_cd")){
							show_message("매체배부비만 처리할 수 있습니다.");
							datagrid1.valueMatrix(datagrid1.row,datagrid1.col) = get_refvalue("/root/tempData/focusIn_val");
							return;
						}
						
						datagrid1.valueMatrix(datagrid1.row,datagrid1.col) = get_popup_rcvs_data(popid,"cd").substr(2,4);
						datagrid1.valueMatrix(datagrid1.row,datagrid1.col+1) = get_popup_rcvs_data(popid,"cdnm");
						
						if(datagrid1.rowStatus(selectedIdx) == "0"){
							datagrid1.rowStatus(selectedIdx) = "2";
						}
					}
	
					return rcvs_flag;
				}
				
