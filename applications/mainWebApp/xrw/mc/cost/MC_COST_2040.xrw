				// 매체코드조회 //
				function open_fc_co_8010(p_where,p_mode,p_title,p_readonly,p_to,p_job_clsf,p_cd_clsf) {
					var need_popup = true;
					var rcvs_flag = false;
					var selectedValue = datagrid1.valueMatrix(datagrid1.row,datagrid1.col);
					var selectedIdx = datagrid1.row;
					
					//검색조건이 모두 충족하지 않으면 종료한다.
					if (p_where == "FOCUS" && selectedValue.Trim() == "" ) {
						datagrid1.valueMatrix(datagrid1.row,datagrid1.col+1)	= "";
						need_popup = false;
						return rcvs_flag;
					}
					
					p_where = p_where.toUpperCase();
					
					var popid = "fc_co_8010";
					var subid = "fc_co_8010_l";
					
					reset_popup_rcvs_data(popid);
	
					set_popup_send_data(popid		,"mode"			,p_mode			);
					set_popup_send_data(popid		,"title"			,p_title			);
					set_popup_send_data(popid		,"readonly"		,p_readonly		);
					set_popup_send_data(popid		,"job_clsf"			,p_job_clsf		);
					set_popup_send_data(popid		,"cd_clsf"			,p_cd_clsf			);
					set_popup_send_data(popid		,"cd"				,selectedValue		);
					set_popup_send_data(popid		,"cd_abrv_nm"		,""				);
	
					//포커스에서 호출 한 경우 직접 서블릿을 실행시켜서 데이터를 확인한다.
					if (p_where == "FOCUS") {
						//포커스로 검색이 시작되는 경우 무조껀 검색 결과에 해당하는 노드를 삭제하고 시작한다.
						//버튼의 경우는 검색결과가 "" 이면 지우도록 설정되어있음
						model.removenode(get_popup_rcvs_ref(popid));
						//인스턴스 구조가 없으면 copynode 동작이 되지 않으므로 기본 구조는 만들어준다.
						model.makeNode(get_popup_rcvs_ref(popid));
						
						//받는위치 분리를 위해서 receive 위치를 popid가 아닌 subid 기준으로 설정
						send_submission(
								"auto_" + subid
							,	get_popup_send_ref(popid)
							,	get_popup_rcvs_ref(subid) + "/dataSet"
							,	"/fc/co8000/8011"
							,	"urlencoded-post"
							,	"application/x-www-form-urlencoded"
						);
						
						totalcnt = model.getValue(get_popup_rcvs_ref(subid) + "/dataSet" + "/CURLIST1/totalcnt");

						if (totalcnt == 1 && selectedValue.length == get_refvalue(get_popup_rcvs_ref(subid) + "/dataSet" + "/CURLIST1/record/cd").length) {
							//데이터가 1건이고 focus out에서 호출한 경우
							//데이터를 설정하고 나간다.
	
							model.copyNode(get_popup_rcvs_ref(popid) , get_popup_rcvs_ref(subid) + "/dataSet/CURLIST1/record");

							need_popup = false;
							rcvs_flag = true;
							
						}else{
							//show_message("매체코드의 검색결과가 없습니다.");
							datagrid1.valueMatrix(datagrid1.row,datagrid1.col) = "";
							datagrid1.valueMatrix(datagrid1.row,datagrid1.col+1) = "";
							
							need_popup = false;	
							rcvs_flag = false;
						}
					}
					
					//그외 버튼인 경우는 계속 진행
					if (need_popup && event.name == "onbuttonclick") {
						rcvs_flag = open_popup(popid,"640");
					}
	
					//반드시 입력되어야 하는 필수 항목의 경우 검색결과가 공백이 아닌 경우만 반영해서
					//다시 조회 하기 쉽도록 하고
					if (get_popup_rcvs_data(popid,"cd") != "") {
						datagrid1.valueMatrix(datagrid1.row,datagrid1.col) = get_popup_rcvs_data(popid,"cd");
						datagrid1.valueMatrix(datagrid1.row,datagrid1.col+1) = get_popup_rcvs_data(popid,"cd_abrv_nm");
						
						if(datagrid1.rowStatus(selectedIdx) == "0"){
							datagrid1.rowStatus(selectedIdx) = "2";
						}
						
						reset_popup_send_data(popid);
					}
					return rcvs_flag;
				}
				
				// 조회 //
				function send_mc_cost_2041_l(){
					var subid1 = "mc_cost_2041_l";
					var subref1 = eval(subid1).attribute("ref");
					var subresultref1 = eval(subid1).attribute("resultref");
					var subid1_cur1 = "CURLIST";
					
					if(srch_work_cym.value == ""){
						show_message("년월을 입력하세요.");
						return;
					}
					
					set_refvalue("/root/reqData/multiUpDate/work_cym",srch_work_cym.value);
					
					datagrid1.refresh();
					model.send(subid1);
					datagrid1.refresh();
					
					if(event.name == "onclick"){
						for(i=0; i<datagrid1.rows; i++){
							datagrid1.rowStyle(i, "data", "background-color") = "#FFFFFF";
						}
					}
					
					totalcnt = get_refvalue(subresultref1 + "/" + subid1_cur1 + "/totalcnt");
					
					for (i=0; i<10; i++){
						datagrid1.addRow(false);
						datagrid1.colDisabled(1) = true;
					}
					
					if (totalcnt == 0) {
						show_message("검색결과가 없습니다.");
					}
				}

				// 저장 //
				function save_process(){
					var cnt = 0;
					
					// 필수항목 체크 //
					if (!commReqCheck(group_scr)) {
						return false;
					}
					
					// 배부비등록 마감여부체크
					model.send("mc_cost_2003_l");

					if(get_refvalue("/root/resData/resform/mc_cost_2003_l/dataSet/CURLIST/record/clos_yn") == "Y"){
						show_message("이미 마감 처리가 되어있습니다.");
						return;
					}
					
					for(i = 1 ; i < datagrid1.rows ; i++ ){
						var cost_dstb_cd_var = datagrid1.valueMatrix(i,2);
						var medi_cd_var = datagrid1.valueMatrix(i,6);
						var qunt_var = datagrid1.valueMatrix(i,8);
						
						// 코드명이 비어있는경우 저장안함							
						if (cost_dstb_cd_var == "" && medi_cd_var == "" && qunt_var == "" ){
							datagrid1.rowStatus(i) = 0;
						}
						
						if(datagrid1.rowStatus(i) == "1" || datagrid1.rowStatus(i) == "2" || datagrid1.rowStatus(i) == "3"){
							cnt = cnt+1;
							if(cost_dstb_cd_var == ""){
								show_message(i+"번 ROW의 "+"원가배부코드를 입력하세요.");
								return;
