			function send_mc_budg_4024_l(){
				//필수항목체크(검색)
				if(!commReqCheck(group_scr)) {
					return false;
				}
				var ref_4024= "/root/reqData/searchData/mc_budg_4024_l/";
				set_refvalue(ref_4024 + "work_dt",srch_work_mm.value+srch_work_dd.value);
				model.send("mc_budg_4024_l");
				var ref_yramt = datagrid1.colRef("yramt");
				var ref_yrsqty = datagrid1.colRef("yrsqty");
				var ref_yrdan = datagrid1.colRef("yrdan");
				var ref_amt = datagrid1.colRef("amt");
				var ref_qunt = datagrid1.colRef("qunt");
				var ref_uprc = datagrid1.colRef("uprc");
				for(i = datagrid1.fixedCols ; i <= datagrid1.rows ; i++){
					if(datagrid1.valueMatrix(i,ref_yrsqty) != "0"){
						datagrid1.isReadOnly(i,ref_amt) = true;
						datagrid1.isReadOnly(i,ref_qunt) = false;
						datagrid1.isReadOnly(i,ref_uprc) = false;
					}else{
						datagrid1.isReadOnly(i,ref_amt) = false;
						datagrid1.isReadOnly(i,ref_qunt) = true;
						datagrid1.isReadOnly(i,ref_uprc) = true;
					}
					if(clsf_cd.value == ""){
						datagrid1.valueMatrix(i,ref_amt) = datagrid1.valueMatrix(i,ref_yramt);
						datagrid1.valueMatrix(i,ref_qunt) = datagrid1.valueMatrix(i,ref_yrsqty);
						datagrid1.valueMatrix(i,ref_uprc) = datagrid1.valueMatrix(i,ref_yrdan);
						if(srch_dept_cd.value.substing(0,2)){
							datagrid1.valueMatrix(i,ref_amt) = "";
						}
						if(i == datagrid1.rows){
							datagrid1.colStyle(ref_amt,"font") = "#ff0000";
							datagrid1.colStyle(ref_qunt,"font") = "#ff0000";
							datagrid1.colStyle(ref_uprc,"font") = "#ff0000";
						}
					}
				}
			}
			
