				set_refvalue(ref_4020 + "kind","1");
				model.send("mc_budg_4020_m");
				makeComboGubun("","/root/initData/comboSet/mc_budg_4020_m/dataSet/CURLIST1","record","work_dt", "work_dt");
				makeComboGubun("","/root/initData/comboSet/mc_budg_4020_m/dataSet/CURLIST2","record","hsty_no", "hsty_no");
				makeComboGubun("","/root/initData/comboSet/mc_budg_4020_m/dataSet/CURLIST3","record","dept_nm", "dept_cd");
				model.copyNode( "/root/initData/comboSet/temp_mc_budg_4020_m_1/dataSet/CURLIST1", "/root/initData/comboSet/mc_budg_4020_m/dataSet/CURLIST1");
