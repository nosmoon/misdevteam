			function fun_radio_ch_6(){
				var to_yymm_var;
				var dept_cd_var;
				if(from_budg_yymm_6.value == ""){
					show_message("[년도]는(은) 필수입력사항입니다.");
					return false;
				}
				if(dept_cd_6.value != ""){
					combo_data_clear(evnt_cd_6);
				}
				if(to_budg_yymm_6.value == ""){
					to_yymm_var = from_budg_yymm_6.value.substring(0,4) + "12";
				}else{
					to_yymm_var = to_budg_yymm_6.value;
				}
				if(dept_cd_6.value != ""){
					dept_cd_var = dept_cd_6.value.substring(0,dept_code_6.value);
				}
				
				set_refvalue("/root/reqData/case6/mc_budg_5043_l/dept_cd", dept_cd_var);
				set_refvalue("/root/reqData/case6/mc_budg_5043_l/from_yymm", from_budg_yymm_6.value);
				set_refvalue("/root/reqData/case6/mc_budg_5043_l/to_yymm", to_yymm_var);
				
				model.send("mc_budg_5043_l");
				
				makeComboGubun("A","/root/initData/comboSet/mc_budg_5043_l/dataSet/CURLIST","record","evnt_nm", "evnt_cd");
				makeComboGubun("","/root/initData/comboSet/mc_budg_5043_l/dataSet/CURLIST","record","evnt_nm", "evnt_cd");
				model.refresh();
			}
			
			function fun_radio_ch_1(){
				if(dept_cd_1.value == ""){
					return false;
				}
				if(in_bulen_1.value == "2"){
					set_refvalue(dept_cd_1, dept_cd_1.value.substring(0, 2) + "0000");
				}else{
					set_refvalue(dept_cd_1, dept_cd_1.value.substring(0, 4) + "00");
				}
				dept_cd_1.dispatch("xforms-value-changed");
				
				var to_yymm_var;
				var dept_cd_var;
				if(from_budg_yymm_1.value == ""){
					return false;
				}
				if(dept_cd_1.value != ""){
					combo_data_clear(evnt_cd_1);
				}
				if(to_budg_yymm_1.value == ""){
					to_yymm_var = from_budg_yymm_1.value.substring(0,4) + "12";
				}else{
					to_yymm_var = to_budg_yymm_1.value;
				}
				if(dept_cd_1.value != ""){
					dept_cd_var = dept_cd_1.value.substring(0,in_bulen_1.value);
				}
				
				set_refvalue("/root/reqData/case1/mc_budg_5080_l/dept_cd", dept_cd_var);
				set_refvalue("/root/reqData/case1/mc_budg_5080_l/from_yymm", from_budg_yymm_1.value);
				set_refvalue("/root/reqData/case1/mc_budg_5080_l/to_yymm", to_yymm_var);
				
				model.send("mc_budg_5080_l");
				
				makeComboGubun("A","/root/initData/comboSet/mc_budg_5080_l/dataSet/CURLIST","record","evnt_nm", "evnt_cd");
				model.refresh();
			}
							
			function send_mc_budg_5080_p(){
				//필수항목체크(검색)
				if(from_budg_yymm_1.value == ""){
					show_message("[년도]는(은) 필수입력사항입니다.");
					return false;
				}
				if (fn_cmpcMgmt(dept_cd_1.value) == false) { return; }
				
				resetOznode();
				
				// ozr경로 및 odi 파일명으로 사용될 노드 및 value셋팅 ( OZR파일경로 .ozr 제거)
				if(in_brlen_1.value == ""){
					makeNodeValue("/root/oz_data/ozrName","fcmc/SP_MC_BUDG_5081_P");
				}else{
					makeNodeValue("/root/oz_data/ozrName","fcmc/SP_MC_BUDG_5080_P");
				}
								
				// odi 파라미터 설정.
				makeNodeValue("/root/oz_data/odiParam/iv_cmpy_cd", getProperty("ss_cmpycd"));
				if(in_bulen_1.value == "2"){
					makeNodeValue("/root/oz_data/odiParam/iv_in_bulen", in_bulen_1.value);
					makeNodeValue("/root/oz_data/odiParam/iv_in_buzero", "0000");
					makeNodeValue("/root/oz_data/odiParam/iv_in_jogun", "=");
				}else{
					makeNodeValue("/root/oz_data/odiParam/iv_in_bulen", in_bulen_1.value);
					makeNodeValue("/root/oz_data/odiParam/iv_in_buzero", "00");
					makeNodeValue("/root/oz_data/odiParam/iv_in_jogun", ">");
				}
				if(in_brlen_1.value != ""){
					if(in_brlen_1.value == "6"){
						makeNodeValue("/root/oz_data/odiParam/iv_in_brzero", "00");
					}else{
						makeNodeValue("/root/oz_data/odiParam/iv_in_brzero", "");
					}
					makeNodeValue("/root/oz_data/odiParam/iv_in_brlen", in_brlen_1.value);
				}			
				makeNodeValue("/root/oz_data/odiParam/iv_evnt_cd", evnt_cd_1.value);
				makeNodeValue("/root/oz_data/odiParam/iv_dept_cd", dept_cd_1.value);
				makeNodeValue("/root/oz_data/odiParam/iv_from_budg_yymm", from_budg_yymm_1.value);
				makeNodeValue("/root/oz_data/odiParam/iv_to_budg_yymm", to_budg_yymm_1.value);
								
				// 레포트뷰어 호출.
				openOzwin("/root/oz_data");					
			}
			
			function setDept_cd_1(iv_dept_cd, iv_dept_nm, iv_dept_team_clsf, iv_offi_nm, iv_dept_team_nm, iv_part_nm, iv_part_dn_nm){	
				set_refvalue(dept_cd_1, iv_dept_cd);
				set_refvalue(dept_cdnm_1, iv_dept_nm);
				if(dept_cd_1.value != ""){
					if (fn_cmpcMgmt(dept_cd_1.value) == false) { 
						dept_cd_1.value= "";
						return; 
					}
					
					if(dept_cd_1.value == ""){
						return false;
					}
					
					if(in_bulen_1.value != ""){
						if(in_bulen_1.value == "2"){
							if(get_refvalue(dept_cd_1).substring(2,6) != "0000"){
								set_refvalue(dept_cd_1, dept_cd_1.value.substring(0, 2) + "0000");
								dept_cd_1.dispatch("xforms-value-changed");
							}
						}else{
							if(get_refvalue(dept_cd_1).substring(4,6) != "00"){
								set_refvalue(dept_cd_1, dept_cd_1.value.substring(0, 4) + "00");
								dept_cd_1.dispatch("xforms-value-changed");
							}
						}
					}
				}else{
					if(evnt_cd_1.value != ""){
						combo_data_clear(dept_cd_1);
					}
				}
			}
			
			function send_mc_budg_5090_p(){
				//필수항목체크(검색)
				if(!commReqCheck(group2)) {
					return false;
				}
				
				resetOznode();
				
				// ozr경로 및 odi 파일명으로 사용될 노드 및 value셋팅 ( OZR파일경로 .ozr 제거)
				if(in_tms_2.value == "" || in_tms_2.value == "00"){
					makeNodeValue("/root/oz_data/ozrName","fcmc/SP_MC_BUDG_5090_P");
				}else{
					makeNodeValue("/root/oz_data/ozrName","fcmc/SP_MC_BUDG_5091_P");
				}
				
								
				// odi 파라미터 설정.
				makeNodeValue("/root/oz_data/odiParam/iv_cmpy_cd", getProperty("ss_cmpycd"));
				makeNodeValue("/root/oz_data/odiParam/iv_yymm", yyyy_2.value);
				makeNodeValue("/root/oz_data/odiParam/iv_in_tms", in_tms_2.value);
				
				// 레포트뷰어 호출.
				openOzwin("/root/oz_data");					
			}
			
