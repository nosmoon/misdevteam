			}
			
			function send_mc_budg_5100_p(){
				if(!checkref(yyyy_3)) {
					return false;
				}

				resetOznode();
				
				// ozr경로 및 odi 파일명으로 사용될 노드 및 value셋팅 ( OZR파일경로 .ozr 제거)
				if(in_tms_3.value == "" || in_tms_3.value == "00"){
					makeNodeValue("/root/oz_data/ozrName","fcmc/SP_MC_BUDG_5100_P");
				}else{
					makeNodeValue("/root/oz_data/ozrName","fcmc/SP_MC_BUDG_5101_P");
				}
				
				var yyyy_var;
				if(from_mm_3.value == "" &&  to_mm_3.value == ""){
					model.send("mc_budg_2061_l");
					yyyy_var = get_refvalue("/root/resData/resForm/mc_budg_2061_l/dataSet/CURLIST/record/max_sub_cd");
					if(yyyy_var != ""){
						if(yyyy_var.substring(0,4) == yyyy_3.value){
							set_refvalue("/root/reqData/case3/mc_budg_5100_p/from_issu_dt", yyyy_3.value + "01");
							set_refvalue("/root/reqData/case3/mc_budg_5100_p/to_issu_dt", yyyy_3.value + yyyy_var.substring(4,6));
							if(yyyy_var.substring(4,6) == "12"){
								set_refvalue("/root/reqData/case3/mc_budg_5100_p/from_issu_dt1", "0");
								set_refvalue("/root/reqData/case3/mc_budg_5100_p/to_issu_dt1","0");
							}else{
								var ex_var = Number(yyyy_var.substring(4,6)) + 1;
								if(ex_var < 10){
									ex_var = "0" + ex_var;
								}
								set_refvalue("/root/reqData/case3/mc_budg_5100_p/from_issu_dt1", yyyy_3.value + ex_var);
								set_refvalue("/root/reqData/case3/mc_budg_5100_p/to_issu_dt1",yyyy_3.value + "12");
							}
						}else if(yyyy_var.substring(0,4) > yyyy_3.value){
							set_refvalue("/root/reqData/case3/mc_budg_5100_p/from_issu_dt", yyyy_3.value + "01");
							set_refvalue("/root/reqData/case3/mc_budg_5100_p/to_issu_dt", yyyy_3.value + "12");
							set_refvalue("/root/reqData/case3/mc_budg_5100_p/from_issu_dt1", "0");
							set_refvalue("/root/reqData/case3/mc_budg_5100_p/to_issu_dt1","0");
						}else{
							set_refvalue("/root/reqData/case3/mc_budg_5100_p/from_issu_dt", "0");
							set_refvalue("/root/reqData/case3/mc_budg_5100_p/to_issu_dt","0");
							set_refvalue("/root/reqData/case3/mc_budg_5100_p/from_issu_dt1", yyyy_3.value + "01");
							set_refvalue("/root/reqData/case3/mc_budg_5100_p/to_issu_dt1",yyyy_3.value + "12");
						}
