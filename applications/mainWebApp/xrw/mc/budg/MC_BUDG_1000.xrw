					}
				}
				root.reqData.multiUpDate.mc_budg_1008_d.jobtag.value = "Y"
				var ch_ref = datagrid1.colRef("ch");
				for(i = datagrid1.fixedRows ; i <= datagrid1.rows ; i++){
