				}
				function send_mc_budg_1001_l(){
					if(root.tempData.keyData.value == 1){
						srch_budg_cd.value = budg_cd.value;
					}
					if(srch_budg_cd.value.length != 6 || srch_budg_cd.value.length == null ){
						if (!check_reqItem(srch_budg_cd,"trim","focus","[예산코드]는 필수입력항목입니다..")) { return false; }
						return;
					}
//					for(var i=1; i <=datagrid2.rows;i++){
//						datagrid2.textmatrix(i,0) = false;
//					}
					datagrid2.fixedcellcheckbox(0, datagrid2.colRef("ch"), false) = true;
					datagrid2.fixedcellischeck(0,datagrid2.colRef("ch")) = false;
					datagrid2.fixedcellcheckbox(0, datagrid2.colRef("ch"), false) = false;
					model.send("mc_budg_1001_l",false);
					if(submit_completed()){
						if(root.resData.resform.mc_budg_1001_l.dataSet.CURLIST.totalcnt.value > 0){
							budg_cdnm1.value = budg_cdnm.value;
							if(root.resData.resform.mc_budg_1001_l.dataSet.CURLIST.record.use_yn.value == "Y"){
								send_mc_budg_1002_l();
								budg_cd.value = srch_budg_cd.value;
								acct_cd.value = srch_budg_cd.value.substring(0,4);
								model.refresh();
								clas_cd_sum();
								//btn_insert.disabled = true;
							}
						}else{
							show_message("[예산코드]의 검색결과가 없습니다.");
							srch_budg_cd.value = "";
							model.setFocus("srch_budg_cd");
						}
					}
				}
				function send_mc_budg_1002_l(){
					model.send("mc_budg_1002_l");
					datagrid_col_ch();
				}
				
				function datagrid_col_ch(){		
					var col1 =datagrid1.colRef("mang_item_cd");
					var col2 = datagrid1.colRef("mang_item_val");			
					for(var i = datagrid1.fixedRows; i <= datagrid1.rows ; i++){
						//if(datagrid1.valueMatrix(i,col1) != "05"){
						if(datagrid1.valueMatrix(i,col1) != model1.getValue("/root/initData/comboSet/mc_budg_1000_m/dataSet/xx_cd_010")){
							datagrid1.isReadOnly(i,col2) = true;
							datagrid1.valueMatrix(i,col2) = "";
						}else{
							datagrid1.isReadOnly(i,col2) = false;
						}
					}
				}
				
				function send_mc_budg_1006_l(up_down){
					if(up_down == "up"){
						if(srch_budg_cd.value == ""){
						root.reqData.searchData.mc_budg_1006_l.min_max.value = "MAX";
						root.reqData.searchData.mc_budg_1006_l.sql.value = " > 0";
						}else{
							root.reqData.searchData.mc_budg_1006_l.min_max.value = "MIN";
							root.reqData.searchData.mc_budg_1006_l.sql.value = "> '" + srch_budg_cd.value + "'";
						}
					}else if(up_down == "down"){
						if(srch_budg_cd.value == ""){
						root.reqData.searchData.mc_budg_1006_l.min_max.value = "MIN";
						root.reqData.searchData.mc_budg_1006_l.sql.value = " > 0";
						}else{
							root.reqData.searchData.mc_budg_1006_l.min_max.value = "MAX";
							root.reqData.searchData.mc_budg_1006_l.sql.value = "< '" + srch_budg_cd.value + "'";
						}
