		<script type="javascript">
			<![CDATA[
				
				function send_mc_budg_5100_p(){
					if(!checkref(yyyy)) {
						return false;
					}

					resetOznode();
					
					// ozr경로 및 odi 파일명으로 사용될 노드 및 value셋팅 ( OZR파일경로 .ozr 제거)
					if(in_tms.value == "" || in_tms.value == "00"){
						makeNodeValue("/root/oz_data/ozrName","fcmc/SP_MC_BUDG_5100_P");
					}else{
						makeNodeValue("/root/oz_data/ozrName","fcmc/SP_MC_BUDG_5101_P");
					}
					
					var yyyy_var;
					if(from_mm.value == "" &&  to_mm.value == ""){
						model.send("mc_budg_2061_l");
						yyyy_var = get_refvalue("/root/resData/resForm/mc_budg_2061_l/dataSet/CURLIST/record/max_sub_cd");
						if(yyyy_var != ""){
							if(yyyy_var.substring(0,4) == yyyy.value){
								set_refvalue("/root/reqData/searchData/from_issu_dt", yyyy.value + "01");
								set_refvalue("/root/reqData/searchData/to_issu_dt", yyyy.value + yyyy_var.substring(4,6));
								if(yyyy_var.substring(4,6) == "12"){
									set_refvalue("/root/reqData/searchData/from_issu_dt1", "0");
									set_refvalue("/root/reqData/searchData/to_issu_dt1","0");
								}else{
									var ex_var = Number(yyyy_var.substring(4,6)) + 1;
									if(ex_var < 10){
										ex_var = "0" + ex_var;
									}
									set_refvalue("/root/reqData/searchData/from_issu_dt1", yyyy.value + ex_var);
									set_refvalue("/root/reqData/searchData/to_issu_dt1",yyyy.value + "12");
								}
							}else if(yyyy_var.substring(0,4) > yyyy.value){
								set_refvalue("/root/reqData/searchData/from_issu_dt", yyyy.value + "01");
								set_refvalue("/root/reqData/searchData/to_issu_dt", yyyy.value + "12");
								set_refvalue("/root/reqData/searchData/from_issu_dt1", "0");
								set_refvalue("/root/reqData/searchData/to_issu_dt1","0");
							}else{
								set_refvalue("/root/reqData/searchData/from_issu_dt", "0");
								set_refvalue("/root/reqData/searchData/to_issu_dt","0");
								set_refvalue("/root/reqData/searchData/from_issu_dt1", yyyy.value + "01");
								set_refvalue("/root/reqData/searchData/to_issu_dt1",yyyy.value + "12");
							}
							set_refvalue("/root/reqData/searchData/from_issu_dt2", get_refvalue("/root/reqData/searchData/from_issu_dt1"));
							set_refvalue("/root/reqData/searchData/to_issu_dt2", get_refvalue("/root/reqData/searchData/to_issu_dt1"));
						}
					}else{
						set_refvalue("/root/reqData/searchData/from_issu_dt", yyyy.value + from_mm.value);
						set_refvalue("/root/reqData/searchData/to_issu_dt", yyyy.value + to_mm.value);
						if(from_mm.value == "01" && to_mm.value == "12"){
							set_refvalue("/root/reqData/searchData/from_issu_dt", "0");
							set_refvalue("/root/reqData/searchData/to_issu_dt","0");
							set_refvalue("/root/reqData/searchData/from_issu_dt1", "0");
							set_refvalue("/root/reqData/searchData/to_issu_dt1", "0");
						}else if(from_mm.value == "01" && to_mm.value != "12"){
							var ex_var = Number(to_mm.value) + 1;
							if(ex_var < 10){
								ex_var = "0" + ex_var;
							}
							set_refvalue("/root/reqData/searchData/from_issu_dt1", yyyy.value + ex_var);
							set_refvalue("/root/reqData/searchData/to_issu_dt1",yyyy.value + "12");
							set_refvalue("/root/reqData/searchData/from_issu_dt2", get_refvalue("/root/reqData/searchData/from_issu_dt1"));
							set_refvalue("/root/reqData/searchData/to_issu_dt2", get_refvalue("/root/reqData/searchData/to_issu_dt1"));
						}else if(from_mm.value > "01" && to_mm.value == "12"){
							var ex_var = Number(from_mm.value) - 1;
							if(ex_var < 10){
								ex_var = "0" + ex_var;
							}
							set_refvalue("/root/reqData/searchData/from_issu_dt1", yyyy.value + "01");
							set_refvalue("/root/reqData/searchData/to_issu_dt1",yyyy.value + ex_var);
							set_refvalue("/root/reqData/searchData/from_issu_dt2", get_refvalue("/root/reqData/searchData/from_issu_dt1"));
							set_refvalue("/root/reqData/searchData/to_issu_dt2", get_refvalue("/root/reqData/searchData/to_issu_dt1"));
						}else if(from_mm.value > "01" && to_mm.value != "12"){
							set_refvalue("/root/reqData/searchData/from_issu_dt1", yyyy.value + "01");
							var ex_var = Number(from_mm.value) - 1;
							if(ex_var < 10){
								ex_var = "0" + ex_var;
							}
							set_refvalue("/root/reqData/searchData/to_issu_dt1",yyyy.value + ex_var);
							var ex_var = Number(to_mm.value) + 1;
							if(ex_var < 10){
								ex_var = "0" + ex_var;
							}
							set_refvalue("/root/reqData/searchData/from_issu_dt2", yyyy.value + ex_var);
							set_refvalue("/root/reqData/searchData/to_issu_dt2", yyyy.value + "12");
						}
					}
					
					// odi 파라미터 설정.
					makeNodeValue("/root/oz_data/odiParam/iv_cmpy_cd", getProperty("ss_cmpycd"));
					makeNodeValue("/root/oz_data/odiParam/iv_from_issu_dt", get_refvalue("/root/reqData/searchData/from_issu_dt"));
					makeNodeValue("/root/oz_data/odiParam/iv_to_issu_dt", get_refvalue("/root/reqData/searchData/to_issu_dt"));
					makeNodeValue("/root/oz_data/odiParam/iv_from_issu_dt1", get_refvalue("/root/reqData/searchData/from_issu_dt1"));
					makeNodeValue("/root/oz_data/odiParam/iv_to_issu_dt1", get_refvalue("/root/reqData/searchData/to_issu_dt1"));
					makeNodeValue("/root/oz_data/odiParam/iv_from_issu_dt2", get_refvalue("/root/reqData/searchData/from_issu_dt2"));
					makeNodeValue("/root/oz_data/odiParam/iv_to_issu_dt2", get_refvalue("/root/reqData/searchData/to_issu_dt2"));
					makeNodeValue("/root/oz_data/odiParam/iv_in_tms", in_tms.value);
					
					// 레포트뷰어 호출.
					openOzwin("/root/oz_data");					
				}
				
			
			function in_yymm_ch(){
				model.send("mc_budg_5098_m");
				makeComboGubun("","/root/initData/comboSet/mc_budg_5098_m/dataSet/CURLIST","record","exec_no", "exec_no");
			}
			
			]]>
		</script>
