					dept_cd.refresh();
					dept_nm.refresh();
				}				
				
				// 조회 //
				function send_mc_budg_2011_l(){
					var subid1 = "mc_budg_2011_l";
					var subref1 = eval(subid1).attribute("ref");
					var subresultref1 = eval(subid1).attribute("resultref");
					var subid1_cur1 = "CURLIST";
					
					// 필수항목 체크 //
					if (!commReqCheck(group_scr)) {
						return false;
					}
					
					if(book_select.value == "0"){
						set_refvalue("/root/reqData/searchData/dtls_clsf_cd1","25");
						set_refvalue("/root/reqData/searchData/dtls_clsf_cd2","26");
					}else{
						set_refvalue("/root/reqData/searchData/dtls_clsf_cd1","27");
						set_refvalue("/root/reqData/searchData/dtls_clsf_cd2","28");
					}

					model.send(subid1);
					
					totalcnt = get_refvalue(subresultref1 + "/" + subid1_cur1 + "/totalcnt");

					/*----------------------------------------------------------
					    ' space권한일 경우 1999.06.17
					    '==========================================================
					    If W_GUNHAN = " " Then
					        If GF_GetSpaceGunHanChk(IN_GCCYM$, IN_GCEMGBCD$, "", IN_GCMAECD$, GCPSBSCD$(0), True) = False Then
					            W_PSBSCD = ""
					            W_Select_Cnt = 0
					            Screen.MousePointer = vbArrow
					            Exit Sub
					        End If
					    End If
					    '==========================================================
					    '----------------------------------------------------------*/
					
					dept_cd.value = get_refvalue("/root/resData/gridData/mc_budg_2011_l/dataSet/CURLIST/record["+1+"]/form_dept_cd");
					event.name = "xforms-value-changed";
					
					if(totalcnt < 12){
						
						model.copyNode("/root/tempData/mc_budg_2011_l","/root/resData/gridData/mc_budg_2011_l");
						datagrid_data_clear(datagrid1);
						
						for(i = 1 ; i < 13 ; i++){
							datagrid1.addRow(false, true);
						}
						
						datagrid1.refresh();
						
						// 각 월에 해당하는 위치에 데이터 설정 //
						var totcnt = get_refvalue("/root/tempData/mc_budg_2011_l/dataSet/CURLIST/totalcnt");
					
						for(i = 0 ; i < 12 ; i++){

							var mon = Number(get_refvalue("/root/tempData/mc_budg_2011_l/dataSet/CURLIST/record["+i+"]/yymm").substr(4,2));
							
							if(mon > 0){
								model.copyNode("/root/resData/gridData/mc_budg_2011_l/dataSet/CURLIST/record["+mon+"]","/root/tempData/mc_budg_2011_l/dataSet/CURLIST/record["+i+"]");
								datagrid1.rowStatus(mon) = "0";
							}
						}
					}
					
					for(i = 1 ; i < 13 ; i++){
						set_refvalue("/root/resData/gridData/mc_budg_2011_l/dataSet/CURLIST/record["+i+"]/month",i+"월");
					}
					
					datagrid1.refresh();
					
					grid_sum();
					
					if(datagrid1.valueMatrix(13,3) == 0 && datagrid1.valueMatrix(13,4) == 0){
						dept_cd.value = "";
						dept_nm.value = "";
						show_message("검색결과가 없습니다.");
						default_grid();
					}else{
						dept_cd.value = get_refvalue("/root/resData/gridData/mc_budg_2011_l/dataSet/CURLIST/record/form_dept_cd");
						dept_cd.dispatch("xforms-value-changed");
					}
				}
				
				// 년도변경시 초기화 //
				function year_change_event(){
					srch_medi_cd.select(0);
					datagrid_data_clear(datagrid1);
					default_grid();
					dept_cd.value = "";
					dept_nm.value = "";
				}
				
				// 그리드내 합계계산 //
				function grid_sum(){
					datagrid1.subtotal("clear", -1, 3, 1, "합계");
					datagrid1.subtotal("clear", -1, 4, 1, "합계");
					datagrid1.subtotal("sum", -1, 3, "#,###", "", 1, "합계");
					datagrid1.subtotal("sum", -1, 4, "#,###", "", 1, "합계");
				}
				
				// 저장 //
				function send_mc_budg_1010_i(){
					var cnt = 0;
					
					// 필수항목 체크 //
					if (!commReqCheck(group_scr)) {
						return false;
					}
					
					if(dept_cd.value == ""){
						show_message("편성부서코드를 입력하세요");
						return;
					}
					
					for (i=1; i<datagrid1.rows; i++){
						var sum_ini_plan_qunt_01_var = datagrid1.valueMatrix(i,3);
						var sum_ini_plan_qunt_02_var = datagrid1.valueMatrix(i,4);
						
						// 발행부수와 발행면수가 비어있는경우 저장안함							
						if (sum_ini_plan_qunt_01_var == "" && sum_ini_plan_qunt_02_var ==""){
							datagrid1.rowStatus(i) = 0;
						}else if(datagrid1.rowStatus(i) == "3" || datagrid1.rowStatus(i) == "2" || datagrid1.rowStatus(i) == "1"){
							cnt = cnt+1;
							if(sum_ini_plan_qunt_01_var == ""){
								show_message(i+"번 ROW의 "+"발행부수를 입력하세요.");
								return;
							}else if(sum_ini_plan_qunt_02_var == ""){
								show_message(i+"번 ROW의 "+"발행면수를 입력하세요.");
								return;
							}
						}												
					}
					
					if(cnt == 0){
						show_message("저장할 항목이 없습니다.");
						return;
					}
					
					if(show_message("저장하시겠습니까?", "원가배부코드 저장", 4)  == 7){	
						return;
					}
					
					// MultiUpDate 설정 //
					set_refvalue("/root/reqData/multiUpDate/medi_cd",srch_medi_cd.value);
					set_refvalue("/root/reqData/multiUpDate/form_dept_cd",dept_cd.value);

					// 신규일경우 그리드 설정 //
					for(i = 1 ; i < 13 ; i++){
						if(datagrid1.rowStatus(i) == "3"){
							set_refvalue("/root/resData/gridData/mc_budg_2011_l/dataSet/CURLIST/record["+i+"]/dept_cd"," ");
							set_refvalue("/root/resData/gridData/mc_budg_2011_l/dataSet/CURLIST/record["+i+"]/yymm",srch_yyyy.value+string_lpad(String(i),2,"0"));
						}
					}
					
					// 신규등록 및 업데이트 //
					for(i = 1 ; i < 3 ; i++){
						if(i == 1){
							for(j = 1 ;  j < 13 ; j++){
								set_refvalue("/root/resData/gridData/mc_budg_2011_l/dataSet/CURLIST/record["+j+"]/ini_plan_qunt",get_refvalue("/root/resData/gridData/mc_budg_2011_l/dataSet/CURLIST/record["+j+"]/sum_ini_plan_qunt_01"));
								set_refvalue("/root/resData/gridData/mc_budg_2011_l/dataSet/CURLIST/record["+j+"]/dept_cd"," ");
								set_refvalue("/root/resData/gridData/mc_budg_2011_l/dataSet/CURLIST/record["+j+"]/dtls_clsf_cd",get_refvalue("/root/reqData/searchData/dtls_clsf_cd1"));
							}
						}else{
							for(j = 1 ;  j < 13 ; j++){
								set_refvalue("/root/resData/gridData/mc_budg_2011_l/dataSet/CURLIST/record["+j+"]/ini_plan_qunt",get_refvalue("/root/resData/gridData/mc_budg_2011_l/dataSet/CURLIST/record["+j+"]/sum_ini_plan_qunt_02"));
								set_refvalue("/root/resData/gridData/mc_budg_2011_l/dataSet/CURLIST/record["+j+"]/dtls_clsf_cd",get_refvalue("/root/reqData/searchData/dtls_clsf_cd2"));
							}
						}
						
						set_refvalue("/root/reqData/multiUpDate/multiUpdateData", datagrid1.getUpdateData());
						model.send("mc_co_1010i");
						
						if(get_refvalue("/root/resData/MsgData/errData/errcode") != ""){
							send_mc_budg_2011_l();
							return;
						}
					}
					
					if(submit_completed()){
						btn_select.dispatch("onclick");
						datagrid1.refresh();
						show_message("저장되었습니다.");
					}						
					else{
						return;	
					}
				}
				
				// 삭제 //
				function delete_process(){
					var cnt = 0;
					
					for (i = 1 ; i < 13 ; i++){
						var sum_ini_plan_qunt_01_var = datagrid1.valueMatrix(i,3);
						var sum_ini_plan_qunt_02_var = datagrid1.valueMatrix(i,4);
						
						if (sum_ini_plan_qunt_01_var == "" && sum_ini_plan_qunt_02_var ==""){
							datagrid1.rowStatus(i) = "0";
						}else if(datagrid1.rowStatus(i) == "4"){						
							cnt = cnt+1;
						}else{
							datagrid1.rowStatus(i) = "0";
						}
					}
					
					if(cnt == 0){
						show_message("삭제할 대상이 없습니다.");
						return;	
					}
					
					if(show_message("해당자료 "+cnt+"개를 삭제하시겠습니까?", "삭제", 4)  == 7){
						return;
					}
					
					// MultiUpDate 설정 //
					set_refvalue("/root/reqData/multiUpDate/medi_cd",srch_medi_cd.value);
					set_refvalue("/root/reqData/multiUpDate/form_dept_cd",dept_cd.value);

					// 삭제인 경우는 값을 "0"로 한 저장과 같게 작성했슴 (이유: 삭제는 해당년도에 대한 삭제이므로 전제가 삭제되버림) //
					for(i = 1 ; i < 3 ; i++){
						if(i == 1){
							for(j = 1 ;  j < 13 ; j++){
								set_refvalue("/root/resData/gridData/mc_budg_2011_l/dataSet/CURLIST/record["+j+"]/ini_plan_qunt","0");
								set_refvalue("/root/resData/gridData/mc_budg_2011_l/dataSet/CURLIST/record["+j+"]/dept_cd"," ");
								set_refvalue("/root/resData/gridData/mc_budg_2011_l/dataSet/CURLIST/record["+j+"]/rmks","");
								set_refvalue("/root/resData/gridData/mc_budg_2011_l/dataSet/CURLIST/record["+j+"]/dtls_clsf_cd",get_refvalue("/root/reqData/searchData/dtls_clsf_cd1"));
								
								if(datagrid1.rowStatus(j) == "4"){
									datagrid1.rowStatus(j) = "2";
								}
							}
						}else{
							for(j = 1 ;  j < 13 ; j++){
								set_refvalue("/root/resData/gridData/mc_budg_2011_l/dataSet/CURLIST/record["+j+"]/dtls_clsf_cd",get_refvalue("/root/reqData/searchData/dtls_clsf_cd2"));
							}
						}
						
						set_refvalue("/root/reqData/multiUpDate/multiUpdateData", datagrid1.getUpdateData("update"));
						model.send("mc_co_1010i");
						
						if(get_refvalue("/root/resData/MsgData/errData/errcode") != ""){
							send_mc_budg_2011_l();
							return;
						}
					}

					if(submit_completed()){
						btn_select.dispatch("onclick");
						datagrid1.refresh();
						show_message("삭제되었습니다.");
					}						
					else{
						return;	
					}
				}
				
			]]>
		</script>
