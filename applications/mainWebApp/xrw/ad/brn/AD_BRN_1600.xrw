		<caption id="caption11" class="list_cap01" style="left:222px; top:97px; width:80px; height:28px; ">광고주</caption>
		<datagrid id="datagrid1" nodeset="/root/resData/gridData/record" caption="매체코드^일련번호^매체^증감구분^광고주^수정전금액^수정후금액^증감액^작성일자" colsep="^" colwidth="30, 22, 100, 100, 169, 120, 120, 120, 120" extendlastcol="scroll" frozencols="5" mergecellsfixedrows="bycolrec" rowheader="seq" rowsep="|" style="left:9px; top:147px; width:927px; height:595px; ">
			<col ref="medi_cd" visibility="hidden"/>
			<col ref="seq" visibility="hidden"/>
			<col ref="medi_nm"/>
			<col ref="icdc_clsf"/>
			<col ref="dlco_nm" style="text-align:left; word-wrap:none; "/>
			<col ref="upd_prv_amt" format="#,###" style="text-align:right; "/>
			<col ref="upd_aft_amt" format="#,###" style="text-align:right; "/>
			<col ref="icdc_amt" format="(-)#,###" style="text-align:right; "/>
			<col ref="make_dt" format="yyyy-mm-dd"/>
			<script type="javascript" ev:event="ondblclick">
				<![CDATA[

				if(!isDataRegion(datagrid1)) return;
				
				var row = datagrid1.row;
						
				if(row >= datagrid1.fixedRows){
					
					var  medi_cd =	datagrid1.valueMatrix(row, datagrid1.colRef("medi_cd")); //model.getValue("/root/resData/gridData/record[" + row + "]/medi_cd");
					var  seq 	=	datagrid1.valueMatrix(row, datagrid1.colRef("seq"));//model.getValue("/root/resData/gridData/record[" + row + "]/seq");
					//alert(medi_cd+"::"+seq);					
					
					if(instance1.selectSingleNode("/root/tempData/keyData/medi_cd") == null) { 
						model.makeNode("/root/tempData/keyData/medi_cd");
					}
					if(instance1.selectSingleNode("/root/tempData/keyData/seq") == null) { 
						model.makeNode("/root/tempData/keyData/seq");
					}
					
					model1.setValue("/root/tempData/keyData/medi_cd" ,medi_cd);
					model1.setValue("/root/tempData/keyData/seq" ,seq);
					
					window.load("./AD_BRN_1601.xrw","","","","/root/tempData/keyData","/root/reqData/fwdData");
					
				}
				
				]]>
			</script>
		</datagrid>
		<button id="button12" class="btn05" crud="R" style="left:765px; top:40px; width:110px; height:21px; ">
