					return;
				}
				
				var filename = window.fileDialog("save", ",", false, "조정증감사유서", "xls", "Excel File(*.xls)|*.xls");
				if(filename!="")
				{
					datagrid1.saveExcel(filename, "sheetname: 조정증감사유서");
				}
				
				]]>
