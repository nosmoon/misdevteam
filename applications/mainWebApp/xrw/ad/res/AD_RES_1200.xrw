				//광고주일때만 체크함
				if(hndl_clsf == "1"){
					//체크 확인
					if(rcpm01 == "N" && rcpm02 == "N"){
						alert("입금정보는 필수사항입니다.");
						return false;
					}else{
						// 입금시 체크
						if(rcpm01 == "Y"){
							var acct_num = model.getValue("/root/resData/resForm/acct_num");
							var tran_nm = model.getValue("/root/resData/resForm/tran_nm");
							var tran_amt = model.getValue("/root/resData/resForm/tran_amt");
							var tran_dt = model.getValue("/root/resData/resForm/tran_dt");
							var pubc_dt = model.getValue("/root/reqData/searchData/preng_occr_dt");
							
							if(acct_num  == "" ){
								alert("입금 체크시 계좌번호는 필수 입력 사항입니다.");
								return false;
							}
							if(tran_nm  == "" ){
								alert("입금 체크시 입금주는 필수 입력 사항입니다.");
								return false;
							}
							if(tran_amt  == "" ){
								alert("입금 체크시 입금액은 필수 입력 사항입니다.");
								return false;
							}
							if(tran_dt  == "" ){
								alert("입금 체크시 입금일은 필수 입력 사항입니다.");
								return false;
							}		
							if(tran_dt > pubc_dt){
								alert("입금일이 미래일자입니다.게재일자보다 이전 일자로 선택해 주세요.");
								return false;
							}							
							if(advt_emp_nm == ""){
								alert("거래처 담당자명은 필수 입력 사항입니다.");
								return false;
							}
							if(advt_email == ""){
								alert("거래처 이메일은 필수 입력 사항입니다.");
								return false;
							}
							if(advt_email.indexOf("@") < 0) {
								alert("거래처 이메일을 확인해주세요.");
								return false;								
							}
							if(advt_email.indexOf(".") < 0){
								alert("거래처 이메일을 확인해주세요.");
								return false;								
							}
							if(advt_telno == ""){
								alert("거래처 연락처는 필수 입력 사항입니다.");
								return false;
							}							
						}
						//미입금시 체크
						if(rcpm02 == "Y"){
							var tran_schd_dt = model.getValue("/root/resData/resForm/tran_schd_dt");
							var pubc_dt = model.getValue("/root/reqData/searchData/preng_occr_dt");
							
							if(tran_schd_dt  == "" ){
