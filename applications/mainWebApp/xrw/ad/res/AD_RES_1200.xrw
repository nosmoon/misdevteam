								return false;
							}
							if(tran_schd_dt < pubc_dt){
								alert("입금예정일이 과거일자입니다.게재일자보다 이후 일자로 선택해 주세요.");
								return false;
							}
							if(advt_emp_nm == ""){
								alert("거래처 담당자명은 필수 입력 사항입니다.");
								return false;
							}
							if(advt_email == ""){
								alert("거래처 이메일은 필수 입력 사항입니다.");
								return false;
							}
							if(advt_email.indexOf("@") < 0) {
								alert("거래처 이메일을 확인해주세요.");
								return false;								
							}
							if(advt_email.indexOf(".") < 0){
								alert("거래처 이메일을 확인해주세요.");
								return false;								
							}
							if(advt_telno == ""){
								alert("거래처 연락처는 필수 입력 사항입니다.");
								return false;
							}														
						}
					}
				}
				return true;
			}
