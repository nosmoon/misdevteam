							var sum_amt	=	amt + vat;
							gd_evnt.valueMatrix(row,gd_evnt.colRef("tot_amt")) = sum_amt;								
						}
					}
					if(col == gd_evnt.colRef("tot_amt")){
						if(tot_amt != 0 ){
							var advt_fee = Math.round((tot_amt * 10) / 11);
							var advt_fee_vat = Math.round((tot_amt * 1) / 11);							
							gd_evnt.valueMatrix(row,gd_evnt.colRef("amt")) = advt_fee;
							gd_evnt.valueMatrix(row,gd_evnt.colRef("vat")) = advt_fee_vat;
						}
					}					
					if(col == gd_evnt.colRef("fee") || col == gd_evnt.colRef("fee_vat")){
						if(fee != 0 && fee_vat != 0){
							var sum_fee = fee + fee_vat;
							gd_evnt.valueMatrix(row,gd_evnt.colRef("tot_fee")) = sum_fee;
						}
						if(fee != 0 && fee_vat == 0){
							fee_vat		=	Math.round(fee / 10);
							gd_evnt.valueMatrix(row,gd_evnt.colRef("fee_vat")) = fee_vat;
							var sum_fee	=	fee + fee_vat;
							gd_evnt.valueMatrix(row,gd_evnt.colRef("tot_fee")) = sum_fee;								
						}						
					}
					if(col == gd_evnt.colRef("tot_fee")){
						if(tot_fee != 0 ){
							var advt_fee = Math.round((tot_fee * 10) / 11);
							var advt_fee_vat = Math.round((tot_fee * 1) / 11);							
							gd_evnt.valueMatrix(row,gd_evnt.colRef("fee")) = advt_fee;
							gd_evnt.valueMatrix(row,gd_evnt.colRef("fee_vat")) = advt_fee_vat;
						}
					}						
