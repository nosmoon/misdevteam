			 	function on_mouse_down(id){

					var datagrid = document.controls(id);													
					if (datagrid.isCell(event.target)){
						if(datagrid.mouseCol>= datagrid.colRef("sect") && datagrid.mouseCol <= datagrid.colRef("chro_clsf_nm")){
