							var to_dt 		= 	model.getValue(path+"/pubc_todt");
							
							makeNodeValue("/root/oz_data/odiParam/iv_cmpy_cd",model.property("ss_cmpycd"));	//테스트는 고정   model.property("ss_cmpycd")
							makeNodeValue("/root/oz_data/odiParam/iv_medi_chk", medi_chk);
							makeNodeValue("/root/oz_data/odiParam/iv_from_dt", from_dt);
							makeNodeValue("/root/oz_data/odiParam/iv_to_dt", to_dt);
							makeNodeValue("/root/oz_data/odiParam/iv_prt_kind1",chk1);
							makeNodeValue("/root/oz_data/odiParam/iv_prt_kind2",chk2);							
						
							// 레포트뷰어 호출.					
							openOzwin("/root/oz_data");								
