						var chk1 = model.getValue("/root/reqData/case5/prt_kind1");
						var chk2 = model.getValue("/root/reqData/case5/prt_kind2");
						if(chk1 == "" && chk2 == ""){
							alert("출력하고자하는 장표를 선택하세요.");
						}else{
							// ozr경로 및 odi 파일명으로 사용될 노드 및 value셋팅 ( OZR파일경로 .ozr 제거)	
							makeNodeValue("/root/oz_data/ozrName", "ad/SP_AD_DEP_3950_P");
							
							path = "/root/reqData/case"+type;
							var medi_chk 	= 	model.getValue(path+"/proc_clsf");
