		<script type="javascript">
			<![CDATA[
			function goReport(gubun){					
					// 오즈관련 노드 초기화..					
					resetOznode();					
					
					var type,path,medi_cd,medi_cd2,pubc_dt,chro_no,chrg_nm,ck_clsf;//medi_cd = 세션  medi_cd2 = 화면에서 
					
					type = radio1.value;
					/*
					if(model.property("ss_dealmedicd") == "1")
					{
						medi_cd	= 	"100";
					}
					*/
					if(model.property("ss_cmpycd") == "100"){
						if(model.property("ss_dealmedicd") == "2")
						{
							medi_cd 	= 	"200";	
						}	
						else if(model.property("ss_dealmedicd") == "5")
						{
							medi_cd 	= 	"350";	
						}	
						else //if(model.property("ss_dealmedicd") == "3")
						{
							medi_cd 	= 	"100";	
						}					
					}else
					{
						medi_cd = "000";
					}
					if( type == "1" )
					{//입금명세서_신문용
						var chk1,chk2,chk3,chk4,chk5,chk6,chk7,chk8;
						chk1 = model.getValue("/root/reqData/case1/prt_kind1");
						chk2 = model.getValue("/root/reqData/case1/prt_kind2");
						chk3 = model.getValue("/root/reqData/case1/prt_kind3");
						chk4 = model.getValue("/root/reqData/case1/prt_kind4");
						chk5 = model.getValue("/root/reqData/case1/prt_kind5");
						chk6 = model.getValue("/root/reqData/case1/prt_kind6");
						chk7 = model.getValue("/root/reqData/case1/prt_kind7");
						chk8 = model.getValue("/root/reqData/case1/prt_kind8");
						
						if(chk1=="" && chk2=="" && chk3=="" && chk3=="" && chk4=="" && chk5=="" && chk6=="" && chk7=="" && chk8==""){
							alert("출력하고자하는 장표를 선택하세요.");
						}else{
							// ozr경로 및 odi 파일명으로 사용될 노드 및 value셋팅 ( OZR파일경로 .ozr 제거)	
							//SP_AD_DEP_3910_P_신문용
							makeNodeValue("/root/oz_data/ozrName", "ad/SP_AD_DEP_3910_500_P");	
															
							path = "/root/reqData/case"+type;
							
							pubc_dt	=	model.getValue(path+"/rcpm_dt");
							
							makeNodeValue("/root/oz_data/odiParam/iv_cmpy_cd",model.property("ss_cmpycd"));	
							makeNodeValue("/root/oz_data/odiParam/iv_medi_cd", medi_cd);
							makeNodeValue("/root/oz_data/odiParam/iv_pubc_dt", pubc_dt);
							makeNodeValue("/root/oz_data/odiParam/iv_prt_kind1", chk1);
							makeNodeValue("/root/oz_data/odiParam/iv_prt_kind2", chk2);
							makeNodeValue("/root/oz_data/odiParam/iv_prt_kind3", chk3);
							makeNodeValue("/root/oz_data/odiParam/iv_prt_kind4", chk4);
							makeNodeValue("/root/oz_data/odiParam/iv_prt_kind5", chk5);
							makeNodeValue("/root/oz_data/odiParam/iv_prt_kind6", chk6);
							makeNodeValue("/root/oz_data/odiParam/iv_prt_kind7", chk7);
							makeNodeValue("/root/oz_data/odiParam/iv_emp_nm", getProperty("ss_emp_nm"));
							makeNodeValue("/root/oz_data/odiParam/iv_dept_nm", getProperty("ss_deptnm"));
							makeNodeValue("/root/oz_data/odiParam/iv_prt_kind8", chk8);
							
						
							// 레포트뷰어 호출.					
							openOzwin("/root/oz_data");								
						}
					}
					else if( type == "2" )
					{//광고입금일보
						// ozr경로 및 odi 파일명으로 사용될 노드 및 value셋팅 ( OZR파일경로 .ozr 제거)	
						makeNodeValue("/root/oz_data/ozrName", "ad/SP_AD_DEP_3925_P");
						
