						else
						{
							// ozr경로 및 odi 파일명으로 사용될 노드 및 value셋팅 ( OZR파일경로 .ozr 제거)	
							//SP_AD_DEP_3910_P_신문용
							makeNodeValue("/root/oz_data/ozrName", "ad/SP_AD_DEP_3980_P");	
															
							path 		= 	"/root/reqData/case"+type;
							pubc_dt	=	model.getValue(path+"/rcpm_dt");
							
							makeNodeValue("/root/oz_data/odiParam/iv_cmpy_cd",model.property("ss_cmpycd"));	
							makeNodeValue("/root/oz_data/odiParam/iv_medi_cd", medi_cd);
							makeNodeValue("/root/oz_data/odiParam/iv_pubc_dt", pubc_dt);
							makeNodeValue("/root/oz_data/odiParam/iv_prt_kind", chkValue);
//							makeNodeValue("/root/oz_data/odiParam/iv_prt_kind2", chk2);
//							makeNodeValue("/root/oz_data/odiParam/iv_prt_kind3", chk3);
//							makeNodeValue("/root/oz_data/odiParam/iv_prt_kind4", chk4);
//							makeNodeValue("/root/oz_data/odiParam/iv_prt_kind5", chk5);
//							makeNodeValue("/root/oz_data/odiParam/iv_prt_kind6", chk6);
//							makeNodeValue("/root/oz_data/odiParam/iv_prt_kind7", chk7);
//							makeNodeValue("/root/oz_data/odiParam/iv_prt_kind8", chk8);
							makeNodeValue("/root/oz_data/odiParam/iv_emp_nm", getProperty("ss_emp_nm"));
							makeNodeValue("/root/oz_data/odiParam/iv_dept_nm", getProperty("ss_deptnm"));
						
							// 레포트뷰어 호출.					
							openOzwin("/root/oz_data");								
						}
					}
					else
					{
						
						var chk1,chk3,chk4,chk5,chk6,chk7;
						chk1 = model.getValue("/root/reqData/case7/prt_kind1");
						chk3 = model.getValue("/root/reqData/case7/prt_kind3");
						chk4 = model.getValue("/root/reqData/case7/prt_kind4");
						chk5 = model.getValue("/root/reqData/case7/prt_kind5");
						chk6 = model.getValue("/root/reqData/case7/prt_kind6");
						chk7 = model.getValue("/root/reqData/case7/prt_kind7");
						
						if(chk1=="" && chk3=="" && chk3=="" && chk4=="" && chk5=="" && chk6=="" && chk7==""){
							alert("출력하고자하는 장표를 선택하세요.");
						}else{
							// ozr경로 및 odi 파일명으로 사용될 노드 및 value셋팅 ( OZR파일경로 .ozr 제거)	
							//SP_AD_DEP_3910_P_출판용
							makeNodeValue("/root/oz_data/ozrName", "ad/SP_AD_DEP_3910_P2");	
															
							path = "/root/reqData/case"+type;
							
							pubc_dt	=	model.getValue(path+"/rcpm_dt");
							
							makeNodeValue("/root/oz_data/odiParam/iv_cmpy_cd",model.property("ss_cmpycd"));	
							makeNodeValue("/root/oz_data/odiParam/iv_medi_cd", "200");
							makeNodeValue("/root/oz_data/odiParam/iv_pubc_dt", pubc_dt);
							makeNodeValue("/root/oz_data/odiParam/iv_prt_kind1", chk1);
							makeNodeValue("/root/oz_data/odiParam/iv_prt_kind3", chk3);
							makeNodeValue("/root/oz_data/odiParam/iv_prt_kind4", chk4);
