					}
					else if( type == "6" )
					{// 거래처별 채권채무 명세서
							// ozr경로 및 odi 파일명으로 사용될 노드 및 value셋팅 ( OZR파일경로 .ozr 제거)	
							makeNodeValue("/root/oz_data/ozrName", "ad/SP_AD_DEP_3960_P");
															
							path 		= 	"/root/reqData/case"+type;
							pubc_dt	=	model.getValue(path+"/basi_dt");
							chk 		=  	model.getValue(path+"/prt_exp");
							
							makeNodeValue("/root/oz_data/odiParam/iv_cmpy_cd",model.property("ss_cmpycd"));	//테스트는 하드  서버올릴때는 세션값으로   model.property("ss_cmpycd")
