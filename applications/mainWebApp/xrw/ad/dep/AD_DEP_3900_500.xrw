						// 레포트뷰어 호출.					
						openOzwin("/root/oz_data");							
					}
					else if( type == "3" )
					{//관리담당별 입금일람표
						makeNodeValue("/root/oz_data/ozrName", "ad/SP_AD_DEP_3930_P");
						
