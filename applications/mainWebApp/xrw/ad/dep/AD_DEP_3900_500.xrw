						makeNodeValue("/root/oz_data/odiParam/iv_cmpy_cd",model.property("ss_cmpycd"));	//테스트는 하드  서버올릴때는 세션값으로   model.property("ss_cmpycd")
						makeNodeValue("/root/oz_data/odiParam/iv_medi_cd", medi_cd2);
