						cb_medi_cd.select(1);
						cb_medi_cd2.select(1);
						cb_medi_cd3.select(1);
					
						cpt_cover.visible = false;
						cpt_coverNewMedia.visible = true;
						model.toggle("case3");
					}
					else
					{
						model.setValue("/root/reqData/prt_type", "3");
						cb_medi_cd.refresh();
						cb_medi_cd.select(0);
						cb_medi_cd2.refresh();
						cb_medi_cd2.select(0);
						cb_medi_cd3.refresh();
						cb_medi_cd3.select(0);
						
						cpt_cover.visible = false;
