				function goReport( index )
				{
					var print_clsf = model.getValue("/root/reqData/print_clsf");
					var print_clsf2 = model.getValue("/root/reqData/print_clsf2");
					var emp_nm = "";
					var dept_nm = "";
					// 오즈관련 노드 초기화..
					resetOznode();
					// ozr경로 및 odi 파일명으로 사용될 노드 및 value셋팅 ( OZR파일경로 .ozr 제거)
					if( index == "1" )
					{
						makeNodeValue("/root/oz_data/ozrName", "ad/SP_AD_BAS_3710_P");
//						makeNodeValue("/root/oz_data/conParam/uv_summary");
					}
					else if( index == "2" )
					{
						if(print_clsf == "01"){
							makeNodeValue("/root/oz_data/ozrName", "ad/SP_AD_BAS_3711_P");
							emp_nm = getProperty("ss_emp_nm");
							dept_nm = getProperty("ss_deptnm");
						}else if(print_clsf == "02"){//팀
							makeNodeValue("/root/oz_data/ozrName", "ad/SP_AD_BAS_3713_P");
							emp_nm = "";
							dept_nm = model.getValue("/root/reqData/print_clsf2");
							if(dept_nm == ""){
								alert("팀을 선택해 주세요");
								return;
							}
						}else if(print_clsf == "03"){//담당
							makeNodeValue("/root/oz_data/ozrName", "ad/SP_AD_BAS_3713_P");
							emp_nm = model.getValue("/root/reqData/print_clsf2");
							dept_nm = "";
							if(emp_nm == ""){
								alert("담당을 선택해 주세요");
								return;
							}
						}else{
							alert("구분값이 없습니다.");
							return;							
						}
					}
					else
					{
						alert("출력하실 장표를 선택해 주세요.");
						return;
					}
					
					var pubc_dt      	= model.getValue( "/root/reqData/print_dt" );
					if( pubc_dt == "" )
					{
						commAlert("게재일자가 없습니다.");
						return;
					}				
			
					makeNodeValue("/root/oz_data/odiParam/iv_cmpy_cd"		, model.property("ss_cmpycd"));					
					makeNodeValue("/root/oz_data/odiParam/iv_medi_cd"		, "110");					
					makeNodeValue("/root/oz_data/odiParam/iv_pubc_dt"		, pubc_dt);	
					makeNodeValue("/root/oz_data/odiParam/iv_emp_nm"		, emp_nm);
					makeNodeValue("/root/oz_data/odiParam/iv_dept_nm"		, dept_nm);	
					// 레포트뷰어 호출.					
					openOzwin("/root/oz_data");	
				}
			]]>
		</script>
