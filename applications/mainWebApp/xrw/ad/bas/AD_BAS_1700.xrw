		    function addCommaStr(str) {
		        var num = "";
		        var sign = "";
		
		        if (str.charAt(0) == "+" || str.charAt(0) == "-") {
		            sign = str.charAt(0);
		            str = str.substr(1);
		        }
		
		        var index = str.indexOf('.');
		
		        if (index != -1) {
		            num = str.substr(index);
		        } else {
		            index = str.length;
		        }
		
		        for (var i = index - 3; i > 0; ) {
		            num = ',' + str.substr(i, 3) + num;
		
		            index = i;
		            i -= 3;
		        }
		
		        num = sign + str.substr(0, index) + num;
		
		        return	num;
		    }
		    
