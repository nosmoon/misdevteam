		<button id="button7" class="btn02" crud="C" style="left:718px; top:40px; width:50px; height:21px; ">
			<caption>저장</caption>
			<script type="javascript" ev:event="onclick">
				<![CDATA[
				
				if(!isDataGridUpdated(datagrid1) &&
			 	   !isDataGridUpdated(datagrid2) &&
			 	   !isDataGridUpdated(datagrid3) &&
			 	   !isDataGridUpdated(datagrid4)){
			 	   	alert("저장할 내역이 없습니다.");
			 	   	return;
		 	   	}				
									
				if(windowConfirm("저장하시겠습니까?")){				
					
					if(isDataGridUpdated(datagrid1)){			
						makeNodeValue("/root/reqData/multiUpdateData1", datagrid1.getUpdateData());
						model.send("ad_bas_3221_a");
					}
					if(!submit_completed())	return;
					
					if(isDataGridUpdated(datagrid2)){			
						makeNodeValue("/root/reqData/multiUpdateData2", datagrid2.getUpdateData());
						model.send("ad_bas_3222_a");
					}
					if(!submit_completed())	return;
					
					if(isDataGridUpdated(datagrid3)){			
						makeNodeValue("/root/reqData/multiUpdateData3", datagrid3.getUpdateData());
						model.send("ad_bas_3223_a");
					}
					if(!submit_completed())	return;
					
					if(isDataGridUpdated(datagrid4)){			
						makeNodeValue("/root/reqData/multiUpdateData4", datagrid4.getUpdateData());
						model.send("ad_bas_3224_a");
					}
					if(!submit_completed())	return;
					
					if(submit_completed()){
						loadData();
					}
				}
				]]>
			</script>
		</button>
