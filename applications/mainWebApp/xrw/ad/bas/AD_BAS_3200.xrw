			<col ref="advt_ocpy_rate" type="input" format="#,###.9" style="text-align:right; "/>
			<col ref="seq" visibility="hidden"/>
			<script type="javascript" ev:event="xforms-value-changed">
				<![CDATA[
					
					if( datagrid1.colRef("c0101_pcnt")  ||
						datagrid1.colRef("c0102_pcnt")  ||
						datagrid1.colRef("c0103_pcnt")  ||
						datagrid1.colRef("c0104_pcnt")  ||
						datagrid1.colRef("c0105_pcnt")  ||
						datagrid1.colRef("c0106_pcnt")  ||
						datagrid1.colRef("c0107_pcnt")  ||
						datagrid1.colRef("c0108_pcnt")  ||
						datagrid1.colRef("c0109_pcnt") )
					{
						var c0101_pcnt = Number(datagrid1.valueMatrix(datagrid1.row, datagrid1.colRef("c0101_pcnt")));
						var c0102_pcnt = Number(datagrid1.valueMatrix(datagrid1.row, datagrid1.colRef("c0102_pcnt")));
						var c0103_pcnt = Number(datagrid1.valueMatrix(datagrid1.row, datagrid1.colRef("c0103_pcnt")));
						var c0104_pcnt = Number(datagrid1.valueMatrix(datagrid1.row, datagrid1.colRef("c0104_pcnt")));
						var c0105_pcnt = Number(datagrid1.valueMatrix(datagrid1.row, datagrid1.colRef("c0105_pcnt")));
						var c0106_pcnt = Number(datagrid1.valueMatrix(datagrid1.row, datagrid1.colRef("c0106_pcnt")));
						var c0107_pcnt = Number(datagrid1.valueMatrix(datagrid1.row, datagrid1.colRef("c0107_pcnt")));
						var c0108_pcnt = Number(datagrid1.valueMatrix(datagrid1.row, datagrid1.colRef("c0108_pcnt")));
						var c0109_pcnt = Number(datagrid1.valueMatrix(datagrid1.row, datagrid1.colRef("c0109_pcnt")));
						
						var tot_pcnt		=	c0101_pcnt +	c0102_pcnt + c0103_pcnt + c0104_pcnt + c0105_pcnt + c0106_pcnt + c0107_pcnt + c0108_pcnt + c0109_pcnt;
						
						datagrid1.valueMatrix(datagrid1.row, datagrid1.colRef("tot_pcnt")) = tot_pcnt; 
						
						var dataIndex 	= 	getSelectedRecordIndex(datagrid1);
						var tot_pcnt		=	Number(model.getValue("/root/resData/gridData/curlist1["+dataIndex+"]/tot_pcnt"));
						var tot_cell		=	Number(model.getValue("/root/resData/gridData/curlist1["+dataIndex+"]/tot_cell"));
						
						var 	advt_ocpy_rate	=	fn_round(tot_cell * 100 / (12*24*tot_pcnt),2);
						
						model.setValue("/root/resData/gridData/curlist1["+dataIndex+"]/advt_ocpy_rate", advt_ocpy_rate);
						datagrid1.refresh();
					}
				]]>
			</script>
