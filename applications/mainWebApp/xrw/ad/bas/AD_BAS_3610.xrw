				function chkKey(outMsg)
				{
					if( oup_dlco_no.value != oup_dlco_no_pre.value )
					{
						if( outMsg == true )
						{
							alert("거래처정보가 변경되었습니다.\n해당 인물을 재조회 하세요.");
							return 1;
						}
						else
						{
							return 1;
						}
					}
					
					if( oup_cust_seq.value != oup_cust_seq_pre.value )
					{
						if( outMsg == true )
						{
							alert("인물정보가 변경되었습니다.\n해당 인물을 재조회 하세요.");
							return 2;
						}
						else
						{
							return 2;
						}
					}
					
					return 0;
				}
				
