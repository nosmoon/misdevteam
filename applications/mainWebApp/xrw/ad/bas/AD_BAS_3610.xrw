				function loadDlcoInfoSet()
				{
					//거래처 조회 후 로딩할 내용들.
					var medi_clsf 	= model.getValue("/root/resData/resForm/medi_clsf");
					var dlco_no	= model.getValue("/root/resData/resForm/dlco_no");
					var zip_1		= "";
					var zip_2		= "";
					var addr		= "";
					var dtls_addr	= "";
					var slcrg_pers	= "";
					var slcrg_pers_nm = "";
					var mchrg_pers	= "";
					var mchrg_pers_nm = "";
					
					if( medi_clsf != "" && dlco_no != "" )
					{
						model.setValue("/root/reqData/medi_clsf"		,medi_clsf);
						model.setValue("/root/reqData/dlco_no"		,dlco_no);

						model.send("ad_bas_3612_s");
						if( submit_completed() )
						{
							zip_1		=	model.getValue("/root/dlcoData/zip1");
							zip_2		=	model.getValue("/root/dlcoData/zip2");
							addr		=	model.getValue("/root/dlcoData/addr");
							dtls_addr	=	model.getValue("/root/dlcoData/dtls_addr");
							slcrg_pers		=	model.getValue("/root/dlcoData/slcrg_pers");
							slcrg_pers_nm	=	model.getValue("/root/dlcoData/slcrg_pers_nm");
							mchrg_pers	=	model.getValue("/root/dlcoData/mchrg_pers");
							mchrg_pers_nm=	model.getValue("/root/dlcoData/mchrg_pers_nm");
							
							model.setValue("/root/resData/resForm/cmpy_zip1"		, zip_1);
							model.setValue("/root/resData/resForm/cmpy_zip2"		, zip_2);
							model.setValue("/root/resData/resForm/cmpy_addr"		, addr);
							model.setValue("/root/resData/resForm/cmpy_dtls_addr"	, dtls_addr);
							model.setValue("/root/resData/resForm/slcrg_pers"		, slcrg_pers);
							model.setValue("/root/resData/resForm/slcrg_pers_nm"	, slcrg_pers_nm);
