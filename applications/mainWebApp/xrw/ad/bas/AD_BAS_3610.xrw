				function initPage()
				{
					if( model.property(KEY_MEDI_CLSF) != "" && model.property(KEY_DLCO_NO) != "" && model.property(KEY_SEQ) != "" )
					{
						model.setValue("/root/resData/resForm/medi_clsf"		,model.property(KEY_MEDI_CLSF));
						model.setValue("/root/resData/resForm/dlco_no"		,model.property(KEY_DLCO_NO));
						model.setValue("/root/resData/resForm/cust_seq"		,model.property(KEY_SEQ));
						
						model.property(KEY_MEDI_CLSF)	= "";
						model.property(KEY_DLCO_NO) 	= "";
						model.property(KEY_SEQ)		= "";
						loadData();
					}
					else
					{
						//행추가
						for( var i=dg_school.rows; i<4; i++)		addRow(dg_school);
						for( var i=dg_career.rows; i<4; i++)		addRow(dg_career);
						EnabledPeopleDB();
						model.refresh();
					}
				}
				
				function loadData()
				{
					var medi_clsf 	= model.getValue("/root/resData/resForm/medi_clsf");
					var dlco_no	= model.getValue("/root/resData/resForm/dlco_no");
					var cust_seq	= model.getValue("/root/resData/resForm/cust_seq");
					
					if( medi_clsf != "" && dlco_no != "" && cust_seq != "" )
					{
						model.setValue("/root/reqData/medi_clsf"		,medi_clsf);
						model.setValue("/root/reqData/dlco_no"		,dlco_no);
						model.setValue("/root/reqData/cust_seq"		,cust_seq);
						model.send("ad_bas_3611_s");
						if( submit_completed() )
						{
							loadDataSet();
						}
						else
						{
							model.reset();
							initPage();
						}
					}
					else
					{
//						alert("조회할 대상을 확인해주세요.");
						model.reset();
						initPage();
						return false;						
					}
				}
				
