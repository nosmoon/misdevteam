				// 공통그리드 타이틀 설정
				function convertGeneralGridTitleName( index, datagrid )
				{
					var i = 0;
					var titleCaption	=	datagrid.attribute("caption").split("^");
					var todt		=	model.getValue("/root/reqData/todt");
					// 변경항목					
					var titleName 	= 	TAB_NAME[i].substr(0, TAB_NAME[i].length-2) + "명";
					for( i=0; i<DG_TITLE_POS.length; i++ )
					{
						titleCaption[DG_TITLE_POS[i]] = titleName;
					}
					
					// 조회년 변경
					if( todt.length == 8 )
					{
						todt = todt.substr(0,4);
						for( i=0; i<DG_YEAR1_POS.length; i++ )
						{
							titleCaption[DG_YEAR1_POS[i]] = todt + "년";
						}
						
						todt -= 1;
						for( i=0; i<DG_YEAR2_POS.length; i++ )
						{
							titleCaption[DG_YEAR2_POS[i]] = todt + "년";
						}
						
						todt -= 1;
						for( i=0; i<DG_YEAR3_POS.length; i++ )
						{
							titleCaption[DG_YEAR3_POS[i]] = todt + "년";
						}
					}
					
					// 변경사항 적용
					var convertCaption = "";
					for( i=0; i<titleCaption.length; i++ )
					{
						convertCaption += titleCaption[i];
						if( i < titleCaption.length-1 )	convertCaption += "^";
					}
					
					datagrid.caption = convertCaption;
				}
				
