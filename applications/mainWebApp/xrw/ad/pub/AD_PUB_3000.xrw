				</item>
			</choices>
			<script type="javascript" ev:event="xforms-value-changed">
				<![CDATA[
					var curDate	= getCurrentDate();
					var curYear	= "";
					var frdt		= "";
					var todt		= "";
								
					if( curDate.length != 8 )		return;
					else						curYear = curDate.substr(0,4);
					
					//선택한 콤보박스 값
					var dt_clsf = model.getValue("/root/reqData/dt_clsf"); 
					switch( dt_clsf )
					{
					case "01":
						frdt	= curYear + "0101";
						todt	= curYear + "0331";
					break;
					case "02":
						frdt	= curYear + "0401";
						todt	= curYear + "0630";
					break;
					case "03":
						frdt	= curYear + "0701";
						todt	= curYear + "0930";
					break;
					case "04":	//분기
						frdt	= curYear + "1001";
						todt	= curYear + "1231";
					break;
					case "05":	//반기
						frdt	= curYear + "0101";
						todt	= curYear + "0630";
					break;
					case "06":	//연간
						frdt	= curYear + "0101";
						todt	= curDate;
					break;
					default:	return;
					break;
					}
					
					model.setValue("/root/reqData/frdt"	, frdt);
					model.setValue("/root/reqData/todt"	, todt);
				
					inp_frdt.refresh();
					inp_todt.refresh();
					convertAllGridTitleName();
				]]>
			</script>
