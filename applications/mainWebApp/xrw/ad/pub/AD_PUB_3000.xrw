				// 초기화
				function initPage()
				{
					// 게재기간 Default 현재일자(연간)
					var curDate = dateAdd("d", 0, getCurrentDate());
					if( curDate != "" )
					{
						model.setValue("/root/reqData/frdt"	, curDate.substr(0,4)+"0101");
						model.setValue("/root/reqData/todt"	, curDate);
					}
					
					var dealmedicd =  getProperty("ss_dealmedicd");
					if( dealmedicd == "5" )		cmb_medi_cd.value = "350";
					else						cmb_medi_cd.value = "110";
					
					cmb_hndl_clsf.value = "1";
//					cmb_medi_cd.select(1);
//					cmb_hndl_clsf.select(1);
					
					convertAllGridTitleName();
					
					model.refresh();
				}
				
