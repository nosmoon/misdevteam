		</button>
		<button id="btnIns" class="btn02" crud="C" style="left:715px; top:40px; width:52px; height:21px; ">
			<caption>저장</caption>
			<script type="javascript" ev:event="onclick">
				<![CDATA[
				var val = window.alert("저장하시겠습니까?", "확인", 1) ;
				// 1은 확인 2는 취소
				if ( val == 2 ){
					return;
				}
				
				var acwr_reg_seq = model.getValue("/root/reqData/searchData/acwr_reg_seq");
				if(acwr_reg_seq == ""){
					alert("승낙서번호를 입력하세요.");
					return;
				}
				var work_clsf = model.getValue("/root/resData/resForm/work_clsf");
				if(work_clsf == ""){
					alert("작업구분을 입력하세요.");
					return;
				}
				
				
				var flag = model.getValue("/root/resData/resForm/flag");
				
				if(flag == "U"){
					model.setValue("/root/resData/resForm/flag", "U");
				}else{
					model.setValue("/root/resData/resForm/flag", "I");
				}					
				
				
				
				model.copyNode("/root/reqData/reqForm", "/root/resData/resForm");
				
				model.makeNode("/root/reqData/reqForm/acwr_reg_dt");
				model.makeNode("/root/reqData/reqForm/acwr_reg_seq");
				model.setValue("/root/reqData/reqForm/acwr_reg_dt", model.getValue("/root/reqData/searchData/acwr_reg_dt"));
				model.setValue("/root/reqData/reqForm/acwr_reg_seq", acwr_reg_seq);
				model.send("is_agr_2715_a");
				
				if( submit_completed() ){
					model.setValue("/root/reqData/searchData/make_ask_reg_dt", model.getValue("/root/msgData/make_ask_reg_dt"));
					model.setValue("/root/reqData/searchData/make_ask_reg_seq", model.getValue("/root/msgData/make_ask_reg_seq"));
					
					model.send("is_agr_2710_s");
				}
				
				]]>
			</script>
		</button>
