		<button id="btnQry" class="btn02" crud="R" style="left:658px; top:40px; width:52px; height:21px; ">
			<caption>조회</caption>
			<script type="javascript" ev:event="onclick">
				<![CDATA[
				var make_ask_reg_seq = model.getValue("/root/reqData/searchData/make_ask_reg_seq");
				var acwr_reg_seq = model.getValue("/root/reqData/searchData/acwr_reg_seq");
				if(make_ask_reg_seq == "" && acwr_reg_seq == ""){
					alert("제작의뢰서번호 또는 승낙서번호를 입력하세요.");
					return;
				}
				
				model.send("is_agr_2710_s");
				
				model.setValue("/root/reqData/searchData/make_ask_reg_dt", model.getValue("/root/resData/resForm/make_ask_reg_dt"));
				model.setValue("/root/reqData/searchData/make_ask_reg_seq", model.getValue("/root/resData/resForm/make_ask_reg_seq"));
				
				model.setValue("/root/reqData/searchData/acwr_reg_dt", model.getValue("/root/resData/resForm/acwr_reg_dt"));
