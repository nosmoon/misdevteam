			/**datagrid3의 공급가액의 합계,세액, 합계**/
			function setAmtSum(){
				var amtSum = 0;
				for(i=1; i<datagrid3.rows; i++){
					amtSum += eval(datagrid3.valueMatrix(i,datagrid3.colRef("a_tgt_suply_amt")));
				}
				
				
				
				
				model.setValue("/root/resData/resForm/suply_amt", amtSum);  /**공급가액**/
				model.setValue("/root/resData/resForm/vat_amt",  Math.floor(amtSum * 0.1));	/**새액**/
				model.setValue("/root/resData/resForm/tot_amt", (amtSum+Math.floor(amtSum * 0.1)));	/**합계**/
				
				model.refresh();
			}
			
			
			
			
			
			
			
	
			
