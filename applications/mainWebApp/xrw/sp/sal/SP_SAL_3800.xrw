		<script type="javascript">
			<![CDATA[
						
			/**거래처 팝업**/
			function setDlcoInfo(menu_id, menu_nm){
				model.setValue("/root/reqData/searchData/dlco_clsf_cd_seq", menu_id);
				model.setValue("/root/reqData/searchData/dlco_nm", menu_nm);

				model.refresh();	
			}
			
			/**공급받는자  등록번호(거래처) 팝업**/
			function setDlcoInfo2(sel_row){				
				/**등록번호, 법인명, 상호(법인명 풀명), 성명, 사업장주소, 업태, 종목**/

				var dlco_clsf = datagrid1.valueMatrix(sel_row,datagrid1.colRef("dlco_clsf"));
				var dlco_cd = datagrid1.valueMatrix(sel_row,datagrid1.colRef("dlco_cd"));
				var dlco_seq = datagrid1.valueMatrix(sel_row,datagrid1.colRef("dlco_seq"));
				
				var dlco_clsf_cd_seq = dlco_clsf+dlco_cd+dlco_seq;

				if(dlco_clsf == "" || sel_row < 1){
					return;
				}
				model.setValue("/root/resData/resForm/dlco_clsf_cd_seq", dlco_clsf_cd_seq);
				
				model.send("sp_sal_3830_s");
				
				var ern = model.getValue("/root/tempData/ern");					/**등록번호**/
				var dlco_abrv_nm = model.getValue("/root/tempData/dlco_abrv_nm");	/**법인명약어**/
				var dlco_nm = model.getValue("/root/tempData/dlco_nm");			/*상호**/				
				var presi_nm = model.getValue("/root/tempData/presi_nm");			/*성명**/
				var addr = model.getValue("/root/tempData/addr");					/*사업장주소**/
				var bizcond = model.getValue("/root/tempData/bizcond");				/*업태**/
				var item = model.getValue("/root/tempData/item");					/*종목**/
				
				model.setValue("/root/resData/resForm/demand_dlco_no", dlco_clsf_cd_seq);
				model.setValue("/root/resData/resForm/demand_ern", ern);
				model.setValue("/root/resData/resForm/demand_dlco_nm", dlco_nm);
				model.setValue("/root/resData/resForm/demand_presi_nm", presi_nm);
				model.setValue("/root/resData/resForm/demand_addr", addr);
				model.setValue("/root/resData/resForm/demand_bizcond", bizcond);
				model.setValue("/root/resData/resForm/demand_item", item);
				
				model.refresh();	
			}
			
			
			/**공급받는자  등록번호(거래처) 팝업**/
			function resetDlcoInfo(){				
				/**등록번호, 법인명, 상호(법인명 풀명), 성명, 사업장주소, 업태, 종목**/
				
				var num = datagrid3.rows;
				
				if( num == 1){
					model.setValue("/root/resData/resForm/demand_dlco_no", "");
					model.setValue("/root/resData/resForm/demand_ern", "");
					model.setValue("/root/resData/resForm/demand_dlco_nm", "");
					model.setValue("/root/resData/resForm/demand_presi_nm", "");
					model.setValue("/root/resData/resForm/demand_addr", "");
					model.setValue("/root/resData/resForm/demand_bizcond", "");
					model.setValue("/root/resData/resForm/demand_item", "");
				}
				
				
				
				model.refresh();	
			}
			
			
			
			/**거래처 담당자 팝업**/
			function setEmailInfo(email){
				model.setValue("/root/resData/resForm/email_id", email);
				
				model.refresh();
			}
			
			/**거래처 팝업**/
			function setDlcoInfo3(menu_id, menu_nm){
				model.setValue("/root/reqData/searchData2/dlco_clsf_cd_seq", menu_id);
				model.setValue("/root/reqData/searchData2/dlco_nm", menu_nm);

				model.refresh();	
			}
			
			/**datagrid3의 공급가액의 합계,세액, 합계**/
			function setAmtSum(){
				var suply_amtSum = 0;
				var vat_amtSum = 0;
				var tot_rvord_amtSum = 0;
				
				for(i=1; i<datagrid3.rows; i++){
					suply_amtSum += eval(datagrid3.valueMatrix(i,datagrid3.colRef("a_suply_amt")));			/**공급가액**/
					vat_amtSum += eval(datagrid3.valueMatrix(i,datagrid3.colRef("a_vat_amt")));				/**부가세액**/
					tot_rvord_amtSum += eval(datagrid3.valueMatrix(i,datagrid3.colRef("a_tot_rvord_amt")));		/**합계**/
				}
				
				
				
				
				model.setValue("/root/resData/resForm/suply_amt", suply_amtSum);  /**공급가액**/
				model.setValue("/root/resData/resForm/vat_amt",  vat_amtSum);	/**새액**/
				model.setValue("/root/resData/resForm/tot_amt", tot_rvord_amtSum);	/**합계**/
				
				model.refresh();
			}
			
			
			
			
			
			
			
	
			
			
			]]>
		</script>
