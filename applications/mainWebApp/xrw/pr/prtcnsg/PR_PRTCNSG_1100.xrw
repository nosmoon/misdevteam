							var total = 0;
							for(var i=0 ; i <= 7 ; i++){
								if(datagrid4.valueMatrix(1, i).length >= 1){
									total = parseInt(total) + parseInt(datagrid4.valueMatrix(1, i));
								}
							}
							model1.setValue("/root/resData/curlist4/record/total", total);
							datagrid4.refresh();
						}
					}
					
