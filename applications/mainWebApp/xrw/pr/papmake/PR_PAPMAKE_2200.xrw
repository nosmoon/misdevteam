<?xml version="1.0" encoding="EUC-KR"?>
<?xml-stylesheet href="../../../css/style_sheet.css" type="text/css" ?>
<xhtml:html xmlns:ex="http://www.comsquare.co.kr/xforms/excel" xmlns:xlink="http://www.w3.org/1999/xlink" xmlns:xhtml="http://www.w3.org/1999/xhtml" xmlns:my="http://www.comsquare.co.kr/example" xmlns:ms="urn:schemas-microsoft-com:xslt" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns="http://www.w3.org/2002/01/xforms" xmlns:ev="http://www.w3.org/2001/xml-events" xmlns:cs="http://www.comsquare.co.kr/xforms">
	<xhtml:head>
		<xhtml:title>Untitle</xhtml:title>
		<script type="javascript" src="/js/CIISComm.js"/>
		<script type="javascript" src="/js/TFCommon.js"/>
		<model id="model1">
			<instance id="instance1">
				<root xmlns="">
					<initData>
						<!-- 화면 로딩시의 초기 데이터를 담당 -->
						<session/>
						<!-- 서버의 세션 정보를 담당한다 -->
						<comboSet>
							<!-- 선택계열의 정보를 담당 -->
							<fac_clsf>
								<item/>
							</fac_clsf>
						</comboSet>
						<!-- 콤보리스트와 같은 선택게열의 데이터는 <item>을 반복함으로써 list를 구성한다. -->
						<pageInfo>
							<!-- 해당 화면의 필요한 기타 정보들을 추가운영 할 수 있다 -->
						</pageInfo>
						<!-- 해당 화면의 필요한 기타 정보들을 추가운영 할 수 있다 -->
					</initData>
					<reqData>
						<!-- 전달하고자 하는 데이터를 담당 ( submit 함수에서 처리) -->
						<!-- 검색조건 처리 -->
						<!-- formData 팝업,화면전환시 data이동시 -->
						<!-- MultiUpDate유형 사용시 처리 -->
						<searchData>
							<clos_yymm/>
						</searchData>
						<fwdData>
						</fwdData>
						<setlData>
						</setlData>
						<reqForm>
							<frdt/>
							<todt/>
							<multiUpData/>
						</reqForm>
					</reqData>
					<resData>
						<!-- 전달 받은 데이터를 담당 ( submit의 결과가 담김) -->
						<!-- 전달 받은 formData를 담당 ( submit의 결과가 담김) -->
						<!-- 전달 받은 데이터를 가공할 임시저장소 ( submit의 결과가 담김) -->
						<!-- 전달 받은 errorCode를 담당 ( submit의 결과가 담김) -->
						<!-- 전달 받은 errorMsg를 담당 ( submit의 결과가 담김) -->
						<gridData>
							<!-- 전달 받은 grid데이터를 담당 ( submit의 결과가 담김) -->
							<record>
								<!-- datagrid1 -->
							</record>
							<sub/>
							<!-- datagrid2 -->
							<detail/>
							<!-- datagrid3 -->
						</gridData>
						<resForm>
						</resForm>
						<msg/>
					</resData>
					<tempData/>
				</root>
			</instance>
			<script type="javascript">
				<![CDATA[
						
				]]>
			</script>
			<script type="javascript" ev:event="xforms-ready">
				<![CDATA[
					model.removenode("/root/resData/gridData/record");
					model.setValue("/root/reqData/searchData/clos_yymm", getDate("yyyymm"));
					
					model.send("pr_papmake_2202");
					model.copyNode("/root/initData/comboSet/fac_clsf", "/root/tempData/fac_clsf");
					
					model1.refresh();
				]]>
			</script>
			<submission id="pr_papmake_2200" action="/mt/papmake/2200" mediatype="application/x-www-form-urlencoded" method="post" ref="/root/reqData/searchData" resultref="/root/tempData"/>
			<submission id="pr_papmake_2201" action="/mt/papmake/2201" mediatype="application/x-www-form-urlencoded" method="post" ref="/root/reqData/reqForm" resultref="/root/tempData"/>
			<submission id="pr_papmake_2202" action="/mt/papmake/2202" mediatype="application/x-www-form-urlencoded" method="post" ref="/root/reqData/reqForm" resultref="/root/tempData"/>
		</model>
	</xhtml:head>
	<xhtml:body class="pop640" pagewidth="640" pageheight="480" guideline="2,34;2,39;2,40;2,49;2,69;2,79;2,80;2,409;1,936;1,927;1,926;" style="font-family:굴림; ">
		<caption id="caption1" class="tit01" style="left:15px; top:10px; width:620px; height:27px; ">PM실월간보고 PS판/필름</caption>
		<group id="group_scr" class="gro_sel" style="left:9px; top:78px; width:620px; height:27px; ">
			<caption id="caption2" class="list_cap01" style="left:0px; top:0px; width:100px; height:26px; text-align:center; ">마감년월</caption>
			<caption id="caption6" class="list_cap02" style="left:99px; top:0px; width:521px; height:26px; text-align:center; "/>
			<input id="input1" ref="/root/reqData/searchData/clos_yymm" inputtype="date" format="yyyy-mm" style="left:105px; top:4px; width:100px; height:18px; "/>
		</group>
		<group id="group_btnG2" class="gro_btn" style="left:10px; top:115px; width:620px; height:294px; ">
			<datagrid id="datagrid1" nodeset="/root/resData/gridData/record" caption="마감년월^공장^PS판 로스율^필름 로스율" colsep="#" colwidth="100, 110, 110, 110" mergecellsfixedrows="bycolrec" rowheader="update" rowsep="|" selectionmode="free" style="left:0px; top:20px; width:455px; height:274px; ">
				<col ref="clos_yymm" type="inputdate" format="yyyy-mm"/>
				<col editmode="inputsearch" ref="fac_clsf" type="combo" appearance="minimal">
					<choices>
						<itemset nodeset="/root/initData/comboSet/fac_clsf/item">
							<label ref="label"/>
							<value ref="value"/>
						</itemset>
					</choices>
				</col>
				<col ref="ps_loss_rate" type="input" format="#,###.###"/>
				<col ref="film_loss_rate" type="input" format="#,###.###"/>
			</datagrid>
			<button id="button9" class="btn_grid03" style="left:345px; top:0px; width:52px; height:17px; ">
				<caption>행입력</caption>
				<script type="javascript" ev:event="onclick">
					<![CDATA[
						datagrid1.addRow(true, true);
						
						var iSelectedRow = 0;
						iSelectedRow = datagrid1.selectedRow(0);
						//alert(iSelectedRow);
						model1.setValue("/root/resData/gridData/record["+iSelectedRow+"]/clos_yymm", model1.getValue("/root/reqData/searchData/clos_yymm"));
	 					]]>
				</script>
			</button>
			<button id="button10" class="btn_grid03" style="left:400px; top:0px; width:52px; height:17px; ">
				<caption>행삭제</caption>
				<script type="javascript" ev:event="onclick">
					<![CDATA[
						var iSelectedRow = 0;
						var sDelGubun = "";
						
						iSelectedRow = datagrid1.selectedRow(0);
						
						sDelGubun = model1.getValue("/root/resData/gridData/record["+iSelectedRow+"]/rowCnt");
						
						if(sDelGubun != "") {
							datagrid1.addStatus(datagrid1.row, "delete");
						} else {
							datagrid1.deleteRow(iSelectedRow,true);
						}
						
					]]>
				</script>
			</button>
		</group>
		<group id="group_1" class="gro_sel" style="left:10px; top:45px; width:620px; height:22px; ">
			<button id="btnA4" class="btn02" style="left:565px; top:0px; width:52px; height:21px; ">
				<caption>종료</caption>
				<script type="javascript" ev:event="onclick">
					<![CDATA[
				      window.close();
				     ]]>
				</script>
			</button>
			<button id="button2" class="btn02" style="left:454px; top:0px; width:52px; height:21px; ">
				<caption>조회</caption>
				<script type="javascript" ev:event="onclick">
					<![CDATA[
						//model.removenode("/root/resData/gridData/record");
						//datagrid1.refresh();
						
						model.send("pr_papmake_2200");
						model.copyNode("/root/resData/gridData", "/root/tempData/CURLIST");
						datagrid1.refresh();
					]]>
				</script>
			</button>
			<button id="button3" class="btn02" style="left:395px; top:0px; width:52px; height:21px; ">
				<caption>초기화</caption>
				<script type="javascript" ev:event="onclick">
					<![CDATA[
						model1.reset();
					]]>
				</script>
			</button>
			<button id="button1" class="btn03" style="left:510px; top:0px; width:52px; height:21px; ">
				<caption>저장</caption>
				<script type="javascript" ev:event="onclick">
					<![CDATA[
						model.setValue("/root/reqData/reqForm/multiUpData", datagrid1.getUpdateData());
						model.send("pr_papmake_2201");
						
						if(submit_completed()) {
							var clos_yymm 	= model.getValue("/root/reqData/searchData/clos_yymm");
							
							model.reset();
							model.setValue("/root/reqData/searchData/clos_yymm", clos_yymm);
							
							button2.dispatch("onclick");
						}
						
					]]>
				</script>
			</button>
		</group>
	</xhtml:body>
</xhtml:html>
