		<script type="javascript">
			<![CDATA[
				var	_oth_clsf = "1";
				
				function init_controls(){
					set_default(datagrid1);							//datagrid
				}	
				
				function nullCheck(){
					if (!check_reqItem(srch_comp_dt,"trim","focus","기산일은(는) 필수입력 항목입니다.")) { return false; }
					return true;
				}
				function send_fc_func_6510_l(){
					if(nullCheck()){
						model1.send("fc_func_6510_l");
						if(model1.getValue("/root/resData/gridData/fc_func_6510_l/dataSet/CURLIST/totalcnt") == 0){
								model.alert("검색결과가 없습니다.");
								return false;
						}
//						var grow  = datagrid1.rows;
//						for (i=0; i<10; i++){
//							datagrid1.addRow(false);
//							datagrid1.removeStatus(grow+i,"insert");
//						}
//						datagrid1.removeStatus(datagrid1.rows,"insert");
//						datagrid1.refresh();
					}
				}
				function send_fc_func_6511_i(){
					if(nullCheck()){
						var tcnt = model1.getValue("/root/resData/gridData/fc_func_6510_l/dataSet/CURLIST/totalcnt");
						if(tcnt==0){
							alert("처리할 자료가 없습니다.");
							return false;
						}
						var jj = 0;
						for(i=1;i<datagrid1.rows;i++){
							if(datagrid1.rowStatus(i)==2){
								jj= jj+1
							}
						}
//						alert("jj : "+jj);
						if(jj==0){
							alert("처리할 자료가 없습니다.");
							return false;
						}
						for(i=1;i<datagrid1.rows;i++){
							var njpno = model1.getValue("/root/resData/gridData/fc_func_6510_l/dataSet/CURLIST/record["+i+"]/ybjpno");
							if(datagrid1.rowStatus(i)==2){
								if(njpno<0){
							      		alert("이미 마감된 자료입니다");
							      		return false;
								}
							}
						}
						model1.setValue("/root/reqData/multiUpDate/comp_dt",srch_comp_dt.value);
//						alert(model1.getValue("/root/reqData/multiUpDate/comp_dt"));
						model1.setValue("/root/reqData/multiUpDate/multiUpdateData",datagrid1.getUpdateData());
						model1.send("fc_func_6511_i");
						return check_submit_completed("저장되었습니다.","");
					}
				}
				function send_fc_func_6512_d(){
					var tcnt = model1.getValue("/root/resData/gridData/fc_func_6510_l/dataSet/CURLIST/totalcnt");	
					if(tcnt==0){
						alert("처리할 자료가 없습니다.");
						return false;
					}
						var totalcnt = eval(model1.getValue("/root/resData/gridData/fc_func_6510_l/dataSet/CURLIST/totalcnt"))+1;	
						var j=0;
						for(i=0;i<totalcnt;i++){
							if(datagrid1.rowStatus(i)==2){
								j = j+1;
							}
						}
//						alert("j 의 값  :"+j);
						if(j>1){
							alert("선택을 하나만 하십시오");
							return false
						}
						if(j=0){
							alert("마감취소할 자료를 선택하십시오.");
						}
						var growd = model1.getValue("/root/resData/gridData/fc_func_6510_l/dataSet/CURLIST/record["+datagrid1.row+"]/ybjpno");
//						alert(growd);
						if(growd==0){
							alert("마감취소 대상 자료가 아닙니다.");
						}
						if (show_message("삭제하시겠습니까?","삭제 확인",MB_YESNO) != IDYES) {
							return false;
						}
						var sjpno= jpno.value;
						var slip_occr_dt = sjpno.substr(0,8);
						var slip_clsf_cd = sjpno.substr(8,2);
						var slip_seq = sjpno.substr(10,3);
						model1.setValue("/root/reqData/reqform/slip_occr_dt",slip_occr_dt);
						model1.setValue("/root/reqData/reqform/slip_clsf_cd",slip_clsf_cd);
						model1.setValue("/root/reqData/reqform/slip_seq",slip_seq);
						model1.send("fc_func_6512_d");
						if(submit_completed()){
							show_message("삭제되었습니다.")
							btn_select.dispatch("onclick");
							jpno.value = "";							
						}						
				}
				function selectAllrow(){
					jpno.value="";
					var totalcnt = eval(model1.getValue("/root/resData/gridData/fc_func_6510_l/dataSet/CURLIST/totalcnt"))+1;
//					alert(totalcnt);
					for(i=0;i<totalcnt;i++){
							datagrid1.addStatus(i, "update");
					}
				      model1.refresh();
				}
				function deselectAllrow(){
					jpno.value="";
					datagrid1.clearStatus();
				}
				function gridClick(grow){
					jpno.value = model1.getValue("/root/resData/gridData/fc_func_6510_l/dataSet/CURLIST/record["+grow+"]/ybjpno");
					if(datagrid1.rowStatus(grow)==0){
					      datagrid1.addStatus(grow,"update");						
					}else{
						datagrid1.removeStatus(grow,"update");
					}

				}
				
			]]>
		</script>
	</xhtml:head>
	<xhtml:body class="main" pagewidth="946" pageheight="765" guideline="2,50;1,936;1,10;2,5;2,734;2,91;2,105;2,721;">
