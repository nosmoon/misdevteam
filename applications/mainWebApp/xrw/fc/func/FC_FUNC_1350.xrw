	<xhtml:head>
		<xhtml:title>수금누계상황표출력(FC_FUNC_1350)</xhtml:title>
		<model id="model1">
			<instance id="instance1">
				<root xmlns="">
					<initData>
						<session/>
						<comboSet>
						</comboSet>
						<pageInfo/>
						<hndl_nm/>
					</initData>
					<reqData>
						<fwdData/>
						<searchData>
							<cymd/>
						</searchData>
						<reqForm>
						</reqForm>
						<multiUpdateData/>
					</reqData>
					<resData>
						<resForm>
						</resForm>
						<gridData>
						</gridData>
					</resData>
					<tempData>
						<keyData/>
					</tempData>
					<msgData/>
				</root>
			</instance>
			<script type="javascript" ev:event="xforms-ready">
				<![CDATA[
					srch_cymd.value = commCurDate();
				]]>
			</script>
			<script type="javascript" ev:event="xforms-model-construct-done">
				<![CDATA[
					init_controls();
					
					ss_cmpy_cd = getProperty("ss_cmpycd");
				]]>
			</script>
		</model>
		<script type="javascript" src="/js/CIISComm.js"/>
		<script type="javascript" src="/js/TFCommon.js"/>
		<script type="javascript" src="/js/fcmc_common.js"/>
		<script type="javascript">
			<![CDATA[
				var ss_cmpy_cd;
				
				function init_controls(){
					set_default(
						srch_cymd
					);					//input date
					set_format(
						"date_1"
					,	srch_cymd
					);					
				}
				function send_fc_func_1350_p(){
					if(!commReqCheck(group_scr)) {
						return false;
					}						
					if(srch_cymd.value != "" && !validate_date(srch_cymd.value)){
						show_message("일자입력이 잘못되었습니다.");
						return false;
					}

					resetOznode();
					
					// ozr경로 및 odi 파일명으로 사용될 노드 및 value셋팅 ( OZR파일경로 .ozr 제거)
					makeNodeValue("/root/oz_data/ozrName","fcmc/SP_FC_FUNC_1350_P");
					
					// connection 파라미터 설정.
//					makeNodeValue("/root/oz_data/conParam/sv_url","19110");
//					makeNodeValue("/root/oz_data/conParam/sv_uid");
//					makeNodeValue("/root/oz_data/conParam/sv_bonm");
//					makeNodeValue("/root/oz_data/conParam/uv_line");
//					makeNodeValue("/root/oz_data/conParam/uv_summary");
					
					// odi 파라미터 설정.
					makeNodeValue("/root/oz_data/odiParam/iv_cmpy_cd",ss_cmpy_cd);
					makeNodeValue("/root/oz_data/odiParam/iv_cymd", srch_cymd.value);
					
					//makeNodeValue("/root/oz_data/preview", "print");
					
					// 레포트뷰어 호출.
					openOzwin("/root/oz_data");					
				}
			]]>
		</script>
	</xhtml:head>
