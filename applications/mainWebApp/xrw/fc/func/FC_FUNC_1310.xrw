		<group id="group9" style="left:110px; top:110px; width:440px; height:235px; ">
			<group id="group_scr" style="left:30px; top:80px; width:375px; height:76px; ">
				<caption id="caption4" class="list_cap02" style="left:0px; top:50px; width:375px; height:26px; "/>
				<caption id="caption3" class="list_cap03" style="left:0px; top:50px; width:125px; height:26px; ">출력순서</caption>
				<caption id="caption28" class="list_cap02" style="left:0px; top:25px; width:375px; height:26px; "/>
				<caption id="caption29" class="list_cap02" style="left:0px; top:0px; width:375px; height:26px; "/>
				<caption id="caption30" class="list_cap03" style="left:0px; top:0px; width:125px; height:26px; ">준비일</caption>
				<caption id="caption1" class="list_cap03" style="left:0px; top:25px; width:125px; height:26px; ">만기</caption>
				<caption id="caption2" style="left:230px; top:30px; width:15px; height:20px; ">~</caption>
				<input id="srch_fromcymd" ref="/root/reqData/searchData/fromcymd" class="req" navindex="2" inputtype="date" format="yyyy-mm-dd" caption="준비일" style="left:130px; top:29px; width:95px; height:18px; "/>
				<input id="srch_tocymd" ref="/root/reqData/searchData/tocymd" class="req" navindex="3" inputtype="date" format="yyyy-mm-dd" caption="만기FR" style="left:245px; top:29px; width:95px; height:18px; "/>
				<input id="srch_stat_dt" ref="/root/reqData/searchData/stat_dt" class="req" navindex="1" inputtype="date" format="yyyy-mm-dd" caption="만기TO" style="left:130px; top:4px; width:95px; height:18px; "/>
				<select1 id="srch_kind" ref="/root/reqData/searchData/kind" navindex="4" appearance="full" cellspacing="10" cols="2" overflow="visible" style="left:130px; top:55px; width:160px; height:20px; border-style:none; ">
					<choices>
						<item>
							<label>거래처순</label>
							<value>1</value>
						</item>
						<item>
							<label>만기일순</label>
							<value>2</value>
						</item>
					</choices>
				</select1>
			</group>
		</group>
		<caption id="caption6" tyle="left:15px; top:180px; width:940px; height:0px;" style="left:0px; top:10px; width:0px; height:0px; ">caption6</caption>
		<group id="group1" style="left:15px; top:45px; width:615px; height:25px; ">
			<button id="button1" class="btn02" navindex="5" crud="P" style="left:450px; top:0px; width:52px; height:21px; ">
				<caption>출 력</caption>
				<script type="javascript" ev:event="onclick">
					<![CDATA[
						send_fc_func_1310_p();
					]]>
				</script>
			</button>
			<button id="btn_help" class="btn02" navindex="6" style="left:505px; top:0px; width:52px; height:21px; ">
				<caption>도움말</caption>
			</button>
			<button id="btnClose" class="btn02" navindex="7" style="left:560px; top:0px; width:52px; height:21px; ">
