	<xhtml:head>
		<xhtml:title>배서준비현황출력(전표구분별)(FC_FUNC_1310)</xhtml:title>
		<model id="model1">
			<instance id="instance1">
				<root xmlns="">
					<initData>
						<session/>
						<comboSet>
						</comboSet>
						<pageInfo/>
						<hndl_nm/>
					</initData>
					<reqData>
						<fwdData/>
						<searchData>
							<fromcymd/>
							<tocymd/>
							<stat_dt/>
							<kind/>
						</searchData>
						<reqForm>
						</reqForm>
						<multiUpdateData/>
					</reqData>
					<resData>
						<resForm>
						</resForm>
						<gridData>
						</gridData>
					</resData>
					<tempData>
						<keyData/>
					</tempData>
					<msgData/>
				</root>
			</instance>
			<script type="javascript" ev:event="xforms-ready">
				<![CDATA[
					srch_stat_dt.value = commCurDate();
					srch_fromcymd.value = commCurDate();
					srch_tocymd.value = commCurDate();
					srch_kind.value = "1";
				]]>
			</script>
			<script type="javascript" ev:event="xforms-model-construct-done">
				<![CDATA[
					init_controls();
					
					ss_cmpy_cd = getProperty("ss_cmpycd");
				]]>
			</script>
		</model>
		<script type="javascript" src="/js/CIISComm.js"/>
		<script type="javascript" src="/js/TFCommon.js"/>
		<script type="javascript" src="/js/fcmc_common.js"/>
		<script type="javascript">
			<![CDATA[
				var ss_cmpy_cd;
				
				function init_controls(){
					set_default(
						srch_stat_dt
					,	srch_fromcymd
					,	srch_tocymd
					);					//input date
					set_format(
						"date_1"
					,	srch_stat_dt
					,	srch_fromcymd
					,	srch_tocymd
					);					
				}
				function send_fc_func_1310_p(){
					if(!commReqCheck(group_scr)) {
						return false;
					}						

					if(srch_stat_dt.value !="" && !validate_date(srch_stat_dt.value)){
						show_message("일자입력이 잘못되었습니다.");
						return false;
					}					
					if(srch_fromcymd.value !="" && !validate_date(srch_fromcymd.value)){
						show_message("일자입력이 잘못되었습니다.");
						return false;
					}
					if(srch_tocymd.value !="" && !validate_date(srch_tocymd.value)){
						show_message("일자입력이 잘못되었습니다.");
						return false;
					}
					resetOznode();

					// ozr경로 및 odi 파일명으로 사용될 노드 및 value셋팅 ( OZR파일경로 .ozr 제거)
					if(srch_kind.value == "1"){
						makeNodeValue("/root/oz_data/ozrName","fcmc/SP_FC_FUNC_1310_P");
					}
					else if(srch_kind.value == "2"){
						makeNodeValue("/root/oz_data/ozrName","fcmc/SP_FC_FUNC_1311_P");
					}
					
					// connection 파라미터 설정.
//					makeNodeValue("/root/oz_data/conParam/sv_url","19110");
//					makeNodeValue("/root/oz_data/conParam/sv_uid");
//					makeNodeValue("/root/oz_data/conParam/sv_bonm");
//					makeNodeValue("/root/oz_data/conParam/uv_line");
//					makeNodeValue("/root/oz_data/conParam/uv_summary");
					
					// odi 파라미터 설정.
					makeNodeValue("/root/oz_data/odiParam/iv_cmpy_cd",ss_cmpy_cd);
					makeNodeValue("/root/oz_data/odiParam/iv_fromcymd", srch_fromcymd.value);
					makeNodeValue("/root/oz_data/odiParam/iv_tocymd", srch_tocymd.value);
					makeNodeValue("/root/oz_data/odiParam/iv_stat_dt", srch_stat_dt.value);
					makeNodeValue("/root/oz_data/odiParam/iv_kind", srch_kind.value);

					
					//makeNodeValue("/root/oz_data/preview", "print");
					
					// 레포트뷰어 호출.
					openOzwin("/root/oz_data");					
				}
			]]>
		</script>
	</xhtml:head>
	<xhtml:body class="main" pagewidth="640" pageheight="480" guideline="1,10;1,936;2,64;2,70;2,76;1,480;" style="font-family:굴림; ">
