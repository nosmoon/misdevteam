				<select1 id="srch_slip_clsf_cd" ref="/root/reqData/searchData/slip_clsf_cd" class="req" navindex="3" appearance="minimal" itemcount="8" caption="전표구분" style="left:130px; top:54px; width:150px; height:18px; ">
