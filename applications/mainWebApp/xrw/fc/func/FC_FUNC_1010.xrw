			<caption id="caption3" class="list_cap02" style="left:0px; top:0px; width:926px; height:26px; text-align:center; "/>
			<input id="srch_note_no" ref="/root/reqData/search/note_no" class="req_100" navindex="1" maxlength="8" caption="어음번호" style="left:113px; top:4px; width:100px; height:18px; ">
				<script type="javascript" ev:event="xforms-value-changed">
					<![CDATA[
					//직접 조회기능이 없으므로 값 변경에 대한 체크는 제외
					if (open_fc_co_1360()) {
						//팝업에서 확인 버튼을 누른 경우(혹은 데이터를 더블클릭 한 경우)
						send_fc_func_1011_l();
					}
					else {
						//팝업에서 취소 버튼을 누른 경우
					}
					]]>
				</script>
			</input>
			<caption id="caption2" class="list_cap03" style="left:0px; top:0px; width:110px; height:26px; text-align:center; ">어음번호</caption>
