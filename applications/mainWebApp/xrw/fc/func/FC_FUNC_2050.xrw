		<script type="javascript" src="../../../js/fcmc_common.js"/>
		<script type="javascript">
			<![CDATA[
			var	_oth_clsf = "1";
						
			function send_fc_func_2050_m() {
				model.send("fc_func_2050_m");
				
				//전체항목 추가
				makeComboGubun("A", "/root/initData/comboSet/fc_func_2050_m/dataSet/CURLIST1", "record", "cdabrvnm_cd", "cd")
				
				//combobox refresh
				//srch_clsf_cd.refresh();
				srch_seci_clsf_cd.select(0);
				//srch_clsf_cd.dispatch("xforms-value-changed");
			}
			function send_fc_func_2051_l() {
				//입력값 설정
				if(srch_seci_clsf_cd.value==""){
					model1.setValue("/root/reqData/searchData/pch_sql","SECI_CLSF_CD LIKE '"+model1.getValue("/root/initData/comboSet/fc_func_2050_m/dataSet/xx_seci_clsf_cd")+"%'");
				}else{
					model1.setValue("/root/reqData/searchData/pch_sql","SECI_CLSF_CD =  '"+srch_seci_clsf_cd.value+"'");
				}
				if(endyn.value=="1"){
					model1.setValue("/root/reqData/searchData/pch_sql",model1.getValue("/root/reqData/searchData/pch_sql")+" AND SECI_STAT_CD <> '"+model1.getValue("/root/initData/comboSet/fc_func_2050_m/dataSet/xx_seci_stat_cd")+"'");
				}else{
					model1.setValue("/root/reqData/searchData/pch_sql",model1.getValue("/root/reqData/searchData/pch_sql")+" AND SECI_STAT_CD = '"+model1.getValue("/root/initData/comboSet/fc_func_2050_m/dataSet/xx_seci_stat_cd")+"'");
				}
				model1.setValue("/root/reqData/searchData/pch_sql",model1.getValue("/root/reqData/searchData/pch_sql")+" ORDER BY  SECI_CLSF_CD, SECI_NO ");
				//alert(model1.getValue("/root/reqData/searchData/pch_sql"));
				model.send("fc_func_2051_l");
			}
			
			]]>
		</script>
