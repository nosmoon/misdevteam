				 function send_fc_func_1061_l(){
						 	datagrid_data_clear(datagrid1);
							datagrid1.visible=true;
							datagrid2.visible=false;
							slip_no.value = "";
							checkbox1.disabled = false;
							chain.value = "";
					 		sbbsamt.value = "";
					 		cashamttot.value = "";
					 		eeamttot.value = "";
							 var setoff_reserv_dt = get_refvalue("/root/reqData/search/stat_dt");
							 set_refvalue("/root/reqData/search/setoff_reserv_dt",setoff_reserv_dt);
						 
							 if(nullCheck()){
