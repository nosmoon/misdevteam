				 function wcjs_Compute(){
					    var w_wcjs = 0;
					    var W_SDSE = 0;
					    var W_JMSE = 0;
					    var w_gwapyo = 0; 	//과표
					    var W_AMT = 0;
					    var W_EEAMTTOT = 0;
					    var W_CHAIN = 0;
					    
				 	var incm_clsf_cd = get_refvalue("/root/reqData/multiUpdate/incm_clsf_cd");
						if(incm_clsf_cd==""){//원천징수
							w_wcjs = 0;
						}else{
							 w_wcjs = get_refvalue("/root/reqData/reqform/wcjs");
//							 alert("w_wcjs   ->"+w_wcjs);
						}
						
						if(get_refvalue("/root/resData/gridData/fc_func_1061_l/dataSet/CURLIST/record/sbbsamt")==""){//과표
							w_gwapyo = 0;
						}else{
							w_gwapyo = get_refvalue("/root/resData/gridData/fc_func_1061_l/dataSet/CURLIST/record/sbbsamt");
						}	

						//if(incm_clsf_cd=="9001"){
						if(incm_clsf_cd==model1.getValue("/root/initData/comboSet/fc_func_1060_m/dataSet/xx_incm_clsf_cd_010")){
							//'9001'은 원천징수액 반드시 입력
							W_SDSE = Math.floor(Number(w_wcjs/1.1/10))*10;  // 소득세=원천징수/1.1, 원단위 버림
							W_JMSE = w_wcjs - W_SDSE;
						//}else if(incm_clsf_cd=="9003"){
						}else if(incm_clsf_cd==model1.getValue("/root/initData/comboSet/fc_func_1060_m/dataSet/xx_incm_clsf_cd_020")){
							//9003은 원천징수액 입력하지 않으며, 과표로 계산
							W_SDSE = Math.floor(Number(w_gwapyo*0.03/10))*10;  //소득세=과표*0.03, 원단위 버림
							W_JMSE = Math.floor(Number(W_SDSE*0.1/10))*10; 	//주민세=소득세*0.1, 원단위 버림
							w_wcjs = W_SDSE + W_JMSE;	//원천징수=소득세+주민세
						//}else if(incm_clsf_cd=="9004"){
						}else if(incm_clsf_cd==model1.getValue("/root/initData/comboSet/fc_func_1060_m/dataSet/xx_incm_clsf_cd_030")){
							//9004는 원천징수액 입력하지 않으며, 과표로 계산
					              W_SDSE = Math.floor(Number(w_gwapyo * 0.2 * 0.2 / 10)) * 10;  //소득세=과표*0.25*0.2, 원단위 버림
					              W_JMSE = Math.floor(Number(W_SDSE * 0.1 / 10)) * 10;           //주민세=소득세*0.1, 원단위 버림
					              w_wcjs = W_SDSE + W_JMSE;                       //원천징수=소득세+주민세
						//}else if(incm_clsf_cd=="90041"){
						}else if(incm_clsf_cd==model1.getValue("/root/initData/comboSet/fc_func_1060_m/dataSet/xx_incm_clsf_cd_040")){
							 //9004는 원천징수액 입력하지 않으며, 과표로 계산
					              W_SDSE = Math.floor(Number(w_gwapyo * 0.2 / 10)) * 10;         //소득세=과표*0.2, 원단위 버림
					              W_JMSE = Math.floor(Number(W_SDSE * 0.1 / 10)) * 10;           //주민세=소득세*0.1, 원단위 버림
					              w_wcjs = W_SDSE + W_JMSE;                       //원천징수=소득세+주민세
						//}else if(incm_clsf_cd=="9005"){
						}else if(incm_clsf_cd==model1.getValue("/root/initData/comboSet/fc_func_1060_m/dataSet/xx_incm_clsf_cd_050")){
							//9005는 원천징수액을 입력할수도 있고, 하지 않을수도 있다.
						          if(get_refvalue("/root/reqData/reqform/wcjs") != "0"){ 	//원천징수액 입력시
						                W_SDSE = Math.floor(Number(w_wcjs / 1.1 / 10)) * 10;      //소득세=원천징수/1.1, 원단위 버림
						                W_JMSE = w_wcjs - W_SDSE;
						          }else{
						                W_SDSE = Math.floor(Number(w_gwapyo * 0.2 / 10)) * 10;     //소득세=과표*0.2, 원단위 버림
						                W_JMSE = Math.floor(Number(W_SDSE * 0.1 / 10)) * 10;       //주민세=소득세*0.1, 원단위 버림
						                w_wcjs = W_SDSE + W_JMSE;                   //원천징수=소득세+주민세
						           }
						}else{
						       w_wcjs = 0;
						       W_SDSE = 0;
						       W_JMSE = 0;
						}	
						//alert("W_SDSE   ->"+W_SDSE);
						if(W_SDSE < 1000){                      //소득세가 1000원미만이면 비과세
						        w_wcjs = 0;
						        W_SDSE = 0;
						        W_JMSE = 0;
						}
						set_refvalue("/root/reqData/reqform/wcjs",w_wcjs); 		//원천징수
						set_refvalue("/root/reqData/reqform/sdse",W_SDSE);		//소득세
						set_refvalue("/root/reqData/reqform/jmse",W_JMSE);		//주민세
						model1.refresh();
//						alert("wcjc  -->"+get_refvalue("/root/reqData/reqform/wcjs"));
//						alert("sdse  -->"+get_refvalue("/root/reqData/reqform/sdse"));
//						alert("jmse  -->"+get_refvalue("/root/reqData/reqform/jmse"));
						if(get_refvalue("/root/resData/gridData/fc_func_1062_l/dataSet/CURLIST/record/note_amt")==""){		//어음금액
							W_EEAMTTOT = 0;
						}else{
							W_EEAMTTOT = get_refvalue("/root/reqData/reqform/eeamttot");
						}					
					    //alert("w_gwapyo   ->["+parseInt(w_gwapyo)+"]           w_wcjs   ->["+parseInt(w_wcjs)+"]");
					    W_CHAIN = parseInt(w_gwapyo) - parseInt(w_wcjs);
					    set_refvalue("/root/reqData/reqform/chain",W_CHAIN);		//차인지금액
					    set_refvalue("/root/reqData/reqform/cashamttot",Number(W_CHAIN)-Number(W_EEAMTTOT));		//현금
					    model1.refresh();
				 }
				 function girdClick(irow,grid){
				 	var WEEAMTTOT = 0;
				 	var ECASHAMTTOT = 0;
				 	var WCHAIN = 0;
				 	if(grid == "2"){
				 		slip_no.value = get_refvalue("/root/resData/gridData/fc_func_1063_l/dataSet/CURLIST/record["+irow+"]/slipno");
				 	}
				 	
				 	if(get_refvalue("/root/reqData/reqform/chain") != ""){
				 		WCHAIN = get_refvalue("/root/reqData/reqform/chain");
				 	}else{
				 		WCHAIN = 0;	
				 	}
//				 	alert("2 : "+get_refvalue("/root/reqData/reqform/cashamttot"));
				 	//if(datagrid1.rowStatus(datagrid1.row)=="2"){
//				 	alert(datagrid1.rowStatus(irow));
				 	if(grid != "2"){	
				 		if(datagrid1.rowStatus(irow)=="2"){
						 	if(get_refvalue("/root/reqData/reqform/eeamttot") != ""){
						 		WEEAMTTOT = Number(get_refvalue("/root/reqData/reqform/eeamttot"));
						 	}
						 		set_refvalue("/root/reqData/reqform/eeamttot",Number(WEEAMTTOT)-Number(get_refvalue("/root/resData/gridData/fc_func_1062_l/dataSet/CURLIST/record["+irow+"]/note_amt")));
						 		set_refvalue("/root/reqData/reqform/cashamttot",Number(WCHAIN) - Number(get_refvalue("/root/reqData/reqform/eeamttot")));
	//					 		alert("3 : "+get_refvalue("/root/reqData/reqform/cashamttot"));
						 	datagrid1.removeStatus(irow,"update");
					 	}else{
					 		if(get_refvalue("/root/reqData/reqform/eeamttot") != ""){
					 			WEEAMTTOT = Number(get_refvalue("/root/reqData/reqform/eeamttot"));
					 		}
						 		set_refvalue("/root/reqData/reqform/eeamttot",Number(WEEAMTTOT)+Number(get_refvalue("/root/resData/gridData/fc_func_1062_l/dataSet/CURLIST/record["+irow+"]/note_amt")));
						 		set_refvalue("/root/reqData/reqform/cashamttot",Number(get_refvalue("/root/reqData/reqform/chain"))-Number(get_refvalue("/root/reqData/reqform/eeamttot")));
	//					 		alert("4 : "+get_refvalue("/root/reqData/reqform/cashamttot"));
					 		datagrid1.addStatus(irow,"update");
					 	}
				 	}
				 }
