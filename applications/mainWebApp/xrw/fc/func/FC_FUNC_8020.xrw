				if (show_message("삭제하시겠습니까?","삭제 확인",MB_YESNO) != IDYES) {
					return false;
				}
				
				//입력값 설정

				//실행
				model.send("fc_func_8024_d");
				
				return check_submit_completed("삭제되었습니다.","");
