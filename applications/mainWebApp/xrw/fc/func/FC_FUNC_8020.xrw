			function send_fc_func_8021_s() {
				//조건에 문제가 없다면 처리
				if (!prepare_fc_func_8021_s()) {
					return false;
				}
				
				//입력값 설정
				if (srch_leas_no.value.Trim() != "" && srch_fst_leas_no.value.Trim() == "") {
					//재리스번호가 입력되었고 최초리스번호가 입력되지 않은 경우
					model.setValue("/root/reqData/searchData/workflag","A");
				}
				else if (srch_leas_no.value.Trim() == "" && srch_fst_leas_no.value.Trim() != "") {
					//재리스번호가 입력되지 않았고 최초리스번호만 입력된 경우
					//조회 모드를 'B' 최초리스번호 기준 조회로 변경하고
					//SP에 인수로 설정되는 leas_no에 최초리스번호를 넣는다.
					model.setValue("/root/reqData/searchData/workflag","B");
					srch_leas_no.value = srch_fst_leas_no.value;
				}
				else if (srch_leas_no.value.Trim() != "" && srch_fst_leas_no.value.Trim() != "") {
					//둘다 설정되어있는 경우 재리스번호 기준으로 조회한다.
					model.setValue("/root/reqData/searchData/workflag","A");
				}
				else {
					if (!check_reqItem(srch_leas_no,"trim","focus","재리스번호 혹은 최초리스번호중 하나는 필수입력항목입니다.")) {return false;}
				}
				
				//실행
				model.send("fc_func_8021_s");
				
				totalcnt = model.getValue("/root/resData/resform/fc_func_8021_s/dataSet/CURLIST1/totalcnt");
				if (totalcnt == 0) {
					reset_model(true);
					show_message("검색결과가 없습니다.");
				}
				else {
					srch_leas_no.value = model.getValue("/root/resData/resform/fc_func_8021_s/dataSet/CURLIST1/record/leas_no");
					srch_fst_leas_no.value = model.getValue("/root/resData/resform/fc_func_8021_s/dataSet/CURLIST1/record/fst_leas_no");
					srch_fst_leas_nm.value = model.getValue("/root/resData/resform/fc_func_8021_s/dataSet/CURLIST1/record/fst_leas_nm");
				}
				
				//담보
				totalcnt = model.getXPathValue("count(/root/resData/resform/fc_func_8021_s/dataSet/CURLIST2/record)");
				if (totalcnt == 0) {
					datagrid1.addRow();
				}
				
				//규손금
				totalcnt = model.getXPathValue("count(/root/resData/resform/fc_func_8021_s/dataSet/CURLIST3/record)");
				if (totalcnt == 0) {
					datagrid2.addRow();
				}
				
				reset_cntr_clsf_cd();
			}
			function open_fc_co_1060(p_where,p_mode,p_title,p_readonly,p_to) {
				var need_popup = true;
				var rcvs_flag = false;
				
				//검색조건이 모두 충족하지 않으면 종료한다.
				if (p_where == "FOCUS" && (model.getValue("/root/reqData/searchData/cntr_clsf_cd") == "" || srch_leas_no.value.Trim() == "")) {
					//srch_leas_sub_seq.value	= "";
					return rcvs_flag;
				}
				
				p_where = p_where.toUpperCase();
				
				var popid = "fc_co_1060";
				var subid = "fc_co_1060_l";

				set_popup_send_data(popid		,"mode"			,p_mode			);
				set_popup_send_data(popid		,"title"			,p_title			);
				set_popup_send_data(popid		,"readonly"		,p_readonly		);
				
				//조회할 데이터를 저장하는 코드
				//이부분은 호출하는 폼 마다 변경 될 수 있다.
				//현재 폼에서는 두가지 종류의 리스번호를 하나의 팝업으로 처리하므로 호출 시점에 따라서
				//조회할 데이터 설정이 변경되어야 한다.
				
				if (p_to == "A") {
					//조회용 리스번호 조회시
					set_popup_send_data(popid		,"cntr_clsf_cd"		,model.getValue("/root/reqData/searchData/cntr_clsf_cd")	);
					//set_popup_send_data(popid		,"datafilter"			,"[re_leas_yn = 'Y']");
				}
				else if (p_to == "B") {
					//최초리스번호 조회시
					set_popup_send_data(popid		,"cntr_clsf_cd"		,model.getValue("/root/reqData/searchData/cntr_clsf_cd"));
				}

				//포커스에서 호출 한 경우 직접 서블릿을 실행시켜서 데이터를 확인한다.
				if (p_where == "FOCUS") {
					//포커스로 검색이 시작되는 경우 무조껀 검색 결과에 해당하는 노드를 삭제하고 시작한다.
					//버튼의 경우는 검색결과가 "" 이면 지우도록 설정되어있음
					model.removenode(get_popup_rcvs_ref(popid));
					//인스턴스 구조가 없으면 copynode 동작이 되지 않으므로 기본 구조는 만들어준다.
					model.makeNode(get_popup_rcvs_ref(popid));
					
					//받는위치 분리를 위해서 receive 위치를 popid가 아닌 subid 기준으로 설정
					send_submission(
							"auto_" + subid
						,	get_popup_send_ref(popid)
						,	get_popup_rcvs_ref(subid) + "/dataSet"
						,	"/fc/co1000/1061"
						,	"urlencoded-post"
						,	"application/x-www-form-urlencoded"
					);
					
					totalcnt = model.getValue(get_popup_rcvs_ref(subid) + "/dataSet" + "/CURLIST/totalcnt");
			
					if (totalcnt == 1) {
						//데이터가 1건이고 focus out에서 호출한 경우
						//데이터를 설정하고 나간다.

//						model.duplicate(get_popup_rcvs_ref(popid) , get_popup_rcvs_ref(subid) + "/dataSet/CURLIST1/record");
						model.copyNode(get_popup_rcvs_ref(popid) , get_popup_rcvs_ref(subid) + "/dataSet/CURLIST/record");
//						return;
						need_popup = false;
						rcvs_flag = true;
					}
					else if (totalcnt == 0) {
						show_message("리스번호의 검색결과가 없습니다.");
//						return;
						need_popup = false;		
						rcvs_flag = false;				
					}
					else {
						//포커스에서 호출 한 경우 1건이 아니면 무조건 종료
						//만약 1건 이상인 경우 popup을 호출해 달라고 하면
						//return을 지우면 됨
						//show_message("거래처코드의 검색결과가 2건 이상입니다.");
						//return;
						need_popup = true;
					}
				}
				
				//그외 버튼인 경우는 계속 진행
				if (need_popup) {
					rcvs_flag = open_popup(popid,"640");
				}
				
				//조회된 데이터를 저장하는 코드
				//이부분은 호출하는 폼 마다 변경 될 수 있다.
				//현재 폼에서는 두가지 종류의 리스번호를 하나의 팝업으로 처리하므로 호출 시점에 따라서
				//데이터의 입력 위치가 바뀌어야 한다.
				
				if (p_to == "A") {
					//조회용 리스번호 조회시
					
					//반드시 입력되어야 하는 필수 항목의 경우 검색결과가 공백이 아닌 경우만 반영해서
					//다시 조회 하기 쉽도록 하고
					if (get_popup_rcvs_data(popid,"cntr_clsf_cd") != "")			model.setValue("/root/reqData/searchData/cntr_clsf_cd",get_popup_rcvs_data(popid,"cntr_clsf_cd1"));
					if (get_popup_rcvs_data(popid,"leas_no") != "")				srch_leas_no.value		=	get_popup_rcvs_data(popid,"leas_no");
					//그외의 값은 지워지도록 한다.
					srch_fst_leas_no.value			=	"";
					srch_fst_leas_nm.value			=	"";
					
					reset_cntr_clsf_cd();
				}
				else if (p_to == "B") {
					//최초리스번호 조회시
					if (get_popup_rcvs_data(popid,"leas_no") != "")				srch_fst_leas_no.value	=	get_popup_rcvs_data(popid,"leas_no");
					srch_fst_leas_nm.value			=	get_popup_rcvs_data(popid,"leas_nm");
					srch_leas_no.value				=	"";
				}
				
				reset_popup_send_data(popid);
				
				return rcvs_flag;
			}
			function prepare_fc_func_8021_s() {
				if(!check_search_values("s")) {
					return false;
				}
				
				return true;
			}
			function set_column_visibility() {
				//모든 컬럼을 감춘다.
				hide_all_columns(datagrid1);
				//공통항목을 보여준다.
				show_columns(datagrid1,"4,5,6");
				//모든 컬럼을 감춘다.
				hide_all_columns(datagrid2);
				//공통항목을 보여준다.
				show_columns(datagrid2,"4,5,6");
			}
			function prepare_fc_func_8022_i() {
				if (!check_key_values("i")) {
					return false;
				}
				
				if(!check_search_values("i")) {
					return false;
				}

				if(!check_input_values("i")) {
					return false;
				}
				
				return true;
			}
			function send_fc_func_8022_i() {
				//조건에 문제가 없다면 처리
				if (!prepare_fc_func_8022_i()) {
					return false;
				}
				
				if (show_message("저장하시겠습니까?","저장 확인",MB_YESNO) != IDYES) {
					return false;
				}
				
				//입력값 설정
				model.makeValue("/root/resData/resform/fc_func_8021_s/dataSet/CURLIST1/record/cntr_clsf_cd",model.getValue("/root/reqData/searchData/cntr_clsf_cd"));
				model.makeValue("/root/resData/resform/fc_func_8021_s/dataSet/CURLIST1/record/leas_clsf_cd",model.getValue("/root/reqData/searchData/leas_clsf_cd"));
				model.makeValue("/root/resData/resform/fc_func_8021_s/dataSet/CURLIST1/record/leas_no",srch_leas_no.value);
				model.makeValue("/root/resData/resform/fc_func_8021_s/dataSet/CURLIST1/record/fst_leas_clsf_cd",model.getValue("/root/reqData/searchData/leas_clsf_cd"));
				model.makeValue("/root/resData/resform/fc_func_8021_s/dataSet/CURLIST1/record/fst_leas_no",srch_fst_leas_no.value);
				model.makeValue("/root/resData/resform/fc_func_8021_s/dataSet/CURLIST1/record/re_leas_yn","Y");
				
				//실행
				model.send("fc_func_8022_i");
				
				if(!check_submit_completed("저장되었습니다.","")) {
					//실패시 다시 저장 가능하도록 키값 체크에 사용되는 값들을 제거한다.
					//하나만 제거해도 키값이 같지 않음으로 처리됨
					model.setValue("/root/resData/resform/fc_func_8021_s/dataSet/CURLIST1/record/leas_no","");
					return false;
				}
				
				return true;
			}
			function check_search_values(p_siud) {
				//필수항목체크(검색)
				if(!commReqCheck(group_scr)) {
					return false;
				}
				
				switch (p_siud.toUpperCase()) {
					case "S":
						//조회시
						
						//재리스번호 혹은 최초리스번호 둘 중 하나의 값은 지정되어야 함
						//재리스번호와 최초리스번호에 따른 조회 값 분기가 있으므로
						//여기 위치해야 하지만 send_fc_func_8021_s() 함수에서 처리하자
						break;
					case "I":
						//입력시						
						break;
					case "U":
						//수정시
						break;
					case "D":
						//삭제시
						break;
					default:
						show_message("key 값 확인을 위한 구분자가 지정되지 않았습니다.");
						break;
				}
				
				return true;
			}
			function check_input_values(p_siud) {
				//필수항목체크(내용)
				if (!commReqCheck(group_content1)) {
					return false;
				}
				//필수항목체크(grid);
				if(!commGridReqCheck("datagrid1")) {
					return false;
				}		
				//필수항목체크(grid);
				if(!commGridReqCheck("datagrid2")) {
					return false;
				}		
				//선택적필수항목체크
				if (model.getValue("/root/reqData/searchData/cntr_clsf_cd") == "1") {
					//if (!check_reqItem(srch_cntr_clsf_cd,"trim","focus","계약구분이 견적인 경우 리스순번은 필수입력항목입니다.")) {return false;}
				}

				//유효성체크


				switch (p_siud.toUpperCase()) {
					case "S":
						//조회시
						break;
					case "I":
						//입력시
						break;
					case "U":
						//수정시
						break;
					case "D":
						//삭제시
						break;
					default:
						show_message("key 값 확인을 위한 구분자가 지정되지 않았습니다.");
						break;
				}
				
				return true;
			}
			function check_key_values(p_siud) {
				var key_flag = false;
				var key_msg = "";
				//키값 변경 체크
				var prev_cntr_clsf_cd		=	model.getValue("/root/resData/resform/fc_func_8021_s/dataSet/CURLIST1/record/cntr_clsf_cd");
				var prev_leas_clsf_cd	=	model.getValue("/root/resData/resform/fc_func_8021_s/dataSet/CURLIST1/record/leas_clsf_cd");
				var prev_leas_no		=	model.getValue("/root/resData/resform/fc_func_8021_s/dataSet/CURLIST1/record/leas_no");
				
				if (
						prev_cntr_clsf_cd	==	model.getValue("/root/reqData/searchData/cntr_clsf_cd")
					&&	prev_leas_no		==	srch_leas_no.value
				) {
					
					//키값이 변경되지 않은 경우
					key_flag = true;
				}
				else {
					//키값이 변경 된 경우
					key_flag = false;
				}
				
				switch (p_siud.toUpperCase()) {
					case "S":
						//조회시
						break;
					case "I":
						//입력시
						
						//입력인 경우 키값이 변경되지 않은 경우
						//입력할 필요가 없는 데이터이므로
						//key_flag를 반전시킨다.
						key_flag = !key_flag;
						key_msg = "조회된 key 값과 입력할 key 값이 같습니다.";
						if (!check_reqItem("/root/resData/resform/fc_func_8021_s/dataSet/CURLIST1/record/fst_leas_no","trim","none","조회되지 않은 데이터입니다. 최초리스번호를 조회 후 입력하세요.")) {return false;}
						if (!check_reqItem(srch_leas_no,"trim","focus","입력시 재리스번호는 필수입력항목입니다.")) {return false;}
						if (!check_reqItem(srch_fst_leas_no,"trim","focus","입력시 최초리스번호는 필수입력항목입니다.")) {return false;}
						
						break;
					case "U":
						//수정시
						if (!check_reqItem("/root/resData/resform/fc_func_8021_s/dataSet/CURLIST1/record/cmpy_cd","trim","none","조회되지 않은 데이터입니다. 재리스번호를 조회 후 수정하세요.")) {return false;}
						key_msg = "Key 값이 변경되었습니다. 조회 후 수정하세요.";
						break;
					case "D":
						//삭제시
						if (!check_reqItem("/root/resData/resform/fc_func_8021_s/dataSet/CURLIST1/record/cmpy_cd","trim","none","조회되지 않은 데이터입니다. 재리스번호를 조회 후 삭제하세요.")) {return false;}
						key_msg = "Key 값이 변경되었습니다. 조회 후 삭제하세요.";
						break;
					default:
						show_message("key 값 확인을 위한 구분자가 지정되지 않았습니다.");
						break;
				}
//				if (!key_flag) show_message(key_msg);
				if (!key_flag) check_reqItem("","none","none",key_msg);			
				return key_flag;
			}
