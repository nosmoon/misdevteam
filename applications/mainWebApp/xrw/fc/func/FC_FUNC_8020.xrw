			function reset_model(p_save) {
				var temp_srch_cntr_clsf_cd = model.getValue("/root/reqData/searchData/cntr_clsf_cd");
				var temp_srch_leas_no = srch_leas_no.value;
//				var temp_srch_leas_sub_seq = srch_leas_sub_seq.value;
				var temp_srch_fst_leas_no = srch_fst_leas_no.value;
				var temp_srch_fst_leas_nm = srch_fst_leas_nm.value;

				model.reset();
				
				if (p_save) {
					model.setValue("/root/reqData/searchData/cntr_clsf_cd",temp_srch_cntr_clsf_cd);
					srch_leas_no.value = temp_srch_leas_no;
//					srch_leas_sub_seq.value = temp_srch_leas_sub_seq;
					srch_fst_leas_no.value = temp_srch_fst_leas_no;
					srch_fst_leas_nm.value = temp_srch_fst_leas_nm;
				}
				reset_cntr_clsf_cd();
			}
			]]>
		</script>
