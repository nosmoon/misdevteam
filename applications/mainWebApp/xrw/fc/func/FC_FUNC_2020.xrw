		<script type="javascript">
			<![CDATA[
				var	_oth_clsf = "1";
				
				function nullCheck(){
					if (!check_reqItem(srch_comp_dt,"trim","focus","필수항목 : 매각일")) { return false; }
					if (!check_reqItem(srch_mtry_dt,"trim","focus","필수항목 : 만기일")) { return false; }
					if (!check_reqItem(srch_seci_clsf_cd,"trim","focus","필수항목 : 증권구분")) { return false; }
					return true;
				}
				function nullCheck2(){
					if (!check_reqItem(srch_comp_dt,"trim","focus","필수항목 : 매각일")) { return false; }
					if (!check_reqItem(srch_seci_clsf_cd,"trim","focus","필수항목 : 증권구분")) { return false; }
					return true;
				}
				function send_fc_func_2020_m(){
					model1.send("fc_func_2020_m");
				}
				function send_fc_func_2021_l(){
					if(nullCheck()){
						model1.send("fc_func_2021_l");
						if(model1.getValue("/root/resData/gridData/fc_func_2021_l/dataSet/CURLIST/totalcnt") == 0){
							model.alert("검색결과가 없습니다.");
							return;
						}
					}
					calc_intamt();
				}
				function send_fc_func_2022_l(){
					selectAll.visible=false;
					selectAll.disabled=true;
					model1.setValue("/root/tempData/shwamt","");
 				        model1.setValue("/root/tempData/siamt","");
				        model1.setValue("/root/tempData/biamt","");
					 model1.setValue("/root/tempData/jpno","");
					 model1.setValue("/root/reqData/multiUpDate/jeog","");
					 model1.refresh();
					if(nullCheck2()){
						model1.setValue("/root/reqData/searchData/repay_dt",srch_comp_dt.value);
						model1.send("fc_func_2022_l");
						if(model1.getValue("/root/resData/gridData/fc_func_2022_l/dataSet/CURLIST/totalcnt") == 0){
							model.alert("검색결과가 없습니다.");
							return false;
						}
					}
					calc_intamt();
				}
				function send_fc_func_2023_i(){
					if(nullCheck2()){
						model1.setValue("/root/reqData/multiUpDate/seci_clsf_cd",srch_seci_clsf_cd.value);
						model1.setValue("/root/reqData/multiUpDate/comp_dt",srch_comp_dt.value);
						model1.setValue("/root/reqData/multiUpDate/jeog",jeog.value);
						model1.setValue("/root/reqData/multiUpDate/rcpay_amt",model1.getValue("/root/resData/gridData/fc_func_2021_l/dataSet/CURLIST/record/rcpay_amt"));
						model1.setValue("/root/reqData/multiUpDate/multiUpdateData",datagrid1.getUpdateData());
						model1.send("fc_func_2023_i");
						if(submit_completed()){
							btn_select.dispatch("onclick");
							model.alert("저장되었습니다.");
						}
					}
				}
				function send_fc_func_2024_d(){
					var sjpno= jpno.value;
						if(nullCheck2()){
							if(sjpno==""){
								alert("처리할 데이터가 없습니다.");
								return false;
							}else{
								if(model.alert("해당자료를 삭제하시겠습니까?", "유가증권일괄매각등록", 4)  == 6){
									
									var slip_occr_dt = sjpno.substr(0,8);
									var slip_clsf_cd = sjpno.substr(9,2);
									var slip_seq = sjpno.substr(12,3);
									model1.setValue("/root/reqData/reqform/slip_occr_dt",slip_occr_dt);
									model1.setValue("/root/reqData/reqform/slip_clsf_cd",slip_clsf_cd);
									model1.setValue("/root/reqData/reqform/slip_seq",slip_seq);
								}
		
								model1.setValue("/root/reqData/reqform/seci_clsf_cd",srch_seci_clsf_cd.value);
								model1.setValue("/root/reqData/reqform/repay_dt",srch_comp_dt.value);
								model1.setValue("/root/reqData/reqform/multiUpdateData",datagrid1.getUpdateData());
								model1.send("fc_func_2024_d");
	
								if(submit_completed()){
									btn_select.dispatch("onclick");
									model.alert("삭제되었습니다.");
								}
							}
						}
				}
						
						

				function gridClick(grow){
					if(datagrid1.rowStatus(grow)==0){
					      datagrid1.addStatus(grow,"update");						
					      model1.setValue("/root/tempData/shwamt",model1.getValue("/root/resData/gridData/fc_func_2021_l/dataSet/CURLIST/record["+grow+"]/jbgamt"));
					      model1.setValue("/root/tempData/siamt",model1.getValue("/root/resData/gridData/fc_func_2021_l/dataSet/CURLIST/record["+grow+"]/rcpay_amt"));
					      model1.setValue("/root/tempData/biamt",model1.getValue("/root/resData/gridData/fc_func_2021_l/dataSet/CURLIST/record["+grow+"]/corp_tax_amt"));
					      model1.setValue("/root/tempData/jpno",model1.getValue("/root/resData/gridData/fc_func_2021_l/dataSet/CURLIST/record["+grow+"]/jpno"));
					      model1.setValue("/root/reqData/multiUpDate/jeog",model1.getValue("/root/resData/gridData/fc_func_2021_l/dataSet/CURLIST/record["+grow+"]/rmks"));
					      model1.refresh();
					}else{
						datagrid1.removeStatus(grow,"update");
					      model1.setValue("/root/tempData/shwamt",model1.getValue("/root/resData/gridData/fc_func_2021_l/dataSet/CURLIST/record["+grow+"]/jbgamt"));
					      model1.setValue("/root/tempData/siamt",model1.getValue("/root/resData/gridData/fc_func_2021_l/dataSet/CURLIST/record["+grow+"]/rcpay_amt"));
					      model1.setValue("/root/tempData/biamt",model1.getValue("/root/resData/gridData/fc_func_2021_l/dataSet/CURLIST/record["+grow+"]/corp_tax_amt"));
					      model1.setValue("/root/tempData/jpno",model1.getValue("/root/resData/gridData/fc_func_2021_l/dataSet/CURLIST/record["+grow+"]/jpno"));
					      model1.setValue("/root/reqData/multiUpDate/jeog",model1.getValue("/root/resData/gridData/fc_func_2021_l/dataSet/CURLIST/record["+grow+"]/rmks"));							
					      model1.refresh();
					}

				}
				function selectAllrow(){
					for(i=0;i<datagrid1.rows;i++){
						datagrid1.addStatus(i, "update");
					}
					var grow = datagrid1.rows-1;
					model1.setValue("/root/tempData/shwamt",model1.getValue("/root/resData/gridData/fc_func_2021_l/dataSet/CURLIST/record["+grow+"]/jbgamt"));
				      model1.setValue("/root/tempData/siamt",model1.getValue("/root/resData/gridData/fc_func_2021_l/dataSet/CURLIST/record["+grow+"]/rcpay_amt"));
				      model1.setValue("/root/tempData/biamt",model1.getValue("/root/resData/gridData/fc_func_2021_l/dataSet/CURLIST/record["+grow+"]/corp_tax_amt"));
				      model1.setValue("/root/tempData/jpno",model1.getValue("/root/resData/gridData/fc_func_2021_l/dataSet/CURLIST/record["+grow+"]/jpno"));
				      model1.setValue("/root/reqData/multiUpDate/jeog",model1.getValue("/root/resData/gridData/fc_func_2021_l/dataSet/CURLIST/record["+grow+"]/rmks"));
				      model1.refresh();
				}
				function deselectAllrow(){
					datagrid1.clearStatus();
				}
				function open_fc_co_1000(p_where,p_mode,p_title,p_readonly,p_to,p_query_type) {
					var need_popup = true;
					var rcvs_flag = false;
					
					//검색조건이 모두 충족하지 않으면 종료한다.
					if (p_where == "FOCUS" && bank_cd.value.Trim() == "") {
						bank_nm.value	= "";
						return rcvs_flag;
					}
					
					p_where = p_where.toUpperCase();
					
					var popid = "fc_co_1001";
					var subid = "fc_co_1002_l";
	
					set_popup_send_data(popid		,"mode"			,p_mode			);
					set_popup_send_data(popid		,"title"			,p_title			);
					set_popup_send_data(popid		,"readonly"		,p_readonly		);
					set_popup_send_data(popid		,"cd"				,pay_bank_cd.value		);
					set_popup_send_data(popid		,"query_type"		,p_query_type		);
	
					//포커스에서 호출 한 경우 직접 서블릿을 실행시켜서 데이터를 확인한다.
					if (p_where == "FOCUS") {
						//포커스로 검색이 시작되는 경우 무조껀 검색 결과에 해당하는 노드를 삭제하고 시작한다.
						//버튼의 경우는 검색결과가 "" 이면 지우도록 설정되어있음
						model.removenode(get_popup_rcvs_ref(popid));
						//인스턴스 구조가 없으면 copynode 동작이 되지 않으므로 기본 구조는 만들어준다.
						model.makeNode(get_popup_rcvs_ref(popid));
						
						//받는위치 분리를 위해서 receive 위치를 popid가 아닌 subid 기준으로 설정
						send_submission(
								"auto_" + subid
							,	get_popup_send_ref(popid)
							,	get_popup_rcvs_ref(subid) + "/dataSet"
							,	"/fc/co1000/1002"
							,	"urlencoded-post"
							,	"application/x-www-form-urlencoded"
						);
						
						totalcnt = model.getValue(get_popup_rcvs_ref(subid) + "/dataSet" + "/CURLIST/totalcnt");
				
						if (totalcnt == 1) {
							//데이터가 1건이고 focus out에서 호출한 경우
							//데이터를 설정하고 나간다.
	
	//						model.duplicate(get_popup_rcvs_ref(popid) , get_popup_rcvs_ref(subid) + "/dataSet/CURLIST1/record");
							model.copyNode(get_popup_rcvs_ref(popid) , get_popup_rcvs_ref(subid) + "/dataSet/CURLIST/record");
	//						return;
							need_popup = false;
							rcvs_flag = true;
						}
						else if (totalcnt == 0) {
							show_message("[금융기관코드]의 검색결과가 없습니다.");
	//						return;
							need_popup = false;
							rcvs_flag = true;					
						}
						else {
							//포커스에서 호출 한 경우 1건이 아니면 무조건 종료
							//만약 1건 이상인 경우 popup을 호출해 달라고 하면
							//return을 지우면 됨
							//show_message("[거래처코드]의 검색결과가 2건 이상입니다.");
							//return;
							need_popup = true;
						}
					}
	
					//그외 버튼인 경우는 계속 진행
					if (need_popup) {
						rcvs_flag = open_popup(popid,"640");
					}
	
					//반드시 입력되어야 하는 필수 항목의 경우 검색결과가 공백이 아닌 경우만 반영해서
					//다시 조회 하기 쉽도록 하고
					if (get_popup_rcvs_data(popid,"cd") != "")				pay_bank_cd.value			=	get_popup_rcvs_data(popid,"cd");
					//그외의 값은 지워지도록 한다.
					banknm.value			=	get_popup_rcvs_data(popid,"cdnm");
					
					return rcvs_flag;
				}
				
				 // 그리드내 총이자 계산 //
		                function calc_intamt(){
					if(event.name == "xforms-value-changed"){
						var row = datagrid1.row;
			                	datagrid1.valueMatrix(row,8) = Number(datagrid1.valueMatrix(row,5)) - Number(datagrid1.valueMatrix(row,6));
					}else{
						for(i = 1 ; i < datagrid1.rows ; i++){
							datagrid1.valueMatrix(i,8) = Number(datagrid1.valueMatrix(i,5)) - Number(datagrid1.valueMatrix(i,6));
						}
					}
		                }
			]]>
		</script>
