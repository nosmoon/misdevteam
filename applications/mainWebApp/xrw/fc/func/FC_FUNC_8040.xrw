				function F_Won_Compute(){
					if(pymt_prcp.value == ""){
						w_libiwgamt = 0;
					}else{
						w_libiwgamt = pymt_prcp.value;
					}
					if(pymt_int.value == ""){
						w_libiint = 0;
					}else{
						w_libiint = pymt_int.value;
					}
					if(exrate.value == ""){
						w_liexcrate = 0;
					}else{
						w_liexcrate = exrate.value;
					}
//					alert("w_libiwgamt : "+w_libiwgamt+"     w_libiint : "+w_libiint+"       w_liexcrate : "+w_liexcrate);
					w_liwonhamt = (Number(w_libiwgamt) + Number(w_libiint)) * Number(w_liexcrate);
					won_amt.value = w_liwonhamt;
				}
				function F_Int_Compute(){
					if(pymt_prcp.value == ""){
						w_libiwgamt = 0;
					}else{
						w_libiwgamt = pymt_prcp.value;
					}
					if(libo_int_rate.value == ""){
						w_liliboriyul = 0;
					}else{
						w_liliboriyul = libo_int_rate.value;
					}
					if(dedu_int_rate.value == ""){
						w_licgiyul = 0;
					}else{
						w_licgiyul = dedu_int_rate.value;
					}
					w_libiint = (Number(w_libiwgamt) * (Number(w_liliboriyul) + Number(w_licgiyul)) / 100 * 10000) / 10000;
					pymt_int.value = w_libiint;
				}
				
