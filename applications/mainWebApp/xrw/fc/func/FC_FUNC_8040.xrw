				function send_fc_func_8044_i(){
					if(srch_leas_no.value != get_refvalue("/root/tempData/keyData/k_leas_no")){
						show_message("처리할 수 없는 자료입니다. [선조회 후 작업가능]");
						return false;
					}
					//if(get_refvalue("/root/resData/resform/fc_func_8041_l/dataSet/CURLIST1/record/leas_stat_cd") != "1"){
					if(get_refvalue("/root/resData/resform/fc_func_8041_l/dataSet/CURLIST1/record/leas_stat_cd") != model1.getValue("/root/initData/comboSet/fc_func_8040_m/dataSet/xx_leas_stat_cd")){
						show_message("처리할 수 없는 자료입니다 [종결된 자료임]");
						
					}
					if(get_refvalue("/root/resData/gridData/fc_func_8042_l/dataSet/CURLIST1/record["+datagrid1.row+"]/pymt_yn") == "Y" ){
						show_message("입력할 수 없는 자료입니다");
						return false;
					}
//					alert(group_input.disabled);
					if(group_input.disabled == true){
						show_message("처리할 수 없는 자료입니다 [이전회차 미불입자료 존재]");
						return false;
					}
					if (!check_reqItem(srch_leas_no,"trim","focus","리스번호은 필수입력항목입니다.")) {return false;}
					if (!check_reqItem(pymt_dt,"trim","focus","불입일은 필수입력항목입니다.")) {return false;}
					if (!check_reqItem(comp_dt,"trim","focus","기산일은 필수입력항목입니다.")) {return false;}
					if (!check_reqItem(mtry_dt,"trim","focus","만기일은 필수입력항목입니다.")) {return false;}
					if (!check_reqItem(pymt_prcp,"trim","focus","원금은 필수입력항목입니다.")) {return false;}
					if (!check_reqItem(libo_int_rate,"trim","focus","리보이율은 필수입력항목입니다.")) {return false;}
					if (!check_reqItem(dedu_int_rate,"trim","focus","차감이율은 필수입력항목입니다.")) {return false;}
					if (!check_reqItem(pymt_int,"trim","focus","이자는 필수입력항목입니다.")) {return false;}
					if (!check_reqItem(exrate,"trim","focus","환율은 필수입력항목입니다.")) {return false;}
					if (!check_reqItem(acctcd1,"trim","focus","상대계정은 필수입력항목입니다.")) {return false;}
					//if(acctcd1.value == "0114"){
					if(acctcd1.value == model1.getValue("/root/initData/comboSet/fc_func_8040_m/dataSet/xx_acct_cd_010")){
						if (!check_reqItem(mang_clsf_cd,"trim","focus","관리구분은 필수입력항목입니다.")) {return false;}
					}
					set_refvalue("/root/reqData/reqform/leas_no",srch_leas_no.value);
					set_refvalue("/root/reqData/reqform/pay_cost_clsf",get_refvalue("/root/resData/gridData/fc_func_8042_l/dataSet/CURLIST1/record["+datagrid1.row+"]/pay_cost_clsf"));
					set_refvalue("/root/reqData/reqform/hsty_seq",get_refvalue("/root/resData/gridData/fc_func_8042_l/dataSet/CURLIST1/record["+datagrid1.row+"]/hsty_seq"));
					set_refvalue("/root/reqData/reqform/dlco_clsf_cd",get_refvalue("/root/resData/resform/fc_func_8041_l/dataSet/CURLIST1/record/dlco_clsf_cd"));
					set_refvalue("/root/reqData/reqform/dlco_cd",get_refvalue("/root/resData/resform/fc_func_8041_l/dataSet/CURLIST1/record/dlco_cd"));
					set_refvalue("/root/reqData/reqform/leimchagbcd",imchagbnm.value);
					set_refvalue("/root/reqData/reqform/pch_compnm",pch_compnm.value);
					set_refvalue("/root/reqData/reqform/acctcd",acctcd1.value+"00");
					model1.send("fc_func_8044_i");
					return check_submit_completed("저장되었습니다.","");
				}
				function send_fc_func_8045_d(){
					if (!check_reqItem(srch_leas_no,"trim","focus","리스번호은 필수입력항목입니다.")) {return false;}
					if(srch_leas_no.value != get_refvalue("/root/tempData/keyData/k_leas_no")){
						show_message("처리할 수 없는 자료입니다. [선조회 후 작업가능]");
						return false;
					}
					if(get_refvalue("/root/resData/gridData/fc_func_8042_l/dataSet/CURLIST1/record["+datagrid1.row+"]/pymt_yn") != "Y" ){
						show_message("입력할 수 없는 자료입니다");
						return false;
					}
					if (show_message("삭제하시겠습니까?","삭제 확인",MB_YESNO) != IDYES) {
						return false;
					}
					set_refvalue("/root/reqData/reqform/leas_no",srch_leas_no.value);
					set_refvalue("/root/reqData/reqform/pay_cost_clsf",get_refvalue("/root/resData/gridData/fc_func_8042_l/dataSet/CURLIST1/record["+datagrid1.row+"]/pay_cost_clsf"));
