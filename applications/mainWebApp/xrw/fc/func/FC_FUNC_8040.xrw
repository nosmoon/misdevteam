					model1.send("fc_func_8041_l");
					totalcnt = model.getValue("/root/resData/resform/fc_func_8041_l/dataSet/CURLIST1/totalcnt");
					if (totalcnt == 0) {
						reset_model(true);
						show_message("검색결과가 없습니다.");
						return false;
					}
//					set_refvlalue("/root/tempData/keyData/k_leas_no",srch_leas_no.value);
					w_leliboriyul 	= get_refvalue("/root/resData/resform/fc_func_8041_l/dataSet/CURLIST1/record[1]/libo_int_rate");
