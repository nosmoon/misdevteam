					set_refvalue("/root/reqData/reqform/frex_cd",get_refvalue("/root/resData/gridData/fc_func_8052_l/dataSet/CURLIST1/record["+grow+"]/frex_cd"));
					set_refvalue("/root/reqData/reqform/exrate",get_refvalue("/root/resData/gridData/fc_func_8052_l/dataSet/CURLIST1/record["+grow+"]/exrate"));
