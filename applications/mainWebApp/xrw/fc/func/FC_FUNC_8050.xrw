				function send_fc_func_8051_l(){
					insr_nm.value = "";
					insr_amt.value = "";
					insr_rate.value = "";
					frcr_amt.value = "";
					exrate.value = "";
					won_amt.value = "";
					frex_cd.value = "";
					comp_dt.value = "";
					mtry_dt.value = "";
					dlco_clsf_cd.value = "";
					dlco_cd.value = "";
					bhcompnm.value = "";
					medi_cd.value = "";
					mcnm.value = "";
					dtls_medi_cd.value = "";
					use_dept_cd.value = "";
					buseonm.value = "";
					pymt_rmks.value = "";
					fisc_dt.value = "";
					jpno.value = "";
					insr_stat_cdnm.value = "";
					model1.refresh();
					if(!commReqCheck(group_scr)) {return false;}
					
					model1.send("fc_func_8051_l");
					totalcnt = model.getValue("/root/resData/resform/fc_func_8051_l/dataSet/CURLIST1/totalcnt");
					
					if (totalcnt == 0) {
						reset_model(true);
						show_message("검색결과가 없습니다.");
						return false;
					}
					model1.send("fc_func_8052_l");
					var rows = datagrid1.rows;
					if(datagrid1.rows < 14){
						for(i = 0 ; i < 14 - rows ; i++){
							datagrid1.addRow();
						}
					}
					set_refvalue("/root/tempData/keyData/k_leas_no",srch_leas_no.value);
				}
				function send_fc_func_8053_i(){
					if(!commReqCheck(group_scr)) {return false;}
					if(!commReqCheck(group_input)) {return false;}
					if(srch_leas_no.value!=get_refvalue("/root/tempData/keyData/k_leas_no")){
						show_message("Key 항목이 변경되었습니다. 조회 후 작업하십시오");
						return false;
					}
					set_refvalue("/root/reqData/reqform/insr_no",srch_leas_no.value);
										
					model1.send("fc_func_8053_i");
					return check_submit_completed("저장되었습니다.","");
				}
				function send_fc_func_8054_d(){
					if(srch_leas_no.value!=get_refvalue("/root/tempData/keyData/k_leas_no")){
						show_message("Key 항목이 변경되었습니다. 조회 후 작업하십시오");
						return false;
					}
					if(!commReqCheck(group_scr)) {return false;}
					if (!check_reqItem(comp_dt,"trim","focus","기산일은 필수입력항목입니다.")) {return false;}
					set_refvalue("/root/reqData/reqform/insr_no",srch_leas_no.value);
					set_refvalue("/root/reqData/reqform/comp_dt",comp_dt.value);
					set_refvalue("/root/reqData/reqform/slip_occr_dt",get_refvalue("/root/resData/gridData/fc_func_8052_l/dataSet/CURLIST1/record["+datagrid1.row+"]/pymt_slip_occr_dt"));
					set_refvalue("/root/reqData/reqform/slip_clsf_cd",get_refvalue("/root/resData/gridData/fc_func_8052_l/dataSet/CURLIST1/record["+datagrid1.row+"]/pymt_slip_clsf_cd"));
					set_refvalue("/root/reqData/reqform/slip_seq",get_refvalue("/root/resData/gridData/fc_func_8052_l/dataSet/CURLIST1/record["+datagrid1.row+"]/pymt_slip_seq"));
					if (show_message("삭제하시겠습니까?","삭제 확인",MB_YESNO) != IDYES) {
						return false;
					}
					model1.send("fc_func_8054_d");
					return check_submit_completed("삭제되었습니다.","");
				}
