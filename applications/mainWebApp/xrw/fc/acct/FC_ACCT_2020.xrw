					datagrid1.addRow(false);
					//datagrid1.rebuild();
				}
				
				for(i = datagrid1.fixedRows ; i <= datagrid1.rows ; i++){
					datagrid1.removeStatus(i,"insert");
				}
				datagrid1.refresh();
				
				//srch_slip_clsf_cd.select(32); //2012.01.11 62 원고료 
				srch_slip_clsf_cd.value = "62";
				
