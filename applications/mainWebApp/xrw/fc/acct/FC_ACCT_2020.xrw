				var ch_var;
				
				var orgStr = model.getValue("/root/initData/comboSet/fc_acct_2020_m/dataSet/xx_ch_var");
				for(i = datagrid1.fixedRows ; i < rows ; i++){
					if(datagrid1.valueMatrix(i,col_acct_no) != ""){
						ch_amt.value == "";
					}
					ch_var = datagrid1.valueMatrix(i,col);		
					
					if(instr_yn(orgStr, ch_var)){ // 2011.01.11					
