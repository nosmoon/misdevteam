				model.send("fc_acct_2020_m");				
				makeComboGubun("","/root/initData/comboSet/fc_acct_2020_m/dataSet/CURLIST1","record","cdabrvnm_cd", "cd");
				makeComboGubun("","/root/initData/comboSet/fc_acct_2020_m/dataSet/CURLIST2","record","cdabrvnm_cd", "cd");
