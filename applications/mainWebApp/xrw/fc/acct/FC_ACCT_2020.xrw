				//날짜
				set_format(
						"month_1"
					,	budg_yymm					//날짜포맷,중간정렬,중간정렬,yyyy-mm
				);
				
				//문자열(왼쪽)
				set_format(
						"string_l"						//문자열,왼쪽정렬,중간정렬
					,	datagrid1				,"2,4,5,6,7,14,21"
					,	srch_slip_clsf_cd
					,	pch_buseonm
					,	titl
					,	pch_mcnm
					,	pch_ddjnm
					,	budg_cd
					,	dtls_medi_cdnm
