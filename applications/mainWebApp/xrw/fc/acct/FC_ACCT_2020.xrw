					,	medi_cd
					,	pch_mcnm
					,	draft_doc_no
					,	chrg_emp_no
					,	dtls_medi_cd
					,	dtls_medi_cdnm
					,	adpay_expn_no
					,	end_yn
				);
				//input date init
				set_default(
						"it was comment field"
					,	srch_slip_occr_dt
				);

				//control format 설정 적용
				
				//코드
				set_format(
						"code"						//코드포맷,중간정렬,중간정렬
					,	datagrid1				,"3,6,19,20"
					,	srch_slip_seq
					,	clam_dept_cd
					,	medi_cd
					,	draft_doc_no
					,	chrg_emp_no
					,	dtls_medi_cd
					,	end_yn
				);
				
				//금액
				set_format(
						"number4"						//숫자포맷,오른쪽정렬,중간정렬,(#,###)
					,	datagrid1				,"10,11,12,13,15,16,17,18"
					,	clam_amt
					,	atic_fee_sum
					,	coms_sum
					,	with_tax_sum
					,	cal_pay_amt_sum
					,	draft_expn_amt
				);
				
