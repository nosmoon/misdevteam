							datagrid1.isReadOnly(i,col_res_tax) = true; 
							datagrid1.isReadOnly(i,col_with_tax) = true; 
							datagrid1.isReadOnly(i,col_cal_pay_amt) = true; 
						}
					}
				}
			}
			
