			function amt_sum(){
				var row = datagrid1.fixedRows;
				var rows = datagrid1.rows;
				
				var atic_fee_col = datagrid1.colRef("atic_fee");
				var coms_col = datagrid1.colRef("coms");
				var with_tax_col = datagrid1.colRef("with_tax");

				var atic_fee_sum_t = 0;
				var coms_sum_t = 0;
				var with_tax_sum_t = 0;
				
				for(var i = datagrid1.fixedRows ; i < rows ; i++){
					atic_fee_sum_t = atic_fee_sum_t + Number(datagrid1.valueMatrix(i,atic_fee_col));
					coms_sum_t = coms_sum_t + Number(datagrid1.valueMatrix(i,coms_col));
					with_tax_sum_t = with_tax_sum_t + Number(datagrid1.valueMatrix(i,with_tax_col));
				}
				set_refvalue(atic_fee_sum,atic_fee_sum_t);
				set_refvalue(coms_sum,coms_sum_t);
				set_refvalue(with_tax_sum,with_tax_sum_t);
				set_refvalue(cal_pay_amt_sum,(atic_fee_sum_t - with_tax_sum_t));
				clam_amt.value = atic_fee_sum_t;
				
			}
			
