					if(ch_var == "9004" && model.property("ss_cmpycd") == "326"){ //2015.08.06 봉은정 추가
							datagrid1.isReadOnly(i,col_incm_tax) = false; 
							datagrid1.isReadOnly(i,col_res_tax) = false; 
							datagrid1.isReadOnly(i,col_with_tax) = false; 
							datagrid1.isReadOnly(i,col_cal_pay_amt) = false;
