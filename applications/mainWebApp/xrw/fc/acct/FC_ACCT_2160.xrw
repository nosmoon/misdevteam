					    	show_message("마감된 자료이니, 입력, 수정할 수 없습니다.");
					    	return false;
					}
					
					var cnt = 0;

					for (i=1; i<datagrid1.rows; i++){	
						var dlco_clsf_cd  = datagrid1.valueMatrix(i, 1);
						var dlco_cd = datagrid1.valueMatrix(i, 2);
						var dlco_nm = datagrid1.valueMatrix(i, 3);
						var incm_clsf_cd = datagrid1.valueMatrix(i, 4);
						var obj_amt = datagrid1.valueMatrix(i, 5);
						var incm_tax = datagrid1.valueMatrix(i, 6);
						var res_tax = datagrid1.valueMatrix(i, 7);
						var budg_cd = datagrid1.valueMatrix(i, 9);
						var mang_clsf_cd = datagrid1.valueMatrix(i, 11);
						var comp_dt = datagrid1.valueMatrix(i, 12);
						
						if (dlco_clsf_cd == "" && dlco_cd == "" && dlco_nm == "" && incm_clsf_cd =="" &&obj_amt == "" && incm_tax =="" && res_tax ==""  && budg_cd==""){
							datagrid1.rowStatus(i) =0;
						}
						else if(datagrid1.rowStatus(i) == "3" || datagrid1.rowStatus(i) == "2" || datagrid1.rowStatus(i) == "1"){
							cnt = cnt+1;
							if(dlco_clsf_cd == ""){
								show_message("거래처구분은 필수입력항목입니다.");
								return;
							}
							else if(dlco_cd ==""){
								show_message("거래처코드는 필수입력항목입니다.");
								return;
							}
							else if(dlco_nm ==""){
								show_message("거래처명은 필수입력항목입니다.");
								return;
							}
							else if(incm_clsf_cd ==""){
								show_message("소득구분은 필수입력항목입니다.");
								return;
							}
							else if(obj_amt ==""){
								show_message("대상금액은 필수입력항목입니다.");
								return;
							}
							else if(incm_tax ==""){
								show_message("소득세는 필수입력항목입니다.");
								return;
							}
							else if(res_tax ==""){
								show_message("주민세는 필수입력항목입니다.");
								return;
							}
							else if(budg_cd == model.getValue("/root/initData/comboSet/fc_acct_2160_m/dataSet/xx_budg_cd")){	//입금구부이 보통예금인경우 관리구분, 기산일 필수
								 if(mang_clsf_cd ==""){
									show_message("입금구분이 보통예금인 경우 관리구분은 필수입력항목입니다.");
									return;
								}
								else if(comp_dt ==""){
									show_message("입금구분이 보통예금인 경우 기산일은 필수입력항목입니다.");
									return;
								}
							}
						}
					}

					if(cnt == 0){
						show_message("저장할 대상이 없습니다.");
						change_status();
						return;	
					}									
					if (show_message("저장하시겠습니까?","저장 확인",MB_YESNO) != IDYES) {
					   	return false;
					}
					
					set_refvalue("/root/reqData/multiUpDate/multiUpdateData", datagrid1.getUpdateData());
					set_refvalue("/root/reqData/multiUpDate/incmg_dt",srch_incmg_dt.value);
					set_refvalue("/root/reqData/multiUpDate/impt_dept",srch_impt_dept.value);
			
					model.send("fc_acct_2164_i");

					if(submit_completed()){					
						show_message("저장되었습니다.");	
						btn_select.dispatch("onclick");
					}
					else{
						change_status();
						return;	
					}
		
				}				
