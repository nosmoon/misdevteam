				function send_fc_acct_2160_m(){
					model.send("fc_acct_2160_m");
					makeComboGubun("","/root/initData/comboSet/fc_acct_2160_m/dataSet/CURLIST","record","cdabrvnm_cd", "cd");
					makeComboGubun("", "/root/initData/comboSet/fc_acct_2160_m/dataSet/CURLIST2", "record", "cd_abrv_nm", "cd");
					makeComboGubun("", "/root/initData/comboSet/fc_acct_2160_m/dataSet/CURLIST3", "record", "cd_abrv_nm", "cd");
					makeComboGubun("", "/root/initData/comboSet/fc_acct_2160_m/dataSet/CURLIST4", "record", "budg_abrv_nm", "budg_cd");
				}
				function send_fc_acct_2161_l(){
					if (!prepare_fc_acct_2161_l()) {
						return false;
					}
//    '-----------------------------------------------------------------
//    ' 개인별권한관리
//    '=================================================================
//    ''이화면은 원천세테이블에서 담당자를 관리 안해서 담당자 체크는 안하는걸로 했다
//    If GF_SYBUSEOCDYB(IN_INBUSEOCD$) = False Then
//       If W_GUNHAN = "G" Then
//          MsgBox "사용가능한 부서코드가 아닙니다", vbCritical, Cmndll.GF_GetMessage(1, 201)
//          Exit Sub
//       End If
//       Exit Sub
//    End If

					datagrid1.allstyle( "data", "background-color" ) = "#ffffff";
					reset_refnode("/root/resData/gridData/fc_acct_2161_l/dataSet");
					reset_refnode("/root/resData/resForm/siamt");
					reset_refnode("/root/resData/resForm/jpno");
					reset_refnode("/root/resData/resForm/jpsiyb");					
					
					if (fn_cmpcMgmt(srch_impt_dept.value) == false) { return; }
					
					model.send("fc_acct_2161_l");
					
					var var_siamt = 0;
					var totalcnt = get_refvalue("/root/resData/gridData/fc_acct_2161_l/dataSet/CURLIST/totalcnt");
					if (totalcnt == 0) {
						reset_refnode("/root/resData/gridData/fc_acct_2161_l/dataSet");
						reset_refnode("/root/resData/resForm/siamt");
						reset_refnode("/root/resData/resForm/jpno");
						reset_refnode("/root/resData/resForm/jpsiyb");
						model.refresh();
						show_message("검색결과가 없습니다.");
						datagrid_addrow(datagrid1,10,false);
						return;
					}
					
					var slip_occr_dt;
					var slip_clsf_cd;
					var slip_seq;
					if(get_refvalue("/root/resData/gridData/fc_acct_2161_l/dataSet/CURLIST/record[1]/slip_occr_dt") != ""){
						slip_occr_dt = get_refvalue("/root/resData/gridData/fc_acct_2161_l/dataSet/CURLIST/record[1]/slip_occr_dt");
						slip_clsf_cd = get_refvalue("/root/resData/gridData/fc_acct_2161_l/dataSet/CURLIST/record[1]/slip_clsf_cd");
						slip_seq = get_refvalue("/root/resData/gridData/fc_acct_2161_l/dataSet/CURLIST/record[1]/slip_seq");
						
						set_refvalue("/root/resData/resForm/jpno",slip_occr_dt +"-" + slip_clsf_cd + "-" + slip_seq);
						set_refvalue("/root/resData/resForm/rmks1", get_refvalue("/root/resData/gridData/fc_acct_2161_l/dataSet/CURLIST/record[1]/rmks1"));
						set_refvalue("/root/resData/resForm/budg_yymm", get_refvalue("/root/resData/gridData/fc_acct_2161_l/dataSet/CURLIST/record[1]/budg_yymm"));
						
						set_refvalue("/root/reqData/searchData/slip_occr_dt",slip_occr_dt);
						set_refvalue("/root/reqData/searchData/slip_clsf_cd",slip_clsf_cd);
						set_refvalue("/root/reqData/searchData/slip_seq",slip_seq);
						
						model.send("fc_acct_2168_l");
						var totalcnt = get_refvalue("/root/resData/resForm/fc_acct_2168_l/dataSet/CURLIST/totalcnt");
						if (totalcnt == 0) {
							show_message("전표승인 조회중 error.");
							return false;
						}
						if(get_refvalue("/root/resData/resForm/fc_acct_2168_l/dataSet/CURLIST/record/slip_aprv_yn") == "2"){
							jpsiyb.value = "승인";
						}
						else{
							jpsiyb.value = "미승인";
						}
					}
					else{
//						set_refvalue("/root/resData/resForm/jpno","");
						set_refvalue("/root/resData/resForm/rmks1", "");
						set_refvalue("/root/reqData/resForm/budg_yymm",commCurDate().substring(0,6));												
					}

					for(i=1; i<=datagrid1.rows;i++){
						var_siamt = Number(var_siamt) + Number(get_refvalue("/root/resData/gridData/fc_acct_2161_l/dataSet/CURLIST/record["+i+"]/rcpm_amt"));
					}
					siamt.value = var_siamt;

					//  key save
					w_bscymd = srch_incmg_dt.value;
					W_SIBuseoCd = srch_impt_dept.value;
					
					datagrid_addrow(datagrid1,10,false);
					datagrid1.refresh();
				}
				function prepare_fc_acct_2161_l(){
					if(!check_search_values("s")) {
						return false;
					}
					return true;
				}
				function check_search_values(p_siud) {
					//필수항목체크(검색)
					if(!commReqCheck(group_scr)) {
						return false;
					}
					
					switch (p_siud.toUpperCase()) {
						case "S":
							//조회시
							break;
						case "I":
							//입력시
							break;
						case "U":
							//수정시
							break;
						case "D":
							//삭제시
							break;
						default:
							show_message("key 값 확인을 위한 구분자가 지정되지 않았습니다.");
							break;
					}
				
					return true;
				}
				function open_fc_acct_1180(p_where,p_mode,p_title,p_readonly) {
					var need_popup = true;
					

					//버튼클릭으로 이벤트가 발생하기 전에
					//value-changed 이벤트가 먼저 발생한 경우
					//input button의 내부 버튼이 클릭됐는지 여부를 확인한다.
					if (check_dup_input_event()) {					
						return rcvs_flag;
					}
										
					//검색조건이 모두 충족하지 않으면 종료한다.
					if (p_where == "FOCUS" && (get_refvalue("/root/resData/gridData/fc_acct_2161_l/dataSet/CURLIST/record["+datagrid1.row+"]/dlco_clsf_cd") == "" || get_refvalue("/root/resData/gridData/fc_acct_2161_l/dataSet/CURLIST/record["+datagrid1.row+"]/dlco_cd") == "")) {
						set_refvalue("/root/resData/gridData/fc_acct_2161_l/dataSet/CURLIST/record["+datagrid1.row+"]/dlco_nm","");
						return;
					}
					
					p_where = p_where.toUpperCase();
					
					var popid = "fc_acct_1180";
					var subid = "fc_acct_1181_l";
	
					set_popup_send_data(popid		,"mode"			,p_mode			);
					set_popup_send_data(popid		,"title"			,p_title			);
					set_popup_send_data(popid		,"readonly"		,p_readonly		);
					set_popup_send_data(popid		,"clsf_cd"			,get_refvalue("/root/resData/gridData/fc_acct_2161_l/dataSet/CURLIST/record["+datagrid1.row+"]/dlco_clsf_cd"));
					set_popup_send_data(popid		,"cd_f"			,get_refvalue("/root/resData/gridData/fc_acct_2161_l/dataSet/CURLIST/record["+datagrid1.row+"]/dlco_cd"));
					set_popup_send_data(popid		,"cd_t"			,get_refvalue("/root/resData/gridData/fc_acct_2161_l/dataSet/CURLIST/record["+datagrid1.row+"]/dlco_cd"));
