				// 그리드 컬럼 합계
				function fn_grid_sum_value (p_obj, p_col, t_obj) {
					if (typeof(p_obj) != DATA_TYPE_OBJECT) {
						return;
					}
					if (typeof(t_obj) != DATA_TYPE_OBJECT) {
						return;
					}
					
					var pobj = eval(p_obj);
					var pcol = eval(p_col);
					var tobj = eval(t_obj);
					
					var var_sum = 0;
					
					for (i=1;i<=pobj.rows;i++) {
						var_sum = var_sum + Number(pobj.valueMatrix(i, pcol));
					}
					
					t_obj.value = var_sum;
				}
				
