				function send_fc_acct_6211_s(){
					if (!prepare_fc_acct_6211_s()) {
						return false;
					}

					var date_diff;
					date_diff = get_datediff(srch_from_fisc_dt.value,srch_to_fisc_dt.value);

					if(date_diff > 366){
						show_message("1년이 초과하는 자료는 출력할 수 없습니다.");
						return;
					}
					
					model.send("fc_acct_6211_s");
					
					model.setValue("/root/reqData/searchData/erplace_cd",model.getValue("/root/resData/resForm/erplace_cd"));
					model.setValue("/root/reqData/searchData/vat_fr_yymm",model.getValue("/root/resData/resForm/vat_fr_yymm"));
					model.setValue("/root/reqData/searchData/vat_to_yymm",model.getValue("/root/resData/resForm/vat_to_yymm"));
					
					if( submit_completed() ){
						var erplace_cd = model.getValue("/root/resData/resForm/erplace_cd");
						if(erplace_cd == ""){
							button1.disabled = true;
							button2.disabled = true;
							button3.disabled = true;
							button4.disabled = true;
							button6.disabled = true;
						}else{
							button1.disabled = false;
							button2.disabled = false;
							button3.disabled = false;
							button4.disabled = false;
							button6.disabled = false;
