				function sum_md_vat_26(){
					var vat_a = Number(model.getValue("/root/resData/resForm/mb_vat_17_d"));	//(다)
					var vat_b = Number(model.getValue("/root/resData/resForm/mc_vat_20"));	//(라)
					var vat_c = Number(model.getValue("/root/resData/resForm/mc_vat_21"));	//(마)
					var vat_d = Number(model.getValue("/root/resData/resForm/mc_vat_22"));	//(바)
					var vat_e = Number(model.getValue("/root/resData/resForm/mc_vat_23"));	//(사)
					var vat_f = Number(model.getValue("/root/resData/resForm/mc_vat_24"));		//(아)
					var vat_g = Number(model.getValue("/root/resData/resForm/mc_vat_25"));	//(자)

					var vat_26 = vat_a - vat_b - vat_c - vat_d - vat_e + vat_f + vat_g;

					model.setValue("/root/resData/resForm/md_vat_26", vat_26);
										
					inp_md_vat_26.refresh();

				}			
