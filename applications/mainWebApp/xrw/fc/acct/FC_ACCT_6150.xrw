				function send_fc_acct_6150_m(){
					model.send("fc_acct_6150_m");
					makeComboGubun("","/root/initData/comboSet/fc_acct_6150_m/dataSet/CURLIST","record","cdabrvnm_cd", "cd");
				}
				function send_fc_acct_6151_l(){
					if (!prepare_fc_acct_6151_l()) {
						return false;
					}

					var date_diff;
					date_diff = get_datediff(srch_from_fisc_dt.value,srch_to_fisc_dt.value);

					if(date_diff > 366){
						show_message("1년이 초과하는 자료는 출력할 수 없습니다.");
						return;
					}
					
					model.send("fc_acct_6151_l");
					
					totalcnt = model.getValue("/root/resData/gridData/fc_acct_6151_l/dataSet/CURLIST1/totalcnt");
					
					if (totalcnt == 0) {
						model.reset();						
						show_message("검색결과가 없습니다.");
					}
				}
				function prepare_fc_acct_6151_l(){
					if(!check_search_values("s")) {
						return false;
					}					
					return true;
				}
				function check_search_values(p_siud) {
					//필수항목체크(검색)
					if(!commReqCheck(group_scr)) {
						return false;
					}			
					return true;	
				}
				function send_fc_acct_6152_l(){					
					if(get_refvalue("/root/resData/gridData/fc_acct_6151_l/dataSet/CURLIST1/record["+(datagrid1.row-1)+"]/acct_cd") == "9999" ){
						reset_refnode("/root/resData/gridData2/fc_acct_6152_l/dataSet");
						datagrid2.refresh();
						datagrid3.refresh();
					   	return false;
					}
					if(get_refvalue("/root/resData/gridData/fc_acct_6151_l/dataSet/CURLIST1/record["+(datagrid1.row-1)+"]/diff_amt") == 0) {
						reset_refnode("/root/resData/gridData2/fc_acct_6152_l/dataSet");
						datagrid2.refresh();
						datagrid3.refresh();						
					   	return false;
					}
					var var_slip_clsf_cd = get_refvalue("/root/resData/gridData/fc_acct_6151_l/dataSet/CURLIST1/record["+(datagrid1.row-1)+"]/slip_clsf_cd");
					var var_acct_cd = get_refvalue("/root/resData/gridData/fc_acct_6151_l/dataSet/CURLIST1/record["+(datagrid1.row-1)+"]/acct_cd");
					var var_from_fisc_dt = get_refvalue("/root/resData/gridData/fc_acct_6151_l/dataSet/CURLIST1/record["+datagrid1.row+"]/from_fisc_dt");
					var var_to_fisc_dt = get_refvalue("/root/resData/gridData/fc_acct_6151_l/dataSet/CURLIST1/record["+datagrid1.row+"]/to_fisc_dt");
					
					set_refvalue("/root/reqData/searchData/slip_clsf_cd",var_slip_clsf_cd);
					set_refvalue("/root/reqData/searchData/acct_cd",var_acct_cd);
					set_refvalue("/root/reqData/searchData/from_fisc_dt1",var_from_fisc_dt);
					set_refvalue("/root/reqData/searchData/to_fisc_dt1",var_to_fisc_dt);
				
					model.send("fc_acct_6152_l");
					
//					totalcnt = model.getValue("/root/resData/gridData2/fc_acct_6152_l/dataSet/CURLIST1/totalcnt");
//					totalcnt1 = model.getValue("/root/resData/gridData2/fc_acct_6152_l/dataSet/CURLIST2/totalcnt");					
//					if (totalcnt == 0 || totalcnt1 == 0) {
//						show_message("검색결과가 없습니다.");
//					}					
				}
			]]>
