				function send_fc_acct_2411_l(){					
					if(model.getValue("/root/reqData/searchData/acct_cd") == "" || model.getValue("/root/reqData/searchData/acct_cd").length != 4){
						show_message("계정코드는 필수입력 항목입니다.");
						model.setFocus("srch_acct_cd");
						return;
					}				
				
					if(model.getValue("/root/reqData/searchData/frfisc_dt") == "" || model.getValue("/root/reqData/searchData/frfisc_dt").length != 8){
						show_message("회계일자FR은 필수입력 항목입니다.");
						model.setFocus("srch_frfisc_dt");
						return;
					}
					if(model.getValue("/root/reqData/searchData/tofisc_dt") == "" || model.getValue("/root/reqData/searchData/tofisc_dt").length != 8){
						show_message("회계일자TO는 필수입력 항목입니다.");
						model.setFocus("srch_tofisc_dt");						
						return;
					}					
					
					if(model.getValue("/root/reqData/searchData/frfisc_dt") > model.getValue("/root/reqData/searchData/tofisc_dt")){
						show_message("회계일자 입력 오류입니다.");
						return;
					}

					var fisc_dt_diff;
					fisc_dt_diff = get_datediff(srch_frfisc_dt.value,srch_tofisc_dt.value);
					
					//if(fisc_dt_diff > 366){
					//	show_message("1년이 초과하는 자료는 조회할 수 없습니다.");
					//	return;
					//}					
					
				 	model.setValue("/root/reqData/searchData/pch_x"," ");
				 	
				 	var ss_deptcd = getProperty("ss_dept_cd");

				 	if(ss_deptcd.substring(0,2) =="31"){
			               	var orgStr = model.getValue("/root/initData/comboSet/dataSet/xx_srch_acct_cd");	 		
				 		
				 		if(! instr_yn(orgStr, srch_acct_cd.value)){
					 	//if(srch_acct_cd.value != "0302" && srch_acct_cd.value != "0313" && srch_acct_cd.value != "4885" && srch_acct_cd.value != "2402"){
					 		show_message("귀하의 부서에서는 조회불가 계정코드입니다.");
					 		return;
					 	}
					}

					if(model.getValue("/root/reqData/searchData/dlco_clsf_cd") != ""){
						model.setValue("/root/reqData/searchData/pch_x","S");
					}
					if(model.getValue("/root/reqData/searchData/dlco_cd") != ""){
						model.setValue("/root/reqData/searchData/pch_x","S");
					}					
					if(model.getValue("/root/reqData/searchData/medi_cd") != ""){
						model.setValue("/root/reqData/searchData/pch_x","S");
					}
					if(model.getValue("/root/reqData/searchData/mang_clsf_cd") != ""){
						model.setValue("/root/reqData/searchData/pch_x","S");
					}
					if(model.getValue("/root/reqData/searchData/evnt_cd").substring(0,3) != ""){
						model.setValue("/root/reqData/searchData/pch_x","S");
					}					

					if(model.getValue("/root/reqData/searchData/drcr_clsf") =="1 9"){
						model.setValue("/root/reqData/searchData/drcr_clsf1","1");
						model.setValue("/root/reqData/searchData/drcr_clsf2","9");
					}
					else if(model.getValue("/root/reqData/searchData/drcr_clsf") =="1"){
						model.setValue("/root/reqData/searchData/drcr_clsf1","1");
						model.setValue("/root/reqData/searchData/drcr_clsf2","");						
					}
					else if(model.getValue("/root/reqData/searchData/drcr_clsf") =="9"){
						model.setValue("/root/reqData/searchData/drcr_clsf1","");						
						model.setValue("/root/reqData/searchData/drcr_clsf2","9");
					}		
					else{
						model.setValue("/root/reqData/searchData/drcr_clsf1","");
						model.setValue("/root/reqData/searchData/drcr_clsf2","");						
					}
					
					model.send("fc_acct_2411_l");
					
					if(model.getValue("/root/resData/gridData/fc_acct_2411_l/dataSet/CURLIST/totalcnt") == "0"){
						show_message("검색결과가 없습니다.");
						datagrid1.refresh();
						reset_dtl();
//						return;
					}			
					reset_dtl();
					
					model.setValue("/root/resData/resform/acct_cd_nm1",model.getValue("/root/reqData/searchData/acct_cd_nm"));
					model.setValue("/root/resData/resform/frobj_dt",model.getValue("/root/reqData/searchData/frfisc_dt"));
					model.setValue("/root/resData/resform/toobj_dt",model.getValue("/root/reqData/searchData/tofisc_dt"));
					model.setValue("/root/resData/resform/dlco_nm",model.getValue(" /root/reqData/searchData/dlco_cd_nm"));
					
					var cbhap = 0;
					var dbhap = 0;
					var juncbamt;
					var jundbamt;
					var cbjan;
					var dbjan;
					for(i=1 ; i <=datagrid1.rows; i++){
						cbhap = Number(cbhap) + Number(datagrid1.valueMatrix(i,4));
						dbhap = Number(dbhap) + Number(datagrid1.valueMatrix(i,5));
					}
					
					if(get_refvalue("/root/resData/gridData/fc_acct_2411_l/dataSet/CURAMT/record/bal_clsf") == "1"){
						juncbamt = (Number(get_refvalue("/root/resData/gridData/fc_acct_2411_l/dataSet/CURAMT/record/fajcbamt")) + Number(get_refvalue("/root/resData/gridData/fc_acct_2411_l/dataSet/CURAMT/record/fdajcbamt")))
								 - (Number(get_refvalue("/root/resData/gridData/fc_acct_2411_l/dataSet/CURAMT/record/fajdbamt")) + Number(get_refvalue("/root/resData/gridData/fc_acct_2411_l/dataSet/CURAMT/record/fdajdbamt")));
						set_refvalue("/root/reqData/reqform/fajcbamt",juncbamt);
						
						//alert("차변합계::>>"+cbhap+"///  대변합계::>>"+dbhap+"///  전작액"+juncbamt);
						
		//alert("A::>>"+Number(get_refvalue("/root/resData/gridData/fc_acct_2411_l/dataSet/CURAMT/record/fajcbamt"))+"B::>>"+Number(get_refvalue("/root/resData/gridData/fc_acct_2411_l/dataSet/CURAMT/record/fdajcbamt")));
		//alert("C::>>"+Number(get_refvalue("/root/resData/gridData/fc_acct_2411_l/dataSet/CURAMT/record/fajdbamt"))+"D::>>"+Number(get_refvalue("/root/resData/gridData/fc_acct_2411_l/dataSet/CURAMT/record/fdajdbamt")));
						
						
						cbjan = (Number(juncbamt) + Number(cbhap)) - Number(dbhap);
					}
					else{
						jundbamt = (Number(get_refvalue("/root/resData/gridData/fc_acct_2411_l/dataSet/CURAMT/record/fajdbamt")) + Number(get_refvalue("/root/resData/gridData/fc_acct_2411_l/dataSet/CURAMT/record/fdajdbamt")))
								 - (Number(get_refvalue("/root/resData/gridData/fc_acct_2411_l/dataSet/CURAMT/record/fajcbamt")) + Number(get_refvalue("/root/resData/gridData/fc_acct_2411_l/dataSet/CURAMT/record/fdajcbamt")));
						set_refvalue("/root/reqData/reqform/fajdbamt",jundbamt);
						dbjan = (Number(jundbamt) + Number(dbhap)) - Number(cbhap);
					}

					set_refvalue("/root/reqData/reqform/fdajcbamt",cbhap);
					set_refvalue("/root/reqData/reqform/fdajdbamt",dbhap);
					set_refvalue("/root/reqData/reqform/fcbjan",cbjan);
					set_refvalue("/root/reqData/reqform/fdbjan",dbjan);																					
					
					model.refresh();					
				}				
