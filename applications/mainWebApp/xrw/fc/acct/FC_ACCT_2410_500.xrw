				function send_fc_acct_2410_m(){
					model.send("fc_acct_2410_m");

					srch_medi_cd.refresh();
					srch_slip_clsf_cd.refresh();
					
					makeComboGubun("","/root/initData/comboSet/dataSet/MEDI_CD","record","cd_nm", "cd");
