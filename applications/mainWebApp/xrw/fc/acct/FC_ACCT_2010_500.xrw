										if (model.getValue("/root/tempData/confirm") != "Y"){
											return;
										}									 	
								 	
									 }
								}
								
								if (datagrid1.valueMatrix(i, 7).substring(0, 3) != "941" && datagrid1.valueMatrix(i, 7).substring(0, 2) != "94") {
