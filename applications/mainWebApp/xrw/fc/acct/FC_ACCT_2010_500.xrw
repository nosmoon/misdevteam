											// 고객의 주민등록번호 체크
											if (commCheckRrnoVal(datagrid1.valueMatrix(i, 8).ReplaceString("-", "")) == false) { return; }
										}
									}
								}
	
								// 예산코드만 선택한 COL은 내역등록 화면을 띄운다.							
	//						        spr1.Row = i
	//						        spr1.Col = 1
	//						        If Trim(spr1.Text) = "" And spr1.BackColor <> spreadcolor_select Then
	//						            spr1.Col = 4
	//						            If Trim(spr1.Text) <> "" Then
	//						                spr1.Col = 4: spr1.Action = 0
	//						                'Call spr1_Click(4, i)
	//						                Call spr1_DblClick(4, i)
	//						                i = 0
	//						            End If
	//						        End If
	
								/*********************************
									데이터 체크2
								**********************************/
								if (datagrid1.valueMatrix(i, 1).substring(0, 3) == "160") {
									var_err_flag = false;
								}
	
								if (Number(datagrid1.valueMatrix(i, 5)) != 0) {
									var_tax_stmt_flag = true;
								}
								else {
									var_tax_stmt_flag = false;
								}
								
								if (datagrid1.valueMatrix(i, 26) != "" && datagrid1.valueMatrix(i, 26).ReplaceString("-", "").substring(0, 6) != budg_yymm.value.ReplaceString("-", "")) {
									show_message("예산년월과 작성일자는 일치해야 합니다.");
									return;
								}
								
								if (datagrid1.valueMatrix(i, 38) == "8900" || datagrid1.valueMatrix(i, 38) == "8950") {
									var_tax_stmt_flag = true;
								}
	
								if (var_tax_stmt_flag == true && datagrid1.valueMatrix(i, 25) == "") {
