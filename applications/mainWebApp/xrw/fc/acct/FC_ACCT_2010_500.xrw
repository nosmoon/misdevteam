										var_suply_amt1 = var_suply_amt1 + Number(datagrid1.valueMatrix(i, 4)); 	// 과표
									}
									var_vat_amt1 = var_vat_amt1 + Number(datagrid1.valueMatrix(i, 5));				// 세액
									var_with_tax1 = var_with_tax1 + Number(datagrid1.valueMatrix(i, 14));			// 원천징수
								}

//						model.refresh();								
								datagrid1.valueMatrix(i, 58) = datagrid1.valueMatrix(i, 4);
								
								// 선급정산
								if (expn_clsf.value == "3") {
									if (datagrid1.valueMatrix(i, 18) != "") {
										datagrid1.valueMatrix(i, 63) = datagrid1.valueMatrix(i, 18).ReplaceString("-", "");
										if (datagrid1.valueMatrix(i, 31) == "Y") {
											datagrid1.valueMatrix(i, 64) = datagrid1.valueMatrix(i, 31);
										}
										if (datagrid1.valueMatrix(i, 1) != "161501") { 								// 물품대부가세는 제외
											datagrid1.valueMatrix(i, 65) = Number(datagrid1.valueMatrix(i, 65)) + Number(datagrid1.valueMatrix(i, 4));
										}
										datagrid1.valueMatrix(i, 65) = Number(datagrid1.valueMatrix(i, 65)) + Number(datagrid1.valueMatrix(i, 5));
										datagrid1.valueMatrix(i, 65) = Number(datagrid1.valueMatrix(i, 65)) - Number(datagrid1.valueMatrix(i, 14));
										datagrid1.valueMatrix(i, 65) = Number(datagrid1.valueMatrix(i, 65)) - Number(datagrid1.valueMatrix(i, 17));
									}
								}
							}
