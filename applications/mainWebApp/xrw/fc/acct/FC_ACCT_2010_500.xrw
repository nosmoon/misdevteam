									var_with_tax = datagrid1.valueMatrix(i, 15);
									if (var_incm_tax != var_with_tax) {
										show_message("원천세가 틀립니다. 기존에 등록된 전표를 복사해서 사용하실 때에는 해당 건을 더블클릭하여 행수정 버튼을 눌러주시기 바랍니다.");
										return;
