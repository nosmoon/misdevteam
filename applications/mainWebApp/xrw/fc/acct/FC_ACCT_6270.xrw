						var sum24_070 = model1.getValue("/root/resData/inputData/fc_acct_6271_s/dataSet/v_secn_24_070");
						var sum24_080 = model1.getValue("/root/resData/inputData/fc_acct_6271_s/dataSet/v_secn_24_080");
						
						//조세특례제한법
						var sum105_1_1 = model1.getValue("/root/resData/inputData/fc_acct_6271_s/dataSet/j_secn_105_1_1");
						var sum105_1_3 = model1.getValue("/root/resData/inputData/fc_acct_6271_s/dataSet/j_secn_105_1_3");
						var sum105_1_32 = model1.getValue("/root/resData/inputData/fc_acct_6271_s/dataSet/j_secn_105_1_32");
						var sum105_1_4 = model1.getValue("/root/resData/inputData/fc_acct_6271_s/dataSet/j_secn_105_1_4");
						var sum105_1_5 = model1.getValue("/root/resData/inputData/fc_acct_6271_s/dataSet/j_secn_105_1_5");
						var sum107 = model1.getValue("/root/resData/inputData/fc_acct_6271_s/dataSet/j_secn_107");
						var sum121_13 = model1.getValue("/root/resData/inputData/fc_acct_6271_s/dataSet/j_secn_121_13");
						
						//부가가치세법 영세율 적용 공급실적 합계
						var secn_tot_amt = Number(sum21_010)+Number(sum21_020)+Number(sum21_030)+Number(sum21_040)+Number(sum21_050)
													+Number(sum22_010)+Number(sum23_010)+Number(sum23_020)+Number(sum24_010)+Number(sum24_020)
													+Number(sum24_030)+Number(sum24_040)+Number(sum24_050)+Number(sum24_060)+Number(sum24_070)
													+Number(sum24_080);
												
						model1.setValue("/root/resData/inputData/fc_acct_6271_s/dataSet/v_secn_tot_amt",secn_tot_amt);
						
						//조세특례제한법 및 그 밖의 법률에 따른 영세율 적용 공급실적 합계
						var j_secn_tot_amt = Number(sum105_1_1)+Number(sum105_1_3)+Number(sum105_1_32)+Number(sum105_1_4)+Number(sum105_1_5)+Number(sum107)+Number(sum121_13);
						model1.setValue("/root/resData/inputData/fc_acct_6271_s/dataSet/j_secn_tot_amt",j_secn_tot_amt);
						
						//총 합계
						var tot_amt = Number(secn_tot_amt)+Number(j_secn_tot_amt);
						model1.setValue("/root/resData/inputData/fc_acct_6271_s/dataSet/tot_amt",tot_amt);
						
						model1.refresh();
