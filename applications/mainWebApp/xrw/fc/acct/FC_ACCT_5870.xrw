				function make_string_date_rate(p_date,p_rate) {
					if(p_rate != 0){
						p_rate = p_rate.GetRound(-2,0);
					}
					else{
						p_rate = 0;
					}				
					
					if (p_date.length < 8){
						return false;
					}
					var yy = p_date.substr(0,4);
					var mm = p_date.substr(4,2);
					var dd = p_date.substr(6,2);					
					
					p_date = yy + "-" + mm + "-" + dd;

					if (false) return "";
					
					return p_date + " " +  p_rate + "%";
				}
