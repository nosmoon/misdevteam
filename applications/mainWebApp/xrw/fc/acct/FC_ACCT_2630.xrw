					}
					
					
					if(get_refvalue("/root/tempData/CURLIST/totalcnt") == 0)
					{
							show_message("검색결과가 없습니다.");
							set_refvalue("/root/resData/tot_dr_amt", "0");
							set_refvalue("/root/resData/tot_crdt_amt", "0");
							set_refvalue("/root/resData/tot_diff_amt", "0");
							set_refvalue("/root/resData/chk_dr_amt", "0");
							set_refvalue("/root/resData/chk_crdt_amt", "0");
							set_refvalue("/root/resData/chk_diff_amt", "0");
