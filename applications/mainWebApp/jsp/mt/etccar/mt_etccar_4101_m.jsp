			//rx.add(record, "cdabrvnm_cd", StringUtil.replaceToXml(rec.cdabrvnm_cd));
		}
		rx.add(recordSet, "totalcnt", ds.curlist3.size());
		/****** CURLIST3 END */
		
		/****** CURLIST2 BEGIN  청구물품종류*/
		recordSet = rx.add(dataSet, "clam_item_kind", "");
		for(int i = 0; i < ds.curlist1.size(); i++) {
			MT_ETCCAR_4101_MCURLIST1Record rec = (MT_ETCCAR_4101_MCURLIST1Record)ds.curlist1.get(i);
