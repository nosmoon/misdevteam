			//rx.add(record, "cdabrvnm_cd", StringUtil.replaceToXml(rec.cdabrvnm_cd));
		}
		rx.add(recordSet, "totalcnt", ds.curlist3.size());
		/****** CURLIST3 END */
		
		/****** CURLIST3 BEGIN 거래처구분 */
		recordSet = rx.add(dataSet, "cus_kind", "");
		for(int i = 0; i < ds.curlist2.size(); i++) {
			MT_ETCCAR_4101_MCURLIST2Record rec = (MT_ETCCAR_4101_MCURLIST2Record)ds.curlist2.get(i);
