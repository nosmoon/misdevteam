    private boolean showMenu(Vector jobcdSet, String menuID){

        boolean ret = false;
        for(int i=0; i<jobcdSet.size(); i++){
            String jobcd = (String)jobcdSet.elementAt(i);
            if(checkAuth(jobcd, menuID)){
                ret = true;
                break;
            }
        }
        return ret;
    }

    private boolean checkAuth(String jobcd, String menuID){
        boolean ret = false;
        int index = Integer.parseInt(menuID);
        for(int i=0; i<jobcdXmenu[index].length; i++){

            if(jobcdXmenu[index][i].equals(jobcd)){
                ret = true;
                break;
            }
        }

        return ret;
    }
