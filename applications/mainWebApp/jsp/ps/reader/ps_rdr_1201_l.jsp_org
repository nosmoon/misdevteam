                lo_form1.sh_phone1.select();
                return false;
            }
        }

        //입력항목 전체 체크
        if(!validate(lo_form1)) return false;

        if(flag=='1') {
          lo_form1.hd_print_flag.value = '1';
        } else if(flag=='2') {
          lo_form1.hd_print_flag.value = '2';
        }

		makeValue(lo_form1);
