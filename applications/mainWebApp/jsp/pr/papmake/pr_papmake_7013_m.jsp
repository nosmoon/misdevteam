<%@	page import="
		java.io.*
	,	java.sql.*
	,	java.util.*
	,	kr.co.comsquare.rwXmlLib.*
	,	kr.co.comsquare.db.*
	,	kr.co.comsquare.util.Tokenizer
	,	chosun.ciis.pr.papmake.rec.*
	,	chosun.ciis.pr.papmake.ds.*;
	"
%>

<%
	RwXml rx = new RwXml();
	PR_PAPMAKE_7013_MDataSet ds = (PR_PAPMAKE_7013_MDataSet)request.getAttribute("ds");
	int root = RwXml.rootNodeID;
	int dataSet = 0;
	int recordSet = 0;
	String errcode = ds.errcode;
	String errmsg = ds.errmsg;
	dataSet = rx.add(root, "dataSet", "");

	try {
		
		/****** COMMCDCUR1 BEGIN */
		recordSet = rx.add(dataSet, "ispt_clas4", "");
		for(int i = 0; i < ds.commcdcur1.size(); i++) {
			PR_PAPMAKE_7013_MCURLISTRecord rec = (PR_PAPMAKE_7013_MCURLISTRecord)ds.commcdcur1.get(i);
			int record = rx.add(recordSet, "item", "");
			//int record = rx.add(cd_clsf, "item", "");
			rx.add(record, "value", rec.cd);
			rx.addCData(record, "label", rec.cd+" "+rec.cdnm);
			//rx.add(record, "label", rec.cd+" "+StringUtil.replaceToXml(rec.cdnm));
		}

	}
	catch (Exception e) {
		errcode += " JSP Error";
		errmsg += " JSP Error Message = " + e.getMessage();
	}
	finally {
		rx.add(dataSet, "errcode", errcode);
		rx.add(dataSet, "errmsg", errmsg);
		out.println(rx.xmlFlush());
		out.println(rx.xmlEndFlush());
	}
%>

